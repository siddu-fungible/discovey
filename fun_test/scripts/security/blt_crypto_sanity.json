{
  "BLTKey256": {
    "volume_count": 1,
    "volume_details": {
      "capacity": 20971520,
      "block_size": 4096,
      "encrypt": "enable"
    },
    "key_size": 32,
    "xtweak_size": 8,
    "command_timeout": 5,
    "iter_interval": 2,
