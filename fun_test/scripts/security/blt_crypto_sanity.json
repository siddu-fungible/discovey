      "read": {"decrypt_done": 1024, "decrypt_fails": 0, "decrypt_reqs": 1024},
      "randwrite": {"decrypt_done": 1024, "decrypt_fails": 0, "decrypt_reqs": 1024},
      "randread": {"decrypt_done": 1024, "decrypt_fails": 0, "decrypt_reqs": 1024}
    },
    "expected_encryption_stats": {
      "write": {"encrypt_done": 1024, "encrypt_fails": 0, "encrypt_reqs": 1024},
      "read": {"encrypt_done": 0, "encrypt_fails": 0, "encrypt_reqs": 0},
