{
  "GlobalSetup": {
    "bootargs": ["app=mdt_test,load_mods cc_huid=3 workload=storage --serial --dpc-server --dpc-uart --all_100g",
      "app=mdt_test,load_mods cc_huid=2 workload=storage --serial --dpc-server --dpc-uart --all_100g"],
    "disable_f1_index": 1,
    "f1_in_use": 0,
    // Script won't disturb the syslog level setting, if it is set to "default". If any other level is needed set this config to that level in int format
    "syslog": "default",
    "attach": {"huid": 7, "ctlid": 0, "transport": "TCP", "port": 1099, "nvme_subsystem": "nqn.2017-05.com.fungible:nss-uuid1"},
    "command_timeout": 5,
    "reboot_timeout": 300,
    "num_ssd": 12,
    "dut_start_index": 2,
    "host_start_index": 1,
    "update_workspace": false,
    "update_deploy_script": false,
    "funcp_mode": ["ep", "storage"],
    "num_f1_per_fs": 1,
    "num_f1s": 1,
    "num_hosts": 1,
    "fpg_int_prefix": "fpg",
    "load_modules": ["nvme", "nvme_tcp"],
    "huid": 7,
    "ctlid": 0,
    "ethernet_adapter": "Mellanox",
    "override_numa_node": {"override": true, "override_node": "NUMA node0 CPU(s)"},
    "api_server_port": 50220,
    "api_server_username": "admin",
    "api_server_password": "password",
    "install": "fresh",
    "sc_script_dir": "/opt/fungible/StorageController/etc",
    "run_sc_script": "start_sc.sh",
    "run_sc_restart_timeout": 120,
    "container_up_timeout": 120,
    "api_server_up_timeout": 300
  },
  "DurVolSingleDriveFailRebuild": {
    "ec_info": {
      "volume_types": {
        "ndata": "VOL_TYPE_BLK_LOCAL_THIN",
        "nparity": "VOL_TYPE_BLK_LOCAL_THIN",
        "ec": "VOL_TYPE_BLK_EC",
        "lsv": "VOL_TYPE_BLK_LSV",
        "jvol": "VOL_TYPE_BLK_NV_MEMORY"
      },
      "volume_block": {
        "ndata": 4096,
        "nparity": 4096,
        "ec": 4096,
        "lsv": 4096,
        "jvol": 512
      },
      "ndata": 4,
      "nparity": 2,
      "capacity": 68719476736,
      "use_lsv": 1,
      "lsv_pct": 1,
      "lsv_chunk_size": 128,
      "jvol_size_multiplier": 8,
      "num_volumes": 1
    },
    // timeout used for the storage controller
    "command_timeout": 10,
    "ns_id": 1,
    "nvme_io_queues": 1,
    "back_pressure": false,
    "back_pressure_io": {"fio_cmd_args": "sudo fio --group_reporting --output-format=json --ioengine=libaio  --numjobs=1 --direct=1 --prio=0 --rw=write --cpus_allowed_policy=split --norandommap --randrepeat=0 --iodepth=8 --bs=4k --time_based --runtime=7200 --name=job0 ", "timeout": 7800},
    "nvme_device": "/dev/nvme",
    "trigger_failure": true,
    "plex_failure_count": 1,
    // If 'fail_drive' is true, initiate drive failure else initiate plex failure
    "fail_drive": true,
    "rebuild_on_spare_volume": true,
    "rebuild_rate": 80,
    "rebuild_timeout": 3600,
    // failure mode can take random or fixed as its value. If it is fixed, initialize failure_drive_index
    "failure_mode": "random",
    "failure_start_time_ratio": 0.05,
    "test_volume_start_index": 0,
    "attach_transport": "TCP",
    "transport_port": 1099,
    "huid": 7,
    "ctlid": 0,
    "nvme_subsystem": "nqn.2017-05.com.fungible:nss-uuid1",
    "warm_up_fio_cmd_args": {"bs": "1024k", "iodepth": 1, "size": "100%", "rw": "write", "direct": 1, "prio": 0, "cpus_allowed_policy": "split", "timeout": 3600},
    //"fio_cmd_args": {"multiple_jobs": "--direct=1 --prio=0 --rw=randrw --rwmixread=70 --ramp_time=10 --cpus_allowed_policy=split --norandommap --randrepeat=0 --cpus_allowed={} --numjobs={} --iodepth={} --bs=4k --io_size={}", "timeout": 150},
    "fio_write_cmd_args": {"bs": "4k", "iodepth": 1, "size": "50%", "rw": "write", "direct": 1, "prio": 0, "cpus_allowed_policy": "split", "verify":"md5", "do_verify": 0, "timeout": 3600},
    "fio_verify_cmd_args": {"bs": "4k", "iodepth": 1, "size": "50%", "rw": "read", "direct": 1, "prio": 0, "cpus_allowed_policy": "split", "verify":"md5", "do_verify": 1, "timeout": 3600},
    "fio_job_name": "inspur_8k_random_read_write"
  },
