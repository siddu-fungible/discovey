{
  "GlobalSetup": {
    "bootargs": "app=mdt_test,load_mods,hw_hsu_test --serial --memvol --dis-stats --dpc-server --dpc-uart --csr-replay --all_100g --nofreeze --useddr",
    "disable_f1_index": 1,
    "f1_in_use": 0,
    "huid": 7,
    "ctlid": 0,
    "syslog_level": 2,
    "command_timeout": 5,
    "retries": 60,
    "test_network": {
      "test_interface_ip": "21.1.1.1/24",
      "test_interface_mac": "fe:dc:ba:44:66:31",
      "test_net_route": {
        "net": "29.1.1.0/24",
        "gw": "21.1.1.2",
        "arp": "00:de:ad:be:ef:00"
      },
      "f1_loopback_ip": "29.1.1.1"
    }
  },
  "RandReadWrite8kBlocks": {
    "ec_info": {
      "volume_types": {
        "ndata": "VOL_TYPE_BLK_LOCAL_THIN",
        "nparity": "VOL_TYPE_BLK_LOCAL_THIN",
        "ec": "VOL_TYPE_BLK_EC",
        "lsv": "VOL_TYPE_BLK_LSV",
        "jvol": "VOL_TYPE_BLK_NV_MEMORY"
      },
      "volume_block": {
        "ndata": 4096,
        "nparity": 4096,
        "ec": 4096,
        "lsv": 4096,
        "jvol": 512
      },
      "ndata": 4,
      "nparity": 2,
      //"capacity": 3298534883328, // 3TB
      //"capacity": 1099511627776, // 1TB
      "capacity": 549755813888, // 512GB
      "use_lsv": 1,
      "lsv_pct": 0.30,
      "lsv_chunk_size": 128,
      "jvol_size_multiplier": 8,
      "num_volumes": 1
    },
    // timeout used for the storage controller
    "command_timeout": 30,
    "iter_interval": 60,
    // Head room needed for the LS volume in percentage
    "ns_id": 1,
    "attach_transport": "TCP",
    "transport_port": 1099,
    "huid": 7,
    "ctlid": 0,
    "nvme_subsystem": "nqn.2017-05.com.fungible:nss-uuid1",
    "vdbench_path": "/usr/local/share/vdbench",
    "warm_up_traffic": 1,
    "warm_up_timeout": 5400, //1 hrs 30 mins // to write 512GB data with 32 threads, it takes ~45 mins
    "warm_up_vdb_config": "sd=sd1,lun=/dev/nvme0n1,openflags=o_direct,threads=32\nwd=wd1,sd=sd*,seekpct=eof,rdpct=0,xfersize=8k\nrd=rd1,wd=wd1,iorate=max,elapsed=6h,interval=30",
    "warm_up_config_file": "populate_disk",
    "read_pct": 0.7,
    "perf_run_vdb_config": "sd=sd1,lun=/dev/nvme0n1,openflags=o_direct,threads=8\nwd=wd1,sd=sd*,seekpct=100,rdpct=70,xfersize=8k\nrd=rd1,wd=wd1,iorate=max,elapsed=60,maxdata=512g,interval=10,warmup=10",
    "perf_run_config_file": "inspur_8k_random_read_write_vdbench",
    "perf_run_timeout": 180,
    "nvme_device": "/dev/nvme",
    "io_queues": 0
  },
  "SequentialReadWrite1024kBlocks": {
