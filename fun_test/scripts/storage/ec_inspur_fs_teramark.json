{
  "GlobalSetup": {
    "bootargs": "app=mdt_test,load_mods,hw_hsu_test --serial --memvol --dis-stats --dpc-server --dpc-uart --csr-replay --all_100g --nofreeze",
    "disable_f1_index": 1,
    "f1_in_use": 0,
    "huid": [3, 2],
    "ctlid": [2, 2],
    "syslog_level": 2,
    "command_timeout": 5,
    "retries": 60
  },
  "RandReadWrite8kBlocks": {
    "ec_info": {
      "volume_types": {"ndata": "VOL_TYPE_BLK_LOCAL_THIN", "nparity": "VOL_TYPE_BLK_LOCAL_THIN",
                       "ec": "VOL_TYPE_BLK_EC", "lsv": "VOL_TYPE_BLK_LSV", "jvol": "VOL_TYPE_BLK_NV_MEMORY"},
      "volume_block": {"ndata": 4096, "nparity": 4096, "ec": 4096, "lsv": 4096, "jvol": 512},
      "ndata": 4,
      "nparity": 2,
      "capacity": 3298534883328,
      "use_lsv": 1,
      "lsv_pct": 0.30,
      "lsv_chunk_size": 128,
      "jvol_size_multiplier": 8,
      "num_volumes": 3
    },
    // timeout used for the storage controller
    "command_timeout": 30,
    "iter_interval": 120,
    // Head room needed for the LS volume in percentage
    "ns_id": 1,
    "attach_transport": "TCP",
    "transport_port": 1099,
    "nvme_subsystem": "nqn.2017-05.com.fungible:nss-uuid1",
    "vdbench_path": "/usr/local/share/vdbench",
    "warm_up_traffic": 1,
    "warm_up_timeout": 120,
    "warm_up_vdb_config": "sd=sd1,lun=/dev/nvme0n1,openflags=o_direct,threads=1,size=3000g\nsd=sd2,lun=/dev/nvme0n2,openflags=o_direct,threads=1,size=3000g\nsd=sd3,lun=/dev/nvme0n3,openflags=o_direct,threads=1,size=3000g\nwd=wd1,sd=sd*,seekpct=0,rdpct=0,xfersize=8k\nrd=rd1,wd=wd1,iorate=max,elapse=1h,maxdata=1,interval=60",
    "warm_up_config_file": "populate_disk",
    "nvme_device": "/dev/nvme0",
    "io_queues": 0
  }
}
