  "GlobalSetup": {
    // Erasure coding ratio
    "ec_coding": {"ndata": 2, "nparity": 1},
    // Whether a separate DPU needs to be created to confiugre the EC & LS volumes of the underlying EC volume through RDS
    "ec_in_sep_dpu": 1
