  "EC21FioSeqWriteSeqReadOnly": {
    // Erasure coding ratio
    "ec_coding": {"ndata": 2, "nparity": 1},
    "volume_types": {"ndata": "VOL_TYPE_BLK_LOCAL_THIN", "nparity": "VOL_TYPE_BLK_LOCAL_THIN",
                     "rds": "VOL_TYPE_BLK_RDS", "ec": "VOL_TYPE_BLK_EC", "lsv": "VOL_TYPE_BLK_LSV"},
    "volume_capacity": {"ndata": 268435456, "nparity": 268435456, "rds": 268435456, "ec": 268435456},
    "volume_block": {"ndata": 4096, "nparity": 4096, "rds": 4096, "ec": 4096, "lsv": 4096},
    "ns_id": {"ndata": 1, "nparity": 1, "rds": 1, "ec": 1, "lsv": 1},
    // timeout used for the storage controller
    "command_timeout": 5,
    "iter_interval": 2,
    "use_lsv": 1,
    "use_rds": 1,
    // Head room needed for the LS volume in percentage
    "lsv_pct": 0.3,
    "check_lsv_stats": 0,
    // FIO commandline arguments
    "fio_cmd_args": {"io_queues": 2, "size": "4m", "timeout": 180},
    "fio_modes": ["write", "read"],
    // The below is the list of tuples in which the first element of the tuple represents the FIO block size in kB and
    // the second element represents the IO depth
    // reduced the size from 16mb to 4mb and iodepth from different values to 1, as a workaround to the bugs #702 & #703
    "fio_bs_iodepth": ["(4, 1)", "(8, 1)", "(16, 1)", "(32, 1)"],
    // Passing threshold for the FIO results from the expected benchmark values
    "fio_pass_threshold": 0.05,
    // Passing threshold of the internal volume stats from the expected benchmark values
    "volume_pass_threshold": 33,
    "expected_fio_result": {
      "(4, 1)": {"write": {"write": {"bw": 62, "iops": 16, "latency": 62216}},
                 "read": {"read": {"bw": 122, "iops": 30, "latency": 33997}}},
      "(8, 1)": {"write": {"write": {"bw": 102, "iops": 12, "latency": 76084}},
                 "read": {"read": {"bw": 222, "iops": 26, "latency": 35860}}},
      "(16, 1)": {"write": {"write": {"bw": 106, "iops": 6, "latency": 148219}},
                  "read": {"read": {"bw": 368, "iops": 22, "latency": 41566}}},
      "(32, 1)": {"write": {"write": {"bw": 108, "iops": 3, "latency": 292512}},
                  "read": {"read": {"bw": 448, "iops": 14, "latency": 68699}}}
    },
    "expected_volume_stats": {"write": {"ndata": {"0": {"fault_injection": 0, "num_reads": 35, "num_writes": 528},
                                                  "1": {"fault_injection": 0, "num_reads": 35, "num_writes": 528}},
