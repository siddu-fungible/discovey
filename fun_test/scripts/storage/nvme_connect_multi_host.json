  "MultiNvmeConnect": {
    "blt_count": 4,
    "no_of_nvme_connect": 50,
    "blt_details": {
      "ns_id": 1,
      "type": "VOL_TYPE_BLK_LOCAL_THIN",
      "capacity": 99999997952,
      "block_size": 4096
    },
    "controllers": 1,
    "syslog": 6,
    "nqn": "nqn1",
    "transport_type": "tcp",
    "transport_port": 1099,
    "nvme_io_q": 16,
    "command_timeout": 30,
    "iter_interval": 2,
    "warm_up_traffic": 1,
    "warm_up_fio_cmd_args": {
      "rw": "write", "bs": "8k",
      "iodepth": 1, "numjobs": 1,
      "ioengine": "libaio", "direct": 1, "prio": 0, "cpus_allowed": "1-19",
      "size": "100%", "timeout": 7200
    },
    "fio_modes": [
      "randread"
    ],
    "fio_bs": "4k",
    "fio_jobs_iodepth": ["(1, 1)", "(32, 1)", "(32, 2)"],
    "fio_job_name": {
      "randread": "nhosts"
    },
    "nvme_device": "/dev/nvme0",
    "host_count": 1
  },
  "NVMeConnectWithSpurious": {
    "blt_count": 1,
    "attach_host": 1,
    "non_attach_host": 1,
