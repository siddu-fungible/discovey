{
  "GlobalSetup": {
    "bootargs": [
      "app=mdt_test,load_mods cc_huid=3 workload=storage module_log=tcp:DEBUG --serial --dpc-uart --all_100g --nofreeze",
      "app=mdt_test,load_mods cc_huid=2 workload=storage module_log=tcp:DEBUG --serial --dpc-uart --all_100g --nofreeze"
    ],
    "dut_start_index": 0,
    "host_start_index": 0,
    "update_workspace": false,
    "update_deploy_script": false,
    "funcp_mode": ["ep", "storage"],
    "num_f1_per_fs": 1,
