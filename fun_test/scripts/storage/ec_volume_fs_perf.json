{
  "GlobalSetup": {
    "bootargs": "app=mdt_test,load_mods,hw_hsu_test workload=storage --serial --memvol --dpc-server --dpc-uart --csr-replay --all_100g --nofreeze  --useddr",
    "disable_f1_index": 1,
    "f1_in_use": 0,
    "huid": 3,
    "ctlid": 2,
    "ns_id": 1,
    "fnid": 2,
    "transport": "PCI",
    "syslog_level": 2,
    "reboot_timeout": 300,
    "num_ssd": 6,
    "num_volume": 1,
    "ec_coding": {
      "ndata": 4,
      "nparity": 2
    },
    "ec_info": {
      "volume_types": {
        "ndata": "VOL_TYPE_BLK_LOCAL_THIN",
        "nparity": "VOL_TYPE_BLK_LOCAL_THIN",
        "ec": "VOL_TYPE_BLK_EC",
        "lsv": "VOL_TYPE_BLK_LSV",
        "jvol": "VOL_TYPE_BLK_NV_MEMORY"
      },
      "volume_block": {
        "ndata": 4096,
        "nparity": 4096,
        "ec": 4096,
        "lsv": 4096,
        "jvol": 512
      },
      "ndata": 4,
      "nparity": 2,
      "capacity": 26843545600,
      "use_lsv": 1,
      "lsv_pct": 1,
      "lsv_chunk_size": 128,
      "jvol_size_multiplier": 8,
      "num_volumes": 1
    },
    "command_timeout": 30,
    "warm_up_traffic": 1,
    "warm_up_fio_cmd_args": {
      "bs": "8k",
      "iodepth": 1,
      "size": "100%",
      "rw": "write",
      "direct": 1,
      "prio": 0,
      "timeout": 720
    }
  },
  "EC42FioSeqRandRead": {
    "command_timeout": 30,
    "iter_interval": 1,
    "fio_cmd_args": {
      "size": "100%",
      "direct": 1,
      "prio": 0,
      "cpus_allowed": "0-7",
      "runtime": 300,
      "timeout": 360,
      "bs": "4k",
      "numjobs": 1,
      "iodepth": 1,
      "rw": "read",
      "name": "fio_ec_default"
    },
    "fio_modes": [
      "read",
      "randread"
    ],
    "fio_njobs_iodepth": [
      "(1, 1)",
      "(8, 1)",
      "(8, 2)",
      "(8, 4)",
      "(8, 8)",
      "(8, 16)"
    ]
  }
}
