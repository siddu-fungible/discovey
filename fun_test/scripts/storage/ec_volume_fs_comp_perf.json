{
  "GlobalSetup": {
    "bootargs": "app=mdt_test,load_mods workload=storage --serial --memvol --dpc-server --dpc-uart --csr-replay --all_100g --useddr",
    "disable_f1_index": 1,
    "f1_in_use": 0,
    "huid": 3,
    "ctlid": 2,
    "fnid": 2,
    "transport": "PCI",
    "reboot_timeout": 300,
    "num_ssd": 6,
    "num_volume": 1,
    "ec_coding": {
      "ndata": 4,
      "nparity": 2
    },
    "command_timeout": 30
  },
  "EC42FioReadEffortAuto": {
    "ns_id": 1,
    "ec_info": {
      "volume_types": {
        "ndata": "VOL_TYPE_BLK_LOCAL_THIN",
        "nparity": "VOL_TYPE_BLK_LOCAL_THIN",
        "ec": "VOL_TYPE_BLK_EC",
        "lsv": "VOL_TYPE_BLK_LSV",
        "jvol": "VOL_TYPE_BLK_NV_MEMORY"
      },
      "volume_block": {
        "ndata": 4096,
        "nparity": 4096,
        "ec": 4096,
        "lsv": 4096,
        "jvol": 512
      },
      "ndata": 4,
      "nparity": 2,
      "capacity": 26843545600,
      "use_lsv": 1,
      "lsv_pct": 0.25,
      "lsv_chunk_size": 128,
      "jvol_size_multiplier": 4,
      "num_volumes": 1,
      "compress": true,
      "zip_effort": "ZIP_EFFORT_64Gbps",
      "zip_filter": "FILTER_TYPE_DEFLATE"
    },
    "syslog_level": 2,
    "write_fio_cmd_args": {
      "bs": "4k",
      "iodepth": 8,
      "size": "100%",
      "rw": "write",
      "direct": 1,
      "prio": 0,
      "timeout": 720,
      "buffer_pattern": "0xdeadbeef",
      "buffer_compress_percentage": 50
    },
    "command_timeout": 30,
    "iter_interval": 1,
    "read_fio_cmd_args": {
      "bs": "4k",
      "numjobs": 8,
      "cpus_allowed": "0-7",
      "iodepth": 16,
      "size": "100%",
      "rw": "read",
      "name": "ec_fio_25G",
      "direct": 1,
      "prio": 0,
      "runtime": 180,
      "timeout": 185
    },
    "fio_modes": [
      "read",
      "randread",
      "write"
    ],
    "test_parameters": [
      {
        "name": "Performance stats for 1% compressed data",
        "compress_percent": 1
      },
      {
        "name": "Performance stats for 50% compressed data",
        "compress_percent": 50
      },
      {
        "name": "Performance stats for 80% compressed data",
        "compress_percent": 80
      }
    ]
  },
  "EC42FioReadCompDisabled": {
    "ns_id": 1,
    "ec_info": {
      "volume_types": {
        "ndata": "VOL_TYPE_BLK_LOCAL_THIN",
        "nparity": "VOL_TYPE_BLK_LOCAL_THIN",
        "ec": "VOL_TYPE_BLK_EC",
        "lsv": "VOL_TYPE_BLK_LSV",
        "jvol": "VOL_TYPE_BLK_NV_MEMORY"
      },
      "volume_block": {
        "ndata": 4096,
        "nparity": 4096,
        "ec": 4096,
        "lsv": 4096,
        "jvol": 512
      },
      "ndata": 4,
      "nparity": 2,
      "capacity": 26843545600,
      "use_lsv": 1,
      "lsv_pct": 1,
      "lsv_chunk_size": 128,
      "jvol_size_multiplier": 4,
      "num_volumes": 1
    },
    "syslog_level": 2,
    "write_fio_cmd_args": {
      "bs": "4k",
      "iodepth": 8,
      "size": "100%",
      "rw": "write",
      "direct": 1,
      "prio": 0,
      "timeout": 720,
      "buffer_pattern": "0xdeadbeef",
      "buffer_compress_percentage": 50
    },
    "command_timeout": 30,
    "iter_interval": 1,
    "read_fio_cmd_args": {
      "bs": "4k",
      "numjobs": 8,
      "cpus_allowed": "0-7",
      "iodepth": 16,
      "size": "100%",
      "rw": "read",
      "name": "ec_fio_25G_comp_disabled",
      "direct": 1,
      "prio": 0,
      "runtime": 180,
      "timeout": 185
    },
    "fio_modes": [
      "read",
      "randread",
      "write"
    ],
    "test_parameters": [
      {
        "name": "Performance stats for Compression Disabled",
        "compress_percent": 50
      }
    ]
  }
}
