{
  "GlobalSetup": {
    "bootargs": ["app=mdt_test,load_mods cc_huid=3 workload=storage --serial --dpc-server --dpc-uart --all_100g",
      "app=mdt_test,load_mods cc_huid=2 workload=storage --serial --dpc-server --dpc-uart --all_100g"],
    "dut_start_index": 0,
    "host_start_index": 0,
    "reboot_hosts" : true,
    "update_workspace": false,
    "update_deploy_script": false,
    "funcp_mode": ["ep", "storage"],
    "num_f1_per_fs": 2,
    "num_hosts": 1,
    "fpg_int_prefix": "fpg",
    "disable_f1_index": 0,
    "f1_in_use": 0,
    "load_modules": ["nvme", "nvme_tcp"],
    "huid": 7,
    "ctlid": 0,
    "syslog": "default",
    "command_timeout": 5,
    "reboot_timeout": 600,
    "ethernet_adapter": "Mellanox",
    "override_numa_node": {"override": true, "override_node": "NUMA node0 CPU(s)"},
    "csi_perf_pdtrace_hbm_size_kb": 204800,
    "api_server_port": 50220,
    "api_server_username": "admin",
    "api_server_password": "password",
    "install": "fresh",
    "sc_script_dir": "/opt/fungible/StorageController/etc",
    "run_sc_script": "start_sc.sh",
    "run_sc_restart_timeout": 120,
    "container_up_timeout": 120,
    "api_server_up_timeout": 300,
    "drive_format_timeout": 120
  },
  "CreateAttachDetachDeleteMultivolMultihost": {
    "ec_info": {
      "capacity": 1073741824,
      "data_protection": {"num_failed_disks": 2, "num_redundant_dpus": 0},
      "encrypt": true,
      "allow_expansion": false,
      "stripe_count": 0,
      "compression_effort": false
    },
    "test_rail_case_id": ["C18036"],
    "num_ssd": 12,
    "raw_api_call": True,
    "command_timeout": 30,
    "test_iteration_count": 1,
    "iter_interval": 10,
    "ns_id": 1,
    "attach_transport": "TCP",
    "transport_port": 4420,
    "huid": 7,
    "ctlid": 0,
    "nvme_device": "/dev/nvme",
    "nvme_subsystem": "nqn.2017-05.com.fungible:nss-uuid1",
    "nvme_io_queues": 16,
    "fio_write_cmd_args": {"name": "RecoveryWithMFailure_RandWrite","ioengine": "libaio", "size": "100%", "rw": "randwrite", "direct": 1, "prio": 0, "iodepth": 8, "numjobs": 1, "verify": "md5", "do_verify": 0, "cpus_allowed_policy": "split", "refill_buffers": "NO_VALUE", "timeout": 3600},
    "fio_read_cmd_args": {"name": "RecoveryWithMFailure_RandRead","ioengine": "libaio", "size": "100%", "rw": "randread", "direct": 1, "prio": 0, "iodepth": 1, "numjobs": 1, "do_verify": 1, "verify": "md5", "cpus_allowed_policy": "split","timeout": 3600},
    "mpstat_args": {"interval": 60, "output_file": "/tmp/mpstat.out", "cpu_list": "ALL"},
    "stats_collect_details": [{"vp_utils": {"interval": 60}}, {"per_vp": {"interval": 60}}, {"resource_bam_args": {"interval": 60}}, {"vol_stats": {"interval": 60}}, {"vppkts_stats": {"interval": 60}}, {"psw_stats": {"interval": 60, "nu": true}}, {"fcp_stats": {"interval": 60, "nu": true}}, {"wro_stats": {"interval": 60, "nu": true}}, {"erp_stats": {"interval": 60, "nu": true}}, {"etp_stats": {"interval": 60, "nu": true}}, {"eqm_stats": {"interval": 60}}, {"hu_stats": {"interval": 60}}, {"ddr_stats": {"interval": 60}}, {"ca_stats": {"interval": 60}}, {"cdu_stats": {"interval": 60}}],
    "collect_network_stats": false,
    "csi_perf_iodepth": [64],
    "cal_amplification": true,
    //value for plex_fail_method could be either 'ssd_power_off' or 'drive_pull'
    "plex_fail_method": "drive_pull",
    //value for combination_of_plex_failure could be either 'random' or 'all'
    "plex_failure_combination": "all"
  }
}
