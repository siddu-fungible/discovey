{
  "GlobalSetup": {
    "bootargs": ["app=load_mods cc_huid=3 workload=storage --serial --dpc-server --dpc-uart --all_100g",
      "app=load_mods cc_huid=2 workload=storage --serial --dpc-server --dpc-uart --all_100g"],
    "update_workspace": false,
    "update_deploy_script": false,
    "funcp_mode": ["ep", "storage"],
    "num_f1_per_fs": 2,
    "num_f1s": 1,
    "num_hosts": 1,
    "fpg_int_prefix": "fpg",
    "disable_f1_index": 0,
    "f1_in_use": 0,
    "load_modules": ["nvme", "nvme_tcp"],
    "syslog": "default",
    "command_timeout": 5,
    "reboot_timeout": 600,
    "ethernet_adapter": "Mellanox",
    "override_numa_node": {"override": true, "override_node": "NUMA node0 CPU(s)"},
    "csi_perf_pdtrace_hbm_size_kb": 204800,
    "api_server_port": 50220,
    "api_server_username": "admin",
    "api_server_password": "password",
    "install": "fresh",
    "sc_script_dir": "/opt/fungible/StorageController/etc",
    "run_sc_script": "start_sc.sh",
    "run_sc_restart_timeout": 120,
    "container_up_timeout": 120,
    "api_server_up_timeout": 300,
    "reboot_hosts": false,
    "already_deployed": false,
    "space_savings": 25
  },
  "RecoveryWithMFailure": {
    "test_rail_case_id": ["C18803"],
    "ec_list": ["4,2"],
    "ec_info": {
      "volume_types": {
        "ndata": "VOL_TYPE_BLK_LOCAL_THIN",
        "nparity": "VOL_TYPE_BLK_LOCAL_THIN",
        "ec": "VOL_TYPE_BLK_EC",
        "lsv": "VOL_TYPE_BLK_LSV",
        "jvol": "VOL_TYPE_BLK_NV_MEMORY"
      },
      "volume_block": {
        "ndata": 4096,
        "nparity": 4096,
        "ec": 4096,
        "lsv": 4096,
        "jvol": 512
      },
      "capacity": 34359738368,
      "use_lsv": 1,
      "lsv_pct": 1,
      "lsv_chunk_size": 128,
      "jvol_size_multiplier": 8,
      "num_volumes": 1,
      "compress": true
    },
    "num_ssd": 12,
    "command_timeout": 30,
    "iter_interval": 10,
    "ns_id": 1,
    "attach_transport": "TCP",
    "transport_port": 4420,
    "nvme_subsystem": "nqn.2017-05.com.fungible",
    "nvme_device": "/dev/nvme",
    "nvme_io_queues": 16,
    "parallel_warm_up": false,
    "fio_write_cmd_args": {"ioengine": "libaio", "size": "100%", "rw": "randwrite", "direct": 1, "iodepth": 8, "numjobs": 1, "verify": "md5", "do_verify": 0, "refill_buffers": "NO_VALUE", "buffer_pattern": "0xdeadface", "timeout": 3600},
    "fio_read_cmd_args": {"ioengine": "libaio", "size": "100%", "rw": "randread", "direct": 1, "iodepth": 8, "numjobs": 1, "do_verify": 1, "verify": "md5", "refill_buffers": "NO_VALUE", "buffer_pattern": "0xdeadface", "timeout": 3600},
    "fio_run_timeout": 150,
    "fio_size_timeout": 3600,
    "mpstat_args": {"interval": 60, "output_file": "/tmp/mpstat.out", "cpu_list": "ALL"},
    "stats_collect_details": [{"vp_utils": {"interval": 60}}, {"per_vp": {"interval": 60}}, {"resource_bam_args": {"interval": 60}}, {"vol_stats": {"interval": 60}}, {"vppkts_stats": {"interval": 60}}, {"psw_stats": {"interval": 60, "nu": true}}, {"fcp_stats": {"interval": 60, "nu": true}}, {"wro_stats": {"interval": 60, "nu": true}}, {"erp_stats": {"interval": 60, "nu": true}}, {"etp_stats": {"interval": 60, "nu": true}}, {"eqm_stats": {"interval": 60}}, {"hu_stats": {"interval": 60}}, {"ddr_stats": {"interval": 60}}, {"ca_stats": {"interval": 60}}, {"cdu_stats": {"interval": 60}}],
    "collect_network_stats": false,
    //value for plex_fail_method could be either 'ssd_power_off' or 'inject_fault'
    "plex_fail_method": "drive_pull",
    //value for combination_of_plex_failure could be either 'random' or 'all'
    "plex_failure_combination": "random"
  },
