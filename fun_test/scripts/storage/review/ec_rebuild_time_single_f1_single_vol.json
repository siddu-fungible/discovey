{
  "GlobalSetup": {
    "bootargs": ["app=mdt_test,load_mods cc_huid=3 workload=storage --serial --dpc-server --dpc-uart --all_100g",
      "app=mdt_test,load_mods cc_huid=2 workload=storage --serial --dpc-server --dpc-uart --all_100g"],
    "dut_start_index": 2,
    "host_start_index": 1,
    "update_workspace": false,
    "update_deploy_script": false,
    "funcp_mode": ["ep", "storage"],
    "num_f1_per_fs": 2,
    "num_f1s": 1,
    "num_hosts": 1,
    "fpg_int_prefix": "fpg",
    "disable_f1_index": 0,
    "f1_in_use": 0,
    "load_modules": ["nvme", "nvme_tcp"],
    "huid": 7,
    "ctlid": 0,
    "syslog_level": 2,
    "command_timeout": 5,
    "reboot_timeout": 600,
    "ethernet_adapter": "Mellanox",
    "override_numa_node": {"override": true, "override_node": "NUMA node0 CPU(s)"},
    "api_server_port": 50220,
    "api_server_username": "admin",
    "api_server_password": "password",
    "install": "fresh",
    "sc_script_dir": "/opt/fungible/StorageController/etc",
    "run_sc_script": "start_sc.sh",
    "run_sc_restart_timeout": 120,
    "container_up_timeout": 120,
    "api_server_up_timeout": 300
  },
  "ECVolRebuildSingleDiskFailure": {
    "ec_info": {
      "volume_types": {
        "ndata": "VOL_TYPE_BLK_LOCAL_THIN",
        "nparity": "VOL_TYPE_BLK_LOCAL_THIN",
        "ec": "VOL_TYPE_BLK_EC",
        "lsv": "VOL_TYPE_BLK_LSV",
        "jvol": "VOL_TYPE_BLK_NV_MEMORY"
      },
      "volume_block": {
        "ndata": 4096,
        "nparity": 4096,
        "ec": 4096,
        "lsv": 4096,
        "jvol": 512
      },
      "ndata": 4,
      "nparity": 2,
      "capacity": 1099511627776,
      "use_lsv": 1,
      "lsv_pct": 1,
      "lsv_chunk_size": 128,
      "jvol_size_multiplier": 4,
      "num_volumes": 1
    },
    "num_ssd": 12,
    "command_timeout": 30,
    "iter_interval": 10,
    "ns_id": 1,
    "attach_transport": "TCP",
    "transport_port": 1099,
    "huid": 7,
    "ctlid": 0,
    "nvme_subsystem": "nqn.2017-05.com.fungible:nss-uuid1",
    "nvme_device": "/dev/nvme",
    "io_queues": 16,
    "warm_up_traffic": 1,
    "parallel_warm_up": true,
    "warm_up_fio_cmd_args": {"iodepth": 16, "numjobs": 1 ,"size": "100%", "rw": "randwrite", "direct": 1, "prio": 0, "cpus_allowed_policy": "split", "verify":"md5", "do_verify": 0, "norandommap": "NO_VALUE", "randrepeat": 0, "timeout": 3600},
    // "fio_cmd_args": {"multiple_jobs": "--direct=1 --prio=0 --rw=randread --cpus_allowed_policy=split --norandommap --randrepeat=0 --numjobs=1 --bs=16k --iodepth=16 --cpus_allowed={}", "timeout": 420},
    "fio_cmd_args": {"iodepth": 16, "numjobs": 1,"size": "100%", "rw": "randread", "direct": 1, "prio": 0, "cpus_allowed_policy": "split",  "verify":"md5", "do_verify": 1, "norandommap": "NO_VALUE", "randrepeat": 0, "timeout": 3600},
    "fio_runtime": 360,
    "fio_run_timeout": 420,
    "fio_size_timeout": 3600,
    "fio_modes": ["randrw"],
    "fio_iodepth": [16],
    "full_run_iodepth": [16],
    "fio_job_name": "ec_vol_rebuild",
    "mpstat_args": {"interval": 60, "output_file": "/tmp/mpstat.out", "cpu_list": "ALL"},
    "iostat_args": {"interval": 1, "output_file": "/tmp/iostat.out"},
    "vp_util_args": {"interval": 60},
    "resource_bam_args": {"interval": 60},
    "collect_network_stats": true,
    "trigger_failure": true,
    "trigger_failure_wait_time": 30,
    // If 'fail_drive' is true, initiate drive failure else initiate plex failure
    "fail_drive": true,
    "trigger_rebuild_wait_time": 25,
    "rebuild_on_spare_volume": true,
    "rebuild_rate": 100,
    "rebuild_timeout": 3600,
    // failure mode can take random or fixed as its value. If it is fixed, initialize failure_drive_index
    "failure_mode": "random",
    //"failure_start_time_ratio": 0.05,
    "test_volume_start_index": 0,
    "status_interval": 10,
    "plex_failure_count": 1,
    "start_stats": true
  }
}
