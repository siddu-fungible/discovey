{
  "GlobalSetup": {
    "already_deployed": false,
    "reboot_hosts" : true,
    "bootargs": [
      "app=mdt_test,load_mods cc_huid=3 workload=storage --serial --dpc-uart --all_100g",
      "app=mdt_test,load_mods cc_huid=2 workload=storage --serial --dpc-uart --all_100g"
    ],
    "dut_start_index": 0,
    "host_start_index": 0,
    "num_f1": 1,
    "num_hosts": 1,
    "fpg_int_prefix": "fpg",
    "disable_f1_index": 1,
    "f1_in_use": 0,
    "load_modules": [
      "nvme",
      "nvme_tcp"
    ],
    "huid": 7,
    "ctlid": 0,
    "syslog": "default",
    "command_timeout": 5,
    "reboot_timeout": 600,
    "ethernet_adapter": "Mellanox",
    "override_numa_node": {
      "override": true,
      "override_node": "NUMA node0 CPU(s)"
    },
    "api_server_port": 50220,
    "api_server_username": "admin",
    "api_server_password": "password",
    "install": "fresh",
    "sc_script_dir": "/opt/fungible/StorageController/etc",
    "run_sc_script": "start_sc.sh",
    "run_sc_restart_timeout": 120,
    "container_up_timeout": 120,
    "api_server_up_timeout": 300
  },
  "DataIntegrityTest":{
    "num_f1": 1,
    "ec_count": 1,
    "num_hosts": 1,
    "capacity": 10737418240,
    "compression_effort": "True",
    "effort_levels": [1, 2, 3, 4, 5, 6, 7, 8],
    "encrypt": "False",
    "name": "ec_volume_",
    "DUT":1,
    "override_numa_node": {"override": true, "override_node": "NUMA node0 CPU(s)"},
    "ethernet_adapter": "Mellanox",
    "fio_write_cmd_args": {
      "multiple_jobs": "--rw=write --bs=4k --numjobs=1 --iodepth=16 --direct=1 --prio=0 --cpus_allowed_policy=split --ioengine=libaio --group_reporting=1 --do_verify=0 --verify=md5 --buffer_compress_percentage=50 --buffer_pattern=0xdeadface ",
      "timeout": 3600
    },
    "fio_read_cmd_args": {
      "multiple_jobs": "--rw=read --bs=4k --numjobs=1 --iodepth=16 --direct=1 --prio=0 --cpus_allowed_policy=split --ioengine=libaio --group_reporting=1 --do_verify=1 --verify=md5 ",
      "timeout": 3600
    }
  }
}
