            "message": "iMIX Slot Minimum IP Total Length cannot be greater than iMIX Slot Maximum IP Total Length"
        },
        {
            "property": "ImixSlotTable.ImixSlotMaxFrameLength",
            "type": "expression",
            "value": "(%ImixSlotTable.ImixSlotMinFrameLength% > %ImixSlotTable.ImixSlotMaxFrameLength%)",
            "message": "iMIX Slot Maximum IP Total Length cannot be less than iMIX Slot Minimum IP Total Length"
        },
        {
            "property": "MinRateValue",
            "type": "expression",
            "value": "(%MinRateValue% > %MaxRateValue%)",
            "message": "Minimum Rate Percentage cannot be greater than the Maximum Rate Percentage"
        },
        {
            "property": "MaxRateValue",
            "type": "expression",
            "value": "(%MinRateValue% > %MaxRateValue%)",
            "message": "Maximum Rate Percentage cannot be less than the Minimum Rate Percentage"
        },
        {
            "property": "RateSearchRes",
            "type": "expression",
            "value": "(%RateSearchRes% >= (%MaxRateValue% - %MinRateValue%))",
            "message": "Rate Search Resolution cannot be greater than or equal to the difference between the Maximum Rate and Minimum Rate Percentages"
        },
        {
            "property": "StartRateValue",
            "type": "expression",
            "value": "((%StartRateValue% < %MinRateValue%) || (%StartRateValue% > %MaxRateValue%))",
            "message": "Rate search starting value must be between the Minimum Rate and Maximum Rate Percentages"
        }
