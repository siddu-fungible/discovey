            "message": "Test criteria for at least one enabled dimension must be enabled to run the test."
        },
        {
            "property": "EnableStorageTestCriteria",
            "type": "expression",
            "value": "((%EnableCpuLoad% == false) || (%EnableCpuTestCriteria% == false)) && ((%EnableMemoryLoad% == false) || (%EnableMemoryTestCriteria% == false)) && ((%EnableNetworkLoad% == false) || (%EnableNetworkTestCriteria% == false)) && ((%EnableStorageLoad% == false) || (%EnableStorageTestCriteria% == false))",
            "message": "Test criteria for at least one enabled dimension must be enabled to run the test."
        },


        {
            "property": "FixedMemoryLoadParams.MemoryReadSize",
            "type": "expression",
            "value": "(%FixedMemoryLoadParams.MemoryReadSize% & (%FixedMemoryLoadParams.MemoryReadSize% - 1)) != 0",
            "message": "The memory read block size must be a power of 2."
        },
        {
            "property": "FixedMemoryLoadParams.MemoryWriteSize",
            "type": "expression",
            "value": "(%FixedMemoryLoadParams.MemoryWriteSize% & (%FixedMemoryLoadParams.MemoryWriteSize% - 1)) != 0",
            "message": "The memory write block size must be a power of 2."
        },
        {
            "property": "FixedMemoryLoadParams.MemoryBufferSize",
            "type": "expression",
            "value": "(%FixedMemoryLoadParams.MemoryBufferSize% % %FixedMemoryLoadParams.MemoryReadSize%) != 0",
            "message": "The memory buffer size must be a multiple of memory read block size and the memory write block size."
        },
        {
            "property": "FixedMemoryLoadParams.MemoryBufferSize",
            "type": "expression",
            "value": "(%FixedMemoryLoadParams.MemoryBufferSize% % %FixedMemoryLoadParams.MemoryWriteSize%) != 0",
            "message": "The memory buffer size must be a multiple of the memory read block size and the memory write block size."
        },


        {
            "property": "FixedNetworkLoadParams.NetworkReadSize",
            "type": "expression",
            "value": "(%FixedNetworkLoadParams.NetworkReadSize% & (%FixedNetworkLoadParams.NetworkReadSize% - 1)) != 0",
            "message": "The network read block size must be a power of 2."
        },
        {
            "property": "FixedNetworkLoadParams.NetworkWriteSize",
            "type": "expression",
            "value": "(%FixedNetworkLoadParams.NetworkWriteSize% & (%FixedNetworkLoadParams.NetworkWriteSize% - 1)) != 0",
            "message": "The network write block size must be a power of 2."
        },


        {
            "property": "FixedStorageLoadParams.StorageReadSize",
            "type": "expression",
            "value": "(%FixedStorageLoadParams.StorageReadSize% & (%FixedStorageLoadParams.StorageReadSize% - 1)) != 0",
            "message": "The storage read block size must be a power of 2."
        },
        {
            "property": "FixedStorageLoadParams.StorageWriteSize",
            "type": "expression",
            "value": "(%FixedStorageLoadParams.StorageWriteSize% & (%FixedStorageLoadParams.StorageWriteSize% - 1)) != 0",
            "message": "The storage write block size must be a power of 2."
        },
        {
            "property": "FixedStorageLoadParams.StorageEnableFileSize",
            "type": "expression",
            "value": "!%FixedStorageLoadParams.StorageEnableFileSize% && %FixedStorageLoadParams.StorageTargetDevice%.starts_with('/')",
            "message": "Storage file size must be enabled when a file path is specified."
        },
        {
            "property": "FixedStorageLoadParams.StorageFileSize",
            "type": "expression",
            "value": "%FixedStorageLoadParams.StorageEnableFileSize% && (%FixedStorageLoadParams.StorageReadSize% >= %FixedStorageLoadParams.StorageFileSize% || %FixedStorageLoadParams.StorageWriteSize% >= %FixedStorageLoadParams.StorageFileSize%)",
            "message": "Storage file size must be greater than both the storage read size and storage write size when enable storage file size is enabled."

        }
    ]
}
