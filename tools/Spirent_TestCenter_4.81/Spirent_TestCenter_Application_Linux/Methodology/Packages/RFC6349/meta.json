        {
          "description": "The time to wait while frames are traveling across the network during the round trip time test",
          "display_name": "RTT Wait Time",
          "property": "RttWaitTime",
          "property_value_type": "INTEGER",
          "is_collection": false,
          "property_value": "10",
          "minimum": "1",
          "maximum": "9999",
          "units": "seconds"
