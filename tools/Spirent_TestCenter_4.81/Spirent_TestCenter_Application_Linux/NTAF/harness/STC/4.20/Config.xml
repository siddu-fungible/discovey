<query-harness xmlns="http://ntaforum.org/2011/harness" harness="http://spirent.com/NTAF/harness/STC/4.20/Config"  xml:lang="en">
<label>Config</label>
<helpURI>http://com.fnfr.svt.help/popups/testcenteraction.html</helpURI>
<author>spirent</author>
    <actionDecl name="GetObjectInfo">
    <label></label>
    <tooltip></tooltip>
    <description></description>
        <parameter  name="Object">
            <label>Object</label>
            <tooltip>The object handle</tooltip>
            <description>The object handle</description>
            <datatype>string</datatype>
            <default>0</default>
        </parameter>
        <responseDecl>
        <item  name="ObjectType">
            <label>ObjectType</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        </responseDecl>
    </actionDecl>

    <actionDecl name="GetObjects">
    <label></label>
    <tooltip></tooltip>
    <description></description>
        <parameter  name="ClassName">
            <label>ClassName</label>
            <tooltip>Class Name specifying the class of the objects to get.</tooltip>
            <description>Class Name specifying the class of the objects to get.</description>
            <datatype>string</datatype>
            <default></default>
        </parameter>
        <parameter  name="RootList">
            <label>RootList</label>
            <tooltip>Limits scope of search to objects under the Root List. If not specified, System is used.</tooltip>
            <description>Limits scope of search to objects under the Root List. If not specified, System is used.</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="RelationType">
            <label>RelationType</label>
            <tooltip>Relation Type</tooltip>
            <description>Relation Type</description>
            <datatype>string</datatype>
            <default>ParentChild</default>
        </parameter>
        <parameter  name="Direction">
            <label>Direction</label>
            <tooltip>Relation direction (Source or Destination)</tooltip>
            <description>Relation direction (Source or Destination)</description>
            <datatype>string</datatype>
            <default>DESTINATION</default>
        </parameter>
        <parameter  name="Condition">
            <label>Condition</label>
            <tooltip>Conditional expression used to filter the returned objects.</tooltip>
            <description>Conditional expression used to filter the returned objects.</description>
            <datatype>string</datatype>
            <default></default>
        </parameter>
        <responseDecl>
        <item  name="ObjectList">
            <label>ObjectList</label>
            <mandatory>false</mandatory>
            <default>0</default>
            <datatype>string</datatype>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </item>
        </responseDecl>
    </actionDecl>

    <actionDecl name="GetRelatives">
    <label></label>
    <tooltip></tooltip>
    <description></description>
        <parameter  name="ExecuteSynchronous">
            <label>ExecuteSynchronous</label>
            <tooltip>Execute Synchronous</tooltip>
            <description>Execute Synchronous</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="ProgressCancelled">
            <label>ProgressCancelled</label>
            <tooltip>Progress was Cancelled</tooltip>
            <description>Progress was Cancelled</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="Active">
            <label>Active</label>
            <tooltip>Active</tooltip>
            <description>Active</description>
            <datatype>boolean</datatype>
            <default>true</default>
        </parameter>
        <parameter  name="Name">
            <label>Name</label>
            <tooltip>Name</tooltip>
            <description>Name</description>
            <datatype>string</datatype>
            <default></default>
        </parameter>
        <parameter  name="Config">
            <label>Config</label>
            <tooltip>Object handle to get relatives from</tooltip>
            <description>Object handle to get relatives from</description>
            <datatype>string</datatype>
            <default>0</default>
        </parameter>
        <parameter  name="RelationType">
            <label>RelationType</label>
            <tooltip>Relation Type</tooltip>
            <description>Relation Type</description>
            <datatype>string</datatype>
            <default>ParentChild</default>
        </parameter>
        <parameter  name="Direction">
            <label>Direction</label>
            <tooltip>Relation direction (Source or Destination)</tooltip>
            <description>Relation direction (Source or Destination)</description>
            <datatype>string</datatype>
            <default>DESTINATION</default>
            <allowedValue label="DESTINATION">DESTINATION</allowedValue>
            <allowedValue label="CHILD">CHILD</allowedValue>
            <allowedValue label="CHILDREN">CHILDREN</allowedValue>
            <allowedValue label="DOWN">DOWN</allowedValue>
            <allowedValue label="DOWNWARD">DOWNWARD</allowedValue>
            <allowedValue label="FORWARD">FORWARD</allowedValue>
            <allowedValue label="SOURCE">SOURCE</allowedValue>
            <allowedValue label="PARENT">PARENT</allowedValue>
            <allowedValue label="UP">UP</allowedValue>
            <allowedValue label="UPWARD">UPWARD</allowedValue>
            <allowedValue label="BACKWARD">BACKWARD</allowedValue>
            <allowedValue label="BIDIRECTIONAL">BIDIRECTIONAL</allowedValue>
            <allowedValue label="BOTH">BOTH</allowedValue>
            <allowedValue label="ALL">ALL</allowedValue>
        </parameter>
        <responseDecl>
        <item  name="State">
            <label>State</label>
            <default>INIT</default>
            <datatype>string</datatype>
            <allowedValue label="INIT">INIT</allowedValue>
            <allowedValue label="START">START</allowedValue>
            <allowedValue label="RUNNING">RUNNING</allowedValue>
            <allowedValue label="PAUSED">PAUSED</allowedValue>
            <allowedValue label="PRECOMPLETE">PRECOMPLETE</allowedValue>
            <allowedValue label="COMPLETED">COMPLETED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
            <allowedValue label="VALIDATION_ERROR">VALIDATION_ERROR</allowedValue>
        </item>
        <item  name="Status">
            <label>Status</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        <item  name="StartTime">
            <label>StartTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="EndTime">
            <label>EndTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="ElapsedTime">
            <label>ElapsedTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="ProgressDisplayCounter">
            <label>ProgressDisplayCounter</label>
            <default>true</default>
            <datatype>boolean</datatype>
        </item>
        <item  name="ProgressStepsCount">
            <label>ProgressStepsCount</label>
            <default>1</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentStep">
            <label>ProgressCurrentStep</label>
            <default>1</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentStepName">
            <label>ProgressCurrentStepName</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        <item  name="ProgressMaxValue">
            <label>ProgressMaxValue</label>
            <default>0</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentValue">
            <label>ProgressCurrentValue</label>
            <default>0</default>
            <datatype>integer</datatype>
        </item>
        <item  name="RelativesList">
            <label>RelativesList</label>
            <mandatory>false</mandatory>
            <default>0</default>
            <datatype>string</datatype>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </item>
        </responseDecl>
    </actionDecl>


  <actionDecl name="get">
    <label>Get value</label>
    <tooltip>stc::get</tooltip>
    <description>Get the value of a TestCenter attribute</description>
    <helpURI>http://com.fnfr.svt.help/popups/stcaction.html</helpURI>
    <parameter name="handle">
      <label>Handle</label>
      <tooltip>Handle</tooltip>
      <description>A string value that identifies the object from which to retrieve values. 
       Either a handle or a DDN path must be used to define an object.</description> 
       <datatype>string</datatype>
    </parameter>
    <parameter name="propertyName">
      <label>Property name</label>
      <tooltip>Property name</tooltip>
      <description>Property name.</description>
      <datatype>string</datatype>
      <allowedCount>
        <min>0</min>
      </allowedCount>
      <mandatory>false</mandatory>
    </parameter>
    <responseDecl>
      <group name="propertyPair">
        <label>Property pair</label>
        <tooltip>Name value property pair</tooltip>
        <description>Name value property pair</description>
        <item name="name">
          <label>Property name</label>
          <tooltip>Property name</tooltip>
          <description>Property name.</description>
          <datatype>string</datatype>
        </item>
        <item name="value">
          <label>Property value</label>
          <tooltip>Property value</tooltip>
          <description>Property value</description>
          <datatype>string</datatype>
        </item>
        <allowedCount>
          <min>1</min>
        </allowedCount> 
      </group>
    </responseDecl>
  </actionDecl>
</query-harness>
