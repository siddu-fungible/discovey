<query-harness xmlns="http://ntaforum.org/2011/harness" harness="http://spirent.com/NTAF/harness/STC/4.20/Basic"  xml:lang="en">
<label>Basic</label>
<helpURI>http://com.fnfr.svt.help/popups/testcenteraction.html</helpURI>
<author>spirent</author>
    
<actionDecl name="SetProperty">
    <label>Set Property</label>
    <tooltip>Set Properties.</tooltip>
    <description>Set Properties.</description>
        <parameter  name="ObjectType">
            <label>ObjectType</label>
            <tooltip>Object Type</tooltip>
            <description>Object Type</description>
            <datatype>string</datatype>
            <default>0</default>
        </parameter>
        <parameter  name="RootList">
            <label>RootList</label>
            <tooltip>Root List</tooltip>
            <description>Root List</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="Property">
            <label>Property</label>
            <tooltip>Property</tooltip>
            <description>Property</description>
            <datatype>string</datatype>
            <default></default>
        </parameter>
        <parameter  name="Value">
            <label>Value</label>
            <tooltip>Value</tooltip>
            <description>Value</description>
            <datatype>string</datatype>
            <default></default>
        </parameter>
        <parameter  name="ApplyImmediately">
            <label>ApplyImmediately</label>
            <tooltip>Apply Immediately</tooltip>
            <description>Apply Immediately</description>
            <datatype>boolean</datatype>
            <default>true</default>
        </parameter>
    </actionDecl>



<actionDecl name="StreamBlockWaitForResolve">
    <label>Wait for Stream Blocks to resolve</label>
    <tooltip>Command to wait for streams to resolve</tooltip>
    <description>Command to wait for streams to resolve</description>
        <parameter  name="ErrorOnFailure">
            <label>ErrorOnFailure</label>
            <tooltip>Error on Failure</tooltip>
            <description>Error on Failure</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="WaitTime">
            <label>WaitTime</label>
            <tooltip>Wait Time (secs)</tooltip>
            <description>Wait Time (secs)</description>
            <datatype>decimal</datatype>
            <default>30.0</default>
        </parameter>
        <parameter  name="ObjectList">
            <label>ObjectList</label>
            <tooltip>Objects to Wait for</tooltip>
            <description>Objects to Wait for</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <responseDecl>
        <item  name="PassFailState">
            <label>PassFailState</label>
            <default>NONE</default>
            <datatype>string</datatype>
            <allowedValue label="NONE">NONE</allowedValue>
            <allowedValue label="PASSED">PASSED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
        </item>
        <item  name="Successful">
            <label>Successful</label>
            <mandatory>false</mandatory>
            <default>0</default>
            <datatype>string</datatype>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </item>
        <item  name="Unsuccessful">
            <label>Unsuccessful</label>
            <mandatory>false</mandatory>
            <default>0</default>
            <datatype>string</datatype>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </item>
        </responseDecl>
    </actionDecl>



	<supercedes>http://spirent.com/NTAF/harness/STC/4.10/Basic</supercedes>
	<subharness>http://spirent.com/NTAF/harness/STC/4.10/Basic</subharness>


</query-harness>

