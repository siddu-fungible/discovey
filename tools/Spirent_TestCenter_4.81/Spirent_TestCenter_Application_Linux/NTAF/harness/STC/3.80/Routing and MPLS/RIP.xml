    <actionDecl name="RipWithdrawRoute">
    <label>Withdraw Routes</label>
    <tooltip>Withdraw routes for the selected RIP routes.</tooltip>
    <description>Withdraw routes for the selected RIP routes.</description>
        <parameter  name="RouteList">
            <label>RouteList</label>
            <tooltip>RIP Route Handle List</tooltip>
            <description>RIP Route Handle List</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="PrefixFilter">
            <label>PrefixFilter</label>
            <tooltip>Route Prefix Filter</tooltip>
            <description>Route Prefix Filter</description>
            <datatype>integer</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>1</max>
            </allowedCount>
        </parameter>
        <parameter  name="CategoryFilter">
            <label>CategoryFilter</label>
            <tooltip>Route Category Filter</tooltip>
            <description>Route Category Filter</description>
            <datatype>string</datatype>
            <default>ANY</default>
            <allowedValue label="UNDEFINED">UNDEFINED</allowedValue>
            <allowedValue label="PRIMARY">PRIMARY</allowedValue>
            <allowedValue label="SECONDARY">SECONDARY</allowedValue>
            <allowedValue label="UNIQUE">UNIQUE</allowedValue>
            <allowedValue label="ANY">ANY</allowedValue>
            <allowedCount>
                <min>0</min>
                <max>1</max>
            </allowedCount>
        </parameter>
        <parameter  name="PercentFilter">
            <label>PercentFilter</label>
