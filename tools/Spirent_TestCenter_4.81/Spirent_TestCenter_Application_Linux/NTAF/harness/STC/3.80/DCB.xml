<query-harness xmlns="http://ntaforum.org/2011/harness" harness="http://spirent.com/NTAF/harness/STC/3.80/DCB"  xml:lang="en">
<label>DCB</label>
<helpURI>http://com.fnfr.svt.help/popups/testcenteraction.html</helpURI>
<author>spirent</author>
    <actionDecl name="TrafficGroupLoadSearch">
    <label>Queueput Search Command</label>
    <tooltip>Performs a maximal load search on multiple traffic groups.  Must be used in conjuction with a supported ResultsWriteIteration command.</tooltip>
    <description>Performs a maximal load search on multiple traffic groups.  Must be used in conjuction with a supported ResultsWriteIteration command.</description>
        <parameter  name="ExecuteSynchronous">
            <label>ExecuteSynchronous</label>
            <tooltip>Execute Synchronous</tooltip>
            <description>Execute Synchronous</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="ProgressCancelled">
            <label>ProgressCancelled</label>
            <tooltip>Progress was Cancelled</tooltip>
            <description>Progress was Cancelled</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="TrafficGroupList">
            <label>TrafficGroupList</label>
            <tooltip>TrafficGroup List</tooltip>
            <description>TrafficGroup List</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <responseDecl>
        <item  name="TestStatus">
            <label>TestStatus</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        <item  name="State">
            <label>State</label>
            <default>INIT</default>
            <datatype>string</datatype>
            <allowedValue label="INIT">INIT</allowedValue>
            <allowedValue label="START">START</allowedValue>
            <allowedValue label="RUNNING">RUNNING</allowedValue>
            <allowedValue label="PAUSED">PAUSED</allowedValue>
            <allowedValue label="PRECOMPLETE">PRECOMPLETE</allowedValue>
            <allowedValue label="COMPLETED">COMPLETED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
            <allowedValue label="VALIDATION_ERROR">VALIDATION_ERROR</allowedValue>
        </item>
        <item  name="Status">
            <label>Status</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        <item  name="StartTime">
            <label>StartTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="EndTime">
            <label>EndTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="ElapsedTime">
            <label>ElapsedTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="ProgressDisplayCounter">
            <label>ProgressDisplayCounter</label>
            <default>true</default>
            <datatype>boolean</datatype>
        </item>
        <item  name="ProgressStepsCount">
            <label>ProgressStepsCount</label>
            <default>1</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentStep">
            <label>ProgressCurrentStep</label>
            <default>1</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentStepName">
            <label>ProgressCurrentStepName</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        <item  name="ProgressMaxValue">
            <label>ProgressMaxValue</label>
            <default>0</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentValue">
            <label>ProgressCurrentValue</label>
            <default>0</default>
            <datatype>integer</datatype>
        </item>
        </responseDecl>
    </actionDecl>

    <actionDecl name="TrafficGroupWeightedLoadSearch">
    <label>Queueput Weighted Search Command</label>
    <tooltip>Performs a maximal load search on weighted traffic groups.  Must be used in conjuction with a supported ResultsWriteIteration command.</tooltip>
    <description>Performs a maximal load search on weighted traffic groups.  Must be used in conjuction with a supported ResultsWriteIteration command.</description>
        <parameter  name="GroupCollection">
            <label>GroupCollection</label>
            <tooltip>GroupCollection</tooltip>
            <description>GroupCollection</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>1</max>
            </allowedCount>
        </parameter>
        <parameter  name="TrafficGroupList">
            <label>TrafficGroupList</label>
            <tooltip>TrafficGroup List</tooltip>
            <description>TrafficGroup List</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="LoadUnits">
            <label>LoadUnits</label>
            <tooltip>Load Units</tooltip>
            <description>Load Units</description>
            <datatype>string</datatype>
            <default>PERCENT_LINE_RATE</default>
            <allowedValue label="PERCENT_LINE_RATE">PERCENT_LINE_RATE</allowedValue>
            <allowedValue label="MEGABITS_PER_SECOND">MEGABITS_PER_SECOND</allowedValue>
            <allowedValue label="KILOBITS_PER_SECOND">KILOBITS_PER_SECOND</allowedValue>
            <allowedValue label="BITS_PER_SECOND">BITS_PER_SECOND</allowedValue>
        </parameter>
        <parameter  name="SearchMode">
            <label>SearchMode</label>
            <tooltip>Search Mode</tooltip>
            <description>Search Mode</description>
            <datatype>string</datatype>
            <default>BINARY</default>
            <allowedValue label="STEP">STEP</allowedValue>
            <allowedValue label="BINARY">BINARY</allowedValue>
            <allowedValue label="COMBO">COMBO</allowedValue>
        </parameter>
        <parameter  name="IgnoreMinMaxLimits">
            <label>IgnoreMinMaxLimits</label>
            <tooltip>Ignore Min/Max Limits</tooltip>
            <description>Ignore Min/Max Limits</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="RateLowerLimit">
            <label>RateLowerLimit</label>
            <tooltip>Rate Lower Limit</tooltip>
            <description>Rate Lower Limit</description>
            <datatype>decimal</datatype>
            <default>10.0</default>
        </parameter>
        <parameter  name="RateUpperLimit">
            <label>RateUpperLimit</label>
            <tooltip>Rate Upper Limit</tooltip>
            <description>Rate Upper Limit</description>
            <datatype>decimal</datatype>
            <default>100.0</default>
        </parameter>
        <parameter  name="RateStep">
            <label>RateStep</label>
            <tooltip>Rate Step</tooltip>
            <description>Rate Step</description>
            <datatype>decimal</datatype>
            <default>10.0</default>
        </parameter>
        <parameter  name="RateInitial">
            <label>RateInitial</label>
            <tooltip>Initial Rate</tooltip>
            <description>Initial Rate</description>
            <datatype>decimal</datatype>
            <default>10.0</default>
        </parameter>
        <parameter  name="Resolution">
            <label>Resolution</label>
            <tooltip>Resolution</tooltip>
            <description>Resolution</description>
            <datatype>decimal</datatype>
            <default>1.0</default>
        </parameter>
        <parameter  name="Backoff">
            <label>Backoff</label>
            <tooltip>Backoff (%)</tooltip>
            <description>Backoff (%)</description>
            <datatype>decimal</datatype>
            <default>50.0</default>
        </parameter>
        <responseDecl>
        <item  name="CurrentLoad">
            <label>CurrentLoad</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        </responseDecl>
    </actionDecl>

    <actionDecl name="DcbIteratePfcQuantaValue">
    <label>Iterate PFC Pause Quanta Command</label>
    <tooltip>This command creates PFC streamblocks and iterates PFC Pause values from the specified devices.</tooltip>
    <description>This command creates PFC streamblocks and iterates PFC Pause values from the specified devices.</description>
        <parameter  name="PriorityFlowControlArray">
            <label>PriorityFlowControlArray</label>
            <tooltip>Specify on which queues to send pause frames</tooltip>
            <description>Specify on which queues to send pause frames</description>
            <datatype>boolean</datatype>
            <default>true</default>
            <allowedCount>
                <min>0</min>
                <max>8</max>
            </allowedCount>
        </parameter>
        <parameter  name="EnableXonXoffFlowControl">
            <label>EnableXonXoffFlowControl</label>
            <tooltip>Send XOFF/XON PFC frame pairs</tooltip>
            <description>Send XOFF/XON PFC frame pairs</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="QuantaIterationMode">
            <label>QuantaIterationMode</label>
            <tooltip>Quanta Iteration Mode</tooltip>
            <description>Quanta Iteration Mode</description>
            <datatype>string</datatype>
            <default>FIXED</default>
            <allowedValue label="CUSTOM">CUSTOM</allowedValue>
            <allowedValue label="FIXED">FIXED</allowedValue>
            <allowedValue label="STEP">STEP</allowedValue>
            <allowedValue label="RANDOM">RANDOM</allowedValue>
        </parameter>
        <parameter  name="CustomQuantaList">
            <label>CustomQuantaList</label>
            <tooltip>Custom Quanta Value List</tooltip>
            <description>Custom Quanta Value List</description>
            <mandatory>false</mandatory>
            <datatype>integer</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="FixedQuanta">
            <label>FixedQuanta</label>
            <tooltip>Fixed Quanta Value</tooltip>
            <description>Fixed Quanta Value</description>
            <datatype>integer</datatype>
            <default>65535</default>
        </parameter>
        <parameter  name="StepQuantaStart">
            <label>StepQuantaStart</label>
            <tooltip>Step Quanta Start</tooltip>
            <description>Step Quanta Start</description>
            <datatype>integer</datatype>
            <default>0</default>
        </parameter>
        <parameter  name="StepQuantaEnd">
            <label>StepQuantaEnd</label>
            <tooltip>Step Quanta End</tooltip>
            <description>Step Quanta End</description>
            <datatype>integer</datatype>
            <default>1024</default>
        </parameter>
        <parameter  name="StepQuantaStep">
            <label>StepQuantaStep</label>
            <tooltip>Step Quanta Step</tooltip>
            <description>Step Quanta Step</description>
            <datatype>integer</datatype>
            <default>128</default>
        </parameter>
        <parameter  name="XonDelayIterationMode">
            <label>XonDelayIterationMode</label>
            <tooltip>XON Delay Iteration Mode</tooltip>
            <description>XON Delay Iteration Mode</description>
            <datatype>string</datatype>
            <default>FIXED</default>
            <allowedValue label="CUSTOM">CUSTOM</allowedValue>
            <allowedValue label="FIXED">FIXED</allowedValue>
            <allowedValue label="STEP">STEP</allowedValue>
        </parameter>
        <parameter  name="XonDelayUnit">
            <label>XonDelayUnit</label>
            <tooltip>XON Delay Unit</tooltip>
            <description>XON Delay Unit</description>
            <datatype>string</datatype>
            <default>QUANTA</default>
            <allowedValue label="QUANTA">QUANTA</allowedValue>
            <allowedValue label="BYTES">BYTES</allowedValue>
        </parameter>
        <parameter  name="CustomXonDelayList">
            <label>CustomXonDelayList</label>
            <tooltip>Custom XON Delay List</tooltip>
            <description>Custom XON Delay List</description>
            <mandatory>false</mandatory>
            <datatype>integer</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="FixedXonDelay">
            <label>FixedXonDelay</label>
            <tooltip>Fixed XON Delay</tooltip>
            <description>Fixed XON Delay</description>
            <datatype>integer</datatype>
            <default>1024</default>
        </parameter>
        <parameter  name="StepXonDelayStart">
            <label>StepXonDelayStart</label>
            <tooltip>Step XON Delay Start</tooltip>
            <description>Step XON Delay Start</description>
            <datatype>integer</datatype>
            <default>1024</default>
        </parameter>
        <parameter  name="StepXonDelayEnd">
            <label>StepXonDelayEnd</label>
            <tooltip>Step XON Delay End</tooltip>
            <description>Step XON Delay End</description>
            <datatype>integer</datatype>
            <default>32768</default>
        </parameter>
        <parameter  name="StepXonDelayStep">
            <label>StepXonDelayStep</label>
            <tooltip>Step XON Delay Step</tooltip>
            <description>Step XON Delay Step</description>
            <datatype>integer</datatype>
            <default>1024</default>
        </parameter>
        <parameter  name="DeviceList">
            <label>DeviceList</label>
            <tooltip>Device List</tooltip>
            <description>Device List</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="PfcPauseTxRate">
            <label>PfcPauseTxRate</label>
            <tooltip>Number of Pauses each device transmits per second</tooltip>
            <description>Number of Pauses each device transmits per second</description>
            <datatype>integer</datatype>
            <default>1</default>
        </parameter>
    </actionDecl>

