<query-harness xmlns="http://ntaforum.org/2011/harness" harness="http://spirent.com/NTAF/harness/STC/3.80/Triple%20Play/IPTV"  xml:lang="en">
<label>IPTV</label>
<helpURI>http://com.fnfr.svt.help/popups/testcenteraction.html</helpURI>
<author>spirent</author>
    <actionDecl name="IptvStartTest">
    <label>Start Test</label>
    <tooltip>Starts IPTV Test on the specified Blocks of STBs.</tooltip>
    <description>Starts IPTV Test on the specified Blocks of STBs.</description>
        <parameter  name="ExecuteSynchronous">
            <label>ExecuteSynchronous</label>
            <tooltip>Execute Synchronous</tooltip>
            <description>Execute Synchronous</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="ProgressCancelled">
            <label>ProgressCancelled</label>
            <tooltip>Progress was Cancelled</tooltip>
            <description>Progress was Cancelled</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="TestName">
            <label>TestName</label>
            <tooltip>Test Name</tooltip>
            <description>Test Name</description>
            <datatype>string</datatype>
            <default>IptvTest</default>
        </parameter>
        <parameter  name="TypeOfTest">
            <label>TypeOfTest</label>
            <tooltip>Type of Test</tooltip>
            <description>Type of Test</description>
            <datatype>string</datatype>
            <default>CHANNEL_VERIFICATION_TEST</default>
            <allowedValue label="CHANNEL_ZAPPING_TEST">CHANNEL_ZAPPING_TEST</allowedValue>
            <allowedValue label="CHANNEL_VERIFICATION_TEST">CHANNEL_VERIFICATION_TEST</allowedValue>
            <allowedValue label="FAST_CHANNEL_CHANGE_TEST">FAST_CHANNEL_CHANGE_TEST</allowedValue>
            <allowedValue label="RELIABLE_RETRANSMISSION_TEST">RELIABLE_RETRANSMISSION_TEST</allowedValue>
            <allowedValue label="FCC_WITH_RRR_TEST">FCC_WITH_RRR_TEST</allowedValue>
        </parameter>
        <parameter  name="TestEnv">
            <label>TestEnv</label>
            <tooltip>IPTV Test Environment</tooltip>
            <description>IPTV Test Environment</description>
            <datatype>string</datatype>
            <default>TESTING_ENV</default>
            <allowedValue label="TESTING_ENV">TESTING_ENV</allowedValue>
            <allowedValue label="REAL_WORLD_ENV">REAL_WORLD_ENV</allowedValue>
        </parameter>
        <parameter  name="TestTime">
            <label>TestTime</label>
            <tooltip>Test Time in Seconds</tooltip>
            <description>Test Time in Seconds</description>
            <datatype>decimal</datatype>
            <default>300</default>
        </parameter>
        <parameter  name="JoinFailPercentageThreshold">
            <label>JoinFailPercentageThreshold</label>
            <tooltip>Join Fail Percentage Threshold</tooltip>
            <description>Join Fail Percentage Threshold</description>
            <datatype>integer</datatype>
            <default>100</default>
        </parameter>
        <parameter  name="UdpPortTypeFilter">
            <label>UdpPortTypeFilter</label>
            <tooltip>Filter on UDP Port Type</tooltip>
            <description>Filter on UDP Port Type</description>
            <datatype>string</datatype>
            <default>SOURCE</default>
            <allowedValue label="SOURCE">SOURCE</allowedValue>
            <allowedValue label="DESTINATION">DESTINATION</allowedValue>
        </parameter>
        <parameter  name="UdpPortNum">
            <label>UdpPortNum</label>
            <tooltip>UDP Port Number</tooltip>
            <description>UDP Port Number</description>
            <datatype>integer</datatype>
            <default>3357</default>
        </parameter>
        <parameter  name="VlanSubFilter">
            <label>VlanSubFilter</label>
            <tooltip>VLAN Subfilter Type (Used only for ports on which PPP and stacked VLANs are enabled)</tooltip>
            <description>VLAN Subfilter Type (Used only for ports on which PPP and stacked VLANs are enabled)</description>
            <datatype>string</datatype>
            <default>OUTER</default>
            <allowedValue label="OUTER">OUTER</allowedValue>
            <allowedValue label="INNER">INNER</allowedValue>
        </parameter>
        <parameter  name="StbBlockList">
            <label>StbBlockList</label>
            <tooltip>STB Block List</tooltip>
            <description>STB Block List</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="InterStbStartDelayStep">
            <label>InterStbStartDelayStep</label>
            <tooltip>Inter-Set Top Block Start Delay (msec)</tooltip>
            <description>Inter-Set Top Block Start Delay (msec)</description>
            <datatype>integer</datatype>
            <default>0</default>
        </parameter>
        <parameter  name="DatabaseFileName">
            <label>DatabaseFileName</label>
            <tooltip>Name of EOT Database</tooltip>
            <description>Name of EOT Database</description>
            <datatype>string</datatype>
            <default>iptvTest.db</default>
        </parameter>
        <parameter  name="AddToIterationResults">
            <label>AddToIterationResults</label>
            <tooltip>Add to iteration results</tooltip>
            <description>Add to iteration results</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="MaxSbitDelay">
            <label>MaxSbitDelay</label>
            <tooltip>Maximum Sbit polling delay (msec)</tooltip>
            <description>Maximum Sbit polling delay (msec)</description>
            <datatype>integer</datatype>
            <default>2000</default>
        </parameter>
        <parameter  name="MinSbitDelay">
            <label>MinSbitDelay</label>
            <tooltip>Minimum Sbit polling delay (msec)</tooltip>
            <description>Minimum Sbit polling delay (msec)</description>
            <datatype>integer</datatype>
            <default>0</default>
        </parameter>
        <parameter  name="ExtHdrCheckEnable">
            <label>ExtHdrCheckEnable</label>
            <tooltip>Enable extension header validation</tooltip>
            <description>Enable extension header validation</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="ExtHdrCheckPatternPeriod">
            <label>ExtHdrCheckPatternPeriod</label>
            <tooltip>Number of packets in an extension header cycle</tooltip>
            <description>Number of packets in an extension header cycle</description>
            <datatype>integer</datatype>
            <default>10</default>
        </parameter>
        <parameter  name="MulticastJoinTrigger">
            <label>MulticastJoinTrigger</label>
            <tooltip>Multicast Join Trigger</tooltip>
            <description>Multicast Join Trigger</description>
            <datatype>string</datatype>
            <default>UNICAST_JOIN_AND_DELAY</default>
            <allowedValue label="UNICAST_JOIN_AND_DELAY">UNICAST_JOIN_AND_DELAY</allowedValue>
            <allowedValue label="SBIT_AND_DELAY">SBIT_AND_DELAY</allowedValue>
        </parameter>
        <parameter  name="UnicastLeaveTrigger">
            <label>UnicastLeaveTrigger</label>
            <tooltip>Unicast Leave Trigger</tooltip>
            <description>Unicast Leave Trigger</description>
            <datatype>string</datatype>
            <default>UNICAST_JOIN_AND_DELAY</default>
            <allowedValue label="UNICAST_JOIN_AND_DELAY">UNICAST_JOIN_AND_DELAY</allowedValue>
            <allowedValue label="MULTICAST_PKT_AND_DELAY">MULTICAST_PKT_AND_DELAY</allowedValue>
        </parameter>
        <parameter  name="RtpRetransmitThreshold">
            <label>RtpRetransmitThreshold</label>
            <tooltip>RTP Retransmit Threshold (msec)</tooltip>
            <description>RTP Retransmit Threshold (msec)</description>
            <datatype>integer</datatype>
            <default>50</default>
        </parameter>
        <responseDecl>
        <item  name="State">
            <label>State</label>
            <default>INIT</default>
            <datatype>string</datatype>
            <allowedValue label="INIT">INIT</allowedValue>
            <allowedValue label="START">START</allowedValue>
            <allowedValue label="RUNNING">RUNNING</allowedValue>
            <allowedValue label="PAUSED">PAUSED</allowedValue>
            <allowedValue label="PRECOMPLETE">PRECOMPLETE</allowedValue>
            <allowedValue label="COMPLETED">COMPLETED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
            <allowedValue label="VALIDATION_ERROR">VALIDATION_ERROR</allowedValue>
        </item>
        <item  name="Status">
            <label>Status</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        <item  name="StartTime">
            <label>StartTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="EndTime">
            <label>EndTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="ElapsedTime">
            <label>ElapsedTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="ProgressDisplayCounter">
            <label>ProgressDisplayCounter</label>
            <default>true</default>
            <datatype>boolean</datatype>
        </item>
        <item  name="ProgressStepsCount">
            <label>ProgressStepsCount</label>
            <default>1</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentStep">
            <label>ProgressCurrentStep</label>
            <default>1</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentStepName">
            <label>ProgressCurrentStepName</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        <item  name="ProgressMaxValue">
            <label>ProgressMaxValue</label>
            <default>0</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentValue">
            <label>ProgressCurrentValue</label>
            <default>0</default>
            <datatype>integer</datatype>
        </item>
        </responseDecl>
    </actionDecl>

    <actionDecl name="IptvStopTest">
