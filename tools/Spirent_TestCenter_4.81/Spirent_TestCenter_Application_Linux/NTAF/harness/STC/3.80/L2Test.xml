        <item  name="Status">
            <label>Status</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        <item  name="StartTime">
            <label>StartTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="EndTime">
            <label>EndTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
