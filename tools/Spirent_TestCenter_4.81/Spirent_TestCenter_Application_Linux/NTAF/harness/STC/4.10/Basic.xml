<query-harness xmlns="http://ntaforum.org/2011/harness" harness="http://spirent.com/NTAF/harness/STC/4.10/Basic"  xml:lang="en">
<label>Basic</label>
<helpURI>http://com.fnfr.svt.help/popups/testcenteraction.html</helpURI>
<author>spirent</author>
    
	<supercedes>http://spirent.com/NTAF/harness/STC/3.80/Basic</supercedes>
<actionDecl name="StreamBlockVerifyForwarding">
    <label>Verify Traffic Forwarding</label>
    <tooltip>Verifies that the test topology can forward test traffic</tooltip>
    <description>Verifies that the test topology can forward test traffic</description>
        <parameter  name="ErrorOnFailure">
            <label>ErrorOnFailure</label>
            <tooltip>Error on Failure</tooltip>
            <description>Error on Failure</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="StreamBlockList">
            <label>StreamBlockList</label>
            <tooltip>StreamBlock List</tooltip>
            <description>StreamBlock List</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="VerificationDurationMode">
            <label>VerificationDurationMode</label>
            <tooltip>Verification Duration Mode</tooltip>
            <description>Verification Duration Mode</description>
            <datatype>string</datatype>
            <default>FRAME_COUNT</default>
            <allowedValue label="FRAME_COUNT">FRAME_COUNT</allowedValue>
            <allowedValue label="SECONDS">SECONDS</allowedValue>
        </parameter>
        <parameter  name="VerificationTxFrameCount">
            <label>VerificationTxFrameCount</label>
            <tooltip>Tx Frame Count (per port)</tooltip>
            <description>Tx Frame Count (per port)</description>
            <datatype>integer</datatype>
            <default>100</default>
        </parameter>
        <parameter  name="VerificationTxSeconds">
            <label>VerificationTxSeconds</label>
            <tooltip>Tx Seconds</tooltip>
            <description>Tx Seconds</description>
            <datatype>decimal</datatype>
            <default>5.0</default>
        </parameter>
        <parameter  name="VerificationLoadMode">
            <label>VerificationLoadMode</label>
            <tooltip>Verification Load Mode</tooltip>
            <description>Verification Load Mode</description>
            <datatype>string</datatype>
            <default>DEFAULT</default>
            <allowedValue label="DEFAULT">DEFAULT</allowedValue>
            <allowedValue label="AS_IS">AS_IS</allowedValue>
        </parameter>
        <parameter  name="VerificationTxFrameRate">
            <label>VerificationTxFrameRate</label>
            <tooltip>Tx Frame Rate (fps; per port)</tooltip>
            <description>Tx Frame Rate (fps; per port)</description>
            <datatype>integer</datatype>
            <default>1000</default>
        </parameter>
        <parameter  name="HistogramMode">
            <label>HistogramMode</label>
            <tooltip>Histogram Mode</tooltip>
            <description>Histogram Mode</description>
            <datatype>string</datatype>
            <default>LATENCY</default>
            <allowedValue label="FRAME_LENGTH">FRAME_LENGTH</allowedValue>
            <allowedValue label="INTERARRIVAL_TIME">INTERARRIVAL_TIME</allowedValue>
            <allowedValue label="JITTER">JITTER</allowedValue>
            <allowedValue label="LATENCY">LATENCY</allowedValue>
        </parameter>
        <parameter  name="HistogramDistributionMode">
            <label>HistogramDistributionMode</label>
            <tooltip>Histogram Bucket Distribution Mode</tooltip>
            <description>Histogram Bucket Distribution Mode</description>
            <datatype>string</datatype>
            <default>NONE</default>
            <allowedValue label="NONE">NONE</allowedValue>
            <allowedValue label="EXPONENTIAL">EXPONENTIAL</allowedValue>
            <allowedValue label="LINEAR">LINEAR</allowedValue>
        </parameter>
        <responseDecl>
        <item  name="PassFailState">
            <label>PassFailState</label>
            <default>NONE</default>
            <datatype>string</datatype>
            <allowedValue label="NONE">NONE</allowedValue>
            <allowedValue label="PASSED">PASSED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
        </item>
        </responseDecl>
    </actionDecl>



<actionDecl name="ExportResults">
    <label>Export Results</label>
    <tooltip>Export results to a file.</tooltip>
    <description>Export results to a file.</description>
        <parameter  name="ExecuteSynchronous">
            <label>ExecuteSynchronous</label>
            <tooltip>Execute Synchronous</tooltip>
            <description>Execute Synchronous</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="ProgressCancelled">
            <label>ProgressCancelled</label>
            <tooltip>Progress was Cancelled</tooltip>
            <description>Progress was Cancelled</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="ResultView">
            <label>ResultView</label>
            <tooltip>Result View</tooltip>
            <description>Result View</description>
            <datatype>string</datatype>
            <default>0</default>
        </parameter>
        <parameter  name="ResultTab">
            <label>ResultTab</label>
            <tooltip>Result Tab Name</tooltip>
            <description>Result Tab Name</description>
            <datatype>string</datatype>
            <default></default>
        </parameter>
        <parameter  name="ColumnHeaderStyle">
            <label>ColumnHeaderStyle</label>
            <tooltip>Column Header Style</tooltip>
            <description>Column Header Style</description>
            <datatype>string</datatype>
            <default>DISPLAY</default>
            <allowedValue label="DISPLAY">DISPLAY</allowedValue>
            <allowedValue label="PROPERTY">PROPERTY</allowedValue>
        </parameter>
        <parameter  name="FileNamePrefix">
            <label>FileNamePrefix</label>
            <tooltip>File Name Prefix</tooltip>
            <description>File Name Prefix</description>
            <datatype>string</datatype>
            <default></default>
        </parameter>
        <parameter  name="WriteMode">
            <label>WriteMode</label>
            <tooltip>Write Mode</tooltip>
            <description>Write Mode</description>
            <datatype>string</datatype>
            <default>OVERWRITE</default>
            <allowedValue label="APPEND">APPEND</allowedValue>
            <allowedValue label="OVERWRITE">OVERWRITE</allowedValue>
        </parameter>
        <parameter  name="OutputFormat">
            <label>OutputFormat</label>
            <tooltip>Output Format</tooltip>
            <description>Output Format</description>
            <datatype>string</datatype>
            <default>CSV</default>
            <allowedValue label="CSV">CSV</allowedValue>
            <allowedValue label="XML">XML</allowedValue>
        </parameter>
        <responseDecl>
        <item  name="State">
            <label>State</label>
            <default>INIT</default>
            <datatype>string</datatype>
            <allowedValue label="INIT">INIT</allowedValue>
            <allowedValue label="START">START</allowedValue>
            <allowedValue label="RUNNING">RUNNING</allowedValue>
            <allowedValue label="PAUSED">PAUSED</allowedValue>
            <allowedValue label="PRECOMPLETE">PRECOMPLETE</allowedValue>
            <allowedValue label="COMPLETED">COMPLETED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
            <allowedValue label="VALIDATION_ERROR">VALIDATION_ERROR</allowedValue>
        </item>
        <item  name="Status">
            <label>Status</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        <item  name="StartTime">
            <label>StartTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="EndTime">
            <label>EndTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="ElapsedTime">
            <label>ElapsedTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="ProgressDisplayCounter">
            <label>ProgressDisplayCounter</label>
            <default>true</default>
            <datatype>boolean</datatype>
        </item>
        <item  name="ProgressStepsCount">
            <label>ProgressStepsCount</label>
            <default>1</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentStep">
            <label>ProgressCurrentStep</label>
            <default>1</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentStepName">
            <label>ProgressCurrentStepName</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        <item  name="ProgressMaxValue">
            <label>ProgressMaxValue</label>
            <default>0</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentValue">
            <label>ProgressCurrentValue</label>
            <default>0</default>
            <datatype>integer</datatype>
        </item>
        <item  name="OutputResult">
            <label>OutputResult</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        </responseDecl>
    </actionDecl>



<actionDecl name="RunExternalProcess">
    <label>Run External Process</label>
    <tooltip>Execute an external script/application.</tooltip>
    <description>Execute an external script/application.</description>
        <parameter  name="ErrorOnFailure">
            <label>ErrorOnFailure</label>
            <tooltip>Error on Failure</tooltip>
            <description>Error on Failure</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="CommandLine">
            <label>CommandLine</label>
            <tooltip>Command-Line</tooltip>
            <description>Command-Line</description>
            <datatype>string</datatype>
            <default></default>
        </parameter>
        <parameter  name="Arguments">
            <label>Arguments</label>
            <tooltip>Arguments</tooltip>
            <description>Arguments</description>
            <datatype>string</datatype>
            <default></default>
        </parameter>
