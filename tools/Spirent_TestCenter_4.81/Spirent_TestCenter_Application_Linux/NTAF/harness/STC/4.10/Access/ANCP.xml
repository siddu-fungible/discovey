<query-harness xmlns="http://ntaforum.org/2011/harness" harness="http://spirent.com/NTAF/harness/STC/4.10/Access/ANCP"  xml:lang="en">
<label>ANCP</label>
<helpURI>http://com.fnfr.svt.help/popups/testcenteraction.html</helpURI>
<author>spirent</author>
    
	<supercedes>http://spirent.com/NTAF/harness/STC/3.80/Access/ANCP</supercedes>
<actionDecl name="AncpInitiateAdjacencyWait">
    <label>Wait for Adjacency to Establish</label>
    <tooltip>Wait for Adjacency to Establish</tooltip>
    <description>Wait for Adjacency to Establish</description>
        <parameter  name="WaitTime">
            <label>WaitTime</label>
            <tooltip>Wait Time (secs)</tooltip>
            <description>Wait Time (secs)</description>
            <datatype>decimal</datatype>
            <default>30.0</default>
        </parameter>
        <parameter  name="ObjectList">
            <label>ObjectList</label>
            <tooltip>Objects to Wait for</tooltip>
            <description>Objects to Wait for</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="ErrorOnFailure">
            <label>ErrorOnFailure</label>
            <tooltip>Error on Failure</tooltip>
            <description>Error on Failure</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="AdjacencyBlockState">
            <label>AdjacencyBlockState</label>
            <tooltip>Adjacency Block State</tooltip>
            <description>Adjacency Block State</description>
            <datatype>string</datatype>
            <default>ESTABLISHED</default>
            <allowedValue label="NONE">NONE</allowedValue>
            <allowedValue label="IDLE">IDLE</allowedValue>
            <allowedValue label="ESTABLISHING">ESTABLISHING</allowedValue>
            <allowedValue label="TERMINATING">TERMINATING</allowedValue>
            <allowedValue label="ESTABLISHED">ESTABLISHED</allowedValue>
        </parameter>
        <responseDecl>
        <item  name="Successful">
            <label>Successful</label>
            <mandatory>false</mandatory>
            <default>0</default>
            <datatype>string</datatype>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </item>
        <item  name="Unsuccessful">
            <label>Unsuccessful</label>
            <mandatory>false</mandatory>
            <default>0</default>
            <datatype>string</datatype>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </item>
        <item  name="PassFailState">
            <label>PassFailState</label>
            <default>NONE</default>
            <datatype>string</datatype>
            <allowedValue label="NONE">NONE</allowedValue>
            <allowedValue label="PASSED">PASSED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
        </item>
        </responseDecl>
    </actionDecl>



