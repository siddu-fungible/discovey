            <allowedValue label="PASSED">PASSED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
        </item>
        </responseDecl>
    </actionDecl>



	<subharness>http://spirent.com/NTAF/harness/STC/3.80/Access/DHCPv6</subharness>


</query-harness>

