                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="ErrorOnFailure">
            <label>ErrorOnFailure</label>
            <tooltip>Error on Failure</tooltip>
            <description>Error on Failure</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="BlockDisconnectState">
            <label>BlockDisconnectState</label>
            <tooltip>Block Connected State</tooltip>
            <description>Block Connected State</description>
            <datatype>string</datatype>
            <default>IDLE</default>
            <allowedValue label="NONE">NONE</allowedValue>
            <allowedValue label="IDLE">IDLE</allowedValue>
            <allowedValue label="CONNECTING">CONNECTING</allowedValue>
            <allowedValue label="CONNECTED">CONNECTED</allowedValue>
            <allowedValue label="DISCONNECTING">DISCONNECTING</allowedValue>
            <allowedValue label="TERMINATING">TERMINATING</allowedValue>
