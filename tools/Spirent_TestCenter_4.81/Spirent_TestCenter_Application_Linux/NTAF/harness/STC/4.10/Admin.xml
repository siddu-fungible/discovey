<query-harness xmlns="http://ntaforum.org/2011/harness" harness="http://spirent.com/NTAF/harness/STC/4.10/Admin"  xml:lang="en">
<label>Admin</label>
<helpURI>http://com.fnfr.svt.help/popups/testcenteraction.html</helpURI>
<author>spirent</author>
    
	<supercedes>http://spirent.com/NTAF/harness/STC/3.80/Admin</supercedes>
<actionDecl name="InstallTestPackage">
    <label></label>
    <tooltip>Install/activate test package.</tooltip>
    <description>Install/activate test package.</description>
        <parameter  name="ExecuteSynchronous">
            <label>ExecuteSynchronous</label>
            <tooltip>Execute Synchronous</tooltip>
            <description>Execute Synchronous</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="ProgressCancelled">
            <label>ProgressCancelled</label>
            <tooltip>Progress was Cancelled</tooltip>
            <description>Progress was Cancelled</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="PortGroupList">
            <label>PortGroupList</label>
            <tooltip>Port Group List</tooltip>
            <description>Port Group List</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="TestPackage">
            <label>TestPackage</label>
            <tooltip>Test Package Name</tooltip>
            <description>Test Package Name</description>
            <datatype>string</datatype>
            <default></default>
        </parameter>
        <parameter  name="Version">
            <label>Version</label>
            <tooltip>Test Package Version</tooltip>
            <description>Test Package Version</description>
            <datatype>string</datatype>
            <default></default>
        </parameter>
        <parameter  name="EquipmentList">
            <label>EquipmentList</label>
            <tooltip>Equipment List</tooltip>
            <description>Equipment List</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="ProfileInstallList">
            <label>ProfileInstallList</label>
            <tooltip>Profiles to Install</tooltip>
            <description>Profiles to Install</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default></default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="ProfileActivateList">
            <label>ProfileActivateList</label>
            <tooltip>Profiles to Activate</tooltip>
            <description>Profiles to Activate</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default></default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <responseDecl>
        <item  name="State">
            <label>State</label>
            <default>INIT</default>
            <datatype>string</datatype>
            <allowedValue label="INIT">INIT</allowedValue>
            <allowedValue label="START">START</allowedValue>
            <allowedValue label="RUNNING">RUNNING</allowedValue>
            <allowedValue label="PAUSED">PAUSED</allowedValue>
            <allowedValue label="PRECOMPLETE">PRECOMPLETE</allowedValue>
            <allowedValue label="COMPLETED">COMPLETED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
            <allowedValue label="VALIDATION_ERROR">VALIDATION_ERROR</allowedValue>
        </item>
        <item  name="Status">
            <label>Status</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        <item  name="StartTime">
            <label>StartTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="EndTime">
            <label>EndTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="ElapsedTime">
            <label>ElapsedTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="ProgressDisplayCounter">
            <label>ProgressDisplayCounter</label>
            <default>true</default>
            <datatype>boolean</datatype>
        </item>
        <item  name="ProgressStepsCount">
            <label>ProgressStepsCount</label>
            <default>1</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentStep">
            <label>ProgressCurrentStep</label>
            <default>1</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentStepName">
            <label>ProgressCurrentStepName</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        <item  name="ProgressMaxValue">
            <label>ProgressMaxValue</label>
            <default>0</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentValue">
            <label>ProgressCurrentValue</label>
            <default>0</default>
            <datatype>integer</datatype>
        </item>
        </responseDecl>
    </actionDecl>



