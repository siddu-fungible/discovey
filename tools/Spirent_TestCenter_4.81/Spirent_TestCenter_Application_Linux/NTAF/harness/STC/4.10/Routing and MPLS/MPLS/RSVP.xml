<query-harness xmlns="http://ntaforum.org/2011/harness" harness="http://spirent.com/NTAF/harness/STC/4.10/Routing%20and%20MPLS/MPLS/RSVP"  xml:lang="en">
<label>RSVP</label>
<helpURI>http://com.fnfr.svt.help/popups/testcenteraction.html</helpURI>
<author>spirent</author>
    
	<supercedes>http://spirent.com/NTAF/harness/STC/3.80/Routing%20and%20MPLS/MPLS/RSVP</supercedes>
<actionDecl name="RsvpWaitForReroute">
    <label>Wait For LSP Re-route</label>
    <tooltip>Waits for LSP re-routed count to be non-zero.</tooltip>
    <description>Waits for LSP re-routed count to be non-zero.</description>
        <parameter  name="WaitTime">
            <label>WaitTime</label>
            <tooltip>Wait Time (secs)</tooltip>
            <description>Wait Time (secs)</description>
            <datatype>decimal</datatype>
            <default>30.0</default>
        </parameter>
        <parameter  name="ObjectList">
            <label>ObjectList</label>
            <tooltip>Objects to Wait for</tooltip>
            <description>Objects to Wait for</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="ErrorOnFailure">
            <label>ErrorOnFailure</label>
            <tooltip>Error on Failure</tooltip>
            <description>Error on Failure</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="WaitLspState">
            <label>WaitLspState</label>
            <tooltip>Wait For Re-routed State</tooltip>
            <description>Wait For Re-routed State</description>
            <datatype>string</datatype>
            <default>RSVP_LSP_STATE_REROUTED</default>
            <allowedValue label="RSVP_LSP_STATE_NON_REROUTED">RSVP_LSP_STATE_NON_REROUTED</allowedValue>
            <allowedValue label="RSVP_LSP_STATE_REROUTED">RSVP_LSP_STATE_REROUTED</allowedValue>
        </parameter>
        <responseDecl>
        <item  name="Successful">
            <label>Successful</label>
            <mandatory>false</mandatory>
            <default>0</default>
            <datatype>string</datatype>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </item>
        <item  name="Unsuccessful">
            <label>Unsuccessful</label>
            <mandatory>false</mandatory>
            <default>0</default>
            <datatype>string</datatype>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </item>
        <item  name="PassFailState">
            <label>PassFailState</label>
            <default>NONE</default>
            <datatype>string</datatype>
            <allowedValue label="NONE">NONE</allowedValue>
            <allowedValue label="PASSED">PASSED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
        </item>
        </responseDecl>
    </actionDecl>



	<subharness>http://spirent.com/NTAF/harness/STC/3.80/Routing%20and%20MPLS/MPLS/RSVP</subharness>


</query-harness>

