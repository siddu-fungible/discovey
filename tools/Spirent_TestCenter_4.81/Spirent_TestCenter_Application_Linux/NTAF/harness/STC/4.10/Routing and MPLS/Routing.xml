<query-harness xmlns="http://ntaforum.org/2011/harness" harness="http://spirent.com/NTAF/harness/STC/4.10/Routing%20and%20MPLS/Routing"  xml:lang="en">
<label>Routing</label>
<helpURI>http://com.fnfr.svt.help/popups/testcenteraction.html</helpURI>
<author>spirent</author>
    
	<supercedes>http://spirent.com/NTAF/harness/STC/3.80/Routing%20and%20MPLS/Routing</supercedes>
<actionDecl name="WaitForRouterState">
    <label>Wait For Router State</label>
    <tooltip>Waits for Router states to be UP or DOWN.</tooltip>
    <description>Waits for Router states to be UP or DOWN.</description>
        <parameter  name="WaitTime">
            <label>WaitTime</label>
            <tooltip>Wait Time (secs)</tooltip>
            <description>Wait Time (secs)</description>
            <datatype>decimal</datatype>
            <default>30.0</default>
        </parameter>
        <parameter  name="ObjectList">
            <label>ObjectList</label>
            <tooltip>Objects to Wait for</tooltip>
            <description>Objects to Wait for</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="ErrorOnFailure">
            <label>ErrorOnFailure</label>
            <tooltip>Error on Failure</tooltip>
            <description>Error on Failure</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="WaitRouterState">
            <label>WaitRouterState</label>
            <tooltip>Wait Router State</tooltip>
            <description>Wait Router State</description>
            <datatype>string</datatype>
            <default>PROTOCOL_UP</default>
            <allowedValue label="PROTOCOL_UP">PROTOCOL_UP</allowedValue>
            <allowedValue label="PROTOCOL_DOWN">PROTOCOL_DOWN</allowedValue>
        </parameter>
        <responseDecl>
        <item  name="Successful">
            <label>Successful</label>
            <mandatory>false</mandatory>
            <default>0</default>
            <datatype>string</datatype>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </item>
        <item  name="Unsuccessful">
            <label>Unsuccessful</label>
            <mandatory>false</mandatory>
            <default>0</default>
            <datatype>string</datatype>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </item>
        <item  name="PassFailState">
            <label>PassFailState</label>
            <default>NONE</default>
            <datatype>string</datatype>
            <allowedValue label="NONE">NONE</allowedValue>
            <allowedValue label="PASSED">PASSED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
        </item>
        </responseDecl>
    </actionDecl>



<actionDecl name="ExportFecDetails">
    <label>Export FEC Details</label>
    <tooltip>Export Mpls FEC Bindings information for the selected routers.</tooltip>
    <description>Export Mpls FEC Bindings information for the selected routers.</description>
        <parameter  name="ExecuteSynchronous">
            <label>ExecuteSynchronous</label>
            <tooltip>Execute Synchronous</tooltip>
            <description>Execute Synchronous</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="ProgressCancelled">
            <label>ProgressCancelled</label>
            <tooltip>Progress was Cancelled</tooltip>
            <description>Progress was Cancelled</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="ProtocolList">
            <label>ProtocolList</label>
            <tooltip>Protocol Handle List</tooltip>
            <description>Protocol Handle List</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="FecResolverList">
            <label>FecResolverList</label>
            <tooltip>MPLS Fec Resolver(Protocol/Device) Handle List</tooltip>
            <description>MPLS Fec Resolver(Protocol/Device) Handle List</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="FecTypes">
            <label>FecTypes</label>
            <tooltip>FEC Types</tooltip>
            <description>FEC Types</description>
            <datatype>string</datatype>
            <default>IPV4_PREFIX</default>
            <allowedValue label="IPV4_PREFIX">IPV4_PREFIX</allowedValue>
            <allowedValue label="IPV6_PREFIX">IPV6_PREFIX</allowedValue>
            <allowedValue label="LDP_VC_FEC_128">LDP_VC_FEC_128</allowedValue>
            <allowedValue label="LDP_VC_FEC_129">LDP_VC_FEC_129</allowedValue>
            <allowedValue label="BGP_VPLS">BGP_VPLS</allowedValue>
            <allowedValue label="VPN_IPV4">VPN_IPV4</allowedValue>
            <allowedValue label="VPN_IPV6">VPN_IPV6</allowedValue>
            <allowedValue label="RSVP">RSVP</allowedValue>
            <allowedValue label="RSVP_END_POINT">RSVP_END_POINT</allowedValue>
            <allowedValue label="MPLS_TP_STATIC_LSP">MPLS_TP_STATIC_LSP</allowedValue>
            <allowedValue label="MPLS_TP_STATIC_PW">MPLS_TP_STATIC_PW</allowedValue>
        </parameter>
        <parameter  name="LabelSource">
            <label>LabelSource</label>
            <tooltip>Label Source</tooltip>
            <description>Label Source</description>
            <datatype>string</datatype>
            <default>LEARNED</default>
            <allowedValue label="LEARNED">LEARNED</allowedValue>
            <allowedValue label="ADVERTISED">ADVERTISED</allowedValue>
        </parameter>
        <parameter  name="TrafficBinding">
            <label>TrafficBinding</label>
            <tooltip>Traffic Binding</tooltip>
            <description>Traffic Binding</description>
            <datatype>string</datatype>
            <default>SHOW_ALL</default>
            <allowedValue label="RESOLVED">RESOLVED</allowedValue>
            <allowedValue label="UNRESOLVED">UNRESOLVED</allowedValue>
            <allowedValue label="TRAFFIC_ALL">TRAFFIC_ALL</allowedValue>
            <allowedValue label="SHOW_ALL">SHOW_ALL</allowedValue>
        </parameter>
        <parameter  name="ExportAll">
            <label>ExportAll</label>
            <tooltip>Export All</tooltip>
            <description>Export All</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="MaxEntries">
            <label>MaxEntries</label>
            <tooltip>Maximum Entries</tooltip>
            <description>Maximum Entries</description>
            <datatype>integer</datatype>
            <default>5000</default>
        </parameter>
        <parameter  name="ExportFilePrefix">
            <label>ExportFilePrefix</label>
            <tooltip>Export File Prefix</tooltip>
            <description>Export File Prefix</description>
            <datatype>string</datatype>
            <default>FecBindings</default>
        </parameter>
        <parameter  name="SaveAsArchive">
            <label>SaveAsArchive</label>
            <tooltip>Save As Archive</tooltip>
            <description>Save As Archive</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="ExportPath">
            <label>ExportPath</label>
            <tooltip>Export Path</tooltip>
            <description>Export Path</description>
            <datatype>string</datatype>
            <default></default>
        </parameter>
        <responseDecl>
        <item  name="State">
            <label>State</label>
            <default>INIT</default>
            <datatype>string</datatype>
            <allowedValue label="INIT">INIT</allowedValue>
            <allowedValue label="START">START</allowedValue>
            <allowedValue label="RUNNING">RUNNING</allowedValue>
            <allowedValue label="PAUSED">PAUSED</allowedValue>
            <allowedValue label="PRECOMPLETE">PRECOMPLETE</allowedValue>
            <allowedValue label="COMPLETED">COMPLETED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
            <allowedValue label="VALIDATION_ERROR">VALIDATION_ERROR</allowedValue>
        </item>
        <item  name="Status">
            <label>Status</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        <item  name="StartTime">
            <label>StartTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="EndTime">
            <label>EndTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="ElapsedTime">
            <label>ElapsedTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="ProgressDisplayCounter">
            <label>ProgressDisplayCounter</label>
            <default>true</default>
            <datatype>boolean</datatype>
        </item>
        <item  name="ProgressStepsCount">
            <label>ProgressStepsCount</label>
            <default>1</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentStep">
            <label>ProgressCurrentStep</label>
            <default>1</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentStepName">
            <label>ProgressCurrentStepName</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        <item  name="ProgressMaxValue">
            <label>ProgressMaxValue</label>
            <default>0</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentValue">
            <label>ProgressCurrentValue</label>
            <default>0</default>
            <datatype>integer</datatype>
        </item>
        <item  name="FecBindingCount">
            <label>FecBindingCount</label>
            <default>0</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ExportedFileNames">
            <label>ExportedFileNames</label>
            <mandatory>false</mandatory>
            <default></default>
            <datatype>string</datatype>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </item>
        </responseDecl>
    </actionDecl>



<actionDecl name="VerifyRouterUp">
    <label>Verify Routing State</label>
    <tooltip>Verify Router Up status for the selected routers.</tooltip>
    <description>Verify Router Up status for the selected routers.</description>
        <parameter  name="ErrorOnFailure">
            <label>ErrorOnFailure</label>
            <tooltip>Error on Failure</tooltip>
            <description>Error on Failure</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="RouterList">
            <label>RouterList</label>
            <tooltip>Router Handle List</tooltip>
            <description>Router Handle List</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <responseDecl>
        <item  name="PassFailState">
            <label>PassFailState</label>
            <default>NONE</default>
            <datatype>string</datatype>
            <allowedValue label="NONE">NONE</allowedValue>
            <allowedValue label="PASSED">PASSED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
        </item>
        </responseDecl>
    </actionDecl>



	<subharness>http://spirent.com/NTAF/harness/STC/3.80/Routing%20and%20MPLS/Routing</subharness>


</query-harness>

