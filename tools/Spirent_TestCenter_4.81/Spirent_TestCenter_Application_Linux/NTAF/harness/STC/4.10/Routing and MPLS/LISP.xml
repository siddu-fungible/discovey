<query-harness xmlns="http://ntaforum.org/2011/harness" harness="http://spirent.com/NTAF/harness/STC/4.10/Routing%20and%20MPLS/LISP"  xml:lang="en">
<label>LISP</label>
<helpURI>http://com.fnfr.svt.help/popups/testcenteraction.html</helpURI>
<author>spirent</author>
    <actionDecl name="LispShowLocalCache">
    <label>Show Local Cache</label>
    <tooltip>Show Local Cache for the selected routers</tooltip>
    <description>Show Local Cache for the selected routers</description>
        <parameter  name="RouterList">
            <label>RouterList</label>
            <tooltip>LISP Router List</tooltip>
            <description>LISP Router List</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="ExecuteSynchronous">
            <label>ExecuteSynchronous</label>
            <tooltip>Execute Synchronous</tooltip>
            <description>Execute Synchronous</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="ProgressCancelled">
            <label>ProgressCancelled</label>
            <tooltip>Progress was Cancelled</tooltip>
            <description>Progress was Cancelled</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="EidTypes">
            <label>EidTypes</label>
            <tooltip>EID Type</tooltip>
            <description>EID Type</description>
            <datatype>string</datatype>
            <default>IPV4</default>
            <allowedValue label="IPV6">IPV6</allowedValue>
            <allowedValue label="IPV4">IPV4</allowedValue>
        </parameter>
        <parameter  name="BindingSource">
            <label>BindingSource</label>
            <tooltip>Binding Source</tooltip>
            <description>Binding Source</description>
            <datatype>string</datatype>
            <default>LEARNED</default>
            <allowedValue label="LEARNED">LEARNED</allowedValue>
            <allowedValue label="ADVERTISED">ADVERTISED</allowedValue>
            <allowedValue label="LEARNED_AND_ADVERTISED">LEARNED_AND_ADVERTISED</allowedValue>
        </parameter>
        <parameter  name="TrafficBinding">
            <label>TrafficBinding</label>
            <tooltip>Traffic Binding</tooltip>
            <description>Traffic Binding</description>
            <datatype>string</datatype>
            <default>SHOW_ALL</default>
            <allowedValue label="RESOLVED">RESOLVED</allowedValue>
            <allowedValue label="UNRESOLVED">UNRESOLVED</allowedValue>
            <allowedValue label="TRAFFIC_ALL">TRAFFIC_ALL</allowedValue>
            <allowedValue label="SHOW_ALL">SHOW_ALL</allowedValue>
        </parameter>
        <parameter  name="ExportAll">
            <label>ExportAll</label>
            <tooltip>Export All</tooltip>
            <description>Export All</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="MaxEntries">
            <label>MaxEntries</label>
            <tooltip>Maximum Entries</tooltip>
            <description>Maximum Entries</description>
            <datatype>integer</datatype>
            <default>5000</default>
        </parameter>
        <parameter  name="ExportFilePrefix">
            <label>ExportFilePrefix</label>
            <tooltip>Export File Prefix</tooltip>
            <description>Export File Prefix</description>
            <datatype>string</datatype>
            <default>LocalCaches</default>
        </parameter>
        <parameter  name="SaveAsArchive">
            <label>SaveAsArchive</label>
            <tooltip>Save As Archive</tooltip>
            <description>Save As Archive</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="ExportPath">
            <label>ExportPath</label>
            <tooltip>Export Path</tooltip>
            <description>Export Path</description>
            <datatype>string</datatype>
            <default></default>
        </parameter>
        <responseDecl>
        <item  name="State">
            <label>State</label>
            <default>INIT</default>
            <datatype>string</datatype>
            <allowedValue label="INIT">INIT</allowedValue>
            <allowedValue label="START">START</allowedValue>
            <allowedValue label="RUNNING">RUNNING</allowedValue>
            <allowedValue label="PAUSED">PAUSED</allowedValue>
            <allowedValue label="PRECOMPLETE">PRECOMPLETE</allowedValue>
            <allowedValue label="COMPLETED">COMPLETED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
            <allowedValue label="VALIDATION_ERROR">VALIDATION_ERROR</allowedValue>
        </item>
        <item  name="Status">
            <label>Status</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        <item  name="StartTime">
            <label>StartTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="EndTime">
            <label>EndTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="ElapsedTime">
            <label>ElapsedTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="ProgressDisplayCounter">
            <label>ProgressDisplayCounter</label>
            <default>true</default>
            <datatype>boolean</datatype>
        </item>
        <item  name="ProgressStepsCount">
            <label>ProgressStepsCount</label>
            <default>1</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentStep">
            <label>ProgressCurrentStep</label>
            <default>1</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentStepName">
            <label>ProgressCurrentStepName</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        <item  name="ProgressMaxValue">
            <label>ProgressMaxValue</label>
            <default>0</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentValue">
            <label>ProgressCurrentValue</label>
            <default>0</default>
            <datatype>integer</datatype>
        </item>
        <item  name="MappingEntriesCount">
            <label>MappingEntriesCount</label>
            <default>0</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ExportedFileNames">
            <label>ExportedFileNames</label>
            <mandatory>false</mandatory>
            <default></default>
            <datatype>string</datatype>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </item>
        </responseDecl>
    </actionDecl>

    <actionDecl name="LispMappingRequest">
    <label>Lisp Mapping Request</label>
    <tooltip>LISP Mapping Request.</tooltip>
    <description>LISP Mapping Request.</description>
        <parameter  name="RouterList">
            <label>RouterList</label>
            <tooltip>LISP Router List</tooltip>
            <description>LISP Router List</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="ExecuteSynchronous">
            <label>ExecuteSynchronous</label>
            <tooltip>Execute Synchronous</tooltip>
            <description>Execute Synchronous</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="ProgressCancelled">
            <label>ProgressCancelled</label>
            <tooltip>Progress was Cancelled</tooltip>
            <description>Progress was Cancelled</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="DstEidIpAddr">
            <label>DstEidIpAddr</label>
            <tooltip>IP/Length/Count/Step</tooltip>
            <description>IP/Length/Count/Step</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>192.0.1.1/32/1/0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="SiteList">
            <label>SiteList</label>
            <tooltip>Site List</tooltip>
            <description>Site List</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="SrcEidIpv4Addr">
            <label>SrcEidIpv4Addr</label>
            <tooltip>Source EID IPv4 Address</tooltip>
            <description>Source EID IPv4 Address</description>
            <datatype>string</datatype>
            <default>10.0.1.1</default>
        </parameter>
        <parameter  name="SrcEidIpv6Addr">
            <label>SrcEidIpv6Addr</label>
            <tooltip>Source EID IPv6 Address</tooltip>
            <description>Source EID IPv6 Address</description>
            <datatype>string</datatype>
            <default>2000::1</default>
        </parameter>
        <responseDecl>
        <item  name="State">
            <label>State</label>
            <default>INIT</default>
            <datatype>string</datatype>
            <allowedValue label="INIT">INIT</allowedValue>
            <allowedValue label="START">START</allowedValue>
            <allowedValue label="RUNNING">RUNNING</allowedValue>
            <allowedValue label="PAUSED">PAUSED</allowedValue>
            <allowedValue label="PRECOMPLETE">PRECOMPLETE</allowedValue>
            <allowedValue label="COMPLETED">COMPLETED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
            <allowedValue label="VALIDATION_ERROR">VALIDATION_ERROR</allowedValue>
        </item>
        <item  name="Status">
            <label>Status</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        <item  name="StartTime">
            <label>StartTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="EndTime">
            <label>EndTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="ElapsedTime">
            <label>ElapsedTime</label>
            <default>0</default>
            <datatype>decimal</datatype>
        </item>
        <item  name="ProgressDisplayCounter">
            <label>ProgressDisplayCounter</label>
            <default>true</default>
            <datatype>boolean</datatype>
        </item>
        <item  name="ProgressStepsCount">
            <label>ProgressStepsCount</label>
            <default>1</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentStep">
            <label>ProgressCurrentStep</label>
            <default>1</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentStepName">
            <label>ProgressCurrentStepName</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        <item  name="ProgressMaxValue">
            <label>ProgressMaxValue</label>
            <default>0</default>
            <datatype>integer</datatype>
        </item>
        <item  name="ProgressCurrentValue">
            <label>ProgressCurrentValue</label>
            <default>0</default>
            <datatype>integer</datatype>
        </item>
        </responseDecl>
    </actionDecl>

</query-harness>
