<query-harness xmlns="http://ntaforum.org/2011/harness" harness="http://spirent.com/NTAF/harness/STC/4.10/Global%20Services"  xml:lang="en">
<label>Global Services</label>
<helpURI>http://com.fnfr.svt.help/popups/testcenteraction.html</helpURI>
<author>spirent</author>
    
	<supercedes>http://spirent.com/NTAF/harness/STC/3.80/Global%20Services</supercedes>
<actionDecl name="RunCustom">
    <label>Run Custom Command</label>
    <tooltip>Executes a Spirent Global Services script.</tooltip>
    <description>Executes a Spirent Global Services script.</description>
        <parameter  name="Parameters">
            <label>Parameters</label>
            <tooltip>Parameters</tooltip>
            <description>Parameters</description>
            <datatype>string</datatype>
            <default></default>
        </parameter>
        <parameter  name="ScriptFileName">
            <label>ScriptFileName</label>
            <tooltip>Script File Name</tooltip>
            <description>Script File Name</description>
            <datatype>string</datatype>
            <default></default>
        </parameter>
        <parameter  name="ResetScriptingEngine">
            <label>ResetScriptingEngine</label>
            <tooltip>Reset Scripting Engine</tooltip>
            <description>Reset Scripting Engine</description>
            <datatype>boolean</datatype>
            <default>true</default>
        </parameter>
        <parameter  name="ErrorOnFailure">
            <label>ErrorOnFailure</label>
            <tooltip>Error on Failure</tooltip>
            <description>Error on Failure</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <responseDecl>
        <item  name="ScriptResult">
            <label>ScriptResult</label>
            <default></default>
            <datatype>string</datatype>
        </item>
        <item  name="PassFailState">
            <label>PassFailState</label>
            <default>NONE</default>
            <datatype>string</datatype>
            <allowedValue label="NONE">NONE</allowedValue>
            <allowedValue label="PASSED">PASSED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
        </item>
        </responseDecl>
    </actionDecl>



	<subharness>http://spirent.com/NTAF/harness/STC/3.80/Global%20Services</subharness>


</query-harness>

