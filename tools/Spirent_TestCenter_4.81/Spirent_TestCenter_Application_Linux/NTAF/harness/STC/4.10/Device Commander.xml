<actionDecl name="ExternalDeviceSessionConnect">
    <label>Connect Session</label>
    <tooltip>Open session to connect to external device.</tooltip>
    <description>Open session to connect to external device.</description>
        <parameter  name="ErrorOnFailure">
            <label>ErrorOnFailure</label>
            <tooltip>Error on Failure</tooltip>
            <description>Error on Failure</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="HandleList">
            <label>HandleList</label>
            <tooltip>Project, ExternalDevice or ExternalDeviceSession handle list</tooltip>
            <description>Project, ExternalDevice or ExternalDeviceSession handle list</description>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="WaitTime">
            <label>WaitTime</label>
            <tooltip>Wait Time (second)</tooltip>
            <description>Wait Time (second)</description>
            <datatype>integer</datatype>
            <default>180</default>
        </parameter>
        <responseDecl>
        <item  name="PassFailState">
            <label>PassFailState</label>
            <default>NONE</default>
            <datatype>string</datatype>
            <allowedValue label="NONE">NONE</allowedValue>
            <allowedValue label="PASSED">PASSED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
        </item>
        </responseDecl>
    </actionDecl>



