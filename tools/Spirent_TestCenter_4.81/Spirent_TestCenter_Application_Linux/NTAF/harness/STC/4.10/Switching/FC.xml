<query-harness xmlns="http://ntaforum.org/2011/harness" harness="http://spirent.com/NTAF/harness/STC/4.10/Switching/FC"  xml:lang="en">
<label>FC</label>
<helpURI>http://com.fnfr.svt.help/popups/testcenteraction.html</helpURI>
<author>spirent</author>
    
	<supercedes>http://spirent.com/NTAF/harness/STC/3.80/Switching/FC</supercedes>
<actionDecl name="FcWaitForAggregateSubState">
    <label>Wait For Aggregate Sub State</label>
    <tooltip>Waits for specified sub state.</tooltip>
    <description>Waits for specified sub state.</description>
        <parameter  name="WaitTime">
            <label>WaitTime</label>
            <tooltip>Wait Time (secs)</tooltip>
            <description>Wait Time (secs)</description>
            <datatype>decimal</datatype>
            <default>30.0</default>
        </parameter>
        <parameter  name="ObjectList">
            <label>ObjectList</label>
            <tooltip>Objects to Wait for</tooltip>
            <description>Objects to Wait for</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="ErrorOnFailure">
            <label>ErrorOnFailure</label>
            <tooltip>Error on Failure</tooltip>
            <description>Error on Failure</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="WaitAggregateSubState">
            <label>WaitAggregateSubState</label>
            <tooltip>Wait Aggregate Sub State</tooltip>
            <description>Wait Aggregate Sub State</description>
            <datatype>string</datatype>
            <default>LOGGED_OUT</default>
            <allowedValue label="NONE">NONE</allowedValue>
            <allowedValue label="LOGGED_OUT">LOGGED_OUT</allowedValue>
            <allowedValue label="FLOGI_SENT">FLOGI_SENT</allowedValue>
            <allowedValue label="FLOGI_TIMED_OUT">FLOGI_TIMED_OUT</allowedValue>
            <allowedValue label="FLOGI_REJECTED">FLOGI_REJECTED</allowedValue>
            <allowedValue label="FLOGI_ACCEPTED">FLOGI_ACCEPTED</allowedValue>
            <allowedValue label="FDISC_SENT">FDISC_SENT</allowedValue>
            <allowedValue label="FDISC_TIMED_OUT">FDISC_TIMED_OUT</allowedValue>
            <allowedValue label="FDISC_REJECTED">FDISC_REJECTED</allowedValue>
            <allowedValue label="FDISC_ACCEPTED">FDISC_ACCEPTED</allowedValue>
            <allowedValue label="PLOGI_SENT">PLOGI_SENT</allowedValue>
            <allowedValue label="PLOGI_TIMED_OUT">PLOGI_TIMED_OUT</allowedValue>
            <allowedValue label="PLOGI_REJECTED">PLOGI_REJECTED</allowedValue>
            <allowedValue label="PLOGI_ACCEPTED">PLOGI_ACCEPTED</allowedValue>
            <allowedValue label="SCR_SENT">SCR_SENT</allowedValue>
            <allowedValue label="SCR_TIMED_OUT">SCR_TIMED_OUT</allowedValue>
            <allowedValue label="SCR_REJECTED">SCR_REJECTED</allowedValue>
            <allowedValue label="SCR_ACCEPTED">SCR_ACCEPTED</allowedValue>
            <allowedValue label="MAX">MAX</allowedValue>
        </parameter>
        <responseDecl>
        <item  name="Successful">
            <label>Successful</label>
            <mandatory>false</mandatory>
            <default>0</default>
            <datatype>string</datatype>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </item>
        <item  name="Unsuccessful">
            <label>Unsuccessful</label>
            <mandatory>false</mandatory>
            <default>0</default>
            <datatype>string</datatype>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </item>
        <item  name="PassFailState">
            <label>PassFailState</label>
            <default>NONE</default>
            <datatype>string</datatype>
            <allowedValue label="NONE">NONE</allowedValue>
            <allowedValue label="PASSED">PASSED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
        </item>
        </responseDecl>
    </actionDecl>



