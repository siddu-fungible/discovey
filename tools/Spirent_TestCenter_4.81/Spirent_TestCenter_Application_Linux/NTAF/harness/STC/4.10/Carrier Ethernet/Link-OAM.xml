<query-harness xmlns="http://ntaforum.org/2011/harness" harness="http://spirent.com/NTAF/harness/STC/4.10/Carrier%20Ethernet/Link-OAM"  xml:lang="en">
<label>Link-OAM</label>
<helpURI>http://com.fnfr.svt.help/popups/testcenteraction.html</helpURI>
<author>spirent</author>
    
	<supercedes>http://spirent.com/NTAF/harness/STC/3.80/Carrier%20Ethernet/Link-OAM</supercedes>
<actionDecl name="LinkOamWaitOnDiscoveryComplete">
    <label>Wait for Discovery Complete state</label>
    <tooltip>Start Link OAM</tooltip>
    <description>Start Link OAM</description>
        <parameter  name="WaitTime">
            <label>WaitTime</label>
            <tooltip>Wait Time (secs)</tooltip>
            <description>Wait Time (secs)</description>
            <datatype>decimal</datatype>
            <default>30.0</default>
        </parameter>
        <parameter  name="ObjectList">
            <label>ObjectList</label>
            <tooltip>Objects to Wait for</tooltip>
            <description>Objects to Wait for</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="ErrorOnFailure">
            <label>ErrorOnFailure</label>
            <tooltip>Error on Failure</tooltip>
            <description>Error on Failure</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <responseDecl>
        <item  name="Successful">
            <label>Successful</label>
            <mandatory>false</mandatory>
            <default>0</default>
            <datatype>string</datatype>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </item>
        <item  name="Unsuccessful">
            <label>Unsuccessful</label>
            <mandatory>false</mandatory>
            <default>0</default>
            <datatype>string</datatype>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </item>
        <item  name="PassFailState">
            <label>PassFailState</label>
            <default>NONE</default>
            <datatype>string</datatype>
            <allowedValue label="NONE">NONE</allowedValue>
            <allowedValue label="PASSED">PASSED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
        </item>
        <item  name="LocalState">
            <label>LocalState</label>
            <default>COMPLETED</default>
            <datatype>string</datatype>
            <allowedValue label="UNSATISFIED">UNSATISFIED</allowedValue>
            <allowedValue label="NOT_COMPLETED">NOT_COMPLETED</allowedValue>
            <allowedValue label="COMPLETED">COMPLETED</allowedValue>
            <allowedValue label="RESERVED">RESERVED</allowedValue>
            <allowedValue label="NONE">NONE</allowedValue>
        </item>
        </responseDecl>
    </actionDecl>



	<subharness>http://spirent.com/NTAF/harness/STC/3.80/Carrier%20Ethernet/Link-OAM</subharness>


</query-harness>

