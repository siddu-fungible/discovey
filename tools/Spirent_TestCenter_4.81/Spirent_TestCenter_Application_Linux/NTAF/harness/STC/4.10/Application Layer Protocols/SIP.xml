<query-harness xmlns="http://ntaforum.org/2011/harness" harness="http://spirent.com/NTAF/harness/STC/4.10/Application%20Layer%20Protocols/SIP"  xml:lang="en">
<label>SIP</label>
<helpURI>http://com.fnfr.svt.help/popups/testcenteraction.html</helpURI>
<author>spirent</author>
    
	<supercedes>http://spirent.com/NTAF/harness/STC/3.80/Application%20Layer%20Protocols/SIP</supercedes>
<actionDecl name="SipRegisterWait">
    <label>Wait For Register/Unregister UA Blocks Command To Finish</label>
    <tooltip>Wait for completion of SipRegisterCommand</tooltip>
    <description>Wait for completion of SipRegisterCommand</description>
        <parameter  name="WaitTime">
            <label>WaitTime</label>
            <tooltip>Wait Time (secs)</tooltip>
            <description>Wait Time (secs)</description>
            <datatype>decimal</datatype>
            <default>30.0</default>
        </parameter>
        <parameter  name="ObjectList">
            <label>ObjectList</label>
            <tooltip>Objects to Wait for</tooltip>
            <description>Objects to Wait for</description>
            <mandatory>false</mandatory>
            <datatype>string</datatype>
            <default>0</default>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </parameter>
        <parameter  name="ErrorOnFailure">
            <label>ErrorOnFailure</label>
            <tooltip>Error on Failure</tooltip>
            <description>Error on Failure</description>
            <datatype>boolean</datatype>
            <default>false</default>
        </parameter>
        <parameter  name="BlockRegistrationState">
            <label>BlockRegistrationState</label>
            <tooltip>Block Registration State</tooltip>
            <description>Block Registration State</description>
            <datatype>string</datatype>
            <default>REGISTRATION_SUCCEEDED</default>
            <allowedValue label="NOT_REGISTERED">NOT_REGISTERED</allowedValue>
            <allowedValue label="REGISTERING">REGISTERING</allowedValue>
            <allowedValue label="REGISTRATION_SUCCEEDED">REGISTRATION_SUCCEEDED</allowedValue>
            <allowedValue label="REGISTRATION_FAILED">REGISTRATION_FAILED</allowedValue>
            <allowedValue label="REGISTRATION_CANCELED">REGISTRATION_CANCELED</allowedValue>
            <allowedValue label="UNREGISTERING">UNREGISTERING</allowedValue>
        </parameter>
        <responseDecl>
        <item  name="Successful">
            <label>Successful</label>
            <mandatory>false</mandatory>
            <default>0</default>
            <datatype>string</datatype>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </item>
        <item  name="Unsuccessful">
            <label>Unsuccessful</label>
            <mandatory>false</mandatory>
            <default>0</default>
            <datatype>string</datatype>
            <allowedCount>
                <min>0</min>
                <max>65535</max>
            </allowedCount>
        </item>
        <item  name="PassFailState">
            <label>PassFailState</label>
            <default>NONE</default>
            <datatype>string</datatype>
            <allowedValue label="NONE">NONE</allowedValue>
            <allowedValue label="PASSED">PASSED</allowedValue>
            <allowedValue label="FAILED">FAILED</allowedValue>
        </item>
        </responseDecl>
    </actionDecl>



