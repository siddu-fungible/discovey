<?xml version="1.0" ?>
<stc:meta package="spirent.xtapi" plugin="stakcommands" version="0.0.0-0" xmlns:stc="urn:www.spirentcom.com:XMLSchema.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">  
    <stc:class name="SaveAsCommand" displayName="Save Configuration To Script" bllDirectInterface="false" pythonVirtualenv="spirent.xtapi">
        <stc:attribute internalIsEnum="false" internalType="bool" name="none" type="core.Sequenceable" value="true"/>
        <stc:attribute internalIsEnum="false" internalType="string" name="none" type="framework.CommandDescription" value="Save Configuration To Script"/>
        <stc:attribute internalIsEnum="false" internalType="string" name="none" type="framework.CommandCategory" value="PL Command"/>
        <stc:property name="DeviceVar" type="string" default=""  category="input" displayName="Device Variable">
          <stc:attribute type="framework.PropertyDescription" value="Device Variable" internalType="string"/>
        </stc:property>
        <stc:property name="ClassName" type="string" default="" category="input" displayName="Class Name">
          <stc:attribute type="framework.PropertyDescription" value="type of protocol under device to do the save as" internalType="string"/>
        </stc:property>
        <stc:property name="ReturnVar" type="string" default="" category="input" displayName="Return Var">
          <stc:attribute type="framework.PropertyDescription" value="the return var will be used in the generted script" internalType="string"/>
        </stc:property>
        <stc:property name="Device" type="handle" default="" category="input" displayName="Device Handle">
          <stc:attribute type="framework.PropertyDescription" value="Device Handle" internalType="string"/>
        </stc:property>
        <stc:property name="Parse" type="bool" default="true" category="input" displayName="Parse the datamodel or not">
          <stc:attribute type="framework.PropertyDescription" value="Parse the datamodel or not" internalType="string"/>
        </stc:property>
        <stc:property category="output" name="Scripts" displayName="Scripts Generated" type="string" default="" minOccurs="0" maxOccurs="unbounded">
            <stc:attribute internalIsEnum="false" internalType="string" type="framework.PropertyDescription" value="script" />
        </stc:property>
    </stc:class>
    <stc:class name="ScriptRunCommand" displayName="RunScript" bllDirectInterface="false" pythonVirtualenv="spirent.xtapi">
        <stc:attribute internalIsEnum="false" internalType="bool" name="none" type="core.Sequenceable" value="true"/>
        <stc:attribute internalIsEnum="false" internalType="string" name="none" type="framework.CommandDescription" value="Run Scripts"/>
        <stc:attribute internalIsEnum="false" internalType="string" name="none" type="framework.CommandCategory" value="PL Command"/>
        <stc:property name="ApiName" type="string" default=""  category="input" displayName="API name">
          <stc:attribute type="framework.PropertyDescription" value="API name to Run Scripts" internalType="string"/>
        </stc:property>
        <stc:property name="InputArgs" type="string" default="" category="input" displayName="API arguments">
          <stc:attribute type="framework.PropertyDescription" value="inputting arguments" internalType="string"/>
        </stc:property>
        <stc:property category="output" name="RunResult" displayName="runResult" type="string" default="">
            <stc:attribute internalIsEnum="false" internalType="string" name="none" type="framework.PropertyDescription" value="runResult" />
        </stc:property>
    </stc:class>
</stc:meta>
