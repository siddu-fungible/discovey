<stc:meta package="spirent.switching.ose" plugin="stakcommands" version="0.0.0-0" xmlns:stc="urn:www.spirentcom.com:XMLSchema.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">  
  <stc:class name="OsePortNameExpansionCommand" displayName="Expand the OSE port name regex" packageName="spirent.switching.ose" bllDirectInterface="true" isInternal="true" canCreate="true">

    <stc:attribute type="core.Sequenceable" value="false"/>
    <stc:attribute type="framework.CommandDescription" value="OSE Port Name Expansion util"/>
    <stc:attribute type="framework.CommandCategory" value="STAK Command"/>

    <stc:property category="input" name="OsePortNameStr" displayName="A regular expression string for a port name" type="string" default="Port-@b-@s-@x">
        <stc:attribute type="framework.PropertyDescription" value="Ose Port Name Regex String"/>
    </stc:property>
    
    <stc:property category="input" name="BlockId" displayName="The device block id" type="u32" default="1">
        <stc:attribute type="framework.PropertyDescription" value="The device block id"/>
    </stc:property>
    
    <stc:property category="input" name="NumSwitches" displayName="Number of switches" type="u32" default="1">
        <stc:attribute type="framework.PropertyDescription" value="Number of switches"/>
    </stc:property>

    <stc:property category="input" name="DevPerSwitch" displayName="Number of devices per switch" type="u32" default="1">
        <stc:attribute type="framework.PropertyDescription" value="Number of devices per switch"/>
    </stc:property>
      
	<stc:property category="output" name="ExpandedNameList" displayName="Port Names" type="string" default="0" minOccurs="0" maxOccurs="unbounded">
      <stc:attribute type="framework.PropertyDescription" value="List of port names"/>
    </stc:property>
    
  </stc:class>
  
 	<stc:class name="OseWaitForTrafficStateCommand" packageName="spirent.switching.ose" displayName="Wait For OSE Traffic State" bllDirectInterface="true"
    baseClass="spirent.core.WaitOnScriptablesCommand">   

		<stc:attribute internalIsEnum="false" internalType="bool" name="none" type="core.Sequenceable" value="true"/>
		<stc:attribute internalIsEnum="false" internalType="string" name="none" type="framework.CommandDescription" value="Waits for a specified OSE traffic state for a specified wait time."/>
		<stc:attribute internalIsEnum="false" internalType="string" name="none" type="framework.CommandCategory" value="Switching//OSE"/>
		
		<stc:enumeration name="EnumOseTrafficState">
			<stc:enum name="STOPPED"  value="0" displayName="STOPPED" />
			<stc:enum name="STARTED"  value="1" displayName="STARTED" />
		</stc:enumeration>
        
		<stc:property category="input" name="WaitState" displayName="Wait State" type="u16" default="STOPPED">
			<stc:attribute internalIsEnum="false" internalType="string" name="none" type="framework.PropertyDescription" value="State to wait on"/>
			<stc:enumerationRef ref="EnumOseTrafficState"/>
		</stc:property>
 	</stc:class> 
    
    <stc:class name="OseViewSwitchConfigCommand" packageName="spirent.switching.ose" displayName="View Ose Switch Config" bllDirectInterface="true" isInternal="true" canCreate="true">

		<stc:attribute type="core.Sequenceable" value="false"/>
        <stc:attribute type="framework.CommandDescription" value="View Ose Switch Config"/>
        <stc:attribute type="framework.CommandCategory" value="STAK Command"/>
        
		<stc:property category="input" name="Params" displayName="Switch Block Parameters" type="string" default="">
			<stc:attribute type="framework.PropertyDescription" value="Dict of Switch Block Parameters"/>
		</stc:property>
        <stc:property category="output" name="TopoDumpHtml" displayName="Ose Topo String" type="string" default="">
            <stc:attribute type="framework.PropertyDescription" value="String Dump of ose topo html"/>
        </stc:property>
 	</stc:class> 
 </stc:meta>