    <stc:class name="OrchestrationCreateCommand"
        baseClass="spirent.vcm.CredentialsCommand"
        displayName="Orchestration Create"
        packageName="spirent.vcm"
        bllDirectInterface="true"
        isInternal="true"
        pythonVirtualenv="spirent.vcm" >

        <stc:attribute type="core.Sequenceable" value="false"/>
        <stc:attribute type="framework.CommandDescription" value="Create an orchestration"/>
        <stc:attribute type="framework.CommandCategory" value="Virtual Deployment Service"/>

        <stc:property category="input" name="OrchestrationName" displayName="Orchestration Name" type="string" default="">
            <stc:attribute type="framework.PropertyDescription" value="The desired orchestration name"/>
        </stc:property>

        <stc:property category="input" name="TemplatePath" displayName="Orchestration Template File Path" type="string" default="">
            <stc:attribute type="framework.PropertyDescription" value="The path to the orchestration template file to upload"/>
        </stc:property>

        <stc:property category="input" name="Overwrite" displayName="Overwrite Orchestration" type="bool" default="false">
            <stc:attribute type="framework.PropertyDescription" value="Whether to overwrite an existing orchestration on create"/>
        </stc:property>

        <stc:property category="input" name="Params" displayName="Additional Orchestration Parameters" type="string" default="">
            <stc:attribute type="framework.PropertyDescription" value="The additional orchestration parameters"/>
        </stc:property>

        <stc:property category="input" name="CreateTimeoutSeconds" displayName="Orchestration Create Timeout Seconds" type="u16" default="360">
            <stc:attribute type="framework.PropertyDescription" value="The orchestration create timeout in seconds"/>
        </stc:property>

        <stc:property category="input" name="CreateIntervalSeconds" displayName="Orchestration Create Check Interval Seconds" type="u16" default="10">
            <stc:attribute type="framework.PropertyDescription" value="The orchestration creation polling interval in seconds"/>
        </stc:property>

        <stc:property category="output" name="OrchestrationPresent" displayName="Orchestration Present" type="bool" default="false">
            <stc:attribute type="framework.PropertyDescription" value="Boolean showing if the orchestration is present"/>
