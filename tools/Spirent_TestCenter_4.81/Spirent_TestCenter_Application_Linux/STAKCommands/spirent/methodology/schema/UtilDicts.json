  "range_modifier": {
    "title": "Schema for range-type ModifierInfo of the StmPropertyModifier.",
    "type": "object",
    "properties": {
      "propertyName": {
        "description": "Name of the property to be modified.",
        "type": "string"
      },
      "modifierType": {
        "description": "Type of modifier to apply.  Currently, only RANGE is supported.",
        "enum": [
          "RANGE"
        ]
      },
      "objectName": {
        "description": "Name of the object type that contains the property to be modified.",
        "type": "string"
      },
      "pduOffsetReference": {
        "description": "An offset into a PDU XML config given in the form pdu_name.pdu_property.  If specified, will update only the referenced attribute in XML.  This should only be used when the value being modified is a PDU XML string.",
        "type": "string"
      },
      "propertyValueDict": {
        "description": "Properties specific to the type of modifier being applied.",
        "type": "object",
        "properties": {
          "repeat": {
            "description": "Scalar repeat value or list of repeat values.  When a list is specified, these are applied to elements in the same index in the start list.  If there are not enough values, the values are repeated.",
            "oneOf": [
              {
                "type": "integer"
              },
              {
                "type": "array",
                "items": {
                  "type": "integer"
                }
              }
            ]
          },
          "resetOnNewTargetObject": {
            "description": "Reset the start value each time a new target (usually parent) object of the objects being modified is encountered.",
            "type": "boolean"
          },
          "start": {
            "description": "Scalar start value or list of start values.  When a list is used, the number of values in the list indicate how long the generated list will be.",
            "oneOf": [
              {
                "type": "string"
              },
              {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            ]
          },
          "step": {
            "description": "Scalar step value or list of step values.  When a list is specified, these are applied to elements in the same index in the start list.  If there are not enough values, the values are repeated.",
            "oneOf": [
              {
                "type": "string"
              },
              {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            ]
          },
          "recycle": {
            "description": "Scalar repeat value or list of repeat values.  When a list is specified, these are applied to elements in the same index in the start list.  If there are notenough values, the values are repeated.",
            "oneOf": [
              {
                "type": "integer"
              },
              {
                "type": "array",
                "items": {
                  "type": "integer"
                }
              }
            ]
          },
          "targetObjectStep": {
            "description": "Scalar target object step value or list of target object step values.  This step will be applied when a new target (usually parent) object of the objects being modified is encountered.  Usually used with resetOnNewTargetObject.  When a list is specified, these are applied to elements in the same index in the start list.  If there are not enough values, the values are repeated.",
            "oneOf": [
              {
                "type": "string"
              },
              {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            ]
          }
        },
        "required": [
          "start",
          "step"
        ]
      }
    },
    "required": [
      "objectName",
      "propertyName",
      "modifierType",
      "propertyValueDict"
    ]
  }
