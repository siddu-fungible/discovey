<frame>
  <template xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="frame_schema.xsd"
    name="EthernetSSMFrame"
    showName="Ethernet SSM Frame"
    version="1.00"
    category="Layer2"
    readonly="true"
    validate="true"
    firstPdu="ethernet:EthernetII"
    description="Ethernet SSM Frame" >

    <pdus>
        <pdu name="proto1" pdu="ethernet:EthernetII">
	    <dstMac>01:80:C2:00:00:02</dstMac>
	</pdu>		    
	<pdu name="proto2" pdu="ethernetssm:EthernetSSM" />
	<pdu name="proto3" pdu="ethernetssm:QlTlv" />
    </pdus>
  </template>
</frame>
