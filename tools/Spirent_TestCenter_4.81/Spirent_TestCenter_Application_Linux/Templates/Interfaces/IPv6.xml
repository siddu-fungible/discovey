<frame>
  <template xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="frame_schema.xsd"
    name="Simple_IPv6"
    showName="IPv6"
    version="1.00"
    category="IP"
    readonly="true"
    validate="true"
    firstPdu="ipv6:IPv6"
    description="IPv6" >
    <modifiers>
      <modifier name="IPv6 Modifier">
        <mode>Increment</mode>
        <count>1</count>
        <data>2000::2</data>
        <datatype>Native</datatype> 
        <mask>FFFF:FFFF:FFFF:FFFF:FFFF:FFFF:FFFF:FFFF</mask>
        <step>::1</step>
        <offset>0</offset>
        <stutterCount>0</stutterCount>
        <forEachModifyList>Mod1,</forEachModifyList>
        <useStream>false</useStream>
      </modifier>
      <modifier name="Mod1">
        <mode>Increment</mode>
        <count>1</count>
        <data>2000::1</data>
        <datatype>Native</datatype>
        <mask>FFFF:FFFF:FFFF:FFFF:FFFF:FFFF:FFFF:FFFF</mask>
        <step>::0</step>
        <offset>0</offset>
        <stutterCount>0</stutterCount>
        <forEachModifyList />
        <useStream>false</useStream>
      </modifier>
    </modifiers>
    <pdus>
      <pdu name="proto1" pdu="ipv6:IPv6"> 
        <sourceAddr modifier="IPv6 Modifier" override="true">2000::2</sourceAddr>
        <gateway modifier="Mod1" override="true">::1</gateway>
      </pdu>
    </pdus>
  </template>
</frame>