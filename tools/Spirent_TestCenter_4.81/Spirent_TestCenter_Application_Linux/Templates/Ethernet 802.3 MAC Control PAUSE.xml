    xsi:noNamespaceSchemaLocation="frame_schema.xsd"
    name="Eth802_3MacControlPause"
    showName="Ethernet 802.3 MAC Control - PAUSE"
    version="1.00"
    category="MAC Control"
    readonly="true"
    validate="true"
    firstPdu="ethernetpause:MacControl"
    description="Ethernet 802.3 MAC Control - PAUSE">

    <pdus>
      <pdu name="proto1" pdu="ethernetpause:MacControl">
        <dstMac override="true">01:80:C2:00:00:01</dstMac>
        <lengthType override="true">8808</lengthType>
      </pdu>
      <pdu name="proto2" pdu="ethernetpause:PauseMacControl"/>
    </pdus>
  </template>
</frame>
