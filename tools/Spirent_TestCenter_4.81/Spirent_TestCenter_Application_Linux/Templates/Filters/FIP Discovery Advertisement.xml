<frame>
  <template xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="frame_schema.xsd"
    name="FIPDiscoveryAdvertisement"
    showName="FIP Discovery Advertisement"
    version="1.00"
    category="Switching"
    readonly="true"
    validate="true"
    firstPdu="fc:FIP"
    description="FIP Discovery Advertisement" >

    <pdus>
      <pdu name="proto1" pdu="fc:FIP" >
        <DL>
          <DL-Choice name="DL-Choice_0">
            <priority/>
          </DL-Choice>
          <DL-Choice name="DL-Choice_1">
            <macAddr/>
          </DL-Choice>
          <DL-Choice name="DL-Choice_2">
            <nameId/>
          </DL-Choice>
          <DL-Choice name="DL-Choice_3">
            <fabricName/>
          </DL-Choice>
		  <DL-Choice name="DL-Choice_4">
            <fkaAdvPeriod/>
          </DL-Choice>
        </DL>
      </pdu>
    </pdus>
  </template>
</frame>
