<frame>
  <template xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="frame_schema.xsd"
    name="Udp_DHCPNak"
    showName="UDP/DHCP Nak"
    version="1.00"
    category="IP"
    readonly="true"
    validate="true"
    firstPdu="udp:Udp"
    description="UDP/DHCP Nak" >

    <pdus>
      <pdu name="proto1" pdu="udp:Udp">
        <sourcePort>67</sourcePort>
      </pdu>

      <pdu name="dhcpNak" pdu="dhcp:Dhcpservermsg">
        <options>
          <DHCPOption name="DHCPOption1">
            <messageType>
              <code>6</code>
            </messageType>
          </DHCPOption>
          <DHCPOption name="DHCPOption2">
            <clientIdHW>
              <clientHWA override="true">00:00:01:00:00:01</clientHWA>
            </clientIdHW>
          </DHCPOption>
          <DHCPOption name="DHCPOption3">
            <serverId />
          </DHCPOption>
          <DHCPOption name="DHCPOption4">
            <message />
          </DHCPOption>
          <DHCPOption name="DHCPOption5">
            <endOfOptions />
          </DHCPOption>
        </options>
      </pdu>
    </pdus>
  </template>
</frame>
