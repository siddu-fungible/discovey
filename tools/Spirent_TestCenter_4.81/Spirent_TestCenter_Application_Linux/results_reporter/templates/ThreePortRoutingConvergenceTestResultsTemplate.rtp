      <name>ConvergenceVerdict</name>
      <persist>false</persist>
      <value type="String"></value>
      <script><![CDATA[set initialRate [expr $PrimaryEgressInitialRate+$SecondaryEgressInitialRate]&#xa;if {$initialRate >= [expr $SecondaryEgressFinalRate -0.005*$initialRate]&#xa;   && $initialRate <= [expr $SecondaryEgressFinalRate +0.005*$initialRate]&#xa;   && $PrimaryEgressFirstLossMsg == "" &#xa;   && $PrimaryEgressLastArrivalMsg == "" &#xa;   && $SecondaryEgressLastLossMsg == ""&#xa;   && $SecondaryEgressFirstArrivalMsg == ""&#xa; } then {&#xa;   set result "Yes"&#xa;} else {&#xa;  set result "No"&#xa;}&#xa;&#xa;]]></script>
      <multivalued>false</multivalued>
      <pageName>ALL</pageName>
    </instance>
