      <persist>false</persist>
      <value type="String"></value>
      <script><![CDATA[set initialRate [expr $PrimaryEgressInitialRate+$SecondaryEgressInitialRate]&#xa;set routeInaccuracy [expr $NumRoutes*1000/$initialRate]&#xa;if { $SamplingInterval > $routeInaccuracy }  {&#xa;  set InaccuracyInResult $SamplingInterval&#xa;} else {&#xa;  set InaccuracyInResult $routeInaccuracy&#xa;}&#xa;&#xa;if {$PrimaryEgressFirstLossMsg == "" &#xa;   && $SecondaryEgressFirstArrivalMsg == ""&#xa;} then {&#xa;   set timeStampDiff [expr $SecondaryEgressFirstArrivalTime - $PrimaryEgressFirstLossTime]&#xa;   if {$timeStampDiff < 0} {&#xa;       set firstRouteConv [expr $InaccuracyInResult + $timeStampDiff]&#xa;       if {$firstRouteConv<0} {&#xa;             set result "N/A"&#xa;         } else {&#xa;             set result "[format "%.0f" [expr $firstRouteConv/2]]+-[format "%.0f" [expr $firstRouteConv/2]]"&#xa;         }&#xa;   } else {&#xa;       set result "[format "%.0f" [expr $timeStampDiff + $InaccuracyInResult/2]]+-[format "%.0f" [expr $InaccuracyInResult/2]]"&#xa;   }&#xa;} else {&#xa;  set result "N/A"&#xa;}&#xa;]]></script>
