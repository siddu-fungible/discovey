      <persist>false</persist>
      <value type="String"></value>
      <script><![CDATA[set initialRate [expr $PrimaryEgressInitialRate+$SecondaryEgressInitialRate]&#xa;if {$initialRate >= [expr $SecondaryEgressFinalRate -0.005*$initialRate]&#xa;   && $initialRate <= [expr $SecondaryEgressFinalRate +0.005*$initialRate]&#xa;   && $PrimaryEgressFirstLossMsg == "" &#xa;   && $PrimaryEgressLastArrivalMsg == "" &#xa;   && $SecondaryEgressLastLossMsg == ""&#xa;   && $SecondaryEgressFirstArrivalMsg == ""&#xa; } then {&#xa;   set result "Full Convergence Recorded"&#xa;} else {&#xa;  if {$initialRate <= [expr $SecondaryEgressFinalRate -0.001*$initialRate]&#xa;   || $initialRate >= [expr $SecondaryEgressFinalRate +0.001*$initialRate]&#xa;  } then {&#xa;    set result  "Final Traffic Rate on Seconday Egress Port not equal to expected rate"&#xa;  } elseif {$PrimaryEgressFirstLossMsg != ""} {&#xa;     set result $PrimaryEgressFirstLossMsg&#xa;  } elseif {$PrimaryEgressLastArrivalTimeMsg != ""} {&#xa;     set result $PrimaryEgressLastArrivalTimeMsg&#xa;  } elseif {$SecondaryEgressLastLossMsg != ""} {&#xa;     set result $SecondaryEgressLastLossMsg&#xa;  } elseif {$SecondaryEgressFirstArrivalTimeMsg != ""} {&#xa;     set result $SecondaryEgressFirstArrivalTimeMsg&#xa;  } else {&#xa;     set result "N/A"&#xa;  }&#xa;}]]></script>
      <multivalued>false</multivalued>
      <pageName>ALL</pageName>
    </instance>
