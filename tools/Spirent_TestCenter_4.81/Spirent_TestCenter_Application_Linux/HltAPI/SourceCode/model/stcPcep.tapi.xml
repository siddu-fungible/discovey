		<tapi:retkey name="start_delegatecommand_hnd" object="StartDeviceDelegateCommandConfig"/>
		<tapi:retkey name="start_updatecommand_hnd" object="StartDeviceUpdateCommandConfig"/>
		<tapi:retkey name="customtlv_hnd" object="PcepCustomTlv"/>
		<tapi:retkey name="lsp_hnd" object="PcepLspObject"/>
		<tapi:retkey name="srp_hnd" object="PcepSrpObject"/>
		<tapi:retkey name="rp_hnd" object="PcepRPObject"/>
		<tapi:retkey name="customobj_hnd" object="PcepCustomObject"/>
