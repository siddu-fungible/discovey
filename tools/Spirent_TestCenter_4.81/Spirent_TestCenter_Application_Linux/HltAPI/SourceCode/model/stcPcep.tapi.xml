		<tapi:param name="$object$.PlspId" alias="plsp_id"/>
		<tapi:param name="$object$.TunnelId" alias="tunnel_id"/>
		<tapi:param name="$object$.TunnelIdStep" alias="tunnel_id_step"/>
		<tapi:param name="$object$.Ipv4ExTunnelIdStep" alias="ipv4_ex_tunnel_id_step"/>
