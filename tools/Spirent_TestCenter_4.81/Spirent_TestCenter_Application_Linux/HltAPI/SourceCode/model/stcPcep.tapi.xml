	<tapi:param name="PcepBaseActiveStatefulCommandConfig.IncludedMetric-targets" alias="included_metric_handle"/>
	<tapi:param name="PcepBaseActiveStatefulCommandConfig.IncludedLspa-targets" alias="included_lspa_handle"/>
	
	<tapi:param name="PcepBaseActiveStatefulCommandConfig.IncludeEndpoints" alias="include_endpoints"/>
	<tapi:param name="PcepBaseActiveStatefulCommandConfig.UseCustomMessage" alias="cmd_use_custom_message"/>
			
