		<tapi:param name="$object$.EroIflag" alias="ero_iflag"/>
	</tapi:map>
	<tapi:map object="PcepIpv4RroObject PcepIpv6RroObject PcepSRRroObject" prefix="ipv4 ipv6 sr">
		<tapi:param name="$object$.RroPflag" alias="rro_pflag"/>
		<tapi:param name="$object$.RroIflag" alias="rro_iflag"/>
	</tapi:map>

	<tapi:map object="PcepIpv4ExplicitRouteParams PcepIpv6ExplicitRouteParams PcepSRExplicitRouteParams" prefix="ipv4_explicit ipv6_explicit sr">
		<tapi:param name="$object$.RouteType" alias="route_type"/>
	</tapi:map>
		
	<tapi:map object="PcepIpv4ReportedRouteParams PcepIpv6ReportedRouteParams" prefix="ipv4_reported ipv6_reported">
