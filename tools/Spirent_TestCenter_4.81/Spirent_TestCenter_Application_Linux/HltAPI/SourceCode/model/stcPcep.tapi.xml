		<tapi:switch value="pcc" 
			object="PcepProtocolConfig PccLspConfig PcepLspObject PcepEndPointObject PcepRPObject PcepIpv4EroObject PcepIpv6EroObject PcepIpv4ExplicitRouteParams 
