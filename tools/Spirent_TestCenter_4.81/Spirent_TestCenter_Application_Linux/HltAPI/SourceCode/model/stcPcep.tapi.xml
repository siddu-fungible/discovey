	</tapi:map>
	
	<tapi:param name="PcepProtocolConfig.IpVersion" alias="ip_version">
		<tapi:process procName="::sth::pcep::process_Usesif"/>
	</tapi:param>
	
	<tapi:param name="PcepCustomTlv.Value" alias="tlv_value">
		<tapi:process procName="::sth::pcep::process_Customtlv_Value"/>
	</tapi:param>
	
	<tapi:param name="PcepProtocolConfig.PcepDeviceRole" alias="pcep_device_role" isParamKey="true">
		<tapi:process procName="::sth::pcep::process_key_role"/>
