<tapi:meta xmlns:tapi="urn:www.spirentcom.com:XMLSchema.xsd" id="0x95000000" package="routing" plugin="pcep" version="0.0.0-0">
 <tapi:class name="PcepActiveStatefulCommandConfig" disable="TRUE">
 </tapi:class>
 <tapi:class name="StartDeviceInitiateCommandConfig">
	<tapi:condition operator="equal" property="PceLspConfig.Characteristic" type="applicable" value="EnumPceCharacteristic.ENABLE_INIT" />
 </tapi:class>
 <tapi:class name="StartDeviceReplyCommandConfig">
	<tapi:condition operator="equal" property="PceLspConfig.Characteristic" type="applicable" value="EnumPceCharacteristic.ENABLE_REPLY" />
 </tapi:class>
 <tapi:class name="StartDeviceSyncCommandConfig">
	<tapi:condition operator="equal" property="PccLspConfig.Characteristic" type="applicable" value="EnumPccCharacteristic.ENABLE_SYNCHRONIZATION" />
 </tapi:class>
 <tapi:class name="StartDeviceRequestCommandConfig">
	<tapi:condition operator="equal" property="PccLspConfig.Characteristic" type="applicable" value="EnumPccCharacteristic.ENABLE_REQUEST" />
 </tapi:class>
 
 <tapi:interface APIName="::sth::emulation_pcep_config" from_object="PcepProtocolConfig">
	<tapi:retval name="return format b">
		<tapi:retkey name="pcep_handle" object="PcepProtocolConfig"/>
		<tapi:retkey name="pce_lsp_hnd" object="PceLspConfig"/>
		<tapi:retkey name="pcc_lsp_hnd" object="PccLspConfig"/>
		<tapi:retkey name="ero_hnd" object="PcepBaseEroObject"/>
		<tapi:retkey name="ipv4_ero_hnd" object="PcepIpv4EroObject"/>
		<tapi:retkey name="ipv4_ero_hnd" object="PcepIpv4EroToUpdateObject"/>
		<tapi:retkey name="ipv6_ero_hnd" object="PcepIpv6EroObject"/>
		<tapi:retkey name="ipv6_ero_hnd" object="PcepIpv6EroToUpdateObject"/>
		<tapi:retkey name="sr_ero_hnd" object="PcepSREroObject"/>
		<tapi:retkey name="rro_hnd" object="PcepBaseRroObject"/>
