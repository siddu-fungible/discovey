	<tapi:param name="PcepIpv6EroObject.EroType" disable="TRUE"/>
	<tapi:param name="PcepIpv6EroToUpdateObject.EroType" disable="TRUE"/>
	<tapi:param name="PcepBwObject.ObjectType" disable="TRUE"/>
	<tapi:param name="PcepBwToUpdateObject.ObjectType" disable="TRUE"/>
	
	<tapi:map object="PcepIpv4EroObject PcepIpv4EroToUpdateObject PcepIpv6EroObject PcepIpv6EroToUpdateObject PcepSREroObject" prefix="ipv4 ipv4_update ipv6 ipv6_update sr">
