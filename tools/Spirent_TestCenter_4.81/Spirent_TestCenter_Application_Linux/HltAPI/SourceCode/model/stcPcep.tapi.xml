		<tapi:param name="$object$.Ipv6TunnelEndAddr" alias="ipv6_tunnel_end_addr"/>
		<tapi:param name="$object$.Ipv6TunnelAddr" alias="ipv6_tunnel_addr"/>	
		<tapi:param name="$object$.Ipv6TunnelEndAddrStep" alias="ipv6_tunnel_end_addr_step"/>
