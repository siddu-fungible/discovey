    <TYPE_DEF name="Ospfv2LsTypes" type="INTEGER" bitLength="8" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="Router" value="1" />
        <VAL_ENUM uiShowName="Network" value="2" />
        <VAL_ENUM uiShowName="Summary" value="3" />
        <VAL_ENUM uiShowName="Summary-ASBR" value="4" />
        <VAL_ENUM uiShowName="AS-External Type-5" value="5" />
        <VAL_ENUM uiShowName="AS-External Type-7" value="7" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>
    
    <TYPE_DEF name="Ospfv2LsTypesWide" type="INTEGER" bitLength="32" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="Router" value="1" />
