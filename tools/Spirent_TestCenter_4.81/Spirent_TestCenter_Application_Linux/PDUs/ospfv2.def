          uiShowName="LSA Header Options" />
      <FIELD name="lsType" type="Ospfv2LsTypes"
          defaultValue="2"
          uiShowName="LS Type" />
      <FIELD name="linkStateId" type="IPV4ADDR"
          defaultValue="1.1.1.1" 
          uiShowName="Link State ID" />
      <FIELD name="advertisingRouter" type="IPV4ADDR"
          defaultValue="1.1.1.1" 
          uiShowName="Advertising Router" />
      <FIELD name="lsSequenceNumber" type="OCTETSTRING" byteLength="4"
