<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="pdu_schema.xsd" name="ospfv2" showName="OSPFv2" version="1.00"
    description="This is a sample PDU library for OSPFv2 as defined in XML Stream Editor.">

  <TYPE_DEFS>

    <TYPE_DEF name="Ospfv2Unknown" type="SEQUENCE" unitType="MESSAGE"
        level="4" uiShowName="OSPFv2 Unknown" >
      <FIELD name="header" type="Ospfv2Header" uiShowName="Header"/>
    </TYPE_DEF>

    <TYPE_DEF name="Ospfv2Hello" type="SEQUENCE" unitType="MESSAGE"
        level="4" uiShowName="OSPFv2 Hello" >
      <FIELD name="header" type="Ospfv2Header" uiNoShowModifier="Interface,Frame" uiShowName="Packet Header"/>
      <FIELD name="networkMask" type="IPV4ADDR"
          defaultValue="255.255.255.0" 
          uiShowName="Network Mask" />
      <FIELD name="helloInterval" type="INTEGER" bitLength="16"
          defaultValue="10"  
          uiShowName="Hello Interval" />
      <FIELD name="helloOptions" type="Ospfv2Options"
          uiShowName="Packet Options" />
      <FIELD name="routerPriority" type="INTEGER" bitLength="8"
          defaultValue="0"  
          uiShowName="Router Priority" />
      <FIELD name="routerDeadInterval" type="INTEGER" bitLength="32"
          defaultValue="40" 
          uiShowName="Router Dead Interval" />
      <FIELD name="designatedRouter" type="IPV4ADDR"
          defaultValue="1.1.1.1" 
          uiShowName="Designated Router" />
      <FIELD name="backupDesignatedRouter" type="IPV4ADDR"
          defaultValue="2.2.2.2" 
          uiShowName="Backup Designated Router" />
      <FIELD name="neighbors" type="Ospfv2NeighborList" uiShowName="Neighbors" />
    </TYPE_DEF>

    <TYPE_DEF name="Ospfv2DatabaseDescription" type="SEQUENCE" unitType="MESSAGE"
          level="4" uiShowName="OSPFv2 Database Description" >
      <FIELD name="header" type="Ospfv2Header" uiShowName="Packet Header"/>
      <FIELD name="interfaceMtu" type="INTEGER" bitLength="16"
          defaultValue="4096"  
          uiShowName="Interface MTU" />
      <FIELD name="ddOptions" type="Ospfv2Options"
          uiNoShowModifier="Interface,Frame" uiShowName="Packet Options" />
      <FIELD name="ddSpecificOptions" type="Ospfv2DDOptions"
          uiNoShowModifier="Interface,Frame" uiShowName="DD Options" />
      <FIELD name="sequenceNumber" type="INTEGER" bitLength="32"
          defaultValue="0" 
          uiShowName="DD Sequence Number" />
      <FIELD name="lsaHeaders" type="Ospfv2LsaHeaderList" uiShowName="LSA Header List" />
    </TYPE_DEF>

    <TYPE_DEF name="Ospfv2LinkStateRequest" type="SEQUENCE" unitType="MESSAGE"
          level="4" uiShowName="OSPFv2 Link State Request" >
      <FIELD name="header" type="Ospfv2Header" uiShowName="Packet Header"/>
      <FIELD name="requestedLsas" type="Ospfv2RequestedLsaList"  uiShowName="Requested LSAs" />
    </TYPE_DEF>

    <TYPE_DEF name="Ospfv2LinkStateUpdate" type="SEQUENCE" unitType="MESSAGE"
          level="4" uiShowName="OSPFv2 Link State Update" >
      <FIELD name="header" type="Ospfv2Header" uiShowName="Packet Header"/>
      <FIELD name="numberOfLsas" type="INTEGER" bitLength="32"
          defaultValue="0"  
          uiShowName="Number of LSAs" />
      <FIELD name="updatedLsas" type="Ospfv2UpdatedLsaList"  uiShowName="Updated LSAs" />
    </TYPE_DEF>

    <TYPE_DEF name="Ospfv2LinkStateAcknowledge" type="SEQUENCE" unitType="MESSAGE"
          level="4" uiShowName="OSPFv2 Link State Acknowledge" >
      <FIELD name="header" type="Ospfv2Header" uiShowName="Packet Header"/>
      <FIELD name="lsaHeaders" type="Ospfv2LsaHeaderList"  uiShowName="Acknowledged List" />
    </TYPE_DEF>

    <TYPE_DEF name="Ospfv2Header" type="SEQUENCE" unitType="ELEMENT" uiShowName="OSPFv2 Header" >
      <FIELD name="version" type="INTEGER" bitLength="8"
          defaultValue="2"
          uiControlState="CALCULATED"  
          uiShowName="Version" />
      <FIELD name="type" type="Ospfv2PacketTypes"
          defaultValue="0"
          uiControlState="CALCULATED" 
          uiShowName="Packet Type" />
      <FIELD name="packetLength" type="INTEGER" bitLength="16"
          defaultValue="0"
          uiControlState="CALCULATED"  
          procfnTarget="TOTALLENGTH"
          uiShowName="Packet Length" />
      <FIELD name="routerID" type="IPV4ADDR"
          defaultValue="1.1.1.1" 
          uiShowName="Router ID" />
      <FIELD name="areaID" type="IPV4ADDR"
          defaultValue="0.0.0.0" 
          uiShowName="Area ID" />
      <FIELD name="checksum" type="OCTETSTRING" byteLength="2"
          defaultValue="0000" 
          uiControlState="ALLOW_OVERRIDE"  
          procfnTarget="OSPFCHECKSUM"          
          uiNoShowModifier="Interface,Frame,Filter"
          uiShowName="Checksum" />
      <FIELD name="authSelect" type="AuthSelect"
          defaultValue="hdrAuthSelectNone" 
          uiShowName="Authentication" />
    </TYPE_DEF>
    
    <TYPE_DEF name="Ospfv2PacketTypes" type="INTEGER" bitLength="8" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="Unknown" value="0" />
        <VAL_ENUM uiShowName="Hello" value="1" />
        <VAL_ENUM uiShowName="Database Description" value="2" />
        <VAL_ENUM uiShowName="Link State Request" value="3" />
        <VAL_ENUM uiShowName="Link State Update" value="4" />
        <VAL_ENUM uiShowName="Link State Acknowledgement" value="5" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>

    <TYPE_DEF name="AuthSelect" type="CHOICE" unitType="ELEMENT" uiShowName="Authentication" >
      <FIELD name="hdrAuthSelectNone" type="HdrAuthSelectNone"   uiShowName="No Authentication" />
      <FIELD name="hdrAuthSelectPassword"  type="HdrAuthSelectPassword"   uiShowName="Simple Password" />
      <FIELD name="hdrAuthSelectCrypto"    type="HdrAuthSelectCrypto"    uiShowName="MD5" />
      <FIELD name="hdrAuthSelectUserDef"   type="HdrAuthSelectUserDef"   uiShowName="User Defined" />
    </TYPE_DEF>

    <TYPE_DEF name="HdrAuthSelectNone" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="No Authentication" >
      <FIELD name="authType" type="INTEGER" bitLength="16"
          defaultValue="0"
          uiControlState="CALCULATED"  
          uiShowName="Authentication Type" />
      <FIELD name="authValue1" type="INTEGER" bitLength="32"
          defaultValue="0"
          uiControlState="CALCULATED"  
          uiShowName="Authentication Value1" />
      <FIELD name="authValue2" type="INTEGER" bitLength="32"
          defaultValue="0"
          uiControlState="CALCULATED"  
          uiShowName="Authentication Value2" />
    </TYPE_DEF>

    <TYPE_DEF name="HdrAuthSelectPassword" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="Simple Password" >
      <FIELD name="authType" type="INTEGER" bitLength="16"
          defaultValue="1"
          uiControlState="CALCULATED"  
          uiShowName="Authentication Type" />
      <FIELD name="authValue1" type="INTEGER" bitLength="32"
          defaultValue="0" 
          uiShowName="Authentication Value1" />
      <FIELD name="authValue2" type="INTEGER" bitLength="32"
          defaultValue="0" 
          uiShowName="Authentication Value2" />
    </TYPE_DEF>

    <TYPE_DEF name="HdrAuthSelectCrypto" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="MD5" >
      <FIELD name="authType" type="INTEGER" bitLength="16"
          defaultValue="2"
          uiControlState="CALCULATED"  
          uiShowName="Authentication Type" />
      <FIELD name="authValue1" type="INTEGER" bitLength="32"
          defaultValue="0" 
          uiShowName="Authentication Value1" />
      <FIELD name="authValue2" type="INTEGER" bitLength="32"
          defaultValue="0" 
          uiShowName="Authentication Value2" />
    </TYPE_DEF>

    <TYPE_DEF name="HdrAuthSelectUserDef" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="User Defined" >
      <FIELD name="authType" type="INTEGER" bitLength="16"
          defaultValue="3" 
          uiShowName="Authentication Type" />
      <FIELD name="authValue1" type="INTEGER" bitLength="32"
          defaultValue="0" 
          uiShowName="Authentication Value1" />
      <FIELD name="authValue2" type="INTEGER" bitLength="32"
          defaultValue="0" 
          uiShowName="Authentication Value2" />
    </TYPE_DEF>
        
    <TYPE_DEF name="Ospfv2Options" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="Options" >
      <FIELD name="reserved7" type="BITSTRING" bitLength="1"
          defaultValue="0" 
          uiNoShowModifier="Interface,Frame"
          uiShowName="Reserved Bit 7" />
      <FIELD name="reserved6" type="BITSTRING" bitLength="1"
          defaultValue="0" 
          uiNoShowModifier="Interface,Frame"
          uiShowName="Reserved Bit 6" />
      <FIELD name="dcBit" type="BITSTRING" bitLength="1"
          defaultValue="0" 
          uiNoShowModifier="Interface,Frame"
          uiShowName="DC Bit" />
      <FIELD name="eaBit" type="BITSTRING" bitLength="1"
          defaultValue="0" 
          uiNoShowModifier="Interface,Frame"
          uiShowName="EA Bit" />
      <FIELD name="npBit" type="BITSTRING" bitLength="1"
          defaultValue="0" 
          uiNoShowModifier="Interface,Frame"
          uiShowName="NP Bit" />
      <FIELD name="mcBit" type="BITSTRING" bitLength="1"
          defaultValue="0" 
          uiNoShowModifier="Interface,Frame"
          uiShowName="MC Bit" />
      <FIELD name="eBit" type="BITSTRING" bitLength="1"
          defaultValue="0" 
          uiNoShowModifier="Interface,Frame"
          uiShowName="E Bit" />
      <FIELD name="reserved0" type="BITSTRING" bitLength="1"
          defaultValue="0" 
          uiNoShowModifier="Interface,Frame"
          uiShowName="Reserved Bit 0" />
    </TYPE_DEF> 

    <TYPE_DEF name="Ospfv2NeighborList" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1024" elementType="Ospfv2Neighbor" unitType="ELEMENT" />

    <TYPE_DEF name="Ospfv2Neighbor" type="SEQUENCE" unitType="ELEMENT" uiShowName="Neighbor" >
      <FIELD name="neighborID" type="IPV4ADDR"
          defaultValue="1.1.1.1" 
          uiShowName="Neighbor ID" />
    </TYPE_DEF>

    <TYPE_DEF name="Ospfv2DDOptions" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="DD Options" >
      <FIELD name="reserved7" type="BITSTRING" bitLength="1"
          defaultValue="0" 
          uiNoShowModifier="Interface,Frame" uiShowName="Reserved Bit 7" />
      <FIELD name="reserved6" type="BITSTRING" bitLength="1"
          defaultValue="0" 
          uiNoShowModifier="Interface,Frame" uiShowName="Reserved Bit 6" />
      <FIELD name="reserved5" type="BITSTRING" bitLength="1"
          defaultValue="0" 
          uiNoShowModifier="Interface,Frame" uiShowName="Reserved Bit 5" />
      <FIELD name="reserved4" type="BITSTRING" bitLength="1"
          defaultValue="0" 
          uiNoShowModifier="Interface,Frame" uiShowName="Reserved Bit 4" />
      <FIELD name="reserved3" type="BITSTRING" bitLength="1"
          defaultValue="0" 
          uiNoShowModifier="Interface,Frame" uiShowName="Reserved Bit 3" />
      <FIELD name="iBit" type="BITSTRING" bitLength="1"
          defaultValue="0" 
          uiNoShowModifier="Interface,Frame" uiShowName="I Bit" />
      <FIELD name="mBit" type="BITSTRING" bitLength="1"
          defaultValue="0" 
          uiNoShowModifier="Interface,Frame" uiShowName="M Bit" />
      <FIELD name="msBit" type="BITSTRING" bitLength="1"
          defaultValue="0" 
          uiNoShowModifier="Interface,Frame" uiShowName="MS Bit" />
    </TYPE_DEF> 

    <TYPE_DEF name="Ospfv2LsaHeaderList" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1024" elementType="Ospfv2LsaHeader" unitType="ELEMENT" />

    <TYPE_DEF name="Ospfv2LsaHeader" type="SEQUENCE" unitType="ELEMENT" uiShowName="LSA Header" >
      <FIELD name="lsaAge" type="INTEGER" bitLength="16"
          defaultValue="0"  
          uiShowName="LSA Age" />
      <FIELD name="lsaHdrOptions" type="Ospfv2Options"
          uiShowName="LSA Header Options" />
      <FIELD name="lsType" type="Ospfv2LsTypes"
          defaultValue="1"
          uiShowName="LS Type" />
      <FIELD name="linkStateId" type="IPV4ADDR"
          defaultValue="1.1.1.1" 
          uiShowName="Link State ID" />
      <FIELD name="advertisingRouter" type="IPV4ADDR"
          defaultValue="1.1.1.1" 
          uiShowName="Advertising Router" />
      <FIELD name="lsSequenceNumber" type="OCTETSTRING" byteLength="4"
          defaultValue="80000001" 
          uiShowName="LS Sequence Number" />
      <FIELD name="lsChecksum" type="OCTETSTRING" byteLength="2"
          defaultValue="0000" 
          uiControlState="CALCULATED"  
          procfnTarget="LSACHECKSUM"
          uiNoShowModifier="Interface,Frame,Filter" 
          uiShowName="LS Checksum" />
      <FIELD name="lsaLength" type="INTEGER" bitLength="16"
          defaultValue="0"
          uiControlState="CALCULATED" 
          procfnTarget="HDRLENGTH"
          uiShowName="LSA Length" />
    </TYPE_DEF>


