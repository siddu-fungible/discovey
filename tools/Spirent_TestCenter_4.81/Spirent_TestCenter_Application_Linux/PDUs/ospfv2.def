      <FIELD name="ospfv2RouterLsa" type="Ospfv2RouterLsa" uiShowName="OSPFv2 Router LSA" />
      <FIELD name="ospfv2NetworkLsa" type="Ospfv2NetworkLsa" uiShowName="OSPFv2 Network LSA" />
