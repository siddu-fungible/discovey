<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="pdu_schema.xsd"
  name="EOAMTLV"
  showName="Ethernet OAM CFM TLV"
  version="2.01"
  description="This is a sample PDU library for Ethernet OAM as defined in XML Stream Editor.">

<TYPE_DEFS>

  <!-- Sender ID TLV ===================================================== -->
  <TYPE_DEF name="SndrID" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet"  uiShowName="Sender ID TLV">

    <FIELD name="Type" type="OCTETSTRING" byteLength="1"
			defaultValue="01" uiNoShow="Interface"
			uiShowCommon="false" uiShowName="Type" />

    <FIELD name="Length" type="OCTETSTRING" byteLength="2" procfnTarget="EOAMTLVLENGTH"
			defaultValue="0000" uiShowName="Length" uiControlState="CALCULATED" />

    <FIELD name="ChassisIDLen" type="OCTETSTRING" byteLength="1" procfnTarget="EOAMCHASSISIDLENGTH"
			defaultValue="00" uiShowName="Chassis ID Length"
      uiControlState="CALCULATED" />

    <FIELD name="theChassisID" type="serviceoam:ChassisIDSelect" uiShowName="Chassis ID"/>

    <FIELD name="theManagementAddressDomain" type="serviceoam:ManagementAddressDomainSelect" uiShowName="Management Address Domain"/>

    <NEXT_HEADERS>
      <NXT_HDR id="EOAMTLV:SndrID" name="Sender ID TLV" />
      <NXT_HDR id="EOAMTLV:OrgSpec" name="Organization-Specific TLV" />
      <NXT_HDR id="EOAMTLV:PrtStat" name="Port Status TLV" />
      <NXT_HDR id="EOAMTLV:IntStat" name="Interface Status TLV" />
      <NXT_HDR id="EOAMTLV:EndTLV" name="End TLV" />
      <NXT_HDR id="EOAMTLV:DataTLV" name="Data TLV" />
      <NXT_HDR id="EOAMTLV:TestTLV" name="Test TLV" />
      <NXT_HDR id="EOAMTLV:LTMEgrID" name="LTM Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:LTREgrID" name="LTR Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:RplyIng" name="Reply Ingress TLV" />
      <NXT_HDR id="EOAMTLV:RplyEgr" name="Reply Egress TLV" />
	  <NXT_HDR id="EOAMTLV:TestIdTLV" name="Test Id TLV" />
    </NEXT_HEADERS>
  </TYPE_DEF>

  <!-- Org Specific TLV ================================================== -->
  <TYPE_DEF name="OrgSpec" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="Organization-Specific TLV">

    <FIELD name="Type" type="OCTETSTRING" byteLength="1"
			uiNoShow="Interface" uiShowCommon="false"
			defaultValue="1F" uiShowName="Type"/>

    <FIELD name="Length" type="OCTETSTRING" byteLength="2" procfnTarget="EOAMTLVLENGTH"
			defaultValue="0000" uiShowName="Length" uiControlState="CALCULATED" />

    <FIELD name="OUI" type="OCTETSTRING" byteLength="3"
			defaultValue="000000" uiShowName="OUI" />

    <FIELD name="SubType" type="OCTETSTRING" byteLength="1"
			defaultValue="00" uiShowName="Sub-Type" />

    <FIELD name="theValue" type="serviceoam:ValueSelect" uiShowName="Value"/>

    <NEXT_HEADERS>
      <NXT_HDR id="EOAMTLV:SndrID" name="Sender ID TLV" />
      <NXT_HDR id="EOAMTLV:OrgSpec" name="Organization-Specific TLV" />
      <NXT_HDR id="EOAMTLV:PrtStat" name="Port Status TLV" />
      <NXT_HDR id="EOAMTLV:IntStat" name="Interface Status TLV" />
      <NXT_HDR id="EOAMTLV:EndTLV" name="End TLV" />
      <NXT_HDR id="EOAMTLV:DataTLV" name="Data TLV" />
      <NXT_HDR id="EOAMTLV:TestTLV" name="Test TLV" />
      <NXT_HDR id="EOAMTLV:LTMEgrID" name="LTM Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:LTREgrID" name="LTR Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:RplyIng" name="Reply Ingress TLV" />
      <NXT_HDR id="EOAMTLV:RplyEgr" name="Reply Egress TLV" />
	  <NXT_HDR id="EOAMTLV:TestIdTLV" name="Test Id TLV" />
    </NEXT_HEADERS>
  </TYPE_DEF>

  <!-- Port Status TLV =================================================== -->
  <TYPE_DEF name="PrtStat" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="Port Status TLV">

    <FIELD name="Type" type="OCTETSTRING" byteLength="1"
			defaultValue="02" uiNoShow="Interface"
			uiShowCommon="false" uiShowName="Type"/>

    <FIELD name="Length" type="OCTETSTRING" byteLength="2" procfnTarget="EOAMTLVLENGTH"
			defaultValue="0000" uiShowName="Length" uiControlState="CALCULATED" />

    <FIELD name="PortStatusValues" type="serviceoam:psTLVvalues"
		       	defaultValue="02" uiShowName="Port Status" />

    <NEXT_HEADERS>
      <NXT_HDR id="EOAMTLV:SndrID" name="Sender ID TLV" />
      <NXT_HDR id="EOAMTLV:OrgSpec" name="Organization-Specific TLV" />
      <NXT_HDR id="EOAMTLV:PrtStat" name="Port Status TLV" />
      <NXT_HDR id="EOAMTLV:IntStat" name="Interface Status TLV" />
      <NXT_HDR id="EOAMTLV:EndTLV" name="End TLV" />
      <NXT_HDR id="EOAMTLV:DataTLV" name="Data TLV" />
      <NXT_HDR id="EOAMTLV:TestTLV" name="Test TLV" />
      <NXT_HDR id="EOAMTLV:LTMEgrID" name="LTM Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:LTREgrID" name="LTR Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:RplyIng" name="Reply Ingress TLV" />
      <NXT_HDR id="EOAMTLV:RplyEgr" name="Reply Egress TLV" />
	  <NXT_HDR id="EOAMTLV:TestIdTLV" name="Test Id TLV" />
    </NEXT_HEADERS>
  </TYPE_DEF>

  <!-- Interface Status ================================================== -->
  <TYPE_DEF name="IntStat" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="Interface Status TLV">

    <FIELD name="Type" type="OCTETSTRING" byteLength="1" uiNoShow="Interface"
			uiShowCommon="false"
			defaultValue="04" uiShowName="Type"/>

    <FIELD name="Length" type="OCTETSTRING" byteLength="2" procfnTarget="EOAMTLVLENGTH"
			defaultValue="0000" uiShowName="Length" uiControlState="CALCULATED" />

    <FIELD name="IntStatusValues" type="serviceoam:intTLVvalues"
		       	defaultValue="01" uiShowName="Interface Status" />

    <NEXT_HEADERS>
      <NXT_HDR id="EOAMTLV:SndrID" name="Sender ID TLV" />
      <NXT_HDR id="EOAMTLV:OrgSpec" name="Organization-Specific TLV" />
      <NXT_HDR id="EOAMTLV:PrtStat" name="Port Status TLV" />
      <NXT_HDR id="EOAMTLV:IntStat" name="Interface Status TLV" />
      <NXT_HDR id="EOAMTLV:EndTLV" name="End TLV" />
      <NXT_HDR id="EOAMTLV:DataTLV" name="Data TLV" />
      <NXT_HDR id="EOAMTLV:TestTLV" name="Test TLV" />
      <NXT_HDR id="EOAMTLV:LTMEgrID" name="LTM Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:LTREgrID" name="LTR Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:RplyIng" name="Reply Ingress TLV" />
      <NXT_HDR id="EOAMTLV:RplyEgr" name="Reply Egress TLV" />
	  <NXT_HDR id="EOAMTLV:TestIdTLV" name="Test Id TLV" />
    </NEXT_HEADERS>
  </TYPE_DEF>

  <!-- Data TLV ========================================================== -->
  <TYPE_DEF name="DataTLV" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="Data TLV">

    <FIELD name="Type" type="OCTETSTRING" byteLength="1"
			uiNoShow="Interface" uiShowCommon="false"
			defaultValue="03" uiShowName="Type"/>

    <FIELD name="Length" type="OCTETSTRING" byteLength="2" procfnTarget="EOAMTLVLENGTH"
			defaultValue="0000" uiShowName="Length" uiControlState="CALCULATED" />

    <FIELD name="theData" type="serviceoam:DataSelect" uiShowName="Data"/>

    <NEXT_HEADERS>
      <NXT_HDR id="EOAMTLV:SndrID" name="Sender ID TLV" />
      <NXT_HDR id="EOAMTLV:OrgSpec" name="Organization-Specific TLV" />
      <NXT_HDR id="EOAMTLV:PrtStat" name="Port Status TLV" />
      <NXT_HDR id="EOAMTLV:IntStat" name="Interface Status TLV" />
      <NXT_HDR id="EOAMTLV:EndTLV" name="End TLV" />
      <NXT_HDR id="EOAMTLV:DataTLV" name="Data TLV" />
      <NXT_HDR id="EOAMTLV:TestTLV" name="Test TLV" />
      <NXT_HDR id="EOAMTLV:LTMEgrID" name="LTM Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:LTREgrID" name="LTR Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:RplyIng" name="Reply Ingress TLV" />
      <NXT_HDR id="EOAMTLV:RplyEgr" name="Reply Egress TLV" />
	  <NXT_HDR id="EOAMTLV:TestIdTLV" name="Test Id TLV" />
    </NEXT_HEADERS>
  </TYPE_DEF>

  <!-- Test TLV ========================================================== -->
  <TYPE_DEF name="TestTLV" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="Test TLV">

    <FIELD name="Type" type="OCTETSTRING" byteLength="1"
			uiNoShow="Interface" uiShowCommon="false"
			defaultValue="20" uiShowName="Type"/>

    <FIELD name="Length" type="OCTETSTRING" byteLength="2" 
			defaultValue="0000" uiShowName="Length" />

    <FIELD name="PatternType" type="serviceoam:PatternTypeValues" byteLength="1"
      uiShowName="Pattern Type"  defaultValue="00" />

    <NEXT_HEADERS>
      <NXT_HDR id="EOAMTLV:SndrID" name="Sender ID TLV" />
      <NXT_HDR id="EOAMTLV:OrgSpec" name="Organization-Specific TLV" />
      <NXT_HDR id="EOAMTLV:PrtStat" name="Port Status TLV" />
      <NXT_HDR id="EOAMTLV:IntStat" name="Interface Status TLV" />
      <NXT_HDR id="EOAMTLV:EndTLV" name="End TLV" />
      <NXT_HDR id="EOAMTLV:DataTLV" name="Data TLV" />
      <NXT_HDR id="EOAMTLV:TestTLV" name="Test TLV" />
      <NXT_HDR id="EOAMTLV:LTMEgrID" name="LTM Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:LTREgrID" name="LTR Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:RplyIng" name="Reply Ingress TLV" />
      <NXT_HDR id="EOAMTLV:RplyEgr" name="Reply Egress TLV" />
	  <NXT_HDR id="EOAMTLV:TestIdTLV" name="Test Id TLV" />
    </NEXT_HEADERS>
  </TYPE_DEF>
  
  <!-- TestId TLV ========================================================== -->
  <TYPE_DEF name="TestIdTLV" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="TestId TLV">
    <FIELD name="Type" type="OCTETSTRING" byteLength="1"
			uiNoShow="Interface" uiShowCommon="false"
			defaultValue="24" uiShowName="Type"/>

    <FIELD name="Length" type="OCTETSTRING" byteLength="2" 
			defaultValue="0004" uiShowName="Length" uiControlState="CALCULATED" />

    <FIELD name="TestId" type="OCTETSTRING" byteLength="4"
            uiShowName="Test Id"  defaultValue="00000000" />

    <NEXT_HEADERS>
      <NXT_HDR id="EOAMTLV:SndrID" name="Sender ID TLV" />
      <NXT_HDR id="EOAMTLV:OrgSpec" name="Organization-Specific TLV" />
      <NXT_HDR id="EOAMTLV:PrtStat" name="Port Status TLV" />
      <NXT_HDR id="EOAMTLV:IntStat" name="Interface Status TLV" />
      <NXT_HDR id="EOAMTLV:EndTLV" name="End TLV" />
      <NXT_HDR id="EOAMTLV:DataTLV" name="Data TLV" />
      <NXT_HDR id="EOAMTLV:TestTLV" name="Test TLV" />
      <NXT_HDR id="EOAMTLV:LTMEgrID" name="LTM Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:LTREgrID" name="LTR Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:RplyIng" name="Reply Ingress TLV" />
      <NXT_HDR id="EOAMTLV:RplyEgr" name="Reply Egress TLV" />
	  <NXT_HDR id="EOAMTLV:TestIdTLV" name="Test Id TLV" />
    </NEXT_HEADERS>
  </TYPE_DEF>

  <!-- End TLV =========================================================== -->
  <TYPE_DEF name="EndTLV" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="End TLV">

    <FIELD name="Type" type="OCTETSTRING" byteLength="1"
		       	defaultValue="00" uiShowName="Type" uiShowCommon="false" />
  </TYPE_DEF>

  <!-- LTM Egress ID TLV ================================================= -->
  <TYPE_DEF name="EgressId" type="SEQUENCE" unitType="ELEMENT" uiShowName="Egress Identifier">
    <FIELD name="Index" type="OCTETSTRING" byteLength="2"
       defaultValue="0000" uiShowName="Index" />

    <FIELD name="MacAddr" type="MACADDR"
       defaultValue="00:00:00:00:00:00" uiShowName="MAC Address" uiControlState="ALLOW_OVERRIDE" />
  </TYPE_DEF>

  <TYPE_DEF name="EgressIdAuto" type="SEQUENCE" unitType="ELEMENT" uiShowName="Egress Identifier">
    <FIELD name="Index" type="OCTETSTRING" byteLength="2"
       defaultValue="0000" uiShowName="Index" uiControlState="CALCULATED" />

    <FIELD name="MacAddr" type="MACADDR"
       defaultValue="00:00:00:00:00:00" uiShowName="MAC Address" uiControlState="CALCULATED" />
  </TYPE_DEF>
  
  <TYPE_DEF name="LTMEgrID" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="LTM Egress Identifier TLV">

    <FIELD name="Type" type="OCTETSTRING" byteLength="1"
			defaultValue="07" uiNoShow="Interface"
			uiShowCommon="false" uiShowName="Type"/>

    <FIELD name="Length" type="OCTETSTRING" byteLength="2" procfnTarget="EOAMTLVLENGTH"
			defaultValue="0000" uiShowName="Length" uiControlState="CALCULATED" />

    <FIELD name="EgressIdent" type="EgressIdAuto" uiShowName="Egress Identifier" uiControlState="CALCULATED" />

    <NEXT_HEADERS>
      <NXT_HDR id="EOAMTLV:SndrID" name="Sender ID TLV" />
      <NXT_HDR id="EOAMTLV:OrgSpec" name="Organization-Specific TLV" />
      <NXT_HDR id="EOAMTLV:PrtStat" name="Port Status TLV" />
      <NXT_HDR id="EOAMTLV:IntStat" name="Interface Status TLV" />
      <NXT_HDR id="EOAMTLV:EndTLV" name="End TLV" />
      <NXT_HDR id="EOAMTLV:DataTLV" name="Data TLV" />
      <NXT_HDR id="EOAMTLV:TestTLV" name="Test TLV" />
      <NXT_HDR id="EOAMTLV:LTMEgrID" name="LTM Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:LTREgrID" name="LTR Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:RplyIng" name="Reply Ingress TLV" />
      <NXT_HDR id="EOAMTLV:RplyEgr" name="Reply Egress TLV" />
	  <NXT_HDR id="EOAMTLV:TestIdTLV" name="Test Id TLV" />
    </NEXT_HEADERS>
  </TYPE_DEF>

  <!-- LTR Egress ID TLV ================================================= -->
  <TYPE_DEF name="LTREgrID" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="LTR Egress Identifier TLV">

    <FIELD name="Type" type="OCTETSTRING" byteLength="1"
			defaultValue="08" uiNoShow="Interface"
			uiShowCommon="false" uiShowName="Type"/>

    <FIELD name="Length" type="OCTETSTRING" byteLength="2" procfnTarget="EOAMTLVLENGTH"
			defaultValue="0000" uiShowName="Length" uiControlState="CALCULATED" />

    <FIELD name="LastEgressIdent" type="EgressId" uiShowName="Last Egress Identifier"/>

    <FIELD name="NextEgressIdent" type="EgressId" uiShowName="Next Egress Identifier"/>

    <NEXT_HEADERS>
      <NXT_HDR id="EOAMTLV:SndrID" name="Sender ID TLV" />
      <NXT_HDR id="EOAMTLV:OrgSpec" name="Organization-Specific TLV" />
      <NXT_HDR id="EOAMTLV:PrtStat" name="Port Status TLV" />
      <NXT_HDR id="EOAMTLV:IntStat" name="Interface Status TLV" />
      <NXT_HDR id="EOAMTLV:EndTLV" name="End TLV" />
      <NXT_HDR id="EOAMTLV:DataTLV" name="Data TLV" />
      <NXT_HDR id="EOAMTLV:TestTLV" name="Test TLV" />
      <NXT_HDR id="EOAMTLV:LTMEgrID" name="LTM Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:LTREgrID" name="LTR Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:RplyIng" name="Reply Ingress TLV" />
      <NXT_HDR id="EOAMTLV:RplyEgr" name="Reply Egress TLV" />
	  <NXT_HDR id="EOAMTLV:TestIdTLV" name="Test Id TLV" />
    </NEXT_HEADERS>
  </TYPE_DEF>

  <!-- Reply Ingress TLV ================================================= -->
  <TYPE_DEF name="RplyIng" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="Reply Ingress TLV">

    <FIELD name="Type" type="OCTETSTRING" byteLength="1"
			defaultValue="05" uiNoShow="Interface"
			uiShowCommon="false" uiShowName="Type"/>

    <FIELD name="Length" type="OCTETSTRING" byteLength="2" procfnTarget="EOAMTLVLENGTH"
			defaultValue="0000" uiShowName="Length" uiControlState="CALCULATED" />

    <FIELD name="IngressAction" type="serviceoam:IngressActionFieldValues"
			defaultValue="01" uiShowName="Ingress Action" uiControlState="ALLOW_OVERRIDE" />

    <FIELD name="IngressMac" type="MACADDR" defaultValue="00:00:00:00:00:00"
			byteLength="6" uiShowName="Ingress MAC Address" uiControlState="ALLOW_OVERRIDE" />

    <FIELD name="theIPID" type="serviceoam:IPIDSelect" uiShowName="Ingress Port ID"/>

    <NEXT_HEADERS>
      <NXT_HDR id="EOAMTLV:SndrID" name="Sender ID TLV" />
      <NXT_HDR id="EOAMTLV:OrgSpec" name="Organization-Specific TLV" />
      <NXT_HDR id="EOAMTLV:PrtStat" name="Port Status TLV" />
      <NXT_HDR id="EOAMTLV:IntStat" name="Interface Status TLV" />
      <NXT_HDR id="EOAMTLV:EndTLV" name="End TLV" />
      <NXT_HDR id="EOAMTLV:DataTLV" name="Data TLV" />
      <NXT_HDR id="EOAMTLV:TestTLV" name="Test TLV" />
      <NXT_HDR id="EOAMTLV:LTMEgrID" name="LTM Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:LTREgrID" name="LTR Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:RplyIng" name="Reply Ingress TLV" />
      <NXT_HDR id="EOAMTLV:RplyEgr" name="Reply Egress TLV" />
	  <NXT_HDR id="EOAMTLV:TestIdTLV" name="Test Id TLV" />
    </NEXT_HEADERS>
  </TYPE_DEF>

  <!-- Reply Egress TLV ================================================== -->
  <TYPE_DEF name="RplyEgr" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="Reply Egress TLV">

    <FIELD name="Type" type="OCTETSTRING" byteLength="1"
			defaultValue="06" uiNoShow="Interface"
			uiShowCommon="false" uiShowName="Type"/>

    <FIELD name="Length" type="OCTETSTRING" byteLength="2" procfnTarget="EOAMTLVLENGTH"
			defaultValue="0000" uiShowName="Length" uiControlState="CALCULATED" />

    <FIELD name="EgressAction" type="serviceoam:EgressActionFieldValues"
			defaultValue="01" uiShowName="Egress Action" uiControlState="ALLOW_OVERRIDE" />

    <FIELD name="EgressMac" type="MACADDR" defaultValue="00:00:00:00:00:00"
			byteLength="6" uiShowName="Egress MAC Address" uiControlState="ALLOW_OVERRIDE" />

    <FIELD name="theEPID" type="serviceoam:EPIDSelect" uiShowName="Egress Port ID"/>

    <NEXT_HEADERS>
      <NXT_HDR id="EOAMTLV:SndrID" name="Sender ID TLV" />
      <NXT_HDR id="EOAMTLV:OrgSpec" name="Organization-Specific TLV" />
      <NXT_HDR id="EOAMTLV:PrtStat" name="Port Status TLV" />
      <NXT_HDR id="EOAMTLV:IntStat" name="Interface Status TLV" />
      <NXT_HDR id="EOAMTLV:EndTLV" name="End TLV" />
      <NXT_HDR id="EOAMTLV:DataTLV" name="Data TLV" />
      <NXT_HDR id="EOAMTLV:TestTLV" name="Test TLV" />
      <NXT_HDR id="EOAMTLV:LTMEgrID" name="LTM Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:LTREgrID" name="LTR Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:RplyIng" name="Reply Ingress TLV" />
      <NXT_HDR id="EOAMTLV:RplyEgr" name="Reply Egress TLV" />
	  <NXT_HDR id="EOAMTLV:TestIdTLV" name="Test Id TLV" />
    </NEXT_HEADERS>
  </TYPE_DEF>

  <!-- PBB-TE MIP TLV ================================================== -->
  <TYPE_DEF name="PbbTeMip" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="PBB-TE MIP TLV">

    <FIELD name="Type" type="OCTETSTRING" byteLength="1"
			defaultValue="09" uiNoShow="Interface"
			uiShowCommon="false" uiShowName="Type"/>

    <FIELD name="Length" type="OCTETSTRING" byteLength="2"
			defaultValue="000E" uiShowName="Length" uiControlState="CALCULATED" />

    <FIELD name="MipMac" type="MACADDR" defaultValue="00:00:00:00:00:00"
			byteLength="6" uiShowName="MIP MAC Address" />

    <FIELD name="ReverseVid" type="INTEGER" defaultValue="0" bitLength="16"
      uiShowName="Reverse VID" />

    <!-- TODO: 2.50 - Add uiControlState="ALLOW_OVERRIDE" -->
    <FIELD name="ReverseMac" type="MACADDR" defaultValue="00:00:00:00:00:00"
			byteLength="6" uiShowName="Reverse MAC Address" />

    <NEXT_HEADERS>
      <NXT_HDR id="EOAMTLV:SndrID" name="Sender ID TLV" />
      <NXT_HDR id="EOAMTLV:OrgSpec" name="Organization-Specific TLV" />
      <NXT_HDR id="EOAMTLV:PrtStat" name="Port Status TLV" />
      <NXT_HDR id="EOAMTLV:IntStat" name="Interface Status TLV" />
      <NXT_HDR id="EOAMTLV:EndTLV" name="End TLV" />
      <NXT_HDR id="EOAMTLV:DataTLV" name="Data TLV" />
      <NXT_HDR id="EOAMTLV:TestTLV" name="Test TLV" />
      <NXT_HDR id="EOAMTLV:LTMEgrID" name="LTM Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:LTREgrID" name="LTR Egress Identifier TLV" />
      <NXT_HDR id="EOAMTLV:RplyIng" name="Reply Ingress TLV" />
      <NXT_HDR id="EOAMTLV:RplyEgr" name="Reply Egress TLV" />
	  <NXT_HDR id="EOAMTLV:TestIdTLV" name="Test Id TLV" />
    </NEXT_HEADERS>
  </TYPE_DEF>

</TYPE_DEFS>
</PDU_LIBRARY>
