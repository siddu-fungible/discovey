<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="pdu_schema.xsd"
  name="hdlc"
  showName="CiscoHDLC"
  version="1.00"
  description="This is a sample PDU library for Ethernet as defined in XML Stream Editor.">
    
  <TYPE_DEFS>
    <TYPE_DEF name="CiscoHDLC" type="SEQUENCE" unitType="HEADER"
        level="2" portType="Pos" uiShowName="Cisco HDLC" niMapped="true">

      <FIELD name="address" type="OCTETSTRING"  byteLength="1"
        defaultValue="0F" 
        uiNoShow="Interface"
        uiNoShowModifier="Interface"
        uiShowName="Address" />
				
      <FIELD name="control" type="OCTETSTRING"  byteLength="1"
        defaultValue="00" 
        uiNoShow="Interface"
		    uiNoShowModifier="Interface"
        uiShowName="Control" />
				
      <FIELD name="protocolType" type="types:EtherTypes"
          defaultValue="0800"
          uiControlState="ALLOW_OVERRIDE" 
          procfnTarget="NEXTHDR" 
		      uiNoShowModifier="Interface"
          uiShowName="Protocol" />

      <NEXT_HEADERS>
        <NXT_HDR id="ipv4:IPv4" name="IPv4 Header" value="0800" />
        <NXT_HDR id="ip:IP" name="IP Headers" value="0800" />
        <NXT_HDR id="ipv6:IPv6" name="IPv6 Header" value="86DD" />
        <NXT_HDR id="ip:IPv6LL" name="IPv6 Header" value="86DD" />
        <NXT_HDR id="mpls:Mpls" name="MPLS Header" value="8847" />
        <NXT_HDR id="custom:Custom" name="Custom" value="0" />
		<NXT_HDR id="customrepeat:CustomRepeat" name="CustomRepeat" value="00" />
      </NEXT_HEADERS>
			
    </TYPE_DEF> 
  </TYPE_DEFS>
</PDU_LIBRARY>
