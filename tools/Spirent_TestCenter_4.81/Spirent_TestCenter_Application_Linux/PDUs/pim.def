       <FIELD name="overrideIntervalValue" type="INTEGER" bitLength="16"
           defaultValue="0"
           uiShowName="Override Interval" />

	  <NEXT_HEADERS>
		<NXT_HDR id="pim:PimHelloHoldTime" name="PIM Hello Hold Time"></NXT_HDR>
		<NXT_HDR id="pim:PimHelloDRPriority" name="PIM Hello DR Priority"></NXT_HDR>
		<NXT_HDR id="pim:PimHelloGenerationID" name="PIM Hello Generation ID"></NXT_HDR>
		<NXT_HDR id="pim:Pimv4HelloAddressList" name="PIMv4 Hello Address List"></NXT_HDR>
