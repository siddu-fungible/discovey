          defaultValue="1"
          uiShowName="Option Type" />
      <FIELD name="length" type="INTEGER" bitLength="16"
          defaultValue="2"
          uiShowName="Option Length" />
      <FIELD name="value" type="INTEGER" bitLength="16"
          defaultValue="105"
          uiShowName="Holdtime" />
		  
	  <NEXT_HEADERS>
		<NXT_HDR id="pim:PimHelloLanPruneDelay" name="Lan Prune Delay"></NXT_HDR>
