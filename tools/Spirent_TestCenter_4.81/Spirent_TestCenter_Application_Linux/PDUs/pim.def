      <FIELD name="encodingType" type="INTEGER" bitLength="8"
          defaultValue="0"
          uiShowName="Encoded Group Address Encoding Type" />
          
      <FIELD name="address" type="IPV4ADDR"
          defaultValue="192.0.0.1"
          uiShowName="Unicast IP Address" />

    </TYPE_DEF>

