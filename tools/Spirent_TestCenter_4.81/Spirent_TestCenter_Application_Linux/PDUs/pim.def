      <FIELD name="maskLen" type="INTEGER" bitLength="8"
          defaultValue="24"
          uiShowName="Mask Length" />
          
      <FIELD name="address" type="IPV6ADDR"
          defaultValue="FF1E:0000:0000:0000:0000:0000:0000:0001"
