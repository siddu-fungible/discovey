<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="pdu_schema.xsd" name="pim" showName="PIM" version="1.00"
    description="This is a sample PDU library for PIM as defined in XML Stream Editor.">
 
  <TYPE_DEFS>
 
    <TYPE_DEF name="PimMessageType" type="INTEGER" bitLength="4" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="Hello" value="0" />
        <VAL_ENUM uiShowName="Register" value="1" />
        <VAL_ENUM uiShowName="Register-Stop" value="2" />
        <VAL_ENUM uiShowName="Join/Prune" value="3" />
        <VAL_ENUM uiShowName="Bootstrap" value="4" />
        <VAL_ENUM uiShowName="Assert" value="5" />
        <VAL_ENUM uiShowName="Graft" value="6" />
        <VAL_ENUM uiShowName="Graft-Ack" value="7" />
        <VAL_ENUM uiShowName="Candidate-RP-Advertisement" value="8" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>
    

    <TYPE_DEF name="Pimv4Header" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="PIMv4 Header" >
			
      <FIELD name="version" type="INTEGER" bitLength="4"
          defaultValue="2"
          uiShowName="Version" />
          
      <FIELD name="type" type="PimMessageType"
          defaultValue="0"
          uiShowName="Message Type" />
          
      <FIELD name="reserved" type="INTEGER" bitLength="8"
          defaultValue="0"
          uiShowName="Reserved" />
          
      <FIELD name="checksum" type="INTEGER" bitLength="16"
          uiControlState="ALLOW_OVERRIDE" 
          defaultValue="0"
          procfnTarget="PIMCHECKSUM"
          uiNoShowModifier="Interface,Filter"
          uiShowName="Checksum" />

    </TYPE_DEF>


    <TYPE_DEF name="Pimv6Header" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="PIMv6 Header" >
			
      <FIELD name="version" type="INTEGER" bitLength="4"
          defaultValue="2"
          uiShowName="Version" />
          
      <FIELD name="type" type="PimMessageType"
          defaultValue="0"
          uiShowName="Message Type" />
          
      <FIELD name="reserved" type="INTEGER" bitLength="8"
          defaultValue="0"
          uiShowName="Reserved" />
          
      <FIELD name="checksum" type="INTEGER" bitLength="16"
          defaultValue="0"
          uiControlState="ALLOW_OVERRIDE" 
          procfnTarget="PIMPSEUDOCHECKSUM"
          uiNoShowModifier="Interface,Filter"
          uiShowName="Checksum" />

    </TYPE_DEF>


