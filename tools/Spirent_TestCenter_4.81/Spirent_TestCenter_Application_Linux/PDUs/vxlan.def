<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="pdu_schema.xsd" 
    name="vxlan" 
    showName="VxLAN" 
    version="1.00"
    description="This is a sample PDU library for VxLAN as defined in XML Stream Editor.">
    
  <TYPE_DEFS>
    <TYPE_DEF name="Vtep" type="SEQUENCE" unitType="ELEMENT" uiShowName="VTEP IP" >
      <FIELD name="ip" type="IPV4ADDR" defaultValue="1.1.1.1" uiShowName="IP" doNotBuild="true"/>
    </TYPE_DEF>    
    <TYPE_DEF name="VtepIpList" type="SEQUENCE_OF" minOccurs="0" maxOccurs="100" elementType="Vtep" unitType="ELEMENT"/>
    <TYPE_DEF name="VxLAN" type="SEQUENCE" unitType="HEADER" level="4" uiShowName="VxLAN Header" niMapped="true" >      

	  <FIELD name="flags" type="INTEGER" bitLength="8"
		defaultValue="8" 
		uiNoShow="Interface,Filter"
		uiNoShowModifier="Interface"
		uiShowCommon="false"
		uiShowName="Flags" />

      <FIELD name="reservedone" type="INTEGER" bitLength="24"
        defaultValue="0"
		uiNoShow="Interface,Filter"
        uiNoShowModifier="Interface"
	    uiShowCommon="false"  
        uiShowName="Reserved 1" />

      <FIELD name="vni" type="INTEGER" bitLength="24"
        defaultValue="0"
        uiShowName="Virtual Network Identifier (VNI)"
        isNiMapped="true"
        isNiPrimaryKey="true" />

      <FIELD name="reservedtwo" type="INTEGER" bitLength="8"
        defaultValue="0"
		uiNoShow="Interface,Filter"
		uiNoShowModifier="Interface"
		uiShowCommon="false"  
        uiShowName="Reserved 2" />

      <FIELD name="multicastGroupAddr" type="IPV4ADDR"
        defaultValue="225.0.0.1"
        doNotBuild="true"
        uiShowName="IPv4 Multicast Group Address"
        uiNoShow="Interface,Filter"  
        uiShowCommon="false"
        isNiMapped="true" 
        isNiPrimaryKey="true"/>
      
      <FIELD name="autoSelectUdpSrcPort" type="INTEGER" bitLength="8"
        defaultValue="1"
        doNotBuild="true"
        uiShowName="Auto Select UDP Source Port"
        uiNoShow="Interface,Filter"  
        uiShowCommon="false"
        isNiMapped="true" 
        isNiPrimaryKey="true"/>

      <FIELD name="evpnLearningEnabled" type="INTEGER" bitLength="8"
        defaultValue="0"
        doNotBuild="true"
        uiShowName="Evpn Learning Enabled"
        uiNoShow="Interface,Filter"  
        uiShowCommon="false"       
        isNiMapped="true"
        isNiPrimaryKey="true"/>

      <FIELD name="vxlanIpAddressMode" type="INTEGER" bitLength="8"
        defaultValue="0"
        doNotBuild="true"
        uiShowName="Vxlan IP Address Mode"
        uiNoShow="Interface,Filter"  
        uiShowCommon="false"       
        isNiMapped="true"
        isNiPrimaryKey="true"/>

       <FIELD name="srcVtepinterfaceAddr" type="IPV4ADDR"
        defaultValue="0.0.0.0"
        doNotBuild="true"
        uiShowName="Source VTEP IPv4 address"
        uiNoShow="Interface,Filter"  
        uiShowCommon="false"
        isNiMapped="true" 
        isNiPrimaryKey="true"/> 

        <FIELD name="srcVtepRouterId" type="IPV4ADDR"
        defaultValue="0.0.0.0"
        doNotBuild="true"
        uiShowName="Source VTEP Router Id"
        uiNoShow="Interface,Filter"  
        uiShowCommon="false"
        isNiMapped="true" 
        isNiPrimaryKey="true"/>        
        
      <FIELD name="resolvedIpv4Addr" type="IPV4ADDR"
        defaultValue="0.0.0.0"
        doNotBuild="true"
        uiShowName="Peer VTEP IPv4 address"
        uiNoShow="Interface,Filter"  
        uiShowCommon="false"
        isNiMapped="true" 
        isNiPrimaryKey="true"/>

      <FIELD name="enableVmArpSendingForEvpnLearning" type="INTEGER" bitLength="8"
        defaultValue="0"
        doNotBuild="true"
        uiShowName="Enable Vm Arp For Evpn Learning"
        uiNoShow="Interface,Filter"
        uiShowCommon="false"
        isNiMapped="true"
        isNiPrimaryKey="true"/>

      <FIELD name="useTargetIpForVmArp" type="INTEGER" bitLength="8"
        defaultValue="0"
        doNotBuild="true"
        uiShowName="Use Target IP For VM ARP"
        uiNoShow="Interface,Filter"
        uiShowCommon="false"
        isNiMapped="true"
        isNiPrimaryKey="true"/>
      
      <FIELD name="floodVtepList" type="VtepIpList"
        doNotBuild="true"
        uiShowName="Flood List"
        uiNoShow="Interface,Filter"
        uiShowCommon="false"
        isNiMapped="true"
        isNiPrimaryKey="true"/>
      
      <FIELD name="trafficControlFlags" type="INTEGER" bitLength="32"
        defaultValue="0"
        doNotBuild="true"
        uiShowName="Traffic Control Flags"
        uiNoShow="Interface,Filter"
        uiShowCommon="false"
        isNiMapped="true"
        isNiPrimaryKey="true"/>

      <NEXT_HEADERS>
           <NXT_HDR id="ethernet:EthernetII" name="Ethernet II Header" />
      </NEXT_HEADERS>
 
    </TYPE_DEF>
  </TYPE_DEFS>
</PDU_LIBRARY>