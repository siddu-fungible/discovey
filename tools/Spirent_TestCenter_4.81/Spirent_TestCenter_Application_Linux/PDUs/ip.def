<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="pdu_schema.xsd" name="ip" showName="IP Combined Header" version="1.00"
    description="This is a sample PDU library for IPv4 and IPv6 combined header as defined in XML Stream Editor, for use in Interfaces.">
    
  <TYPE_DEFS>
        
    <TYPE_DEF name="ToSPrecedence" type="INTEGER" bitLength="3" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="Network Control" value="7" />
        <VAL_ENUM uiShowName="Internetwork Control" value="6" />
        <VAL_ENUM uiShowName="CRITIC/ECP" value="5" />
        <VAL_ENUM uiShowName="Flash Override" value="4" />
        <VAL_ENUM uiShowName="Flash" value="3" />
        <VAL_ENUM uiShowName="Immediate" value="2" />
        <VAL_ENUM uiShowName="Priority" value="1" />
        <VAL_ENUM uiShowName="Routine" value="0" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>
        
    <TYPE_DEF name="ToSByte" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="ToS Byte" >
      <FIELD name="precedence" type="ToSPrecedence"
			defaultValue="0"
			uiNoShow="Filter,Frame,Interface"
			uiShowName="Precedence" uiOverrideEditor="ToSEditor" />   
      <FIELD name="dBit" type="BITSTRING" bitLength="1"
			defaultValue="0"
			uiNoShow="Filter,Frame,Interface"
			uiShowName="D Bit" />
      <FIELD name="tBit" type="BITSTRING" bitLength="1"
			defaultValue="0"
			uiNoShow="Filter,Frame,Interface"
			uiShowName="T Bit" />
      <FIELD name="rBit" type="BITSTRING" bitLength="1"
			defaultValue="0"
			uiNoShow="Filter,Frame,Interface"
			uiShowName="R Bit" />
      <FIELD name="mBit" type="BITSTRING" bitLength="1"
     			defaultValue="0"
     			uiNoShow="Filter,Frame,Interface"
     			uiShowName="M Bit" />
      <FIELD name="reserved" type="BITSTRING" bitLength="1"
			defaultValue="0"
			uiNoShow="Filter,Frame,Interface"
			uiShowName="Reserved" />
    </TYPE_DEF> 
        
    <TYPE_DEF name="DiffServByte" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="DiffServ Byte" >
      <FIELD name="dscpHigh" type="INTEGER" bitLength="3"
			defaultValue="0"
			uiNoShow="Filter,Frame,Interface"
			uiShowName="DSCP High" />
      <FIELD name="dscpLow" type="INTEGER" bitLength="3"
			defaultValue="0"
			uiNoShow="Filter,Frame,Interface"
			uiShowName="DSCP Low" />
      <FIELD name="reserved" type="BITSTRING" bitLength="2"
			defaultValue="00"
			uiNoShow="Filter,Frame,Interface"
			uiShowName="Reserved" />
    </TYPE_DEF>
        
    <TYPE_DEF name="ToSDiffServ" type="CHOICE"
			unitType="ELEMENT" uiShowName="ToS DiffServ Byte" >
      <FIELD name="tos" type="ToSByte" uiNoShow="Filter,Frame,Interface" uiShowName="ToS Byte" />
      <FIELD name="diffServ" type="DiffServByte" uiNoShow="Filter,Frame,Interface" uiShowName="DiffServ Byte" />
    </TYPE_DEF> 
        
      
	<TYPE_DEF name="IP" type="SEQUENCE" unitType="HEADER"
			level="3" uiShowName="IP Header" > 

		<FIELD name="tosDiffserv" type="ToSDiffServ"
			defaultValue="tos"
			doNotBuild="true"
			uiNoShow="Filter,Frame"
			uiOverrideEditor="ToSDiffServEditor"
			uiNoShowModifier="Interface"
			uiShowName="IPv4 ToS/DiffServ" />  

		<FIELD name="ttl" type="INTEGER" bitLength="8"
			defaultValue="255"
			doNotBuild="true"
			uiNoShow="Filter,Frame"
			uiNoShowModifier="Interface"
			uiShowName="IPv4 Time to live" /> 

		<FIELD name="sourceAddr" type="IPV4ADDR"
			defaultValue="192.85.1.2"
			uiControlState="ALLOW_OVERRIDE"  
			doNotBuild="true"
			uiNoShow="Filter,Frame"
			uiShowName="IPv4 Source" /> 
		
		<FIELD name="prefixLength" type="INTEGER" bitLength="8"
			defaultValue="24" minValue="1" maxValue="32" 
			doNotBuild="true"
			uiNoShow="Filter,Frame"
			uiNoShowModifier="Interface"
			uiShowName="IPv4 Prefix Length"/>

		<FIELD name="gateway" type="IPV4ADDR"
			defaultValue="192.85.1.1"
			doNotBuild="true"
			uiNoShow="Filter,Frame"
			uiControlState="ALLOW_OVERRIDE"  
			uiShowName="IPv4 Gateway" />

	    <FIELD name="v6trafficClass" type="INTEGER" bitLength="8"
			defaultValue="0"
      doNotBuild="true"
      uiShowFormat="OCTETSTRING"
			uiNoShow="Filter,Frame"
			uiNoShowModifier="Interface"
      uiOverrideEditor="ToSDiffServEditor"
			uiShowName="IPv6 Traffic Class" />

		<FIELD name="v6hopLimit" type="INTEGER" bitLength="8"
			defaultValue="255" 
			doNotBuild="true"
			uiNoShow="Filter,Frame"
			uiNoShowModifier="Interface"
			uiShowName="IPv6 Hop limit" />

		<FIELD name="v6sourceAddr" type="IPV6ADDR"
			defaultValue="2000::2"
			uiControlState="ALLOW_OVERRIDE" 
			doNotBuild="true"
			uiNoShow="Filter,Frame" 
			uiShowName="IPv6 Source Address" />

		<FIELD name="v6prefixLength" type="INTEGER" bitLength="8"
			defaultValue="64" minValue="1" maxValue="128" 
			doNotBuild="true"
			uiShowName="IPv6 Prefix Length"
			uiNoShowModifier="Interface"
			uiNoShow="Filter,Frame" /> 

		<FIELD name="v6gateway" type="IPV6ADDR"
			defaultValue="2000::1"
			doNotBuild="true"
			uiNoShow="Filter,Frame" 
			uiControlState="ALLOW_OVERRIDE" 
			uiShowName="IPv6 Gateway" />

		 <FIELD name="v6lltrafficClass" type="INTEGER" bitLength="8"
			defaultValue="0"
      doNotBuild="true"
      uiShowFormat="OCTETSTRING"
			uiNoShow="Filter,Frame"
			uiNoShowModifier="Interface"            
			uiShowName="IPv6 Link Local Traffic Class" />

		<FIELD name="v6llhopLimit" type="INTEGER" bitLength="8"
			defaultValue="255" 
			doNotBuild="true"
			uiNoShow="Filter,Frame"
			uiNoShowModifier="Interface"
			uiShowName="IPv6 Link Local Hop limit" />

		<FIELD name="v6llsourceAddr" type="IPV6ADDR"
			uiControlState="ALLOW_OVERRIDE"
			defaultValue="FE80::1" 
			doNotBuild="true"
			uiNoShow="Filter,Frame" 
			uiShowName="IPv6 Link Local Source Address" />

		<FIELD name="v6llprefixLength" type="INTEGER" bitLength="8"
			defaultValue="128" minValue="1" maxValue="128" 
			doNotBuild="true"
			uiShowName="IPv6 Link Local Prefix Length"
			uiNoShowModifier="Interface"
			uiNoShow="Filter,Frame" /> 
				
		<NEXT_HEADERS>
			<NXT_HDR id="ipv4:IPv4" name="IPv4 Header" value="4" />
			<NXT_HDR id="ipv6:IPv6" name="IPv6 Header" value="41" />
			<NXT_HDR id="icmp:IcmpDestUnreach" name="ICMP Destination Unreachable" value="1" />
			<NXT_HDR id="icmp:IcmpTimeExceeded" name="ICMP Time Exceeded" value="1" />
			<NXT_HDR id="icmp:IcmpParameterProblem" name="ICMP Parameter Problem" value="1" />
			<NXT_HDR id="icmp:IcmpSourceQuench" name="ICMP Source Quench" value="1" />
			<NXT_HDR id="icmp:IcmpRedirect" name="ICMP Redirect" value="1" />
			<NXT_HDR id="icmp:IcmpEchoRequest" name="ICMP Echo Request" value="1" />
			<NXT_HDR id="icmp:IcmpEchoReply" name="ICMP Echo Reply" value="1" />
			<NXT_HDR id="icmp:IcmpTimestampRequest" name="ICMP Timestamp Request" value="1" />
			<NXT_HDR id="icmp:IcmpTimestampReply" name="ICMP Timestamp Reply" value="1" />
			<NXT_HDR id="icmp:IcmpInfoRequest" name="ICMP Info Request" value="1" />
			<NXT_HDR id="icmp:IcmpInfoReply" name="ICMP Info Reply" value="1" />
			<NXT_HDR id="icmp:IcmpMaskRequest" name="ICMP Address Mask Request" value="1" />
			<NXT_HDR id="icmp:IcmpMaskReply" name="ICMP Address Mask Reply" value="1" />
			<NXT_HDR id="icmp:IcmpRouterSolicitation" name="ICMP Router Solicitation" value="1" />
			<NXT_HDR id="icmp:IcmpRouterAdvertisement" name="ICMP Router Advertisement" value="1" />
			<NXT_HDR id="igmp:Igmpv1" name="IGMPv1" value="2"/>
			<NXT_HDR id="igmp:Igmpv2" name="IGMPv2" value="2"/>
			<NXT_HDR id="igmp:Igmpv2Query" name="IGMPv2 Query" value="2"/>
			<NXT_HDR id="igmp:Igmpv2Report" name="IGMPv2 Report" value="2"/>
			<NXT_HDR id="igmp:Igmpv3Query" name="IGMPv3 Query" value="2"/>
			<NXT_HDR id="igmp:Igmpv3Report" name="IGMPv3 Report" value="2"/> 
			<NXT_HDR id="tcp:Tcp" name="Tcp Header" value="6" />
			<NXT_HDR id="udp:Udp" name="Udp Header" value="17" />
                        <NXT_HDR id="gre:Gre" name="GRE" value="47" />
			<NXT_HDR id="ospfv2:Ospfv2Unknown" name="OSPFv2 Unknown" value="89" />
			<NXT_HDR id="ospfv2:Ospfv2Hello" name="OSPFv2 Hello" value="89" />
			<NXT_HDR id="ospfv2:Ospfv2DatabaseDescription" name="OSPFv2 Database Description" value="89" />
			<NXT_HDR id="ospfv2:Ospfv2LinkStateRequest" name="OSPFv2 Link State Request" value="89" />
			<NXT_HDR id="ospfv2:Ospfv2LinkStateUpdate" name="OSPFv2 Link State Update" value="89" />
			<NXT_HDR id="ospfv2:Ospfv2LinkStateAcknowledge" name="OSPFv2 Link State Acknowledge" value="89" />
			<NXT_HDR id="pim:Pimv4Hello" name="Pim Hello" value="103" />
			<NXT_HDR id="pim:Pimv4Register" name="Pim Register" value="103" />
			<NXT_HDR id="pim:Pimv4RegisterStop" name="Pim RegisterStop" value="103" />
			<NXT_HDR id="pim:Pimv4JoinPrune" name="Pim Join/Prune" value="103" />
			<NXT_HDR id="pim:Pimv4Assert" name="Pim Assert" value="103" />

			<NXT_HDR id="custom:Custom" name="Custom" value="0" />
			<NXT_HDR id="customrepeat:CustomRepeat" name="CustomRepeat" value="00" />
		</NEXT_HEADERS>
			
			
    </TYPE_DEF> 

		<TYPE_DEF name="IPv6LL" type="SEQUENCE" unitType="HEADER"
			level="3" uiShowName="IPv6 Header" >   

	    <FIELD name="v6trafficClass" type="INTEGER" bitLength="8"
			defaultValue="0"
      doNotBuild="true"
      uiShowFormat="OCTETSTRING"
			uiNoShow="Filter,Frame"
			uiNoShowModifier="Interface"
      uiOverrideEditor="ToSDiffServEditor"
			uiShowName="IPv6 Traffic Class" />

		<FIELD name="v6hopLimit" type="INTEGER" bitLength="8"
			defaultValue="255" 
			doNotBuild="true"
			uiNoShow="Filter,Frame"
			uiNoShowModifier="Interface"
			uiShowName="IPv6 Hop limit" />

		<FIELD name="v6sourceAddr" type="IPV6ADDR"
			defaultValue="2000::2"
			uiControlState="ALLOW_OVERRIDE" 
			doNotBuild="true"
			uiNoShow="Filter,Frame"
			uiShowName="IPv6 Source Address" />

		<FIELD name="v6prefixLength" type="INTEGER" bitLength="8"
			defaultValue="64" minValue="1" maxValue="128" 
			doNotBuild="true"
			uiShowName="IPv6 Prefix Length"
			uiNoShowModifier="Interface"
			uiNoShow="Filter,Frame" /> 

		<FIELD name="v6gateway" type="IPV6ADDR"
			defaultValue="2000::1"
			doNotBuild="true"
			uiNoShow="Filter,Frame" 
			uiControlState="ALLOW_OVERRIDE" 
			uiShowName="IPv6 Gateway" />

		 <FIELD name="v6lltrafficClass" type="INTEGER" bitLength="8"
			defaultValue="0"
			doNotBuild="true"
      uiShowFormat="OCTETSTRING"
      uiNoShow="Filter,Frame"
			uiNoShowModifier="Interface"
			uiShowName="IPv6 Link Local Traffic Class" />

		<FIELD name="v6llhopLimit" type="INTEGER" bitLength="8"
			defaultValue="255" 
			doNotBuild="true"
			uiNoShow="Filter,Frame"
			uiNoShowModifier="Interface"
			uiShowName="IPv6 Link Local Hop limit" />

		<FIELD name="v6llsourceAddr" type="IPV6ADDR"
			uiControlState="ALLOW_OVERRIDE"
			defaultValue="FE80::2" 
			doNotBuild="true"
			uiNoShow="Filter,Frame" 
			uiShowName="IPv6 Link Local Source Address" />

		<FIELD name="v6llprefixLength" type="INTEGER" bitLength="8"
			defaultValue="128" minValue="1" maxValue="128" 
			doNotBuild="true"
			uiShowName="IPv6 Link Local Prefix Length"
			uiNoShowModifier="Interface"
			uiNoShow="Filter,Frame" /> 
				
		<NEXT_HEADERS>
			<NXT_HDR id="ipv4:IPv4" name="IPv4 Header" value="4" />
			<NXT_HDR id="ipv6:IPv6" name="IPv6 Header" value="41" />
			<NXT_HDR id="icmp:IcmpDestUnreach" name="ICMP Destination Unreachable" value="1" />
			<NXT_HDR id="icmp:IcmpTimeExceeded" name="ICMP Time Exceeded" value="1" />
			<NXT_HDR id="icmp:IcmpParameterProblem" name="ICMP Parameter Problem" value="1" />
			<NXT_HDR id="icmp:IcmpSourceQuench" name="ICMP Source Quench" value="1" />
			<NXT_HDR id="icmp:IcmpRedirect" name="ICMP Redirect" value="1" />
			<NXT_HDR id="icmp:IcmpEchoRequest" name="ICMP Echo Request" value="1" />
			<NXT_HDR id="icmp:IcmpEchoReply" name="ICMP Echo Reply" value="1" />
			<NXT_HDR id="icmp:IcmpTimestampRequest" name="ICMP Timestamp Request" value="1" />
			<NXT_HDR id="icmp:IcmpTimestampReply" name="ICMP Timestamp Reply" value="1" />
			<NXT_HDR id="icmp:IcmpInfoRequest" name="ICMP Info Request" value="1" />
			<NXT_HDR id="icmp:IcmpInfoReply" name="ICMP Info Reply" value="1" />
			<NXT_HDR id="icmp:IcmpMaskRequest" name="ICMP Address Mask Request" value="1" />
			<NXT_HDR id="icmp:IcmpMaskReply" name="ICMP Address Mask Reply" value="1" />
			<NXT_HDR id="icmp:IcmpRouterSolicitation" name="ICMP Router Solicitation" value="1" />
			<NXT_HDR id="icmp:IcmpRouterAdvertisement" name="ICMP Router Advertisement" value="1" />
			<NXT_HDR id="igmp:Igmpv1" name="IGMPv1" value="2"/>
			<NXT_HDR id="igmp:Igmpv2" name="IGMPv2" value="2"/>
			<NXT_HDR id="igmp:Igmpv2Query" name="IGMPv2 Query" value="2"/>
			<NXT_HDR id="igmp:Igmpv2Report" name="IGMPv2 Report" value="2"/>
			<NXT_HDR id="igmp:Igmpv3Query" name="IGMPv3 Query" value="2"/>
			<NXT_HDR id="igmp:Igmpv3Report" name="IGMPv3 Report" value="2"/> 
			<NXT_HDR id="tcp:Tcp" name="Tcp Header" value="6" />
			<NXT_HDR id="udp:Udp" name="Udp Header" value="17" />
                        <NXT_HDR id="gre:Gre" name="GRE" value="47" />
			<NXT_HDR id="ospfv2:Ospfv2Unknown" name="OSPFv2 Unknown" value="89" />
			<NXT_HDR id="ospfv2:Ospfv2Hello" name="OSPFv2 Hello" value="89" />
			<NXT_HDR id="ospfv2:Ospfv2DatabaseDescription" name="OSPFv2 Database Description" value="89" />
			<NXT_HDR id="ospfv2:Ospfv2LinkStateRequest" name="OSPFv2 Link State Request" value="89" />
			<NXT_HDR id="ospfv2:Ospfv2LinkStateUpdate" name="OSPFv2 Link State Update" value="89" />
			<NXT_HDR id="ospfv2:Ospfv2LinkStateAcknowledge" name="OSPFv2 Link State Acknowledge" value="89" />
			<NXT_HDR id="pim:Pimv4Hello" name="Pim Hello" value="103" />
			<NXT_HDR id="pim:Pimv4Register" name="Pim Register" value="103" />
			<NXT_HDR id="pim:Pimv4RegisterStop" name="Pim RegisterStop" value="103" />
			<NXT_HDR id="pim:Pimv4JoinPrune" name="Pim Join/Prune" value="103" />
			<NXT_HDR id="pim:Pimv4Assert" name="Pim Assert" value="103" />

			<NXT_HDR id="custom:Custom" name="Custom" value="0" />
			<NXT_HDR id="customrepeat:CustomRepeat" name="CustomRepeat" value="00" />

		</NEXT_HEADERS>

	</TYPE_DEF> 

  </TYPE_DEFS>
</PDU_LIBRARY>
