<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="pdu_schema.xsd"
    name="bip"
    showName="BTS Intranet Protocol"
    version="1.00"
    description="This is a sample PDU library for BIP as defined in XML Stream Editor.">

    <TYPE_DEFS>
        <TYPE_DEF name="Bip" type="SEQUENCE" unitType="HEADER" portType="Ethernet" uiShowName="BTS Intranet Protocol" >
            <FIELD name="bipStreamID" type="INTEGER" bitLength="16" uiShowName="BIPStreamID" />
            <FIELD name="eventSize" type="INTEGER" bitLength="16" uiShowName="EventSize" />
            <FIELD name="eventSequenceNum" type="INTEGER" bitLength="8" uiShowName="EventSequence Num" />
            <FIELD name="fragmentIndex" type="INTEGER" bitLength="8" uiShowName="Fragment Index" />
            <FIELD name="localQueueID" type="INTEGER" bitLength="16" uiShowName="Local Queue ID" />

            <NEXT_HEADERS>
                <NXT_HDR id="custom:Custom" name="Custom" value="1024" />
                <NXT_HDR id="customrepeat:CustomRepeat" name="CustomRepeat" value="1024" />
                <NXT_HDR id="trswdata:TransportSoftwareData" name="TransportSoftwareData" />
            </NEXT_HEADERS>
        </TYPE_DEF>
    </TYPE_DEFS>
</PDU_LIBRARY>

