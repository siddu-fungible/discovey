    <FIELD name="Data" type="ASCII" minByteLength="0" maxByteLength="63"
           defaultValue="SPIRENT" uiShowName="ID" />

    <FIELD name="Pad" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0"
           padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface"
           uiShowName="4 Byte Alignment Padding"/>
  </TYPE_DEF>
  <TYPE_DEF name="AccessLineIdentifyingTlvList" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1024" elementType="AccessLineIdentifyingTlv" unitType="ELEMENT" />
    <TYPE_DEF name="AccessLineIdentifyingTlv" type="CHOICE" unitType="ELEMENT" uiShowName="Access Line Identifying TLV" >
      <FIELD name="AccessLoopCircuitId" type="OamAccessLoopCircuitIdTlv" uiShowName="Access Loop Circuit ID TLV" />
      <FIELD name="AccessAggregationCircuitIdAscii" type="OamAccessAggregationCircuitIdAsciiTlv" uiShowName="Access Aggregation Circuit ID ASCII TLV" />
      <FIELD name="AccessAggregationCircuitIdBinary" type="OamAccessAggregationCircuitIdBinaryVlanTlv" uiShowName="Access Aggregation Circuit ID Binary TLV (VLAN)" />
      <FIELD name="AccessLoopRemoteId" type="OamAccessLoopRemoteIdTlv" uiShowName="Access Loop Remote ID TLV" />    
    </TYPE_DEF>
  
  <!-- Encapsulation HEADER ===================================================== -->
  <TYPE_DEF name="EncapHeader" type="SEQUENCE" unitType="HEADER" uiShowName="TCP/IP Encapsulating Header">
    <FIELD name="Identifier" type="OCTETSTRING" byteLength="2"
          defaultValue="880C"
          uiNoShow="Interface" uiShowCommon="false"
          uiShowName="Identifier"/>
    <FIELD name="length" type="INTEGER" bitLength="16"
          defaultValue="0"
          uiControlState="CALCULATED"
          procfnTarget="PAYLOADLENGTH" 
          uiShowName="Length" uiShowCommon="false" />
  </TYPE_DEF>
  
  <TYPE_DEF name="ANCPGeneralResult" type="INTEGER" bitLength="4" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="00 - Ignore" value="0" />
        <VAL_ENUM uiShowName="01 - Nack" value="01" />
        <VAL_ENUM uiShowName="02 - AckAll" value="02" />
        <VAL_ENUM uiShowName="03 - Success" value="03" />
        <VAL_ENUM uiShowName="04 - Failure" value="04" />
      </VALUE_ENUMERATION>
  </TYPE_DEF>
  <TYPE_DEF name="ANCPGeneralResultCode" type="INTEGER" bitLength="12" unitType="ELEMENT">
    <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="0x0 | No result"  value="0" />
        <VAL_ENUM uiShowName="0x2 | Invalid request message" value="2" />
        <VAL_ENUM uiShowName="0x6 | One or more of the specified ports are down" value="6" />
        <VAL_ENUM uiShowName="0x13 | Out of resources" value="19" />
        <VAL_ENUM uiShowName="0x51 | Request message type not implemented" value="81" />
        <VAL_ENUM uiShowName="0x53 | Malformed message" value="83" />
        <VAL_ENUM uiShowName="0x54 | Mandatory TLV missing" value="84" />
        <VAL_ENUM uiShowName="0x55 | Invalid TLV contents" value="85" />
        <VAL_ENUM uiShowName="0x500 | One or more of the specified ports do not exist" value="1280" />
        <VAL_ENUM uiShowName="0x501 | Loopback test timed out"  value="1281" />
        <VAL_ENUM uiShowName="0x502 | Reserved" value="1282" />
        <VAL_ENUM uiShowName="0x503 | DSL access line status showtime" value="1283" />
        <VAL_ENUM uiShowName="0x504 | DSL access line status idle" value="1284" />
        <VAL_ENUM uiShowName="0x505 | DSL access line status silent" value="1285" />
        <VAL_ENUM uiShowName="0x506 | DSL access line status training" value="1286" />
        <VAL_ENUM uiShowName="0x507 | DSL access line integrity error" value="1287" />
        <VAL_ENUM uiShowName="0x508 | DSLAM resource not available" value="1288" />
        <VAL_ENUM uiShowName="0x509 | Invalid test parameter" value="1289" />
    </VALUE_ENUMERATION>
  </TYPE_DEF>
  <!-- ANCP Port Management Message HEADER ===================================================== -->
  <TYPE_DEF name="AncpPmMsgHeader" type="SEQUENCE" unitType="HEADER" uiShowName="ANCP Port Management Message Header" >
    <FIELD name="version" type="INTEGER" bitLength="8"
          defaultValue="50"
          uiNoShow="Interface" 
          uiShowName="Version" uiShowCommon="false" />
    <FIELD name="generalMessageType" type="INTEGER" bitLength="8"
          defaultValue="0"
          uiNoShow="Interface"
          uiShowName="Message Type" uiShowCommon="false"/>
    <FIELD name="result" type="ANCPGeneralResult" uiNoShow="Interface" defaultValue="0" 
          uiShowName="Result Field" />
    <FIELD name="resultCode" type="ANCPGeneralResultCode" bitLength="12"
          defaultValue="0"
          uiNoShow="Interface"
          uiShowName="Result Code" />
    <FIELD name="partitionId" type="INTEGER" bitLength="8"
          defaultValue="0"
          uiNoShow="Interface"
          uiShowName="Partition ID" />
    <FIELD name="transactionId" type="INTEGER" bitLength="24"
          defaultValue="0"
          uiNoShow="Interface"
          uiShowName="Transaction Identifier" />
    <FIELD name="ignore" type="INTEGER" bitLength="1"
          defaultValue="1" uiShowCommon="false"
          uiNoShow="Interface"
          uiShowName="Ignore" />
    <FIELD name="submessage" type="INTEGER" bitLength="15"
          defaultValue="1"
          uiNoShow="Interface"
          uiShowName="SubMessage Number" uiShowCommon="false"/>
    <FIELD name="length" type="INTEGER" bitLength="16"
          defaultValue="0"
          uiControlState="CALCULATED"
          procfnTarget="ANCPOAMMSGLENGTH" 
          uiShowName="Length" />
    <FIELD name="unused1" type="OCTETSTRING" byteLength="14"  defaultValue="0000000000000000000000000000" uiShowCommon="false" uiShowName="Unused"/>
    <FIELD name="function" type="OCTETSTRING" byteLength="1"  defaultValue="9" uiShowCommon="false" uiShowName="Function"/>
    <FIELD name="xFunction" type="OCTETSTRING" byteLength="1"  defaultValue="0" uiShowCommon="false" uiShowName="X-Function"/>
    <FIELD name="unused2" type="OCTETSTRING" byteLength="4" defaultValue="00000000" uiShowCommon="false" uiShowName="Unused2"/>    
    <FIELD name="extensionFlag" type="OCTETSTRING" byteLength="1"  defaultValue="0" uiShowCommon="false" uiShowName="Extension Flag"/>
    <FIELD name="messageType" type="INTEGER" bitLength="8"  defaultValue="0" uiNoShow="Interface" uiShowCommon="false" uiShowName="Port Management Message Type"/>
    <FIELD name="reserved" type="OCTETSTRING" byteLength="2" defaultValue="0000" uiShowCommon="false" uiShowName="Reserved"/>
    <FIELD name="numberOfTlvs" type="INTEGER" bitLength="16"
          defaultValue="0"  
          uiShowName="Number of TLVs" />
    <FIELD name="extBlockLength" type="INTEGER" bitLength="16" defaultValue="0" uiNoShow="Interface"
           uiControlState="CALCULATED" procfnTarget="PAYLOADLENGTH" 
           uiShowCommon="false" uiShowName="Extension Block Length"/>
  </TYPE_DEF>
  
  
  <!-- ANCP OAM Message ===================================================== -->
  <TYPE_DEF name="AncpOamLoopback" type="SEQUENCE" unitType="MESSAGE" uiShowName="ANCP Port Management (OAM)" >
    <FIELD name="EncapHeader" type="EncapHeader" uiShowName="TCP/IP Encapsulating Header" uiShowCommon="false" />
    <FIELD name="AncpHeader" type="AncpPmMsgHeader" uiShowName="ANCP Port Management Message Header"/>
    <FIELD name="AccessLineIdentifyingTlvs" type="AccessLineIdentifyingTlvList"  uiShowName="Access Line Identifying TLVs" />
    <FIELD name="OAMLoopbackTestTlvs" type="OAMLoopbackTestTlvList"  uiShowName="Testing-related TLVs" />
    <FIELD name="StatusInfoTLV" type="StatusInfoTLVList" uiShowName="Status-Info TLV" />
  </TYPE_DEF>
  
</TYPE_DEFS>
</PDU_LIBRARY>
