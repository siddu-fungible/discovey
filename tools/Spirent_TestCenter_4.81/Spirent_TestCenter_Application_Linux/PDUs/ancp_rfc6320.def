      <NXT_HDR id="ancp_rfc6320:AccessAggregationCircuitIdBinaryAtmTlv" name="Access Aggregation Circuit ID Binary TLV (ATM)" />
    </NEXT_HEADERS>
  </TYPE_DEF>

