<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="pdu_schema.xsd"
  name="ancp_rfc6320"
  showName="ANCP (RFC 6320)"
  version="1.00"
  description="This is a PDU library for ANCP as defined in XML Stream Editor.  ANCP Protocol Version: RFC 6320.">

<TYPE_DEFS>

  <!-- ====================================================================== -->
  <!--
      NOTE: All ANCP TLVs must contain "Type" and "Length" fields.  Any TLVs
      which require 4-byte alignment padding should include an alignment
      field as the last field in the TLV.  TLVs may optionally include a
      "MessageSelect" field which is used to determine which ANCP messages
      the TLVs should be included in.
    -->

  <!-- Common Elements  ===================================================== -->
    <TYPE_DEF name="MessageSelectValues" type="INTEGER" bitLength="2" minValue="1" maxValue="3" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="Port Up" value="1" />
        <VAL_ENUM uiShowName="Port Down" value="2" />
        <VAL_ENUM uiShowName="Port Up, Port Down" value="3" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>

    <TYPE_DEF name="DslTypeValues" type="OCTETSTRING" byteLength="4" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="01 - ADSL1" value="00000001" />
        <VAL_ENUM uiShowName="02 - ADSL2" value="00000002" />
        <VAL_ENUM uiShowName="03 - ADSL2+" value="00000003" />
        <VAL_ENUM uiShowName="04 - VDSL1" value="00000004" />
        <VAL_ENUM uiShowName="05 - VDSL2" value="00000005" />
        <VAL_ENUM uiShowName="06 - SDSL" value="00000006" />
        <VAL_ENUM uiShowName="07 - UNKNOWN" value="00000007" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>

    <TYPE_DEF name="DataLinkValues" type="OCTETSTRING" byteLength="1" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="00 - AAL5" value="00" />
        <VAL_ENUM uiShowName="01 - ETHERNET" value="01" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>

    <TYPE_DEF name="Encaps1Values" type="OCTETSTRING" byteLength="1" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="00 - NA" value="00" />
        <VAL_ENUM uiShowName="01 - Untagged Ethernet" value="01" />
        <VAL_ENUM uiShowName="02 - Single-tagged Ethernet" value="02" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>

    <TYPE_DEF name="Encaps2Values" type="OCTETSTRING" byteLength="1" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="00 - NA" value="00" />
        <VAL_ENUM uiShowName="01 - PPPoA LLC" value="01" />
        <VAL_ENUM uiShowName="02 - PPPoA NULL" value="02" />
        <VAL_ENUM uiShowName="03 - IPoA LLC" value="03" />
        <VAL_ENUM uiShowName="04 - IPoA NULL" value="04" />
        <VAL_ENUM uiShowName="05 - Ethernet over AAL5 LLC with FCS" value="05" />
        <VAL_ENUM uiShowName="06 - Ethernet over AAL5 LLC without FCS" value="06" />
        <VAL_ENUM uiShowName="07 - Ethernet over AAL5 NULL with FCS" value="07" />
        <VAL_ENUM uiShowName="08 - Ethernet over AAL5 NULL without FCS" value="08" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>

  <!-- Custom TLV ===================================================== -->
  <TYPE_DEF name="CustomTlv" type="SEQUENCE" unitType="HEADER" level="2" uiShowName="Custom TLV">

    <FIELD name="MessageSelect" type="MessageSelectValues" defaultValue="3" uiShowName="Message Selection"
           doNotBuild="true"
           />

    <FIELD name="Type" type="OCTETSTRING" byteLength="2"
           defaultValue="0001" uiShowCommon="true" uiShowName="Type" />

    <FIELD name="Length" type="INTEGER" bitLength="16" procfnTarget="ANCPTLVLENGTH"
           defaultValue="0" uiShowName="Length" uiControlState="CALCULATED" />

    <FIELD name="Data" type="OCTETSTRING"  minByteLength="0" maxByteLength="65535"
           uiNoShowModifier="Interface,Frame" uiShowName="Data" wildcardAllowed="true"/>

    <FIELD name="Pad" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0"
           padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface"
           uiShowName="4 Byte Alignment Padding"/>

    <NEXT_HEADERS>
      <NXT_HDR id="ancp_rfc6320:CustomTlv" name="Custom TLV" />
      <NXT_HDR id="ancp_rfc6320:AccessLoopCircuitIdTlv" name="Access Loop Circuit ID TLV" />
      <NXT_HDR id="ancp_rfc6320:AccessLoopRemoteIdTlv" name="Access Loop Remote ID TLV" />
      <NXT_HDR id="ancp_rfc6320:AccessAggregationCircuitIdAsciiTlv" name="Access Aggregation Circuit ID ASCII TLV" />
      <NXT_HDR id="ancp_rfc6320:AccessAggregationCircuitIdBinaryVlanTlv" name="Access Aggregation Circuit ID Binary TLV (VLAN)" />
      <NXT_HDR id="ancp_rfc6320:AccessAggregationCircuitIdBinaryAtmTlv" name="Access Aggregation Circuit ID Binary TLV (ATM)" />
    </NEXT_HEADERS>
  </TYPE_DEF>

  <!-- Access Loop Circuit ID TLV  ================================================ -->
  <TYPE_DEF name="AccessLoopCircuitIdTlv" type="SEQUENCE" unitType="HEADER" level="2" uiShowName="Access Loop Circuit ID TLV">

    <FIELD name="MessageSelect" type="MessageSelectValues" defaultValue="3" uiShowName="Message Selection"
           doNotBuild="true"
           />

    <FIELD name="Type" type="OCTETSTRING" byteLength="2"
           defaultValue="0001" uiNoShow="Interface"
           uiShowCommon="false" uiShowName="Type" />

    <FIELD name="Length" type="INTEGER" bitLength="16" procfnTarget="ANCPTLVLENGTH"
           defaultValue="0" uiShowName="Length" uiControlState="CALCULATED" />

    <!-- Access-Node-Identifier eth slot/port[:vlan-id] -->
    <!-- Access-Node-Identifier atm slot/port:vpi.vci -->
    <FIELD name="ID" type="ASCII" minByteLength="0" maxByteLength="63"
           defaultValue="Access-Node-Identifier eth slot port:vlan-id" uiShowName="ID" wildcardAllowed="true"/>

    <FIELD name="Pad" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0"
            padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface"
            uiShowName="4 Byte Alignment Padding"/>

    <NEXT_HEADERS>
      <NXT_HDR id="ancp_rfc6320:CustomTlv" name="Custom TLV" />
      <NXT_HDR id="ancp_rfc6320:AccessLoopCircuitIdTlv" name="Access Loop Circuit ID TLV" />
      <NXT_HDR id="ancp_rfc6320:AccessLoopRemoteIdTlv" name="Access Loop Remote ID TLV" />
      <NXT_HDR id="ancp_rfc6320:AccessAggregationCircuitIdAsciiTlv" name="Access Aggregation Circuit ID ASCII TLV" />
      <NXT_HDR id="ancp_rfc6320:AccessAggregationCircuitIdBinaryVlanTlv" name="Access Aggregation Circuit ID Binary TLV (VLAN)" />
      <NXT_HDR id="ancp_rfc6320:AccessAggregationCircuitIdBinaryAtmTlv" name="Access Aggregation Circuit ID Binary TLV (ATM)" />
    </NEXT_HEADERS>
  </TYPE_DEF>

  <!-- Access Loop Remote ID TLV ================================================== -->
  <TYPE_DEF name="AccessLoopRemoteIdTlv" type="SEQUENCE" unitType="HEADER" level="2" uiShowName="Access Loop Remote ID TLV">

    <FIELD name="MessageSelect" type="MessageSelectValues" defaultValue="3" uiShowName="Message Selection"
