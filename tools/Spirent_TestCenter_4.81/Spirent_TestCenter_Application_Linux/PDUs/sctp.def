<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="pdu_schema.xsd"
    name="sctp"
    showName="SCTP"
    version="1.00"
    description="This is a sample PDU library for SCTP as defined in XML Stream Editor.">

    <TYPE_DEFS>
        <TYPE_DEF name="Sctp" type="SEQUENCE" unitType="HEADER" portType="Ethernet" uiShowName="SCTP" >
            <FIELD name="header" type="SctpHeader" uiShowName="SCTP Header" />
            <FIELD name="chunks" type="SctpChunksSeqOf" uiShowName="SCTP Chunks" />
        </TYPE_DEF>

        <TYPE_DEF name="SctpHeader" type="SEQUENCE" unitType="ELEMENT" uiShowName="SCTP Header" >
            <FIELD name="sourcePort" type="INTEGER" bitLength="16" defaultValue="3001"  uiShowName="Source port" />
            <FIELD name="destPort" type="types:WellKnownPorts" defaultValue="3002"
               procfnTarget="NEXTHDR" uiShowName="Destination port" />
            <FIELD name="verTag" type="INTEGER" bitLength="32" defaultValue="1" uiShowName="Verification Tag" />
            <FIELD name="checksum" type="INTEGER" bitLength="32" defaultValue="0"
                procfnTarget="SCTPCHECKSUM" uiNoShowModifier="Interface,Frame,Filter" uiGenerateError="true"
                uiShowName="Checksum" uiControlState="ALLOW_OVERRIDE"/>
        </TYPE_DEF>

        <TYPE_DEF name="SctpChunksSeqOf" type="SEQUENCE_OF" minOccurs="0" maxOccurs="250" elementType="SctpChunk" unitType="ELEMENT"/>

        <TYPE_DEF name="SctpChunk" type="SEQUENCE" unitType="ELEMENT" uiShowName="SCTP Chunk">
            <FIELD name="chunk" type="SctpChunkSeqOf" uiShowName="Chunk Type" />
        </TYPE_DEF>

        <TYPE_DEF name="SctpChunkSeqOf" type="SEQUENCE_OF" minOccurs="1" maxOccurs="1" elementType="SctpChunkChoice" unitType="ELEMENT"/>

        <TYPE_DEF name="SctpChunkChoice" type="CHOICE" unitType="ELEMENT" uiShowName="Sctp Chunk">
            <FIELD name="dataChunk" type="SctpDataChunk" uiShowName="DATA Chunk" />
            <FIELD name="initChunk" type="SctpInitChunk" uiShowName="INIT Chunk" />
            <FIELD name="initAckChunk" type="SctpInitAckChunk" uiShowName="INIT ACK Chunk" />
            <FIELD name="sackChunk" type="SctpSackChunk" uiShowName="SACK" />
            <FIELD name="heartbeatChunk" type="SctpHeartbeatChunk" uiShowName="HEARTBEAT Chunk" />
            <FIELD name="heartbeatAckChunk" type="SctpHeartbeatAckChunk" uiShowName="HEARTBEAT ACK Chunk" />
            <FIELD name="abortChunk" type="SctpAbortChunk" uiShowName="ABORT Chunk" />
            <FIELD name="shutdownChunk" type="SctpShutdownChunk" uiShowName="SHUTDOWN Chunk" />
            <FIELD name="shutdownAckChunk" type="SctpShutdownAckChunk" uiShowName="SHUTDOWN ACK Chunk" />
            <FIELD name="errorChunk" type="SctpErrorChunk" uiShowName="ERROR Chunk" />
            <FIELD name="cookieEchoChunk" type="SctpCookieEchoChunk" uiShowName="COOKIE ECHO Chunk" />
            <FIELD name="cookieAckChunk" type="SctpCookieAckChunk" uiShowName="COOKIE ACK Chunk" />
            <FIELD name="shutdownCompleteChunk" type="SctpShutdownCompleteChunk" uiShowName="SHUTDOWN COMPLETE Chunk" />
        </TYPE_DEF>

        <TYPE_DEF name="SctpDataChunk" type="SEQUENCE" unitType="ELEMENT" uiShowName="DATA Chunk" >
            <FIELD name="type" type="INTEGER" bitLength="8" defaultValue="0" uiShowName="Type" />
            <FIELD name="flags" type="SctpChunkFlags" uiShowName="Flags" />
            <FIELD name="length" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="0" uiShowName="Length" uiControlState="CALCULATED" binder="calculated" />

            <FIELD name="tsn" type="INTEGER" bitLength="32" defaultValue="0" uiShowName="TSN"/>
            <FIELD name="sid" type="INTEGER" bitLength="16" defaultValue="0" uiShowName="Stream Identifier"/>
            <FIELD name="ssn" type="INTEGER" bitLength="16" defaultValue="0" uiShowName="Stream Sequence Number"/>
            <FIELD name="pid" type="INTEGER" bitLength="32" defaultValue="0" uiShowName="Payload Protocol Identifier"/>
            <FIELD name="data" type="OCTETSTRING" minByteLength="0" maxByteLength="65535" uiShowName="User Data"></FIELD>

            <FIELD name="padding" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0" padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface" uiShowName="4 Byte Alignment Padding"/>
        </TYPE_DEF>

        <TYPE_DEF name="SctpInitChunk" type="SEQUENCE" unitType="ELEMENT" uiShowName="INIT Chunk" >
            <FIELD name="type" type="INTEGER" bitLength="8" defaultValue="1" uiShowName="Type" />
            <FIELD name="flags" type="SctpChunkFlags" uiShowName="Flags" />
            <FIELD name="length" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="0" uiShowName="Length" uiControlState="CALCULATED" binder="calculated" />

            <FIELD name="initTag" type="INTEGER" bitLength="32" defaultValue="0" uiShowName="Initiate Tag"/>
            <FIELD name="arwc" type="INTEGER" bitLength="32" defaultValue="0" uiShowName="Advertised Receiver Window Credit"/>
            <FIELD name="nos" type="INTEGER" bitLength="16" defaultValue="0" uiShowName="Number of Outbound Streams"/>
            <FIELD name="mis" type="INTEGER" bitLength="16" defaultValue="0" uiShowName="Number of Inbound Streams"/>
            <FIELD name="itsn" type="INTEGER" bitLength="32" defaultValue="0" uiShowName="Initial TSN"/>

            <FIELD name="initOptionalParams" type="InitOptionalParameters" uiShowName="Optional/Variable-Length Parameters"></FIELD>

            <FIELD name="padding" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0" padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface" uiShowName="4 Byte Alignment Padding"/>
        </TYPE_DEF>

        <TYPE_DEF name="InitOptionalParameters" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1024" elementType="InitOptionalParamTLV" unitType="ELEMENT"> </TYPE_DEF>

        <TYPE_DEF name="InitOptionalParamTLV" type="SEQUENCE" unitType="ELEMENT" uiShowName="Init Optional Parameter TLV">
            <FIELD name="type" type="InitOptionalParamType" defaultValue="5" uiShowName="Type"/>
            <FIELD name="length" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="0" uiShowName="Parameter Length" uiControlState="CALCULATED" binder="calculated" />
            <FIELD name="value" type="OCTETSTRING" minByteLength="0" maxByteLength="65535" uiShowName="Parameter Value"></FIELD>
            <FIELD name="padding" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0" padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface" uiShowName="4 Byte Alignment Padding"/>
        </TYPE_DEF>

        <TYPE_DEF name="InitOptionalParamType" type="INTEGER" bitLength="16" unitType="ELEMENT">
            <VALUE_ENUMERATION>
                <VAL_ENUM uiShowName="IPv4 Address" value="5" />
                <VAL_ENUM uiShowName="IPv6 Address" value="6" />
                <VAL_ENUM uiShowName="Cookie Preservative" value="9" />
                <VAL_ENUM uiShowName="Reserved for ECN Capable" value="32768" />
                <VAL_ENUM uiShowName="Host Name Address" value="11" />
                <VAL_ENUM uiShowName="Supported Address Types" value="12" />
            </VALUE_ENUMERATION>
        </TYPE_DEF>

        <TYPE_DEF name="SctpInitAckChunk" type="SEQUENCE" unitType="ELEMENT" uiShowName="INIT ACK Chunk" >
            <FIELD name="type" type="INTEGER" bitLength="8" defaultValue="2" uiShowName="Type" />
            <FIELD name="flags" type="SctpChunkFlags" uiShowName="Flags" />
            <FIELD name="length" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="0" uiShowName="Length" uiControlState="CALCULATED" binder="calculated" />

            <FIELD name="initTag" type="INTEGER" bitLength="32" defaultValue="0" uiShowName="Initiate Tag"/>
            <FIELD name="arwc" type="INTEGER" bitLength="32" defaultValue="0" uiShowName="Advertised Receiver Window Credit"/>
            <FIELD name="nos" type="INTEGER" bitLength="16" defaultValue="0" uiShowName="Number of Outbound Streams"/>
            <FIELD name="mis" type="INTEGER" bitLength="16" defaultValue="0" uiShowName="Number of Inbound Streams"/>
            <FIELD name="itsn" type="INTEGER" bitLength="32" defaultValue="0" uiShowName="Initial TSN"/>

            <FIELD name="stateCookie" type="InitAckStateCookieTLV" uiShowName="State Cookie"></FIELD>
            <FIELD name="optionalParams" type="InitAckOptionalParameters" uiShowName="Optional/Variable-Length Parameters"></FIELD>

            <FIELD name="padding" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0" padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface" uiShowName="4 Byte Alignment Padding"/>
        </TYPE_DEF>

        <TYPE_DEF name="InitAckStateCookieTLV" type="SEQUENCE" unitType="ELEMENT" uiShowName="State Cookie">
            <FIELD name="type" type="INTEGER" bitLength="16" defaultValue="7" uiShowName="Type" />
            <FIELD name="length" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="0" uiShowName="Parameter Length" uiControlState="CALCULATED" binder="calculated" />
            <FIELD name="value" type="OCTETSTRING" minByteLength="0" maxByteLength="65535" uiShowName="Parameter Value"></FIELD>
            <FIELD name="padding" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0" padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface" uiShowName="4 Byte Alignment Padding"/>
        </TYPE_DEF>

        <TYPE_DEF name="InitAckOptionalParameters" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1024" elementType="InitAckOptionalParamTLV" unitType="ELEMENT"> </TYPE_DEF>

        <TYPE_DEF name="InitAckOptionalParamTLV" type="SEQUENCE" unitType="ELEMENT" uiShowName="Init Ack Optional Parameter TLV">
            <FIELD name="type" type="InitAckOptionalParamType" defaultValue="5" uiShowName="Type"/>
            <FIELD name="length" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="0" uiShowName="Parameter Length" uiControlState="CALCULATED" binder="calculated" />
            <FIELD name="value" type="OCTETSTRING" minByteLength="0" maxByteLength="65535" uiShowName="Parameter Value"></FIELD>
            <FIELD name="padding" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0" padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface" uiShowName="4 Byte Alignment Padding"/>
        </TYPE_DEF>

        <TYPE_DEF name="InitAckOptionalParamType" type="INTEGER" bitLength="16" unitType="ELEMENT">
            <VALUE_ENUMERATION>
                <VAL_ENUM uiShowName="IPv4 Address" value="5" />
                <VAL_ENUM uiShowName="IPv6 Address" value="6" />
                <VAL_ENUM uiShowName="Unrecognized Parameter" value="8" />
                <VAL_ENUM uiShowName="Reserved for ECN Capable" value="32768" />
                <VAL_ENUM uiShowName="Host Name Address" value="11" />
            </VALUE_ENUMERATION>
        </TYPE_DEF>

        <TYPE_DEF name="SctpSackChunk" type="SEQUENCE" unitType="ELEMENT" uiShowName="SACK Chunk" >
            <FIELD name="type" type="INTEGER" bitLength="8" defaultValue="3" uiShowName="Type" />
            <FIELD name="flags" type="SctpChunkFlags" uiShowName="Flags" />
            <FIELD name="length" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="0" uiShowName="Length" uiControlState="CALCULATED" binder="calculated" />

            <FIELD name="ctsnack" type="INTEGER" bitLength="32" defaultValue="0" uiShowName="Cumulative TSN Ack "/>
            <FIELD name="arwc" type="INTEGER" bitLength="32" defaultValue="0" uiShowName="Advertised Receiver Window Credit"/>
            <FIELD name="nogab" type="INTEGER" bitLength="16" defaultValue="0" uiShowName="Number of Gap Ack Blocks"/>
            <FIELD name="nodtsns" type="INTEGER" bitLength="16" defaultValue="0" uiShowName="Number of Duplicate TSNs"/>
            <FIELD name="gapackBlocks" type="GapAckBlockList" uiShowName="Gap Ack Blocks"></FIELD>
            <FIELD name="duplicateTSNs" type="DuplicateTSNList" uiShowName="Duplicate TSNs"></FIELD>

            <FIELD name="padding" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0" padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface" uiShowName="4 Byte Alignment Padding"/>
        </TYPE_DEF>

        <TYPE_DEF name="GapAckBlockList" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1024" elementType="GapAckBlock" unitType="ELEMENT"> </TYPE_DEF>

        <TYPE_DEF name="GapAckBlock" type="SEQUENCE" unitType="ELEMENT" uiShowName="Gap Ack Block">
            <FIELD name="gabs" type="INTEGER" bitLength="16" defaultValue="0" uiShowName="Gap Ack Block Start"/>
            <FIELD name="gabe" type="INTEGER" bitLength="16" defaultValue="0" uiShowName="Gap Ack Block End"/>
        </TYPE_DEF>

        <TYPE_DEF name="DuplicateTSNList" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1024" elementType="DuplicateTSN" unitType="ELEMENT"> </TYPE_DEF>

        <TYPE_DEF name="DuplicateTSN" type="SEQUENCE" unitType="ELEMENT" uiShowName="Duplicate TSN">
            <FIELD name="dtsn" type="INTEGER" bitLength="32" defaultValue="0" uiShowName="Value"/>
        </TYPE_DEF>

        <TYPE_DEF name="SctpHeartbeatChunk" type="SEQUENCE" unitType="ELEMENT" uiShowName="HEARTBEAT Chunk" >
            <FIELD name="type" type="INTEGER" bitLength="8" defaultValue="4" uiShowName="Type" />
            <FIELD name="flags" type="SctpChunkFlags" uiShowName="Flags" />
            <FIELD name="length" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="0" uiShowName="Length" uiControlState="CALCULATED" binder="calculated" />
            <FIELD name="heartbeatInformationTLV" type="HeartbeatInformationTLV" uiShowName="Heartbeat Information TLV"></FIELD>
            <FIELD name="padding" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0" padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface" uiShowName="4 Byte Alignment Padding"/>
        </TYPE_DEF>

        <TYPE_DEF name="SctpHeartbeatAckChunk" type="SEQUENCE" unitType="ELEMENT" uiShowName="HEARTBEAT ACK Chunk" >
            <FIELD name="type" type="INTEGER" bitLength="8" defaultValue="5" uiShowName="Type" />
            <FIELD name="flags" type="SctpChunkFlags" uiShowName="Flags" />
            <FIELD name="length" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="0" uiShowName="Length" uiControlState="CALCULATED" binder="calculated" />
            <FIELD name="heartbeatInformationTLV" type="HeartbeatInformationTLV" uiShowName="Heartbeat Information TLV"></FIELD>
            <FIELD name="padding" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0" padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface" uiShowName="4 Byte Alignment Padding"/>
        </TYPE_DEF>

        <TYPE_DEF name="HeartbeatInformationTLV" type="SEQUENCE" unitType="ELEMENT" uiShowName="Heartbeat Information">
            <FIELD name="hbInfoType" type="INTEGER" bitLength="16" defaultValue="1" uiShowName="Type"/>
            <FIELD name="hbInfoLength" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="0" uiShowName="Length" uiControlState="CALCULATED" binder="calculated" />
            <FIELD name="hbInfoValue" type="OCTETSTRING" minByteLength="0" maxByteLength="65535" uiShowName="Value"></FIELD>
            <FIELD name="padding" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0" padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface" uiShowName="4 Byte Alignment Padding"/>
        </TYPE_DEF>

        <TYPE_DEF name="SctpAbortChunk" type="SEQUENCE" unitType="ELEMENT" uiShowName="ABORT Chunk" >
            <FIELD name="type" type="INTEGER" bitLength="8" defaultValue="6" uiShowName="Type" />
            <FIELD name="reserved" type="INTEGER" bitLength="7" defaultValue="0" uiShowName="Reserved"/>
            <FIELD name="t" type="INTEGER" bitLength="1" defaultValue="0" uiShowName="T"/>
            <FIELD name="length" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="0" uiShowName="Length" uiControlState="CALCULATED" binder="calculated" />
            <FIELD name="errorCauses" type="ErrorCausesList" uiShowName="Error Causes"></FIELD>
            <FIELD name="padding" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0" padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface" uiShowName="4 Byte Alignment Padding"/>
        </TYPE_DEF>

        <TYPE_DEF name="SctpShutdownChunk" type="SEQUENCE" unitType="ELEMENT" uiShowName="SHUTDOWN Chunk" >
            <FIELD name="type" type="INTEGER" bitLength="8" defaultValue="7" uiShowName="Type" />
            <FIELD name="flags" type="SctpChunkFlags" uiShowName="Flags" />
            <FIELD name="length" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="0" uiShowName="Length" uiControlState="CALCULATED" binder="calculated" />
            <FIELD name="ctsnack" type="INTEGER" bitLength="32" defaultValue="0" uiShowName="Cumulative TSN Ack"/>
            <FIELD name="padding" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0" padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface" uiShowName="4 Byte Alignment Padding"/>
        </TYPE_DEF>

        <TYPE_DEF name="SctpShutdownAckChunk" type="SEQUENCE" unitType="ELEMENT" uiShowName="SHUTDOWN ACK Chunk" >
            <FIELD name="type" type="INTEGER" bitLength="8" defaultValue="8" uiShowName="Type" />
            <FIELD name="flags" type="SctpChunkFlags" uiShowName="Flags" />
            <FIELD name="length" type="INTEGER" bitLength="16" defaultValue="4" uiShowName="Length" uiControlState="CALCULATED" />
        </TYPE_DEF>

        <TYPE_DEF name="SctpErrorChunk" type="SEQUENCE" unitType="ELEMENT" uiShowName="ERROR Chunk" >
            <FIELD name="type" type="INTEGER" bitLength="8" defaultValue="9" uiShowName="Type" />
            <FIELD name="flags" type="SctpChunkFlags" uiShowName="Flags" />
            <FIELD name="length" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="0" uiShowName="Length" uiControlState="CALCULATED" binder="calculated" />
            <FIELD name="errorCauses" type="ErrorCausesList" uiShowName="Error Causes"></FIELD>
            <FIELD name="padding" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0" padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface" uiShowName="4 Byte Alignment Padding"/>
        </TYPE_DEF>

        <TYPE_DEF name="SctpCookieEchoChunk" type="SEQUENCE" unitType="ELEMENT" uiShowName="COOKIE ECHO Chunk" >
            <FIELD name="type" type="INTEGER" bitLength="8" defaultValue="10" uiShowName="Type" />
            <FIELD name="flags" type="SctpChunkFlags" uiShowName="Flags" />
            <FIELD name="length" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="0" uiShowName="Length" uiControlState="CALCULATED" binder="calculated" />
            <FIELD name="cookie" type="OCTETSTRING" minByteLength="0" maxByteLength="65535" uiShowName="Cookie"></FIELD>
            <FIELD name="padding" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0" padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface" uiShowName="4 Byte Alignment Padding"/>
        </TYPE_DEF>

        <TYPE_DEF name="SctpCookieAckChunk" type="SEQUENCE" unitType="ELEMENT" uiShowName="COOKIE ACK Chunk" >
            <FIELD name="type" type="INTEGER" bitLength="8" defaultValue="11" uiShowName="Type" />
            <FIELD name="flags" type="SctpChunkFlags" uiShowName="Flags" />
            <FIELD name="length" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="4" uiShowName="Length" uiControlState="CALCULATED" binder="calculated" />
        </TYPE_DEF>

        <TYPE_DEF name="SctpShutdownCompleteChunk" type="SEQUENCE" unitType="ELEMENT" uiShowName="SHUTDOWN COMPLETE Chunk" >
            <FIELD name="type" type="INTEGER" bitLength="8" defaultValue="14" uiShowName="Type" />
            <FIELD name="reserved" type="INTEGER" bitLength="7" defaultValue="0" uiShowName="Reserved"/>
            <FIELD name="t" type="INTEGER" bitLength="1" defaultValue="0" uiShowName="T"/>
            <FIELD name="length" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="4" uiShowName="Length" uiControlState="CALCULATED" binder="calculated" />
            <FIELD name="padding" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0" padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface" uiShowName="4 Byte Alignment Padding"/>
        </TYPE_DEF>

        <TYPE_DEF name="SctpChunkFlags" type="SEQUENCE" unitType="ELEMENT" uiShowName="SCTP Chunk Flags" >
            <FIELD name="reserved" type="INTEGER" bitLength="5" defaultValue="0" uiShowName="Reserved"  />
            <FIELD name="ubit" type="INTEGER" bitLength="1" defaultValue="0" uiShowName="U bit"  />
            <FIELD name="bbit" type="INTEGER" bitLength="1" defaultValue="1" uiShowName="B bit"  />
            <FIELD name="ebit" type="INTEGER" bitLength="1" defaultValue="1" uiShowName="E bit"  />
        </TYPE_DEF>

        <TYPE_DEF name="ErrorCausesList" type="SEQUENCE_OF" minOccurs="0" maxOccurs="250" elementType="ErrorCausesTLV" unitType="ELEMENT"/>

        <TYPE_DEF name="ErrorCausesTLV" type="CHOICE" unitType="ELEMENT" uiShowName="Error Causes">
            <FIELD name="invalidStreamIdentifier" type="InvalidStreamIdentifier" uiShowName="Invalid Stream Identifier Cause" />
            <FIELD name="missingMandatoryParameter" type="MissingMandatoryParameter" uiShowName="Missing Mandatory Parameter  Cause" />
            <FIELD name="staleCookieError" type="StaleCookieError" uiShowName="Stale Cookie Error Cause" />
            <FIELD name="outOfResource" type="OutOfResource" uiShowName="Out Of Resource Cause" />
            <FIELD name="unresolvableAddress" type="UnresolvableAddress" uiShowName="Unresolvable Address Cause" />
            <FIELD name="unrecognizedChunk" type="UnrecognizedChunkType" uiShowName="Unrecognized Chunk Type Cause" />
            <FIELD name="invalidMandatoryParameter" type="InvalidMandatoryParameter" uiShowName="Invalid Mandatory Parameter Cause" />
            <FIELD name="unrecognizedParameters" type="UnrecognizedParameters" uiShowName="Unrecognized Parameters Cause" />
            <FIELD name="noUserData" type="NoUserData" uiShowName="No User Data Cause" />
            <FIELD name="cookieReceivedWhileShuttingDown" type="CookieReceivedWhileShuttingDown" uiShowName="Cookie Received While Shutting Down Cause" />
            <FIELD name="restartOfAnAssociationWithNewAddresses" type="RestartOfAnAssociationWithNewAddresses" uiShowName="Restart Of An Association With New Addresses Cause" />
            <FIELD name="userInitiatedAbort" type="UserInitiatedAbort" uiShowName="User-Initiated Abort Cause" />
            <FIELD name="protocolViolation" type="ProtocolViolation" uiShowName="Protocol Violation Cause" />
        </TYPE_DEF>

        <TYPE_DEF name="InvalidStreamIdentifier" type="SEQUENCE" unitType="ELEMENT" uiShowName="Invalid Stream Identifier">
            <FIELD name="causeCode" type="INTEGER" bitLength="16" defaultValue="1" uiShowName="Cause Code" />
            <FIELD name="causeLength" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="0" uiShowName="Cause Length" uiControlState="CALCULATED" binder="calculated" />
            <FIELD name="streamIdentifier" type="INTEGER" bitLength="16" defaultValue="0" uiShowName="Stream Identifier"/>
            <FIELD name="padding" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0" padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface" uiShowName="4 Byte Alignment Padding"/>
        </TYPE_DEF>

        <TYPE_DEF name="MissingMandatoryParameter" type="SEQUENCE" unitType="ELEMENT" uiShowName="Missing Mandatory Parameter">
            <FIELD name="causeCode" type="INTEGER" bitLength="16" defaultValue="2" uiShowName="Cause Code" />
            <FIELD name="causeLength" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="0" uiShowName="Cause Length" uiControlState="CALCULATED" binder="calculated" />
            <FIELD name="numberOfMissingParams" type="INTEGER" bitLength="32" defaultValue="0" uiShowName="Number of Missing Params"/>
            <FIELD name="missingParamTypes" type="MissingParamTypeList" uiShowName="Missing Param Types"></FIELD>
            <FIELD name="padding" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0" padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface" uiShowName="4 Byte Alignment Padding"/>
        </TYPE_DEF>

        <TYPE_DEF name="MissingParamTypeList" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1024" elementType="MissingParamType" unitType="ELEMENT"> </TYPE_DEF>

        <TYPE_DEF name="MissingParamType" type="SEQUENCE" unitType="ELEMENT" uiShowName="Missing Param Type" >
            <FIELD name="missingParamTypeValue" type="INTEGER" bitLength="16" defaultValue="0" uiShowName="Value"/>
        </TYPE_DEF>

        <TYPE_DEF name="StaleCookieError" type="SEQUENCE" unitType="ELEMENT" uiShowName="Stale Cookie Error">
            <FIELD name="causeCode" type="INTEGER" bitLength="16" defaultValue="3" uiShowName="Cause Code" />
            <FIELD name="causeLength" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="0" uiShowName="Cause Length" uiControlState="CALCULATED" binder="calculated" />
            <FIELD name="measureOfStaleness" type="INTEGER" bitLength="32" defaultValue="0" uiShowName="Measure of Staleness (usec)"/>
        </TYPE_DEF>

        <TYPE_DEF name="OutOfResource" type="SEQUENCE" unitType="ELEMENT" uiShowName="Out of Resource">
            <FIELD name="causeCode" type="INTEGER" bitLength="16" defaultValue="4" uiShowName="Cause Code" />
            <FIELD name="causeLength" type="INTEGER" bitLength="16" procfnTarget="SCTPCHUNKLENGTH" defaultValue="0" uiShowName="Cause Length" uiControlState="CALCULATED" binder="calculated" />
