<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="pdu_schema.xsd"
  name="RPR"
  showName="RPR"
  version="2.00"
  description="This is a sample PDU library for RPR as defined in XML Stream Editor.">

  <TYPE_DEFS>
	  <TYPE_DEF name="riValues" type="INTEGER" unitType="ELEMENT" bitLength="1" uiShowName="ri Values">
		  <VALUE_ENUMERATION>
			  <VAL_ENUM uiShowName="Transmitted on ringlet0" value="0"/>
			  <VAL_ENUM uiShowName="Transmitted on ringlet1" value="1"/>
		  </VALUE_ENUMERATION>
	  </TYPE_DEF>

	  <TYPE_DEF name="feValues" type="INTEGER" unitType="ELEMENT" bitLength="1" uiShowName="fe Values">
		  <VALUE_ENUMERATION>
			  <VAL_ENUM uiShowName="Not Fairness Eligible" value="0"/>
			  <VAL_ENUM uiShowName="Fairness Eligible" value="1"/>
		  </VALUE_ENUMERATION>
	  </TYPE_DEF>

	  <TYPE_DEF name="ftValues" type="INTEGER" unitType="ELEMENT" bitLength="2" uiShowName="ft Values">
		  <VALUE_ENUMERATION>
			  <VAL_ENUM uiShowName="Idle Frame" value="0"/>
			  <VAL_ENUM uiShowName="Control Frame" value="1"/>
			  <VAL_ENUM uiShowName="Fairness Frame" value="2"/>
			  <VAL_ENUM uiShowName="Data Frame" value="3"/>
		  </VALUE_ENUMERATION>
	  </TYPE_DEF>

	  <TYPE_DEF name="scValues" type="INTEGER" unitType="ELEMENT" bitLength="2" uiShowName="sc Values">
		  <VALUE_ENUMERATION>
			  <VAL_ENUM uiShowName="Class C" value="0"/>
			  <VAL_ENUM uiShowName="Class B" value="1"/>
			  <VAL_ENUM uiShowName="Class A, subclass A1" value="2"/>
			  <VAL_ENUM uiShowName="Class A, subclass A0" value="3"/>
		  </VALUE_ENUMERATION>
	  </TYPE_DEF>

	  <TYPE_DEF name="weValues" type="INTEGER" unitType="ELEMENT" bitLength="1" uiShowName="we Values">
		  <VALUE_ENUMERATION>
			  <VAL_ENUM uiShowName="Not Wrap Eligible" value="0"/>
			  <VAL_ENUM uiShowName="Wrap Eligible" value="1"/>
		  </VALUE_ENUMERATION>
	  </TYPE_DEF>

 	  <TYPE_DEF name="controlTypeValuesField" type="OCTETSTRING"  unitType="ELEMENT" byteLength="1" uiShowName="Control Type Value">
		  <VALUE_ENUMERATION>
			  <VAL_ENUM uiShowName="Station Attribute Discovery Frame" value="01" />
			  <VAL_ENUM uiShowName="Topology and Protection Protocol Frame" value="02" />
			  <VAL_ENUM uiShowName="Topology and Protection Checksum Frame" value="03" />
			  <VAL_ENUM uiShowName="Link Round Trip Time Measurement Request Frame" value="04" />
			  <VAL_ENUM uiShowName="Link Round Trip Time Measurement Response Frame" value="05" />
			  <VAL_ENUM uiShowName="Fairness Differential Delay Frame" value="06" />
			  <VAL_ENUM uiShowName="OAM Echo Request Frame" value="07" />
			  <VAL_ENUM uiShowName="OAM Echo Response Frame" value="08" />
			  <VAL_ENUM uiShowName="OAM Flush Frame" value="09" />
			  <VAL_ENUM uiShowName="OAM Organization Specific Frame" value="0A" />
		  </VALUE_ENUMERATION>
	  </TYPE_DEF>
		
	  <TYPE_DEF name="ffTypeValues" type="INTEGER" unitType="ELEMENT" bitLength="3" uiShowName="Fairness Frame Values">
		  <VALUE_ENUMERATION>
			  <VAL_ENUM uiShowName="SINGLE_CHOKE" value="0" />
			  <VAL_ENUM uiShowName="MULTI_CHOKE" value="1" />
			  <VAL_ENUM uiShowName="Reserved" value="7" />
		  </VALUE_ENUMERATION>
	  </TYPE_DEF>

	 <TYPE_DEF name="efValues" type="INTEGER" unitType="ELEMENT" bitLength="1" uiShowName="ef Values">
		  <VALUE_ENUMERATION>
			  <VAL_ENUM uiShowName="Not an Extended Frame" value="0"/>
			  <VAL_ENUM uiShowName="Extended Frame" value="1"/>
		  </VALUE_ENUMERATION>
	  </TYPE_DEF>

	  <TYPE_DEF name="fiValues" type="INTEGER" unitType="ELEMENT" bitLength="2" uiShowName="fi Values">
		  <VALUE_ENUMERATION>
			  <VAL_ENUM uiShowName="No Flood" value="0"/>
			  <VAL_ENUM uiShowName="Unidirectional Flood" value="1"/>
			  <VAL_ENUM uiShowName="Bidirectional Flood" value="2"/>
			  <VAL_ENUM uiShowName="Reserved Type of Flood" value="3"/>
		  </VALUE_ENUMERATION>
	  </TYPE_DEF>


	  <TYPE_DEF name="psValues" type="INTEGER" unitType="ELEMENT" bitLength="1" uiShowName="ps Values">
		  <VALUE_ENUMERATION>
			  <VAL_ENUM uiShowName="First Transmitted by Station" value="0"/>
			  <VAL_ENUM uiShowName="Wrapped Frame passes Source Station" value="1"/>
		  </VALUE_ENUMERATION>
	  </TYPE_DEF>

	  <TYPE_DEF name="baseControlField" type="SEQUENCE" unitType="ELEMENT" uiShowName="Base Control">
		  <FIELD name="ri" type="riValues" defaultValue="0" uiShowName="ri Values" />
		  <FIELD name="fe" type="feValues" defaultValue="0" uiShowName="fe Values" />
		  <FIELD name="ft" type="ftValues" defaultValue="00" uiShowName="ft Values" />
		  <FIELD name="sc" type="scValues" defaultValue="00" uiShowName="sc Values" />
		  <FIELD name="we" type="weValues" defaultValue="0" uiShowName="we Values" />
		  <FIELD name="parity" type="BITSTRING" bitLength="1" defaultValue="0" uiShowName="Parity" />
	  </TYPE_DEF>
  
	  <TYPE_DEF name="ffbaseControlField" type="SEQUENCE" unitType="ELEMENT" uiShowName="Base Control">
		  <FIELD name="ri" type="riValues" defaultValue="0" uiShowName="ri Values" />
		  <FIELD name="fe" type="feValues" defaultValue="0" uiShowName="fe Values" />
		  <FIELD name="ft" type="ftValues" defaultValue="00" uiShowName="ft Values" />
		  <FIELD name="sc" type="BITSTRING" bitLength="2" defaultValue="11" uiShowName="sc Values" />
		  <FIELD name="we" type="weValues" defaultValue="0" uiShowName="we Values" />
		  <FIELD name="parity" type="BITSTRING" bitLength="1" defaultValue="0" uiShowName="Parity" />
	  </TYPE_DEF>


	  <TYPE_DEF name="ibaseControlField" type="SEQUENCE" unitType="ELEMENT" uiShowName="Base Control">
		  <FIELD name="ri" type="riValues" defaultValue="0" uiShowName="ri Values" />
		  <FIELD name="fe" type="BITSTRING" bitLength="1" defaultValue="0" uiShowName="fe Value" />
		  <FIELD name="ft" type="ftValues" defaultValue="00" uiShowName="ft Values" />
		  <FIELD name="sc" type="BITSTRING" bitLength="2" defaultValue="11" uiShowName="sc Values" />
		  <FIELD name="we" type="weValues" defaultValue="0" uiShowName="we Values" />
		  <FIELD name="parity" type="BITSTRING" bitLength="1" defaultValue="0" uiShowName="Parity" />
	  </TYPE_DEF>

	 

	  <TYPE_DEF name="soValues" type="INTEGER" unitType="ELEMENT" bitLength="1" uiShowName="so Values">
		  <VALUE_ENUMERATION>
			  <VAL_ENUM uiShowName="Not Required to Provide Strict Ordering" value="0"/>
			  <VAL_ENUM uiShowName="Strict Ordering is Required" value="1"/>
		  </VALUE_ENUMERATION>
	  </TYPE_DEF>

	  <TYPE_DEF name="extendedControlField" type="SEQUENCE" unitType="ELEMENT" uiShowName="Extended Control">
		  <FIELD name="ef" type="efValues" defaultValue="0" uiShowName="ef Values"/>
		  <FIELD name="fi" type="fiValues" defaultValue="00" uiShowName="fi Values"/>
		  <FIELD name="ps" type="psValues" defaultValue="0" uiShowName="ps Values"/>
		  <FIELD name="so" type="soValues" defaultValue="0" uiShowName="so Values"/>
		  <FIELD name="res" type="BITSTRING" bitLength="3" defaultValue="000" uiShowName="Reserved"/>
	  </TYPE_DEF>

	  <TYPE_DEF name="EXTHDRSIZE" type="SEQUENCE" unitType="ELEMENT" uiShowName="Extended Header">
		  <FIELD name="daExtended" type="MACADDR"
			  defaultValue="00:00:01:00:00:01" 
			  uiShowName="Destination Address Extended" />

		  <FIELD name="saExtended" type="MACADDR"
			  defaultValue="00:00:01:00:00:01" 
			  uiShowName="Source Address Extended" />

	  </TYPE_DEF>

	  <TYPE_DEF name="EXTHDR" type="CHOICE"  unitType="ELEMENT" uiShowName="Extended Header">
		  <FIELD name="ExtendHeaderSize" type="EXTHDRSIZE" uiShowName="Extended Header"/>
	  </TYPE_DEF>

	  <TYPE_DEF name="ExtHeadSize" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1" elementType="EXTHDR" unitType="ELEMENT"/>

	  <TYPE_DEF name="DataMin" type="SEQUENCE" unitType="ELEMENT" uiShowName="Minimum Data Frame Size">
		  <FIELD name="min" type="OCTETSTRING" byteLength="0" uiShowName="Minimum Data Frame Size"/>
	  </TYPE_DEF>

	  <TYPE_DEF name="ReservedData" type="SEQUENCE" unitType="ELEMENT" uiShowName="Pad Data">
		  <FIELD name="reserved" type="OCTETSTRING" minByteLength="0" maxByteLength="92" uiShowName="reserved"/>
	</TYPE_DEF>


	<TYPE_DEF name="ReservedDataSelect" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1" elementType="ReservedData" unitType="ELEMENT"/>

	  <TYPE_DEF name="REGULARMAX" type="SEQUENCE" unitType="ELEMENT" uiShowName="Regular Data Frame Size">
		  <FIELD name="reg" type="OCTETSTRING" byteLength="1500" uiShowName="Service Data Unit Field"/>
		  <FIELD name="theReservedDataSelect" type="ReservedDataSelect" uiShowName="reserved"/>
	  </TYPE_DEF>

	  <TYPE_DEF name="JUMBOMAX" type="SEQUENCE" unitType="ELEMENT" uiShowName="Jumbo Data Frame Size">
		  <FIELD name="jumbo" type="OCTETSTRING" byteLength="9100" uiShowName="Service Data Unit Field"/>
		  <FIELD name="theReservedDataSelect" type="ReservedDataSelect" uiShowName="reserved"/>

	  </TYPE_DEF>

	  <TYPE_DEF name="DataFrameSizeChoice" type="CHOICE" unitType="ELEMENT" uiShowName="Data Frame Size">
		  <FIELD name="dataMin" type="DataMin" uiShowName="Minimum Data Frame Size" />
		  <FIELD name="regMax" type="REGULARMAX" uiShowName="Regular Data Frame Size"/>
		  <FIELD name="jumboMax" type="JUMBOMAX" uiShowName="Jumbo Data Frame Size"/>
		  </TYPE_DEF>
	  
	  <TYPE_DEF name="ControlFrameSizeChoice" type="CHOICE" unitType="ELEMENT" uiShowName="Data Frame Size">
		  <FIELD name="dataMin" type="DataMin" uiShowName="Minimum Data Frame Size" />
		  <FIELD name="regMax" type="REGULARMAX" uiShowName="Regular Data Frame Size"/>
	  </TYPE_DEF>


	  <TYPE_DEF name="rprCommonHeader" type="SEQUENCE" unitType="ELEMENT" uiShowName="RPR Common Header Fields">
		 
		  <FIELD name="ttl" type="OCTETSTRING" byteLength="1" defaultValue="1" uiShowName="Time To Live" />
		  <FIELD name="baseControl" type="baseControlField" uiShowName="Base Control"/>

		  <FIELD name="dstMac" type="MACADDR"
			  defaultValue="00:00:01:00:00:01"
			  uiNoShowModifier="Interface"
			  uiNoShow="Interface"
			  uiControlState="ALLOW_OVERRIDE" 
			  uiShowName="Destination MAC" 
			  isNiMapped="true" />

		  <FIELD name="srcMac" type="MACADDR"
			  defaultValue="00:00:01:00:00:02"
			  uiControlState="ALLOW_OVERRIDE"  
			  uiShowName="Source MAC" 
			  isNiMapped="true"
			  isNiPrimaryKey="true"/>

		  <FIELD name="ttlBase" type="OCTETSTRING" byteLength="1" defaultValue="1" uiShowName="Time To Live Base" />
		  <FIELD name="extendedControl" type="extendedControlField" uiShowName="Extended Control" />

		  <FIELD name="hec" type="INTEGER" bitLength="16"
			  defaultValue="0"
			  uiControlState="ALLOW_OVERRIDE"
			  procfnTarget="HDRCHECKSUM" 
			  uiNoShowModifier="Interface,Frame,Filter"
			  uiShowName="Checksum" />
	  </TYPE_DEF>

	

	  <TYPE_DEF name="fairnessHeaderField" type="SEQUENCE" unitType="ELEMENT" uiShowName="Fairness Header">

		  <FIELD name="ffType" type="ffTypeValues" defaultValue="000" uiShowName="Fairness Frame Values"/>
		  <FIELD name="res" type="BITSTRING" bitLength="13" defaultValue="0000000000000" uiShowName="Reserved"/>
	  </TYPE_DEF>
	  <TYPE_DEF name="DataFrame" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="Data Frame" niMapped="true">

		  <FIELD name="rch" type="rprCommonHeader" uiShowName="RPR Common Header"/>
		  <FIELD name="extendedHeader" type="ExtHeadSize"  uiShowName="Extended Header"/>
		  <FIELD name="protocolType" type="INTEGER" bitLength="16" defaultValue="1536" uiShowName="Protocol Type"/>
		  <FIELD name="fs" type="DataFrameSizeChoice" defaultValue="dataMin" uiShowName="Service Data Unit Field Size"/>

	  </TYPE_DEF>

	  <TYPE_DEF name="ControlFrame" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="Control Frame" niMapped="true">

		  <FIELD name="rch" type="rprCommonHeader" uiShowName="RPR Common Header"/>
		  <FIELD name="extendedHeader" type="ExtHeadSize"  uiShowName="Extended Header"/>
		  <FIELD name="controlTypeValue" type="controlTypeValuesField" defaultValue="01" uiShowName="Control Type Value" />
		  <FIELD name="controlVersion" type="OCTETSTRING" byteLength="1" defaultValue="00" uiShowName="Control Version" />
		  <FIELD name="ControlFrameSize" type="ControlFrameSizeChoice" defaultValue="dataMin" uiShowName="Control Data Unit Field Size"/>

	  </TYPE_DEF>		

	  <TYPE_DEF name="FairnessFrame" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="Fairness Frame" niMapped="true">
		 <FIELD name="ttl" type="OCTETSTRING" byteLength="1" defaultValue="1" uiShowName="Time To Live" />
		 <FIELD name="baseControl" type="ffbaseControlField" uiShowName="Base Control"/>

		 <FIELD name="srcMac" type="MACADDR"
			  defaultValue="00:00:01:00:00:02"
			  uiControlState="ALLOW_OVERRIDE"  
			  uiShowName="Source MAC Compact" 
			  isNiMapped="true"
			  isNiPrimaryKey="true"/>

		  <FIELD name="FairnessHeader" type="fairnessHeaderField" uiShowName="Fairness Header"/>
		  <FIELD name="fairRate" type="OCTETSTRING" byteLength="2" defaultValue="0000" uiShowName="Fair Rate"/>

	  </TYPE_DEF>

	  <TYPE_DEF name="IdleFrame" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="Idle Frame" niMapped="true">
		  <FIELD name="ttl" type="OCTETSTRING" byteLength="1" defaultValue="1" uiShowName="Time To Live" />
		  <FIELD name="baseControl" type="ibaseControlField" uiShowName="Base Control"/>
		  
		  <FIELD name="srcMac" type="MACADDR"
			  defaultValue="00:00:01:00:00:02"
			  uiControlState="ALLOW_OVERRIDE"  
			  uiShowName="Source MAC Compact" 
			  isNiMapped="true"
			  isNiPrimaryKey="true"/>

		  <FIELD name="idlePayLoad" type="OCTETSTRING" byteLength="4" defaultValue="00000000" uiShowName="Idle Payload"/>

	  </TYPE_DEF>


  </TYPE_DEFS>
  </PDU_LIBRARY>


		


