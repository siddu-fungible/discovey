<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="pdu_schema.xsd" name="icmp" showName="Icmp" version="1.00"
    description="This is a sample PDU library for Icmp as defined in XML Stream Editor.">

  <TYPE_DEFS>

    <TYPE_DEF name="IcmpType" type="INTEGER" bitLength="8" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="Echo Reply" value="0" />
        <VAL_ENUM uiShowName="Destination Unreachable" value="3" />
        <VAL_ENUM uiShowName="Source Quench" value="4" />
        <VAL_ENUM uiShowName="Redirect" value="5" />
        <VAL_ENUM uiShowName="Echo Request" value="8" />
		<VAL_ENUM uiShowName="Router Advertisement" value="9" />
		<VAL_ENUM uiShowName="Router Solicitation" value="10" />
        <VAL_ENUM uiShowName="Time Exceeded" value="11" />
        <VAL_ENUM uiShowName="Parameter Problem" value="12" />
        <VAL_ENUM uiShowName="Timestamp Request" value="13" />
        <VAL_ENUM uiShowName="Timestamp Reply" value="14" />
        <VAL_ENUM uiShowName="Information Request" value="15" />
        <VAL_ENUM uiShowName="Information Reply" value="16" />
		<VAL_ENUM uiShowName="Address Mask Request" value="17" />
		<VAL_ENUM uiShowName="Address Mask Reply" value="18" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>

    <TYPE_DEF name="IcmpIpData" type="SEQUENCE" unitType="ELEMENT" >
      <FIELD name="ipHdr" type="ipv4:IPv4" uiShowName="IP Header" />
      <FIELD name="data" type="OCTETSTRING"
             minByteLength="0" maxByteLength="1000"
             defaultValue="0000000000000000"
             uiShowName="Data" />

    </TYPE_DEF>
    
    <TYPE_DEF name="IcmpDestUnreachCode" type="INTEGER" bitLength="8" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="Net Unreachable" value="0" />
        <VAL_ENUM uiShowName="Host Unreachable" value="1" />
        <VAL_ENUM uiShowName="Protocol Unreachable" value="2" />
        <VAL_ENUM uiShowName="Port Unreachable" value="3" />
        <VAL_ENUM uiShowName="Fragmentation Needed" value="4" />
        <VAL_ENUM uiShowName="Source Route Failed" value="5" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>
    
    <TYPE_DEF name="IcmpDestUnreach" type="SEQUENCE" unitType="MESSAGE"
        level="3" uiShowName="Icmp Destination Unreachable" >
      <FIELD name="type" type="IcmpType"
         defaultValue="3"
         uiShowName="Type" />
      <FIELD name="code" type="IcmpDestUnreachCode"
         defaultValue="0"
         uiShowName="Code" />
      <FIELD name="checksum" type="INTEGER" bitLength="16"
          defaultValue="0"
          uiControlState="ALLOW_OVERRIDE"
          procfnTarget="ICMPCHECKSUM"
          uiNoShowModifier="Interface,Frame,Filter"
          uiShowName="Checksum" />
      <FIELD name="unused" type="INTEGER" bitLength="32"
         defaultValue="0"
         uiShowName="Unused" />
      <FIELD name="ipData" type="IcmpIpData"
         uiShowName="IP + Data" />
    </TYPE_DEF>

    <TYPE_DEF name="IcmpTimeExceededCode" type="INTEGER" bitLength="8" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="Time To Live Exceeded" value="0" />
        <VAL_ENUM uiShowName="Fragment Reassembly Time Exceeded" value="1" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>

    <TYPE_DEF name="IcmpTimeExceeded" type="SEQUENCE" unitType="MESSAGE"
        level="3" uiShowName="Icmp Time Exceeded" >
      <FIELD name="type" type="IcmpType"
         defaultValue="11"
         uiShowName="Type" />
      <FIELD name="code" type="IcmpTimeExceededCode"
         defaultValue="0"
         uiShowName="Code" />
      <FIELD name="checksum" type="INTEGER" bitLength="16"
          defaultValue="0"
          uiControlState="ALLOW_OVERRIDE"
          procfnTarget="ICMPCHECKSUM"
          uiNoShowModifier="Interface,Frame,Filter"
          uiShowName="Checksum" />
      <FIELD name="unused" type="INTEGER" bitLength="32"
         defaultValue="0"
         uiShowName="Unused" />
      <FIELD name="ipData" type="IcmpIpData"
         uiShowName="IP + Data" />
    </TYPE_DEF>

    <TYPE_DEF name="IcmpParameterProblemCode" type="INTEGER" bitLength="8" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="Pointer Indicates Error" value="0" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>

    <TYPE_DEF name="IcmpParameterProblem" type="SEQUENCE" unitType="MESSAGE"
        level="3" uiShowName="Icmp Parameter Problem" >
      <FIELD name="type" type="IcmpType"
         defaultValue="12"
         uiShowName="Type" />
      <FIELD name="code" type="IcmpParameterProblemCode"
         defaultValue="0"
         uiShowName="Code" />
      <FIELD name="checksum" type="INTEGER" bitLength="16"
          defaultValue="0"
          uiControlState="ALLOW_OVERRIDE"
          procfnTarget="ICMPCHECKSUM"
          uiNoShowModifier="Interface,Frame,Filter"
          uiShowName="Checksum" />
      <FIELD name="pointer" type="INTEGER" bitLength="8"
         defaultValue="0"
         uiShowName="Pointer" />
      <FIELD name="unused" type="INTEGER" bitLength="24"
         defaultValue="0"
         uiShowName="Unused" />
      <FIELD name="ipData" type="IcmpIpData"
         uiShowName="IP + Data" />
    </TYPE_DEF>


    <TYPE_DEF name="IcmpSourceQuench" type="SEQUENCE" unitType="MESSAGE"
        level="3" uiShowName="Icmp Source Quench" >
      <FIELD name="type" type="IcmpType"
         defaultValue="4"
         uiShowName="Type" />
      <FIELD name="code" type="INTEGER" bitLength="8"
         defaultValue="0"
         uiShowName="Code" />
      <FIELD name="checksum" type="INTEGER" bitLength="16"
          defaultValue="0"
          uiControlState="ALLOW_OVERRIDE"
          procfnTarget="ICMPCHECKSUM"
          uiNoShowModifier="Interface,Frame,Filter"
          uiShowName="Checksum" />
      <FIELD name="unused" type="INTEGER" bitLength="32"
