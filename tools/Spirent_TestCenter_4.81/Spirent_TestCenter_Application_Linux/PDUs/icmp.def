         uiShowName="Unused" />
      <FIELD name="ipData" type="IcmpIpData"
         uiShowName="IP + Data" />
    </TYPE_DEF>

    <TYPE_DEF name="IcmpRedirectCode" type="INTEGER" bitLength="8" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="Redirect Datagrams for Network" value="0" />
        <VAL_ENUM uiShowName="Redirect Datagrams for Host" value="1" />
        <VAL_ENUM uiShowName="Redirect Datagrams for ToS and Network" value="2" />
        <VAL_ENUM uiShowName="Redirect Datagrams for ToS and Host" value="3" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>

    <TYPE_DEF name="IcmpRedirect" type="SEQUENCE" unitType="MESSAGE"
        level="3" uiShowName="Icmp Redirect" >
      <FIELD name="type" type="IcmpType"
         defaultValue="5"
         uiShowName="Type" />
      <FIELD name="code" type="IcmpRedirectCode"
         defaultValue="0"
         uiShowName="Code" />
	  <FIELD name="checksum" type="INTEGER" bitLength="16"
          defaultValue="0"
          uiControlState="ALLOW_OVERRIDE"
          procfnTarget="ICMPCHECKSUM"
          uiNoShowModifier="Interface,Frame,Filter"
          uiShowName="Checksum" />
      <FIELD name="gateway" type="IPV4ADDR"
         defaultValue="192.0.0.1"
         uiShowName="Gateway Address" /> 
      <FIELD name="ipData" type="IcmpIpData"
         uiShowName="IP + Data" />
    </TYPE_DEF>

    <TYPE_DEF name="IcmpEchoRequest" type="SEQUENCE" unitType="MESSAGE"
        level="3" uiShowName="Icmp Echo Request" >
      <FIELD name="type" type="IcmpType"
         defaultValue="8"
         uiShowName="Type" />
      <FIELD name="code" type="INTEGER" bitLength="8"
         defaultValue="0"
         uiShowName="Code" />
      <FIELD name="checksum" type="INTEGER" bitLength="16"
