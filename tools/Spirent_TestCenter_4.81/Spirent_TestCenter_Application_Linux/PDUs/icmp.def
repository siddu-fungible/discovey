	<TYPE_DEF name="RouterAddressStructure" type="SEQUENCE" unitType="ELEMENT" uiShowName="Icmp Router Address" >
	  <FIELD name="routerAddress" type="IPV4ADDR"
          defaultValue="225.0.0.1" 
          uiShowName="Router Address" />
	  <FIELD name="preferenceLevel" type="INTEGER" bitLength="32"
          defaultValue="0"
          uiShowName="Preference Level" />
    </TYPE_DEF>

    <TYPE_DEF name="RouterAddressList" type="SET_OF" elementType="RouterAddressStructure"
        minOccurs="0" maxOccurs="1000" unitType="ELEMENT" />
		
