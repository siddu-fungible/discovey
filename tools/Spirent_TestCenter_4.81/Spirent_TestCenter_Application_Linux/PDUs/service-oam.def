<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="pdu_schema.xsd"
  name="serviceoam"
  showName="Ethernet OAM CFM"
  version="1.01"
  description="This is a sample PDU library for Ethernet OAM as defined in XML Stream Editor.">

<TYPE_DEFS>
                        
   <!-- Common Elements =================================================== -->
	<TYPE_DEF name="CFMlv" type="SEQUENCE" unitType="ELEMENT" uiShowName="Connectivity Fault Management">

		<FIELD name="MDlevel" type="INTEGER" defaultValue="0" minValue="0" maxValue="7" uiShowName="MD Level" 
			bitLength="3" />

		<FIELD name="Version" type="BITSTRING" bitLength="5"
		defaultValue="00000"
		uiShowName="Version" />
	</TYPE_DEF>
        
   <!-- =================================================================== -->
   <!-- TLV Elements ====================================================== -->
  <TYPE_DEF name="IanaFamilyValues" type="OCTETSTRING" byteLength="1" unitType="ELEMENT">
    <VALUE_ENUMERATION>
      <VAL_ENUM uiShowName="IPv4" value="01" />
      <VAL_ENUM uiShowName="IPv6" value="02" />
    </VALUE_ENUMERATION>
  </TYPE_DEF>

   <!-- Org Specific TLV ================================================== -->
	<TYPE_DEF name="Value" type="SEQUENCE" unitType="ELEMENT" uiShowName="Value">
		<FIELD name="Value" type="OCTETSTRING" 
			uiNoShowModifier="Interface,Frame" minByteLength="0"
		       	maxByteLength="65535" uiShowName="Value" />
	</TYPE_DEF>

	<TYPE_DEF name="ValueSelect" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1" elementType="Value" unitType="ELEMENT"/>
   
	<TYPE_DEF name="OrgSpecTLVFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="Organization-Specific TLV">
		<FIELD name="Type" type="OCTETSTRING" byteLength="1"
			uiNoShow="Interface" uiShowCommon="false"
			defaultValue="1F" uiShowName="Type"/>

		<FIELD name="Length" type="OCTETSTRING" byteLength="2" procfnTarget="EOAMTLVLENGTH"
			defaultValue="0000" uiShowName="Length" uiControlState="CALCULATED"/>

		<FIELD name="OUI" type="OCTETSTRING" byteLength="3"
			defaultValue="000000" uiShowName="OUI" />

		<FIELD name="SubType" type="OCTETSTRING" byteLength="1"
		   defaultValue="00" uiShowName="Sub-Type" />

		<FIELD name="theValue" type="ValueSelect" uiShowName="Value"/>
	</TYPE_DEF>
                      
   <!-- Sender ID TLV ===================================================== -->
   <!-- Chassis ID -->
   <TYPE_DEF name="ChassisIDSubTypeValues" type="OCTETSTRING" byteLength="1" unitType="ELEMENT">
      <VALUE_ENUMERATION>
         <VAL_ENUM uiShowName="Chassis Component" value="01" />
         <VAL_ENUM uiShowName="Interface Alias" value="02" />
         <VAL_ENUM uiShowName="Port Component" value="03" />
         <VAL_ENUM uiShowName="MAC Address" value="04" />
         <VAL_ENUM uiShowName="Network Address" value="05" />
         <VAL_ENUM uiShowName="Interface Name" value="06" />
         <VAL_ENUM uiShowName="Locally Assigned" value="07" />
      </VALUE_ENUMERATION>
   </TYPE_DEF>
   
   <TYPE_DEF name="CIDChassisComponentFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="Chassis Component">
      <FIELD name="ChassisIDSubtype" type="ChassisIDSubTypeValues" byteLength="1"
			defaultValue="01" uiShowName="Chassis ID Subtype" uiControlState="CALCULATED" />

      <FIELD name="ChassisID" type="ASCII"  minByteLength="1"
			maxByteLength="64" defaultValue="SPIRENT" uiShowName="Chassis ID" />
   </TYPE_DEF>

   <TYPE_DEF name="CIDInterfaceAliasFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="Interface Alias">
      <FIELD name="ChassisIDSubtype" type="ChassisIDSubTypeValues" byteLength="1"
			defaultValue="02" uiShowName="Chassis ID Subtype" uiControlState="CALCULATED" />

      <FIELD name="ChassisID" type="ASCII" minByteLength="1"
			maxByteLength="128" defaultValue="SPIRENT" uiShowName="Chassis ID" />
   </TYPE_DEF>

   <TYPE_DEF name="CIDPortComponentFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="Port Component">
      <FIELD name="ChassisIDSubtype" type="ChassisIDSubTypeValues" byteLength="1"
			defaultValue="03" uiShowName="Chassis ID Subtype" uiControlState="CALCULATED" />

      <FIELD name="ChassisID" type="ASCII" minByteLength="1"
			maxByteLength="64" defaultValue="SPIRENT" uiShowName="Chassis ID" />
   </TYPE_DEF>

   <TYPE_DEF name="CIDMacAddressFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="MAC Address">
      <FIELD name="ChassisIDSubtype" type="ChassisIDSubTypeValues" byteLength="1"
		   defaultValue="04" uiShowName="Chassis ID Subtype" uiControlState="CALCULATED" />

      <FIELD name="ChassisID" type="MACADDR" uiShowName="Chassis ID" />
   </TYPE_DEF>

   <TYPE_DEF name="CIDNetworkAddress4Format" type="SEQUENCE" unitType="ELEMENT" uiShowName="Network Address IPv4">
      <FIELD name="ChassisIDSubtype" type="ChassisIDSubTypeValues" byteLength="1"
			defaultValue="05" uiShowName="Chassis ID Subtype" uiControlState="CALCULATED" />

      <FIELD name="IanaFamilyNumber" type="IanaFamilyValues" byteLength="1"
      defaultValue="01" uiShowName="IANA Family Number" uiControlState="CALCULATED" />

      <FIELD name="ChassisID" type="IPV4ADDR" uiShowName="Chassis ID" />
   </TYPE_DEF>

   <TYPE_DEF name="CIDNetworkAddress6Format" type="SEQUENCE" unitType="ELEMENT" uiShowName="Network Address IPv6">
      <FIELD name="ChassisIDSubtype" type="ChassisIDSubTypeValues" byteLength="1"
			defaultValue="05" uiShowName="Chassis ID Subtype" uiControlState="CALCULATED" />

      <FIELD name="IanaFamilyNumber" type="IanaFamilyValues" byteLength="1"
      defaultValue="02" uiShowName="IANA Family Number" uiControlState="CALCULATED" />

      <FIELD name="ChassisID" type="IPV6ADDR" uiShowName="Chassis ID" />
   </TYPE_DEF>

   <TYPE_DEF name="CIDInterfaceNameFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="Interface Name">
      <FIELD name="ChassisIDSubtype" type="ChassisIDSubTypeValues" byteLength="1"
			defaultValue="06" uiShowName="Chassis ID Subtype" uiControlState="CALCULATED" />

      <FIELD name="ChassisID" type="ASCII" minByteLength="1"
			maxByteLength="255" defaultValue="SPIRENT" uiShowName="Chassis ID" />
   </TYPE_DEF>

   <TYPE_DEF name="CIDLocallyAssignedFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="Locally Assigned">
      <FIELD name="ChassisIDSubtype" type="ChassisIDSubTypeValues" byteLength="1"
			defaultValue="07" uiShowName="Chassis ID Subtype" uiControlState="CALCULATED" />

      <FIELD name="ChassisID" type="ASCII" minByteLength="1"
			maxByteLength="255" defaultValue="SPIRENT" uiShowName="Chassis ID" />
   </TYPE_DEF>

   <TYPE_DEF name="CIDCustomFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="Custom">
      <FIELD name="ChassisIDSubtype" type="OCTETSTRING" byteLength="1"
			defaultValue="00" uiShowName="Chassis ID Subtype" />

      <FIELD name="ChassisID" type="OCTETSTRING" minByteLength="1"
	      maxByteLength="255" defaultValue="00" uiShowName="Chassis ID" />
   </TYPE_DEF>
   
   <TYPE_DEF name="ChassisIDList" type="CHOICE" unitType="ELEMENT" uiShowName="Chassis ID">
      <FIELD name="ChassisComponent" type="CIDChassisComponentFormat"
			uiShowName="Chassis Component" />
      <FIELD name="InterfaceAlias" type="CIDInterfaceAliasFormat"
			uiShowName="Interface Alias" />
      <FIELD name="PortComponent" type="CIDPortComponentFormat"
         uiShowName="Port Component" />
      <FIELD name="MacAddress" type="CIDMacAddressFormat"
         uiShowName="MAC Address" />
      <FIELD name="NetworkAddress4" type="CIDNetworkAddress4Format"
         uiShowName="Network Address IPv4" />
      <FIELD name="NetworkAddress6" type="CIDNetworkAddress6Format"
         uiShowName="Network Address IPv6" />
      <FIELD name="InterfaceName" type="CIDInterfaceNameFormat"
         uiShowName="Interface Name" />
      <FIELD name="LocallyAssigned" type="CIDLocallyAssignedFormat"
         uiShowName="Locally Assigned" />
      <FIELD name="Custom" type="CIDCustomFormat"
         uiShowName="Custom" />
   </TYPE_DEF>

   <TYPE_DEF name="ChassisIDSelect" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1" elementType="ChassisIDList" unitType="ELEMENT"/>
   
   <!-- Management Address Domain -->
   <TYPE_DEF name="IPv4Domain" type="SEQUENCE" unitType="ELEMENT" uiShowName="IPv4 Domain">
      <FIELD name="IPv4" type="IPV4ADDR" uiShowName="IPv4 Address" />
      <FIELD name="Port" type="OCTETSTRING" byteLength="02" defaultValue="0000" uiShowName="Port" />
   </TYPE_DEF>

   <TYPE_DEF name="IPv6Domain" type="SEQUENCE" unitType="ELEMENT" uiShowName="IPv6 Domain">
      <FIELD name="IPv6" type="IPV6ADDR" uiShowName="IPv6 Address" />
      <FIELD name="Port" type="OCTETSTRING" byteLength="02" defaultValue="0000" uiShowName="Port" />
   </TYPE_DEF>

   <TYPE_DEF name="IPv4ZoneDomain" type="SEQUENCE" unitType="ELEMENT" uiShowName="IPv4 Zone Domain">
      <FIELD name="IPv4" type="IPV4ADDR" uiShowName="IPv4 Address" />
      <FIELD name="Zone" type="OCTETSTRING" byteLength="04" defaultValue="00000000" uiShowName="Zone" />
      <FIELD name="Port" type="OCTETSTRING" byteLength="02" defaultValue="0000" uiShowName="Port" />
   </TYPE_DEF>

   <TYPE_DEF name="IPv6ZoneDomain" type="SEQUENCE" unitType="ELEMENT" uiShowName="IPv6 Zone Domain">
      <FIELD name="IPv6" type="IPV6ADDR" uiShowName="IPv6 Address" />
      <FIELD name="Zone" type="OCTETSTRING" byteLength="04" defaultValue="00000000" uiShowName="Zone" />
      <FIELD name="Port" type="OCTETSTRING" byteLength="02" defaultValue="0000" uiShowName="Port" />
   </TYPE_DEF>

   <TYPE_DEF name="StringPortDomain" type="SEQUENCE" unitType="ELEMENT" uiShowName="Domain">
      <FIELD name="Name" type="ASCII" minByteLength="1" maxByteLength="255" defaultValue="SPIRENT"
         uiShowName="Name" />
      <FIELD name="Port" type="OCTETSTRING" byteLength="02" defaultValue="0000" uiShowName="Port" />
   </TYPE_DEF>
   
   <TYPE_DEF name="MADtDU4Format" type="SEQUENCE" unitType="ELEMENT" uiShowName="transportDomainUdpIpv4">
      <FIELD name="MADL" type="OCTETSTRING" byteLength="01"
