<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="pdu_schema.xsd" name="lspping" showName="LSP Ping" version="1.00"
    description="This is a sample PDU library for LSP Ping as defined in XML Stream Editor." userdefined="false">

  <TYPE_DEFS>

    <TYPE_DEF name="MessageTypeType" type="INTEGER" bitLength="8" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="1-MPLS Echo Request" value="1"/>
        <VAL_ENUM uiShowName="2-MPLS Echo Reply" value="2"/>
      </VALUE_ENUMERATION>
    </TYPE_DEF>
    
    <TYPE_DEF name="MessageTypeRequest" type="INTEGER" bitLength="8" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="MPLS Echo Request" value="1"/>        
      </VALUE_ENUMERATION>
    </TYPE_DEF>
    
    <TYPE_DEF name="MessageTypeReply" type="INTEGER" bitLength="8" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="MPLS Echo Reply" value="2"/>
      </VALUE_ENUMERATION>
    </TYPE_DEF>

    <TYPE_DEF name="ReplyModeType" type="INTEGER" bitLength="8" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="1-Do not reply" value="1"/>
        <VAL_ENUM uiShowName="2-Reply via an IPv4/IPv6 UDP Packet" value="2"/>
        <VAL_ENUM uiShowName="3-Reply via an IPv4/IPv6 UDP Packet with Router Alert" value="3"/>
        <VAL_ENUM uiShowName="4-Reply via application level control channel" value="4"/>        
      </VALUE_ENUMERATION>
    </TYPE_DEF>    
    
    <TYPE_DEF name="ReturnCodeType" type="INTEGER" bitLength="8" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="0-No return code" value="0"/>
        <VAL_ENUM uiShowName="1-Malformed echo request received" value="1"/>
        <VAL_ENUM uiShowName="2-One or more of the TLVs was not understood" value="2"/>
        <VAL_ENUM uiShowName="3-Replying router is an egress for the FEC at stack-depth" value="3"/>
        <VAL_ENUM uiShowName="4-Replying router has no mapping for the FEC at stack-depth" value="4"/>
        <VAL_ENUM uiShowName="5-Downstream Mapping Mismatch" value="5"/>
        <VAL_ENUM uiShowName="6-Upstream Interface Index Unknown" value="6"/>
        <VAL_ENUM uiShowName="7-Reserved" value="7"/>
        <VAL_ENUM uiShowName="8-Label switched at stack-depth" value="8"/>
        <VAL_ENUM uiShowName="9-Label switched but no MPLS forwarding at stack-depth" value="9"/>
        <VAL_ENUM uiShowName="10-Mapping for this FEC is not the given label at stack-depth" value="10"/>
        <VAL_ENUM uiShowName="11-No label entry at stack-depth" value="11"/>
        <VAL_ENUM uiShowName="12-Protocol not associated with interface at FEC stack-depth" value="12"/>
        <VAL_ENUM uiShowName="13-Premature termination of ping due to label stack shrinking to a single label" value="13"/>
      </VALUE_ENUMERATION>
    </TYPE_DEF>

    <TYPE_DEF name="AddressTypeType" type="INTEGER" bitLength="8" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="1-IPv4 Numbered" value="1"/>
        <VAL_ENUM uiShowName="2-IPv4 Unnumbered" value="2"/>
        <VAL_ENUM uiShowName="3-IPv6 Numbered" value="3"/>
        <VAL_ENUM uiShowName="4-IPv6 Unnumbered" value="4"/>        
      </VALUE_ENUMERATION>
    </TYPE_DEF>    

    <TYPE_DEF name="MultiPathTypeType" type="INTEGER" bitLength="8" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="0-no multipath" value="0"/>
        <VAL_ENUM uiShowName="2-IP address" value="2"/>
        <VAL_ENUM uiShowName="4-IP address range" value="4"/>
        <VAL_ENUM uiShowName="8-Bit masked IP address set" value="8"/>        
        <VAL_ENUM uiShowName="9-Bit masked label set" value="9"/>        
        </VALUE_ENUMERATION>
    </TYPE_DEF>    
    
    <TYPE_DEF name="ProtocolType" type="INTEGER" bitLength="8" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="0-Unknown" value="0"/>
        <VAL_ENUM uiShowName="1-Static" value="1"/>
        <VAL_ENUM uiShowName="2-BGP" value="2"/>
        <VAL_ENUM uiShowName="3-LDP" value="3"/>        
        <VAL_ENUM uiShowName="4-RSVP-TE" value="4"/>        
        </VALUE_ENUMERATION>
    </TYPE_DEF>    

    <TYPE_DEF name="OptionType" type="INTEGER" bitLength="8" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="1-Drop Pad TLV from reply" value="1"/>
        <VAL_ENUM uiShowName="2-Copy Pad TLV to reply" value="2"/>
      </VALUE_ENUMERATION>
    </TYPE_DEF>    


    <TYPE_DEF name="GlobalFlagType" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="Global Flags Bits" >
      <FIELD name="reserved" type="BITSTRING" bitLength="15"
            defaultValue="000000000000000"
            uiShowName="Reserved" />
      <FIELD name="validateFecStackBit" type="BITSTRING" bitLength="1"
            defaultValue="0"
            uiShowName="V (Validate FEC Stack) Bit" />
    </TYPE_DEF>    
    
    <TYPE_DEF name="LspPingHeader" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="LSP Ping Header" >

      <FIELD name="version" type="INTEGER" bitLength="16" defaultValue="1"
          uiShowName="LSP Ping Version"  uiShowCommon="false"/>

      <FIELD name="globalFlags" type="GlobalFlagType"
          uiShowName="Global Flags" uiShowCommon ="false"/>
          
      <FIELD name ="messageType" type ="MessageTypeType" defaultValue="1"
             uiShowName="Message Type" />

      <FIELD name ="replyMode" type ="ReplyModeType" defaultValue="2"
             uiShowName="Reply Mode" />             

      <FIELD name ="returnCode" type ="INTEGER" bitLength="8" defaultValue="0"  
             uiShowName="Return Code" />

      <FIELD name ="returnSubcode" type ="INTEGER" bitLength="8" defaultValue="0"  
             uiShowName="Return Subcode" />             

      <FIELD name ="senderHandle" type ="INTEGER" bitLength="32" defaultValue="1"  
             uiShowName="Sender Handle" />    
    
      <FIELD name ="sequenceNum" type ="INTEGER" bitLength="32" defaultValue="1"  
             uiShowName="Sequence Number" />

      <FIELD name="timeStampSent" type="OCTETSTRING" byteLength="8" defaultValue="0000000000000000"
        uiShowName="TimeStamp Sent"/>    

      <FIELD name="timeStampReceived" type="OCTETSTRING" byteLength="8" defaultValue="0000000000000000"
        uiShowName="TimeStamp Received"/>
        
    </TYPE_DEF>
    
    <TYPE_DEF name="LspPingRequest" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="LSP Ping Request Header" >

      <FIELD name="version" type="INTEGER" bitLength="16" defaultValue="1"
          uiShowName="LSP Ping Version"  uiShowCommon="false"/>

      <FIELD name="globalFlags" type="GlobalFlagType"
          uiShowName="Global Flags" uiShowCommon ="false"/>
          
      <FIELD name ="messageType" type ="MessageTypeRequest" defaultValue="1"
             uiShowName="Message Type" />

      <FIELD name ="replyMode" type ="ReplyModeType" defaultValue="2"
             uiShowName="Reply Mode" />             

      <FIELD name ="returnCode" type ="ReturnCodeType" defaultValue="0"  
             uiShowName="Return Code" />    

      <FIELD name ="returnSubcode" type ="INTEGER" bitLength="8" defaultValue="0"  
             uiShowName="Return Subcode" />    
      
      <FIELD name ="senderHandle" type ="INTEGER" bitLength="32" defaultValue="1"  
             uiShowName="Sender Handle" />    
             
      <FIELD name ="sequenceNum" type ="INTEGER" bitLength="32" defaultValue="1"  
             uiShowName="Sequence Number" />    

      <FIELD name="timeStampSent" type="OCTETSTRING" byteLength="8" defaultValue="0000000000000000"
        uiShowName="TimeStamp Sent"/>    

      <FIELD name="timeStampReceived" type="OCTETSTRING" byteLength="8" defaultValue="0000000000000000"
        uiShowName="TimeStamp Received"/>
        
    </TYPE_DEF>
    
    <TYPE_DEF name="LspPingReply" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="LSP Ping Header" >

      <FIELD name="version" type="INTEGER" bitLength="16" defaultValue="1"
          uiShowName="LSP Ping Version"  uiShowCommon="false"/>

      <FIELD name="globalFlags" type="GlobalFlagType"
          uiShowName="Global Flags" uiShowCommon ="false"/>
          
      <FIELD name ="messageType" type ="MessageTypeReply" defaultValue="2"
             uiShowName="Message Type" />

      <FIELD name ="replyMode" type ="ReplyModeType" defaultValue="2"
             uiShowName="Reply Mode" />             

      <FIELD name ="returnCode" type ="INTEGER" bitLength="8" defaultValue="0"  
             uiShowName="Return Code" />

      <FIELD name ="returnSubcode" type ="INTEGER" bitLength="8" defaultValue="0"  
             uiShowName="Return Subcode" />             

      <FIELD name ="senderHandle" type ="INTEGER" bitLength="32" defaultValue="1"  
             uiShowName="Sender Handle" />    
             
      <FIELD name ="sequenceNum" type ="INTEGER" bitLength="32" defaultValue="1"  
             uiShowName="Sequence Number" />

      <FIELD name="timeStampSent" type="OCTETSTRING" byteLength="8" defaultValue="0000000000000000"
        uiShowName="TimeStamp Sent"/>    

      <FIELD name="timeStampReceived" type="OCTETSTRING" byteLength="8" defaultValue="0000000000000000"
        uiShowName="TimeStamp Received"/>
        
    </TYPE_DEF>
    
    <TYPE_DEF name="LspPingTlvList" type="SEQUENCE_OF" minOccurs="0" maxOccurs="250" 
                                            elementType="LspPingTlvs" unitType="ELEMENT"/>

    <TYPE_DEF name="LspPingTlvs" type="CHOICE" unitType="ELEMENT" uiShowName="LSP Ping TLVs">
      <FIELD name="targetFecTlv" type="TargetFecTlvType" uiShowName="Target FEC TLV" />
      <FIELD name="downstreamMappingTlv" type="DownstreamMappingTlvType" 
                                                     uiShowName="Downstream Mapping TLV" />
      <FIELD name="padTlv" type="PadTlvType" uiShowName="Pad TLV" />
      <FIELD name="vendorEnterpriseNumberTlv" type="VendorEnterpriseNumberTlvType" uiShowName="VendorEnterprise Number Tlv" />
      <FIELD name="interfaceAndLabelStackTlv" type="InterfaceAndLabelStackTlvType" 
                                                    uiShowName="Interface And Label Stack TLV" />
      <FIELD name="erroredTlv" type="ErroredTlvType" uiShowName="Errored TLV" />
      <FIELD name="replyTOSByteTlv" type="ReplyTOSByteTlvType" uiShowName="Reply TOS Byte TLV" />
      <FIELD name="CustomTlvType" type="CustomTlvType" uiShowName="Custom TLV" />
    </TYPE_DEF>

    <TYPE_DEF name="TargetFecTlvType" type="SEQUENCE" unitType="ELEMENT" uiShowName="Target FEC TLV">
      <FIELD name="type" type="INTEGER" bitLength="16" defaultValue="1" uiShowName="Type" 
                                                                    uiControlState="CALCULATED"/>
      <FIELD name="length" type="INTEGER" bitLength="16" procfnTarget="LSPPINGTLVLENGTH" 
                            defaultValue="20" uiShowName="Length" uiControlState="ALLOW_OVERRIDE" />
      <FIELD name="targetFecSubTlvs" type="TargetFecSubTlvList" uiShowName="Target Fec Sub-TLVs" />
    </TYPE_DEF>    
    
    <TYPE_DEF name="TargetFecSubTlvList" type="SEQUENCE_OF" minOccurs="1" maxOccurs="250" 
                                                elementType="TargetFecSubTlvs" unitType="ELEMENT"/>
    
    <TYPE_DEF name="TargetFecSubTlvs" type="CHOICE" unitType="ELEMENT" 
                                                uiShowName="Target FEC Sub-TLVs">
      <FIELD name="ldpIpv4PrefixSubTlv" type="LdpIpv4PrefixSubTlvType" 
                                                uiShowName="LDP IPv4 Prefix Sub-Tlv" />
      <FIELD name="ldpIpv6PrefixSubTlv" type="LdpIpv6PrefixSubTlvType" 
                                                uiShowName="LDP IPv6 Prefix Sub-Tlv" />
      <FIELD name="rsvpIpv4LspSubTlv" type="RSVPIpv4LspSubTlvType" 
                                                uiShowName="RSVP IPv4 LSP Sub-Tlv" />
      <FIELD name="rspvIpv6LspSubTlv" type="RSVPIpv6LspSubTlvType" 
                                                uiShowName="RSVP IPv6 LSP Sub-Tlv" />
      <FIELD name="vpnIpv4PrefixSubTlv" type="VpnIpv4PrefixSubTlvType" 
                                                uiShowName="VPN IPv4 Prefix Sub-Tlv" />
      <FIELD name="vpnIpv6PrefixSubTlv" type="VpnIpv6PrefixSubTlvType" 
                                                uiShowName="VPN IPv6 Prefix Sub-Tlv" />
      <FIELD name="l2VpnEndpointSubTlv" type="L2VpnEndpointSubTlvType" 
                                                uiShowName="L2 VPN Endpoint Sub-Tlv" />
      <FIELD name="fec128PseudowireSubTlv" type="Fec128PseudowireSubTlvType" 
                                                uiShowName="FEC 128 Pseudowire Sub-Tlv" />
      <FIELD name="fec129PseudowireSubTlv" type="Fec129PseudowireSubTlvType" 
                                                uiShowName="FEC 129 Pseudowire Sub-Tlv" />
      <FIELD name="bgpLabeledIpv4PrefixSubTlv" type="BgpLabeledIpv4PrefixSubTlvType" 
                                                uiShowName="BGP Labeled IPv4 Prefix Sub-Tlv" />
      <FIELD name="bgpLabeledIpv6PrefixSubTlv" type="BgpLabeledIpv6PrefixSubTlvType" 
                                                uiShowName="BGP Labeled IPv6 Prefix Sub-Tlv" />
      <FIELD name="genericIpv4PrefixSubTlv" type="GenericIpv4PrefixSubTlvType" 
                                                uiShowName="Generic Ipv4 Prefix Sub-Tlv" />
      <FIELD name="genericIpv6PrefixSubTlv" type="GenericIpv6PrefixSubTlvType" 
                                                uiShowName="Generic Ipv6 Prefix Sub-Tlv" />
      <FIELD name="staticLspSubTlv" type="StaticLspSubTlvType" 
                                                uiShowName="Static LSP Sub-TLV" />
      <FIELD name="staticPWSubTlv" type="StaticPWSubTlvType" 
                                                uiShowName="Static Pseudowire Sub-TLV" />
      <FIELD name="nilFecSubTlv" type="NilFecSubTlvType" uiShowName="Nil FEC Sub-Tlv" />
      <FIELD name="customSubTlvType" type="CustomSubTlvType" uiShowName="Custom Sub-Tlv" />
      <FIELD name="SegmentIdSubTlvs" type="SegmentIdSubTlvList" 
                                                uiShowName="Segment ID Sub-Tlv List" />
    </TYPE_DEF>

    <TYPE_DEF name="SegmentIdSubTlvList" type="SEQUENCE" unitType="ELEMENT" uiShowName="Segment Id SubTlv List">
      <FIELD name="chunk" type="SegmentIdSubTlvSeqOf" uiShowName="Segment Id Sub-Tlv List" />
    </TYPE_DEF>

	<TYPE_DEF name="SegmentIdSubTlvSeqOf" type="SEQUENCE_OF" minOccurs="0" maxOccurs="20" elementType="SegmentIdSubTlvChunk" unitType="ELEMENT"/>

    <TYPE_DEF name="SegmentIdSubTlvChunk" type="SEQUENCE" unitType="ELEMENT" uiShowName="Segment Id Sub-Tlv Chunk">
      <FIELD name="chunk" type="SegmentIdSubTlvChunkSeqOf" uiShowName="Segment Id Sub-Tlv Type" />
    </TYPE_DEF>

    <TYPE_DEF name="SegmentIdSubTlvChunkSeqOf" type="SEQUENCE_OF" minOccurs="1" maxOccurs="1" elementType="SegmentIdSubTlv" unitType="ELEMENT"/>
		
    <TYPE_DEF name="SegmentIdSubTlv" type="CHOICE" unitType="ELEMENT" 
                                                uiShowName="Segment Id Sub-Tlv">
      <FIELD name="ipv4IgpPrefixSegmentIdSubTlv" type="Ipv4IgpPrefixSegmentIdSubTlv" 
                                                uiShowName="IPv4 IGP-Prefix Segment ID Sub-Tlv" />
      <FIELD name="igpAdjSegmentIdSubTlvOspfv2" type="IgpAdjSegmentIdSubTlvOspfv2" 
                                                uiShowName="IGP-Adjacency Segment ID Sub-Tlv (OSPFv2)" />
      <FIELD name="igpAdjSegmentIdSubTlvOspfv3" type="IgpAdjSegmentIdSubTlvOspfv3" 
                                                uiShowName="IGP-Adjacency Segment ID Sub-Tlv (OSPFv3)" />
      <FIELD name="igpAdjSegmentIdSubTlvIsisIpv4" type="IgpAdjSegmentIdSubTlvIsisIpv4" 
                                                uiShowName="IGP-Adjacency Segment ID Sub-Tlv (IS-IS IPv4)" />
      <FIELD name="igpAdjSegmentIdSubTlvIsisIpv6" type="IgpAdjSegmentIdSubTlvIsisIpv6" 
                                                uiShowName="IGP-Adjacency Segment ID Sub-Tlv (IS-IS IPv6)" />
    </TYPE_DEF>

    <TYPE_DEF name="Ipv4IgpPrefixSegmentIdSubTlv" type="SEQUENCE" unitType="ELEMENT" 
                                                uiShowName="IPv4 IGP-Prefix Segment ID Sub-Tlv">
      <FIELD name="type" type="INTEGER" bitLength="16" defaultValue="34" 
                                                uiShowName="Type" uiControlState="CALCULATED"/>
      <FIELD name="length" type="INTEGER" bitLength="16" defaultValue="6" 
                                                uiShowName="Length" uiControlState="ALLOW_OVERRIDE" />
      <FIELD name="ipv4Prefix" type="IPV4ADDR" defaultValue="192.168.1.1" 
                                                uiShowName="IPv4 Prefix"/>
      <FIELD name="prefixLength" type="INTEGER" bitLength="8" minValue="1" maxValue="32" defaultValue="32" 
                                                uiShowName="Prefix Length"/>
      <FIELD name="protocol" type="INTEGER" bitLength="8" minValue="1" maxValue="2" defaultValue="1" 
                                                uiShowName="Protocol"/>
      <FIELD name="reserved" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0"
                             padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface"/>
    </TYPE_DEF>

    <TYPE_DEF name="IgpAdjSegmentIdSubTlvOspfv2" type="SEQUENCE" unitType="ELEMENT" 
                                                uiShowName="IGP-Adjacency Segment ID Sub-Tlv (OSPFv2)">
      <FIELD name="type" type="INTEGER" bitLength="16" defaultValue="36" 
                                                uiShowName="Type" uiControlState="CALCULATED"/>
      <FIELD name="length" type="INTEGER" bitLength="16" defaultValue="20" 
                                                uiShowName="Length" uiControlState="CALCULATED"/>
      <FIELD name="adjType" type="INTEGER" bitLength="8" defaultValue="4" 
                                                uiShowName="Adj. Type"/>
      <FIELD name="protocol" type="INTEGER" bitLength="8" defaultValue="1" 
                                                uiShowName="Protocol" uiControlState="CALCULATED"/>
      <FIELD name="reserved" type="INTEGER" bitLength="16" defaultValue="0" 
                                                uiShowName="Reserved"/>
      <FIELD name="localInterfaceId" type="IPV4ADDR" defaultValue="0.0.0.1" uiShowName="Local Interface Id" />
      <FIELD name="remoteInterfaceId" type="IPV4ADDR" defaultValue="0.0.0.2" uiShowName="Remote Interface Id" />
      <FIELD name="advNodeId" type="IPV4ADDR" defaultValue="0.0.0.1" uiShowName="Advertising Node Identifier" />
      <FIELD name="rcvNodeId" type="IPV4ADDR" defaultValue="0.0.0.2" uiShowName="Receiving Node Identifier" />
    </TYPE_DEF>

    <TYPE_DEF name="IgpAdjSegmentIdSubTlvOspfv3" type="SEQUENCE" unitType="ELEMENT" 
                                                uiShowName="IGP-Adjacency Segment ID Sub-Tlv (OSPFv3)">
      <FIELD name="type" type="INTEGER" bitLength="16" defaultValue="36" 
                                                uiShowName="Type" uiControlState="CALCULATED"/>
      <FIELD name="length" type="INTEGER" bitLength="16" defaultValue="44" 
                                                uiShowName="Length" uiControlState="CALCULATED"/>
      <FIELD name="adjType" type="INTEGER" bitLength="8" defaultValue="6" 
                                                uiShowName="Adj. Type"/>
      <FIELD name="protocol" type="INTEGER" bitLength="8" defaultValue="1" 
                                                uiShowName="Protocol" uiControlState="CALCULATED"/>
      <FIELD name="reserved" type="INTEGER" bitLength="16" defaultValue="0" 
                                                uiShowName="Reserved"/>
      <FIELD name="localInterfaceId" type="IPV6ADDR" defaultValue="2000::1" uiShowName="Local Interface Id" />
      <FIELD name="remoteInterfaceId" type="IPV6ADDR" defaultValue="2000::2" uiShowName="Remote Interface Id" />
      <FIELD name="advNodeId" type="IPV4ADDR" defaultValue="0.0.0.1" uiShowName="Advertising Node Identifier" />
      <FIELD name="rcvNodeId" type="IPV4ADDR" defaultValue="0.0.0.2" uiShowName="Receiving Node Identifier" />
    </TYPE_DEF>

    <TYPE_DEF name="IgpAdjSegmentIdSubTlvIsisIpv4" type="SEQUENCE" unitType="ELEMENT" 
                                                uiShowName="IGP-Adjacency Segment ID Sub-Tlv (IS-IS IPv4)">
      <FIELD name="type" type="INTEGER" bitLength="16" defaultValue="36" 
                                                uiShowName="Type" uiControlState="CALCULATED"/>
      <FIELD name="length" type="INTEGER" bitLength="16" defaultValue="24" 
                                                uiShowName="Length" uiControlState="CALCULATED"/>
      <FIELD name="adjType" type="INTEGER" bitLength="8" defaultValue="4" 
                                                uiShowName="Adj. Type"/>
      <FIELD name="protocol" type="INTEGER" bitLength="8" defaultValue="2" 
                                                uiShowName="Protocol" uiControlState="CALCULATED"/>
      <FIELD name="reserved" type="INTEGER" bitLength="16" defaultValue="0" 
                                                uiShowName="Reserved"/>
      <FIELD name="localInterfaceId" type="IPV4ADDR" defaultValue="0.0.0.1" uiShowName="Local Interface Id" />
      <FIELD name="remoteInterfaceId" type="IPV4ADDR" defaultValue="0.0.0.2" uiShowName="Remote Interface Id" />
      <FIELD name="advNodeId" type="MACADDR" defaultValue="00:00:00:00:00:01" uiShowName="Advertising Node Identifier" />
      <FIELD name="rcvNodeId" type="MACADDR" defaultValue="00:00:00:00:00:02" uiShowName="Receiving Node Identifier" />
    </TYPE_DEF>

    <TYPE_DEF name="IgpAdjSegmentIdSubTlvIsisIpv6" type="SEQUENCE" unitType="ELEMENT" 
                                                uiShowName="IGP-Adjacency Segment ID Sub-Tlv (IS-IS IPv6)">
      <FIELD name="type" type="INTEGER" bitLength="16" defaultValue="36" 
                                                uiShowName="Type" uiControlState="CALCULATED"/>
      <FIELD name="length" type="INTEGER" bitLength="16" defaultValue="48" 
                                                uiShowName="Length" uiControlState="CALCULATED"/>
      <FIELD name="adjType" type="INTEGER" bitLength="8" defaultValue="6" 
                                                uiShowName="Adj. Type"/>
      <FIELD name="protocol" type="INTEGER" bitLength="8" defaultValue="2" 
                                                uiShowName="Protocol" uiControlState="CALCULATED"/>
      <FIELD name="reserved" type="INTEGER" bitLength="16" defaultValue="0" 
                                                uiShowName="Reserved"/>
      <FIELD name="localInterfaceId" type="IPV6ADDR" defaultValue="2000::1" uiShowName="Local Interface Id" />
      <FIELD name="remoteInterfaceId" type="IPV6ADDR" defaultValue="2000::2" uiShowName="Remote Interface Id" />
      <FIELD name="advNodeId" type="MACADDR" defaultValue="00:00:00:00:00:01" uiShowName="Advertising Node Identifier" />
      <FIELD name="rcvNodeId" type="MACADDR" defaultValue="00:00:00:00:00:02" uiShowName="Receiving Node Identifier" />
    </TYPE_DEF>

    <TYPE_DEF name="LdpIpv4PrefixSubTlvType" type="SEQUENCE" unitType="ELEMENT" 
                                                uiShowName="LDP IPv4 Prefix Sub-Tlv">
      <FIELD name="type" type="INTEGER" bitLength="16" defaultValue="1" 
                                                uiShowName="Type" uiControlState="CALCULATED"/>
      <FIELD name="length" type="INTEGER" bitLength="16" defaultValue="5" 
                                                uiShowName="Length" uiControlState="ALLOW_OVERRIDE" />
      <FIELD name="ipv4Prefix" type="IPV4ADDR" defaultValue="192.168.1.1" 
                                                uiShowName="IPv4 Prefix"/>
      <FIELD name="prefixLength" type="INTEGER" bitLength="8" minValue="1" maxValue="32" defaultValue="24" 
                                                uiShowName="prefix Length"/>
      <FIELD name="reserved" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0"
                             padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface" />
    </TYPE_DEF>

    <TYPE_DEF name="LdpIpv6PrefixSubTlvType" type="SEQUENCE" unitType="ELEMENT" 
                                                uiShowName="LDP IPv6 Prefix Sub-Tlv">
      <FIELD name="type" type="INTEGER" bitLength="16" defaultValue="2" uiShowName="Type" 
                                                uiControlState="CALCULATED"/>
      <FIELD name="length" type="INTEGER" bitLength="16" defaultValue="17" 
                                                uiShowName="Length" uiControlState="ALLOW_OVERRIDE" />
      <FIELD name="ipv6Prefix" type="IPV6ADDR" defaultValue="2000::1" uiShowName="IPv6 Prefix"/>
      <FIELD name="prefixLength" type="INTEGER" bitLength="8" minValue="1" maxValue="128" 
                                                defaultValue="64" uiShowName="prefix Length"/>
      <FIELD name="reserved" type="ALIGNMENTPAD" xAlignment="4" yAlignment="0"
                             padType="NONE" defaultValue="0" uiNoShow="Filter,Frame,Interface" />
    </TYPE_DEF>

    <TYPE_DEF name="RSVPIpv4LspSubTlvType" type="SEQUENCE" unitType="ELEMENT" 
                                                uiShowName="RSVP IPv4 Prefix Sub-Tlv">
      <FIELD name="type" type="INTEGER" bitLength="16" defaultValue="3" 
                                                uiShowName="Type" uiControlState="CALCULATED"/>
      <FIELD name="length" type="INTEGER" bitLength="16" defaultValue="20" 
                                                uiShowName="Length" uiControlState="ALLOW_OVERRIDE" />
      <FIELD name="ipv4TunnelEndPoint" type="IPV4ADDR" defaultValue="192.168.1.2" 
                                                uiShowName="IPv4 Tunnel EndPoint Address"/>
      <FIELD name="reserved1" type="INTEGER" bitLength="16"  defaultValue="0" uiShowName="Reserved" />
      <FIELD name="tunnelId" type="INTEGER" bitLength="16"  defaultValue="1" uiShowName="Tunnel ID" />
      <FIELD name="extendedTunnelId" type="IPV4ADDR" defaultValue="192.168.1.1" 
