<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="pdu_schema.xsd"
  name="mimethernet"
  showName="Ethernet"
  version="1.00"
  description="This is a sample PDU library for Ethernet as defined in XML Stream Editor.">
    
  <TYPE_DEFS>
    <TYPE_DEF name="sTag" type="SEQUENCE" unitType="ELEMENT" uiShowName="Service Tag" niMapped="true">
      <FIELD name="Type" type="OCTETSTRING" byteLength="2"
                defaultValue="88a8"
                uiNoShow="Filter"
                uiNoShowModifier="Interface,Filter"
                uiShowName="Service Vlan Type"
                isNiMapped="true"/>

      <FIELD name="PCP" type="BITSTRING" 
		bitLength="3" defaultValue="001" 
                isNiMapped="true"
		uiShowName="Service VLAN PCP"/>

      <FIELD name="DEI" type="BITSTRING" 
		bitLength="1" defaultValue="0"
                isNiMapped="true"
		uiShowName="Service DEI"/>

      <FIELD name="VID" type="INTEGER" 
		bitLength="12" defaultValue="100"
                isNiMapped="true"
	        uiShowName="Service VID"/>
      </TYPE_DEF>

    <TYPE_DEF name="sTagList" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1" elementType="sTag" unitType="ELEMENT" niMapped="true"/>

    <TYPE_DEF name="cVlan" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="Customer Vlan" niMapped="true">

      <FIELD name="type" type="OCTETSTRING" byteLength="2"
        defaultValue="8100"
		uiNoShow="Interface"
		uiNoShowModifier="Interface"
        uiShowName="Type"/>

      <FIELD name="pcp" type="BITSTRING" bitLength="3"
        defaultValue="001"
        uiShowName="PCP"
		uiNoShowModifier="Interface"
        isNiMapped="true"/>

      <FIELD name="cfi" type="BITSTRING" bitLength="1"
        defaultValue="0" 
        uiShowName="CFI"
		uiNoShowModifier="Interface"
        isNiMapped="true"/>

      <FIELD name="id" type="INTEGER" bitLength="12" 
        defaultValue="100"
        uiShowName="ID"
        isNiMapped="true"
        isNiPrimaryKey="true"/>
    </TYPE_DEF>

    <TYPE_DEF name="cVlansList" type="SEQUENCE_OF" minOccurs="0" maxOccurs="4096" elementType="cVlan" unitType="ELEMENT" niMapped="true"/>
    
    <TYPE_DEF name="EthernetII" type="SEQUENCE" unitType="HEADER"
        level="2" portType="Ethernet" uiShowName="Customer EthernetII" niMapped="true" >

      <FIELD name="preamble" type="OCTETSTRING"
        minByteLength="4" maxByteLength="32"
        defaultValue="55555555555555d5"
        uiNoShow="Interface,Filter"
        uiNoShowModifier="Interface"
        uiShowCommon="false"
        uiOverrideEditor="PreambleEditor"
        uiShowName="Preamble" />
        
      <FIELD name="cDstMac" type="MACADDR"
        defaultValue="00:00:01:00:00:01"
		    uiNoShowModifier="Interface"
		    uiNoShow="Interface"
        uiControlState="ALLOW_OVERRIDE" 
        uiShowName="Customer Destination MAC" 
        isNiMapped="true" />
				
      <FIELD name="cSrcMac" type="MACADDR"
        defaultValue="00:10:94:00:00:02"
        uiControlState="ALLOW_OVERRIDE"  
        uiShowName="Customer Source MAC" 
        isNiMapped="true"
        isNiPrimaryKey="true"/>

      <FIELD name="svlans" type="sTagList" uiShowName="Service Vlans" uiNoShowModifier="Interface,Frame" isNiMapped="true"/>
				
      <FIELD name="cvlans" type="cVlansList"  uiShowName="Customer Vlans" uiNoShowModifier="Interface,Frame" isNiMapped="true"/>
            
      <FIELD name="etherType" type="types:EtherTypes"
        defaultValue="88B5"
        uiControlState="ALLOW_OVERRIDE"   
        procfnTarget="NEXTHDR" 
		uiNoShow="Interface"
		uiNoShowModifier="Interface"
        uiShowName="EtherType" />

      <NEXT_HEADERS>
        <NXT_HDR id="ipv4:IPv4" name="IPv4 Header" value="0800" />
        <NXT_HDR id="ipv6:IPv6" name="IPv6 Header" value="86DD" />
        <NXT_HDR id="mpls:Mpls" name="MPLS Header" value="8847" />
        <NXT_HDR id="pppoe:PPPoEDiscovery" name="PPPoE Discovery" value="8863" />
        <NXT_HDR id="pppoe:PPPoESession" name="PPPoE Session" value="8864" />
        <NXT_HDR id="arp:ARP" name="ARP Header" value="0806" />
        <NXT_HDR id="arp:RARP" name="RARP Header" value="8035" />
        <NXT_HDR id="lacp:LACP" name="LACP PDU" value="8809" />
        <NXT_HDR id="lacp:Marker" name="Marker PDU" value="8809" />
        <NXT_HDR id="serviceoam:CCM" name="Ethernet OAM CCM" value="8902"/>
        <NXT_HDR id="serviceoam:LBR" name="Ethernet OAM LBR" value="8902"/>
        <NXT_HDR id="serviceoam:LBM" name="Ethernet OAM LBM" value="8902"/>
        <NXT_HDR id="serviceoam:LTM" name="Ethernet OAM LTM" value="8902"/>
        <NXT_HDR id="serviceoam:LTR" name="Ethernet OAM LTR" value="8902"/>
        <NXT_HDR id="serviceoam:LMM" name="Ethernet OAM LMM" value="8902"/>
        <NXT_HDR id="serviceoam:LMR" name="Ethernet OAM LMR" value="8902"/>
        <NXT_HDR id="serviceoam:DMM" name="Ethernet OAM DMM" value="8902"/>
        <NXT_HDR id="serviceoam:DMR" name="Ethernet OAM DMR" value="8902"/>
        <NXT_HDR id="serviceoam:CSF" name="Ethernet OAM CSF" value="8902" />
        <NXT_HDR id="serviceoam:R-APS" name="Ethernet OAM R-APS" value="8902" />
        <NXT_HDR id="lldp:ChassisIdTlv" name="Chassis ID TLV" value="88CC" />

        <NXT_HDR id="custom:Custom" name="Custom" value="88B5" />
		<NXT_HDR id="customrepeat:CustomRepeat" name="CustomRepeat" value="88B5" />
        <!-- Next header needed to be defaulted to 88B5 for custom and custom repeat.  -->

      </NEXT_HEADERS>
			
    </TYPE_DEF> 
    
    <TYPE_DEF name="Ethernet8023Raw" type="SEQUENCE" unitType="HEADER"
        level="2" portType="Ethernet" uiShowName="Customer 802.3 Raw" >

      <FIELD name="preamble" type="OCTETSTRING"
        minByteLength="4" maxByteLength="32"
        defaultValue="55555555555555d5"
        uiNoShow="Interface,Filter"
        uiNoShowModifier="Interface"
        uiShowCommon="false"
		    uiOverrideEditor="PreambleEditor"
        uiShowName="Preamble" />
				
      <FIELD name="cDstMac" type="MACADDR"
        defaultValue="00:00:01:00:00:01"
        uiControlState="ALLOW_OVERRIDE" 
        uiShowName="Customer Destination MAC" />
				
      <FIELD name="cSrcMac" type="MACADDR"
        defaultValue="00:10:94:00:00:02"
        uiControlState="ALLOW_OVERRIDE"  
        uiShowName="Customer Source MAC" />

      <FIELD name="svlans" type="sTagList" uiShowName="Service Vlans"/>
				
      <FIELD name="cvlans" type="cVlansList" uiShowName="Customer Vlans" />
            
      <FIELD name="length" type="INTEGER" bitLength="16"
        uiControlState="CALCULATED" 
        uiNoShowModifier="Interface,Frame,Filter"
        uiShowName="Length" />

      <NEXT_HEADERS>
        <NXT_HDR id="custom:Custom" name="Custom" value="88B5" />
		<NXT_HDR id="customrepeat:CustomRepeat" name="CustomRepeat" value="88B5" />
        <!-- Next header needed to be defaulted to 88B5 for custom and custom repeat.  -->
      </NEXT_HEADERS>

    </TYPE_DEF>

    <TYPE_DEF name="Ethernet8022" type="SEQUENCE" unitType="HEADER"
        level="2" portType="Ethernet" uiShowName="802.2" >

      <FIELD name="preamble" type="OCTETSTRING"
        minByteLength="4" maxByteLength="32"
        defaultValue="55555555555555d5"
        uiNoShow="Interface,Filter"
        uiNoShowModifier="Interface"
        uiShowCommon="false"
		    uiOverrideEditor="PreambleEditor"
        uiShowName="Preamble" />
        
      <FIELD name="cDstMac" type="MACADDR"
        defaultValue="00:00:01:00:00:01"
        uiControlState="ALLOW_OVERRIDE" 
        uiShowName="Customer Destination MAC" />

      <FIELD name="cSrcMac" type="MACADDR"
        defaultValue="00:10:94:00:00:02"
        uiControlState="ALLOW_OVERRIDE" 
        uiShowName="Customer Source MAC" />

      <FIELD name="svlans" type="sTagList" uiShowName="Service Vlans"/>

      <FIELD name="cvlans" type="cVlansList" uiShowName="Customer Vlans" />

      <FIELD name="length" type="INTEGER" bitLength="16"
        uiControlState="CALCULATED" 
        uiNoShowModifier="Interface,Frame,Filter"
        uiShowName="Length" />
        
      <FIELD name="llcheader" type="ethernet:LLC"
         uiShowName="LLC Header" />

      <NEXT_HEADERS>
		<NXT_HDR id="ipv4:IPv4" name="IPv4 Header" value="0800" />
        <NXT_HDR id="custom:Custom" name="Custom" value="88B5" />
		<NXT_HDR id="customrepeat:CustomRepeat" name="CustomRepeat" value="88B5" />
        <!-- Next header needed to be defaulted to 88B5 for custom and custom repeat.  -->
      </NEXT_HEADERS>
			
    </TYPE_DEF> 
    
    <TYPE_DEF name="EthernetSnap" type="SEQUENCE" unitType="HEADER"
        level="2" portType="Ethernet" uiShowName="Customer Ethernet Snap" >

      <FIELD name="preamble" type="OCTETSTRING"
        minByteLength="4" maxByteLength="32"
        defaultValue="55555555555555d5"
        uiNoShow="Interface,Filter"
        uiNoShowModifier="Interface"
        uiShowCommon="false"
		    uiOverrideEditor="PreambleEditor"
        uiShowName="Preamble" />
        
      <FIELD name="cDstMac" type="MACADDR"
        defaultValue="00:00:01:00:00:01"
        uiControlState="ALLOW_OVERRIDE"  
        uiShowName="Customer Destination MAC" />
				
      <FIELD name="cSrcMac" type="MACADDR"
        defaultValue="00:10:94:00:00:02"
        uiControlState="ALLOW_OVERRIDE" 
        uiShowName="Customer Source MAC" />

      <FIELD name="svlans" type="sTagList" uiShowName="Service Vlans" />
				
      <FIELD name="cvlans" type="cVlansList" uiShowName="Customer Vlans" />
            
      <FIELD name="length" type="INTEGER" bitLength="16"
        uiControlState="CALCULATED" 
        uiNoShowModifier="Interface,Frame,Filter"
        uiShowName="Length" />
        
      <FIELD name="llcheader" type="ethernet:LLC"
         uiShowName="LLC Header" />
         
      <FIELD name="snapheader" type="ethernet:Snap"
		 procfnTarget="NEXTHDR" 
         uiShowName="Snap Header" />

      <NEXT_HEADERS>
        <NXT_HDR id="ipv4:IPv4" name="IPv4 Header" value="0800" />
        <NXT_HDR id="ipv6:IPv6" name="IPv6 Header" value="86DD" />
        <NXT_HDR id="mpls:Mpls" name="MPLS Header" value="8847" />
        <NXT_HDR id="pppoe:PPPoEDiscovery" name="PPPoE Discovery" value="8863" />
        <NXT_HDR id="pppoe:PPPoESession" name="PPPoE Session" value="8864" />
        <NXT_HDR id="arp:ARP" name="ARP Header" value="0806" /> 
        <NXT_HDR id="arp:RARP" name="RARP Header" value="8035" />
		<NXT_HDR id="ieeeMacControl:MACControl" name="IEEE MAC Control" value="8808" />
		<NXT_HDR id="lldp:ChassisIdTlv" name="Chassis ID TLV" value="88CC" />
        <NXT_HDR id="custom:Custom" name="Custom" value="88B5" />
		<NXT_HDR id="customrepeat:CustomRepeat" name="CustomRepeat" value="88B5" />
        <!-- Next header needed to be defaulted to 88B5 for custom and custom repeat.  -->
      </NEXT_HEADERS>

    </TYPE_DEF>
      
    <TYPE_DEF name="EncapEthernetII" type="SEQUENCE" unitType="HEADER"
              uiShowName="Encapsulated Customer EthernetII" niMapped="true">

        <FIELD name="svlans" type="sTagList" uiShowName="Service Vlans" uiNoShowModifier="Interface,Frame" isNiMapped="true"/>

        <FIELD name="cvlans" type="cVlansList"  uiShowName="Customer Vlans" uiNoShowModifier="Interface,Frame" isNiMapped="true"/>

        <FIELD name="etherType" type="types:EtherTypes"
          defaultValue="88B5"
          uiControlState="ALLOW_OVERRIDE"
          procfnTarget="NEXTHDR"
          uiNoShow="Interface"
          uiNoShowModifier="Interface"
          uiShowName="EtherType" />

        <NEXT_HEADERS>
            <NXT_HDR id="ipv4:IPv4" name="IPv4 Header" value="0800" />
            <NXT_HDR id="ipv6:IPv6" name="IPv6 Header" value="86DD" />
            <NXT_HDR id="mpls:Mpls" name="MPLS Header" value="8847" />
            <NXT_HDR id="pppoe:PPPoEDiscovery" name="PPPoE Discovery" value="8863" />
            <NXT_HDR id="pppoe:PPPoESession" name="PPPoE Session" value="8864" />
            <NXT_HDR id="arp:ARP" name="ARP Header" value="0806" />
            <NXT_HDR id="arp:RARP" name="RARP Header" value="8035" />
            <NXT_HDR id="lacp:LACP" name="LACP PDU" value="8809" />
            <NXT_HDR id="lacp:Marker" name="Marker PDU" value="8809" />
            <NXT_HDR id="serviceoam:CCM" name="Ethernet OAM CCM" value="8902"/>
            <NXT_HDR id="serviceoam:LBR" name="Ethernet OAM LBR" value="8902"/>
            <NXT_HDR id="serviceoam:LBM" name="Ethernet OAM LBM" value="8902"/>
            <NXT_HDR id="serviceoam:LTM" name="Ethernet OAM LTM" value="8902"/>
            <NXT_HDR id="serviceoam:LTR" name="Ethernet OAM LTR" value="8902"/>
            <NXT_HDR id="serviceoam:LMM" name="Ethernet OAM LMM" value="8902"/>
            <NXT_HDR id="serviceoam:LMR" name="Ethernet OAM LMR" value="8902"/>
            <NXT_HDR id="serviceoam:DMM" name="Ethernet OAM DMM" value="8902"/>
            <NXT_HDR id="serviceoam:DMR" name="Ethernet OAM DMR" value="8902"/>
            <NXT_HDR id="serviceoam:CSF" name="Ethernet OAM CSF" value="8902" />
            <NXT_HDR id="serviceoam:R-APS" name="Ethernet OAM R-APS" value="8902" />
            <NXT_HDR id="lldp:ChassisIdTlv" name="Chassis ID TLV" value="88CC" />

            <NXT_HDR id="custom:Custom" name="Custom" value="88B5" />
			<NXT_HDR id="customrepeat:CustomRepeat" name="CustomRepeat" value="88B5" />
			<!-- Next header needed to be defaulted to 88B5 for custom and custom repeat.  -->


        </NEXT_HEADERS>        
    </TYPE_DEF>

	<TYPE_DEF name="EncapEthernetIIPbb" type="SEQUENCE" unitType="HEADER"
              uiShowName="Encapsulated Customer EthernetII" niMapped="true">

        <FIELD name="cvlans" type="cVlansList"  uiShowName="Customer Vlans" uiNoShowModifier="Interface,Frame" isNiMapped="true"/>

        <FIELD name="etherType" type="types:EtherTypes"
          defaultValue="88B5"
          uiControlState="ALLOW_OVERRIDE"
          procfnTarget="NEXTHDR"
          uiNoShow="Interface"
          uiNoShowModifier="Interface"
          uiShowName="EtherType" />

        <NEXT_HEADERS>
            <NXT_HDR id="ipv4:IPv4" name="IPv4 Header" value="0800" />
            <NXT_HDR id="ipv6:IPv6" name="IPv6 Header" value="86DD" />
            <NXT_HDR id="mpls:Mpls" name="MPLS Header" value="8847" />
            <NXT_HDR id="pppoe:PPPoEDiscovery" name="PPPoE Discovery" value="8863" />
            <NXT_HDR id="pppoe:PPPoESession" name="PPPoE Session" value="8864" />
            <NXT_HDR id="arp:ARP" name="ARP Header" value="0806" />
            <NXT_HDR id="arp:RARP" name="RARP Header" value="8035" />
            <NXT_HDR id="lacp:LACP" name="LACP PDU" value="8809" />
            <NXT_HDR id="lacp:Marker" name="Marker PDU" value="8809" />
            <NXT_HDR id="serviceoam:CCM" name="Ethernet OAM CCM" value="8902"/>
            <NXT_HDR id="serviceoam:LBR" name="Ethernet OAM LBR" value="8902"/>
            <NXT_HDR id="serviceoam:LBM" name="Ethernet OAM LBM" value="8902"/>
            <NXT_HDR id="serviceoam:LTM" name="Ethernet OAM LTM" value="8902"/>
            <NXT_HDR id="serviceoam:LTR" name="Ethernet OAM LTR" value="8902"/>
            <NXT_HDR id="serviceoam:LMM" name="Ethernet OAM LMM" value="8902"/>
            <NXT_HDR id="serviceoam:LMR" name="Ethernet OAM LMR" value="8902"/>
            <NXT_HDR id="serviceoam:DMM" name="Ethernet OAM DMM" value="8902"/>
            <NXT_HDR id="serviceoam:DMR" name="Ethernet OAM DMR" value="8902"/>
            <NXT_HDR id="serviceoam:CSF" name="Ethernet OAM CSF" value="8902" />
            <NXT_HDR id="serviceoam:R-APS" name="Ethernet OAM R-APS" value="8902" />
            <NXT_HDR id="lldp:ChassisIdTlv" name="Chassis ID TLV" value="88CC" />

            <NXT_HDR id="custom:Custom" name="Custom" value="88B5" />
			<NXT_HDR id="customrepeat:CustomRepeat" name="CustomRepeat" value="88B5" />
			<!-- Next header needed to be defaulted to 88B5 for custom and custom repeat.  -->


        </NEXT_HEADERS>        
    </TYPE_DEF>
	
    <TYPE_DEF name="EncapEthernet8023Raw" type="SEQUENCE" unitType="HEADER"
        uiShowName="Encapsulated Customer 802.3 Raw" >

        <FIELD name="svlans" type="sTagList" uiShowName="Service Vlans"/>

        <FIELD name="cvlans" type="cVlansList" uiShowName="Customer Vlans" />

        <FIELD name="length" type="INTEGER" bitLength="16"
          uiControlState="CALCULATED"
          uiNoShowModifier="Interface,Frame,Filter"
          uiShowName="Length" />

        <NEXT_HEADERS>
            <NXT_HDR id="custom:Custom" name="Custom" value="88B5" />
			<NXT_HDR id="customrepeat:CustomRepeat" name="CustomRepeat" value="88B5" />
			<!-- Next header needed to be defaulted to 88B5 for custom and custom repeat.  -->
        </NEXT_HEADERS>

    </TYPE_DEF>

    <TYPE_DEF name="EncapEthernet8022" type="SEQUENCE" unitType="HEADER"
        uiShowName="Encapsulated Customer 802.2" >

        <FIELD name="svlans" type="sTagList" uiShowName="Service Vlans"/>

        <FIELD name="cvlans" type="cVlansList" uiShowName="Customer Vlans" />

        <FIELD name="length" type="INTEGER" bitLength="16"
          uiControlState="CALCULATED"
          uiNoShowModifier="Interface,Frame,Filter"
          uiShowName="Length" />

        <FIELD name="llcheader" type="ethernet:LLC"
           uiShowName="LLC Header" />

        <NEXT_HEADERS>
            <NXT_HDR id="ipv4:IPv4" name="IPv4 Header" value="0800" />
            <NXT_HDR id="custom:Custom" name="Custom" value="88B5" />
			<NXT_HDR id="customrepeat:CustomRepeat" name="CustomRepeat" value="88B5" />
			<!-- Next header needed to be defaulted to 88B5 for custom and custom repeat.  -->
