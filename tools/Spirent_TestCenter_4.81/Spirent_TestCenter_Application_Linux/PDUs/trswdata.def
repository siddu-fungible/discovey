<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="pdu_schema.xsd"
    name="trswdata"
    showName="Transport Software Data"
    version="1.00"
    description="This is a sample PDU library for Transport Software Data as defined in XML Stream Editor.">

    <TYPE_DEFS>
        <TYPE_DEF name="TransportSoftwareData" type="SEQUENCE" unitType="HEADER" portType="Ethernet" uiShowName="Transport Software Data" >
            <FIELD name="Reserved" type="INTEGER" bitLength="16" uiShowName="Reserved" />
            <FIELD name="msgId" type="INTEGER" bitLength="16" uiShowName="Message ID" />            
            <FIELD name="Reserved" type="INTEGER" bitLength="32" uiShowName="Reserved" />
            <FIELD name="ccid" type="INTEGER" bitLength="32" uiShowName="CCID" />
            <FIELD name="mlength"  type="INTEGER"  bitLength="16" uiShowName="Message Length" />
            <FIELD name="Reserved" type="INTEGER" bitLength="16" uiShowName="Reserved" />
            <NEXT_HEADERS>
                <NXT_HDR id="custom:Custom" name="Custom" value="1024" />
                <NXT_HDR id="customrepeat:CustomRepeat" name="CustomRepeat" value="1024" />
                <NXT_HDR id="gtpf:GTPf" name="GTPf" value="3386" />
            </NEXT_HEADERS>
        </TYPE_DEF>
    </TYPE_DEFS>
</PDU_LIBRARY>

