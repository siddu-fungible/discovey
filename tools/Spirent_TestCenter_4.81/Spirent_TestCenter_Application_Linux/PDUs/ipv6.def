        <NXT_HDR id="icmpv6:MLDv2Query" name="MLDv2 Query" value="58" />
        <NXT_HDR id="icmpv6:MLDv2Report" name="MLDv2 Report" value="58" />
        <NXT_HDR id="icmpv6:RouterSolicitation" name="Router Solicitation" value="58" />
        <NXT_HDR id="icmpv6:RouterAdvertisement" name="Router Advertisement" value="58" />
        <NXT_HDR id="icmpv6:NeighborSolicitation" name="Neighbor Solicitation" value="58" />
        <NXT_HDR id="icmpv6:NeighborAdvertisement" name="Neighbor Advertisement" value="58" />
        <NXT_HDR id="icmpv6:Redirect" name="Redirect" value="58" />
        <NXT_HDR id="gre:Gre" name="GRE" value="47" />
        <NXT_HDR id="pim:Pimv6Hello" name="Pim Hello" value="103" />
        <NXT_HDR id="pim:Pimv6Register" name="Pim Register" value="103" />
        <NXT_HDR id="pim:Pimv6RegisterStop" name="Pim RegisterStop" value="103" />
        <NXT_HDR id="pim:Pimv6JoinPrune" name="Pim Join/Prune" value="103" />
