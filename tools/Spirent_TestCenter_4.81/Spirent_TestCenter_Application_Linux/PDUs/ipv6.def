<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="pdu_schema.xsd" name="ipv6" showName="IPv6" version="1.00"
    description="This is a sample PDU library for IPv6 as defined in XML Stream Editor.">
    
  <TYPE_DEFS>

    <!-- IPv6 Extension Header Options -->

    <TYPE_DEF name="Ipv6Pad1Option" type="SEQUENCE" 
        unitType="ELEMENT" uiShowName="Pad 1 Option" >
      <FIELD name="pad1" type="INTEGER" bitLength="8"
          defaultValue="0" minValue="0" maxValue="0"
          uiShowName="Pad 1 Option" />
    </TYPE_DEF>
        
    <TYPE_DEF name="Ipv6PadNOption" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="Pad N Option" >
      <FIELD name="padN" type="INTEGER" bitLength="8"
          defaultValue="1" minValue="1" maxValue="1"
          uiShowName="Pad N Option" />
      <FIELD name="length" type="INTEGER" bitLength="8"
          defaultValue="1" minValue="0" maxValue="5"
          uiShowName="Pad N Option Length" />
      <FIELD name="padding" type="OCTETSTRING"
          minByteLength="0" maxByteLength="5"
          defaultValue="00"
          uiShowName="Pad N Option Padding" />
    </TYPE_DEF>

    <TYPE_DEF name="Ipv6CustomOption" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="Custom Option">
      <FIELD name="type" type="INTEGER" bitLength="8"
          defaultValue="15" minValue="0" maxValue="255"
      uiShowName="Custom Option" />
      <FIELD name="data" type="OCTETSTRING"
          minByteLength="0" maxByteLength="128"
          defaultValue="00"
          uiShowName="Data" />
    </TYPE_DEF>

    <TYPE_DEF name="RouterAlertValues" type="INTEGER" bitLength="16" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="MLD" value="0" />
        <VAL_ENUM uiShowName="RSVP" value="1" />
        <VAL_ENUM uiShowName="Active Networks" value="2" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>

    <!-- RFC 2711 IPv6 Router Alert Option -->
    <TYPE_DEF name="Ipv6RouterAlertOption" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="Router Alert Option" >
      <FIELD name="pad" type="ALIGNMENTPAD"
          xAlignment="2" yAlignment="0" padType="IPV6EXT"
      defaultValue="0" uiNoShow="Filter,Frame,Interface"
          uiShowName="2n+0 Byte Alignment Padding" />
      <FIELD name="routerAlert" type="INTEGER" bitLength="8"
          defaultValue="5" minValue="5" maxValue="5" 
          uiShowName="Router Alert Option" />
      <FIELD name="length" type="INTEGER" bitLength="8"
          defaultValue="2" minValue="0" maxValue="255" 
          uiShowName="Router Alert Length" />
      <FIELD name="value" type="RouterAlertValues"
          defaultValue="0" uiShowName="Router Alert Value" />
    </TYPE_DEF>

    <!-- RFC 2675 IPv6 Jumbo Payload Option -->
    <TYPE_DEF name="Ipv6JumboPayloadOption" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="Jumbo Payload Option" >
      <FIELD name="pad" type="ALIGNMENTPAD"
          xAlignment="4" yAlignment="2" padType="IPV6EXT"
          defaultValue="0" uiNoShow="Filter,Frame,Interface"
          uiShowName="4n+2 Byte Alignment Padding" />
      <FIELD name="type" type="INTEGER" bitLength="8"
          defaultValue="194" minValue="194" maxValue="194" 
          uiShowName="Jumbo Payload Option" />
      <FIELD name="length" type="INTEGER" bitLength="8"
          defaultValue="4" minValue="0" maxValue="255"
          uiShowName="Jumbo Payload Option Length" />
      <FIELD name="data" type="INTEGER" bitLength="32"
          defaultValue="0"
          uiShowName="Jumbo Payload Option Value" />
    </TYPE_DEF>


    <!-- IPv6 Extension Headers -->

    <TYPE_DEF name="Ipv6HopByHopOption" type="CHOICE"
        unitType="ELEMENT" uiShowName="IPv6 Option" >
      <FIELD name="pad1" type="Ipv6Pad1Option"
          uiShowName="Pad1 Option" />
      <FIELD name="padN" type="Ipv6PadNOption"
          uiShowName="PadN Option" />
      <FIELD name="routerAlert" type="Ipv6RouterAlertOption"
          uiShowName="Router Alert Option" />
      <FIELD name="jumbo" type="Ipv6JumboPayloadOption"
          uiShowName="Jumbo Payload Option" />
      <FIELD name="custom" type="Ipv6CustomOption"
          uiShowName="Custom Option" />
    </TYPE_DEF>

    <TYPE_DEF name="Ipv6HopByHopOptionsList" type="SEQUENCE_OF"
        minOccurs="0" maxOccurs="100"
        elementType="Ipv6HopByHopOption" unitType="ELEMENT" />
    
    <TYPE_DEF name="Ipv6HopByHopHeader" type="SEQUENCE"
