        <NXT_HDR id="ipv6:Ipv6FragmentHeader" name="IPv6 Fragment Header" value="44" />
        <NXT_HDR id="ipv6:Ipv6HopByHopHeader" name="IPv6 Hop By Hop Header" value="0" />
        <NXT_HDR id="ipv6:Ipv6DestinationHeader" name="IPv6 Destination Header" value="60" />
        <NXT_HDR id="ipv6:Ipv6RoutingHeader" name="IPv6 Routing Header" value="43" />
