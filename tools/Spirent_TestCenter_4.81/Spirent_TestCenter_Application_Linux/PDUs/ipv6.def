        <FIELD name="value" type="IPV6ADDR"
            defaultValue="2000:0000:0000:0000:0000:0000:0000:0001"
            uiShowName="Ipv6 Address" />
    </TYPE_DEF>
    
    <TYPE_DEF name="NodeList" type="SET_OF"
        minOccurs="0" maxOccurs="100"
        elementType="Ipv6Addr" unitType="ELEMENT" />

