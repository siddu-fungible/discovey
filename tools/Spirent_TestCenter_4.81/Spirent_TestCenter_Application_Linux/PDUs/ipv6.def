          uiShowName="IPv6 Options" />
      <FIELD name="pad" type="ALIGNMENTPAD"
          xAlignment="8" yAlignment="0" padType="IPV6EXT"
          defaultValue="0" uiNoShow="Filter,Frame,Interface"
          uiShowName="8 Byte Alignment Padding" />

