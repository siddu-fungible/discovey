        <NXT_HDR id="ipv6:Ipv6RoutingHeader" name="IPv6 Routing Header" value="43" />
        <NXT_HDR id="ipv6:SrHeader" name="IPv6 SR Header" value="43" />
        <NXT_HDR id="ipv6:Ipv6AuthenticationHeader" name="IPv6 Authentication Header" value="51" />
        <NXT_HDR id="ipv6:Ipv6EncapsulationHeader" name="IPv6 Encapsulation Header" value="50" />
        <NXT_HDR id="tcp:Tcp" name="Tcp Header" value="6" />
        <NXT_HDR id="udp:Udp" name="Udp Header" value="17" />
        <NXT_HDR id="sctp:Sctp" name="SCTP Header" value="132"/>
        <NXT_HDR id="icmpv6:Icmpv6DestUnreach" name="Icmpv6 Destination Unreachable" value="58" />
