        <NXT_HDR id="custom:Custom" name="Custom" value="59" />
        <NXT_HDR id="customrepeat:CustomRepeat" name="CustomRepeat" value="59" />

      </NEXT_HEADERS>
    </TYPE_DEF>
        

    <TYPE_DEF name="Ipv6DestinationOption" type="CHOICE"
