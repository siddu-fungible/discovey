<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="pdu_schema.xsd" name="openflow" showName="Openflow" version="1.00"
    description="This a PDU library for Openflow TLVs.">
    
  <TYPE_DEFS>

    
    <TYPE_DEF name="bool_type" type="INTEGER" bitLength="1" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="False" value="0" />
        <VAL_ENUM uiShowName="True" value="1" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>
    <!-- MATCH TLVs -->

    <TYPE_DEF name="in_port" type="SEQUENCE" category="OF10"
        unitType="ELEMENT" uiShowName="Ingress Port (in_port)" >
      <FIELD name="port_number" type="INTEGER" bitLength="32" defaultValue="1" 
          uiShowName="Port Number" uiNoShow="Interface" />
    </TYPE_DEF>
	<TYPE_DEF name="in_phy_port" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="Physical Port (in_phy_port)" >
      <FIELD name="port_number" type="INTEGER" bitLength="32" defaultValue="1" 
          uiShowName="Port Number" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="dl_src" type="SEQUENCE" category="OF10"
        unitType="ELEMENT" uiShowName="Ethernet Source (dl_src)" >
      <FIELD name="addr" type="MACADDR" defaultValue="00:00:01:00:00:01"
          uiShowName="MAC" uiNoShow="Interface" />
      <FIELD name="useMask" type="bool_type" defaultValue="0"
          uiShowName="Use Mask" uiNoShow="Interface" doNotBuild="true" /> 
      <FIELD name="mask" type="MACADDR" defaultValue="FF:FF:FF:FF:FF:FF"
          uiShowName="Mask" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="dl_dst" type="SEQUENCE" category="OF10"
        unitType="ELEMENT" uiShowName="Ethernet Destination (dl_dst)" >
      <FIELD name="addr" type="MACADDR" defaultValue="00:00:02:00:00:01"
          uiShowName="MAC" uiNoShow="Interface" />
      <FIELD name="useMask" type="bool_type" defaultValue="0"
          uiShowName="Use Mask" uiNoShow="Interface" doNotBuild="true" /> 
      <FIELD name="mask" type="MACADDR" defaultValue="FF:FF:FF:FF:FF:FF"
          uiShowName="Mask" uiNoShow="Interface" />    
          
    </TYPE_DEF>
    <TYPE_DEF name="dl_type" type="SEQUENCE" category="OF10"
        unitType="ELEMENT" uiShowName="Ethernet Type (dl_type)" >
      <FIELD name="ethernet_type" type="types:EtherTypes" byteLength="2" defaultValue="800"
          uiShowName="Ethernet Type" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="dl_vlan" type="SEQUENCE" category="OF10"
        unitType="ELEMENT" uiShowName="VLAN ID (dl_vlan)" >
      <FIELD name="id" type="INTEGER" bitLength="12" defaultValue="100"
          uiShowName="VLAN ID" uiNoShow="Interface" />
      <FIELD name="useMask" type="bool_type" defaultValue="0"
          uiShowName="Use Mask" uiNoShow="Interface" doNotBuild="true" />
      <FIELD name="mask" type="OCTETSTRING" byteLength="2" defaultValue="FFFF"
          uiShowName="Mask" uiNoShow="Interface" />      
    </TYPE_DEF>
    <TYPE_DEF name="dl_vlan_pcp" type="SEQUENCE" category="OF10"
        unitType="ELEMENT" uiShowName="VLAN Priority (dl_vlan_pcp)" >
      <FIELD name="priority" type="INTEGER" bitLength="3" defaultValue="1"
          uiShowName="Priority" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="dl_vlan_cfi" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="VLAN CFI (dl_vlan_cfi)" >
      <FIELD name="vlan_cfi" type="bool_type" defaultValue="1"
          uiShowName="VLAN CFI" uiNoShow="Interface" /> 
    </TYPE_DEF>    
    <TYPE_DEF name="nw_src" type="SEQUENCE" category="OF10"
        unitType="ELEMENT" uiShowName="IPv4 Source (nw_src)" >
      <FIELD name="addr" type="IPV4ADDR" defaultValue="192.0.0.1"
          uiShowName="IPv4" uiNoShow="Interface" />
      <FIELD name="useMask" type="bool_type" defaultValue="0"
          uiShowName="Use Mask" uiNoShow="Interface" doNotBuild="true" />     
      <FIELD name="mask" type="IPV4ADDR" defaultValue="255.255.255.255"
          uiShowName="Mask" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="nw_dst" type="SEQUENCE" category="OF10"
        unitType="ELEMENT" uiShowName="IPv4 Destination (nw_dst)" >
      <FIELD name="addr" type="IPV4ADDR" defaultValue="192.0.0.1"
          uiShowName="IPv4" uiNoShow="Interface" />
      <FIELD name="useMask" type="bool_type" defaultValue="0"
          uiShowName="Use Mask" uiNoShow="Interface" doNotBuild="true" />    
      <FIELD name="mask" type="IPV4ADDR" defaultValue="255.255.255.255"
          uiShowName="Mask" uiNoShow="Interface" />

    </TYPE_DEF>
    <TYPE_DEF name="nw_tos" type="SEQUENCE" category="OF10"
        unitType="ELEMENT" uiShowName="IPv4/IPv6 ToS/DSCP (nw_tos)" >
      <FIELD name="tos" type="INTEGER"  bitLength="6" defaultValue="0"
          uiShowName="ToS" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="nw_proto" type="SEQUENCE" category="OF10"
        unitType="ELEMENT" uiShowName="IPv4/IPv6 Protocol (nw_proto)" >
      <FIELD name="protocol" type="types:IpProtocolNumbers" bitLength="8" defaultValue="4"
          uiShowName="Protocol" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="tp_src" type="SEQUENCE" category="OF10"
        unitType="ELEMENT" uiShowName="TCP/UDP Source (tp_src)" >
      <FIELD name="port_number" type="INTEGER" bitLength="16" defaultValue="100"
          uiShowName="Port Number" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="tp_dst" type="SEQUENCE" category="OF10"
        unitType="ELEMENT" uiShowName="TCP/UDP Destination Port (tp_dst)" >
      <FIELD name="port_number" type="INTEGER" bitLength="16" defaultValue="100"
          uiShowName="Port Number" uiNoShow="Interface" />
    </TYPE_DEF>
	<TYPE_DEF name="metadata" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="Metadata" >
      <FIELD name="id" type="INTEGER" bitLength="64" defaultValue="0"
          uiShowName="Metadata" uiNoShow="Interface" />
      <FIELD name="useMask" type="bool_type" defaultValue="0"
          uiShowName="Use Mask" uiNoShow="Interface" doNotBuild="true" /> 
      <FIELD name="mask" type="OCTETSTRING" byteLength="8" defaultValue="FFFFFFFFFFFFFFFF"
          uiShowName="Mask" uiNoShow="Interface" />    
    </TYPE_DEF>
	<TYPE_DEF name="nw_ecn" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="IPv4/IPv6 ToS/ECN (nw_ecn)" >
      <FIELD name="tos" type="INTEGER"  bitLength="2" defaultValue="0"
          uiShowName="ToS/ECN" uiNoShow="Interface" />
    </TYPE_DEF>
	<TYPE_DEF name="icmpv4_type" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="ICMPv4 Type (icmpv4_type)" >
      <FIELD name="type" type="icmp:IcmpType" defaultValue="15"
          uiShowName="Type" uiNoShow="Interface" />
	</TYPE_DEF>
	<TYPE_DEF name="icmpv4_code" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="ICMPv4 Code (icmpV4_code)" >
      <FIELD name="code" type="INTEGER" bitLength="8" defaultValue="0"
          uiShowName="Code" uiNoShow="Interface" />
    </TYPE_DEF>
	<TYPE_DEF name="arp_op" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="ARP OpCode (arp_op)" >
      <FIELD name="type" type="arp:ARPOperationByte" defaultValue="1"
          uiShowName="Code" uiNoShow="Interface" />
    </TYPE_DEF>
	<TYPE_DEF name="arp_spa" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="ARP Source (arp_spa)" >
      <FIELD name="addr" type="IPV4ADDR" defaultValue="192.0.0.1"
          uiShowName="IPv4" uiNoShow="Interface" />
      <FIELD name="useMask" type="bool_type" defaultValue="0"
          uiShowName="Use Mask" uiNoShow="Interface" doNotBuild="true" /> 
      <FIELD name="mask" type="IPV4ADDR" defaultValue="255.255.255.255"
          uiShowName="Mask" uiNoShow="Interface" />
    </TYPE_DEF>
	<TYPE_DEF name="arp_tpa" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="ARP Destination (arp_tpa)" >
      <FIELD name="addr" type="IPV4ADDR" defaultValue="192.0.0.1"
          uiShowName="IPv4" uiNoShow="Interface" />
      <FIELD name="useMask" type="bool_type" defaultValue="0"
          uiShowName="Use Mask" uiNoShow="Interface" doNotBuild="true" /> 
      <FIELD name="mask" type="IPV4ADDR" defaultValue="255.255.255.255"
          uiShowName="Mask" uiNoShow="Interface" />
    </TYPE_DEF>
	<TYPE_DEF name="arp_sha" type="SEQUENCE" 
        unitType="ELEMENT" uiShowName="ARP Source Hardware (arp_sha)" >
      <FIELD name="addr" type="MACADDR" defaultValue="00:00:01:00:00:01"
          uiShowName="MAC" uiNoShow="Interface" />
      <FIELD name="useMask" type="bool_type" defaultValue="0"
          uiShowName="Use Mask" uiNoShow="Interface" doNotBuild="true" /> 
      <FIELD name="mask" type="MACADDR" defaultValue="FF:FF:FF:FF:FF:FF"
          uiShowName="Mask" uiNoShow="Interface" />
    </TYPE_DEF>
	<TYPE_DEF name="arp_tha" type="SEQUENCE" 
        unitType="ELEMENT" uiShowName="ARP Target Hardware (arp_tha)" >
      <FIELD name="addr" type="MACADDR" defaultValue="00:00:01:00:00:01"
          uiShowName="MAC" uiNoShow="Interface" />
      <FIELD name="useMask" type="bool_type" defaultValue="0"
          uiShowName="Use Mask" uiNoShow="Interface" doNotBuild="true" />     
      <FIELD name="mask" type="MACADDR" defaultValue="FF:FF:FF:FF:FF:FF"
          uiShowName="Mask" uiNoShow="Interface" />
    </TYPE_DEF>
	<TYPE_DEF name="tun_id" type="SEQUENCE" 
        unitType="ELEMENT" uiShowName="Tunnel ID (tun_id)" >

      <FIELD name="id" type="INTEGER" bitLength="64" defaultValue="0"
          uiShowName="Tunnel ID" uiNoShow="Interface" />
      <FIELD name="useMask" type="bool_type" defaultValue="0"
          uiShowName="Use Mask" uiNoShow="Interface" doNotBuild="true" />     
      <FIELD name="mask" type="OCTETSTRING" byteLength="8" defaultValue="FFFFFFFFFFFFFFFF"
          uiShowName="Mask" uiNoShow="Interface" />

    </TYPE_DEF>
	<TYPE_DEF name="sctp_src" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="SCTP Source (sctp_src)" >
      <FIELD name="port_number" type="INTEGER" bitLength="16" defaultValue="100"
          uiShowName="Port Number" uiNoShow="Interface" />
    </TYPE_DEF>
	<TYPE_DEF name="sctp_dst" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="SCTP Destination (sctp_dst)" >
      <FIELD name="port_number" type="INTEGER" bitLength="16" defaultValue="100"
          uiShowName="Port Number" uiNoShow="Interface" />
    </TYPE_DEF>
	<TYPE_DEF name="ext_hdr" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="IPv6 Ext Header" >
      <FIELD name="header" type="INTEGER" bitLength="16" defaultValue="0"
          uiShowName="Header" uiNoShow="Interface" />
      <FIELD name="useMask" type="bool_type" defaultValue="0"
          uiShowName="Use Mask" uiNoShow="Interface" doNotBuild="true" /> 
      <FIELD name="mask" type="OCTETSTRING" byteLength="2" defaultValue="FFFF"
          uiShowName="Mask" uiNoShow="Interface" />
    </TYPE_DEF>	
	<TYPE_DEF name="pbb_isid" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="PBB-ISID" >
      <FIELD name="id" type="INTEGER" bitLength="32" defaultValue="0"
          uiShowName="id" uiNoShow="Interface" />
      <FIELD name="useMask" type="bool_type" defaultValue="0"
          uiShowName="Use Mask" uiNoShow="Interface" doNotBuild="true" />
      <FIELD name="mask" type="OCTETSTRING" byteLength="4" defaultValue="FFFFFFFF"
          uiShowName="Mask" uiNoShow="Interface" />          
    </TYPE_DEF>
    <TYPE_DEF name="tcp_src" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="TCP Source (tcp_src)" >
      <FIELD name="port_number" type="INTEGER" bitLength="16" defaultValue="100"
          uiShowName="Port Number" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="tcp_dst" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="TCP Destination (tcp_dst)" >
      <FIELD name="port_number" type="INTEGER" bitLength="16" defaultValue="100"
          uiShowName="Port Number" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="udp_src" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="UDP Source (udp_src)" >
      <FIELD name="port_number" type="INTEGER" bitLength="16" defaultValue="100"
          uiShowName="Port Number" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="udp_dst" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="UDP Destination (udp_dst)" >
      <FIELD name="udp_dst" type="INTEGER" bitLength="16" defaultValue="100"
          uiShowName="Port Number" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="mpls_label" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="MPLS Label (mpls_label)" >
      <FIELD name="mpls_label" type="INTEGER" bitLength="32" defaultValue="10"
          uiShowName="Label" uiNoShow="Interface" />
    </TYPE_DEF>

    <TYPE_DEF name="mpls_tc" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="MPLS Traffic Class (mpls_tc)" >
      <FIELD name="traffic_class" type="INTEGER" bitLength="3" defaultValue="1"
          uiShowName="Traffic Class" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="mpls_bos" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="MPLS Bottom Of Stack (mpls_bos)" >
      <FIELD name="bos" type="INTEGER" bitLength="1" defaultValue="1"
          uiShowName="Bottom Of Stack" uiNoShow="Interface" />
    </TYPE_DEF>

    <TYPE_DEF name="ipv6_src" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="IPv6 Source (ipv6_src)" >
      <FIELD name="addr" type="IPV6ADDR" defaultValue="2000::2:1"
          uiShowName="IPv6" uiNoShow="Interface" />
      <FIELD name="useMask" type="bool_type" defaultValue="0"
          uiShowName="Use Mask" uiNoShow="Interface" doNotBuild="true" />
      <FIELD name="mask" type="IPV6ADDR" defaultValue="FFFF:FFFF:FFFF:FFFF:FFFF:FFFF:FFFF:FFFF"
          uiShowName="Mask" uiNoShow="Interface" />    
    </TYPE_DEF>
    <TYPE_DEF name="ipv6_dst" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="IPv6 Destination (ipv6_dst)" >
      <FIELD name="addr" type="IPV6ADDR" defaultValue="2000::1:1"
          uiShowName="IPv6" uiNoShow="Interface" />
      <FIELD name="useMask" type="bool_type" defaultValue="0"
          uiShowName="Use Mask" uiNoShow="Interface" doNotBuild="true" />
      <FIELD name="mask" type="IPV6ADDR" defaultValue="FFFF:FFFF:FFFF:FFFF:FFFF:FFFF:FFFF:FFFF"
          uiShowName="Mask" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="ipv6_label" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="IPv6 Flow Label (ipv6_label)" >
      <FIELD name="label" type="INTEGER" bitLength="32" defaultValue="1"
          uiShowName="Label" uiNoShow="Interface" />
      <FIELD name="useMask" type="bool_type" defaultValue="0"
          uiShowName="Use Mask" uiNoShow="Interface" doNotBuild="true" /> 
      <FIELD name="mask" type="OCTETSTRING" byteLength="4" defaultValue="FFFFFFFF"
          uiShowName="Mask" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="icmpv6_type" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="ICMPv6 Type (icmpv6_type)" >
      <FIELD name="type" type="icmpv6:Icmpv6Type" defaultValue="135"
          uiShowName="Type" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="icmpv6_code" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="IPv6 Code (icmpv6_code)" >
      <FIELD name="code" type="INTEGER" bitLength="8" defaultValue="0"
          uiShowName="Code" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="nd_target" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="IPv6 Nbr Disc Target (nd_target)" >
      <FIELD name="target" type="IPV6ADDR" defaultValue="2000::1:1"
          uiShowName="Target" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="nd_sll" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="IPv6 Nbr Solicit Source (nd_sll)" >
      <FIELD name="source" type="MACADDR" defaultValue="00:00:01:00:00:01"
          uiShowName="Source" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="nd_tll" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="IPv6 Nbr Advertisement Source (nd_tll)" >
      <FIELD name="source" type="MACADDR" defaultValue="00:00:01:00:00:01"
          uiShowName="Source" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="experimenter" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="Experimenter (exp)" maxOccurs="6">
      <FIELD name="experimenterField" type="INTEGER" bitLength="7" defaultValue="0"
          uiShowName="Experimenter Field" uiNoShow="Interface" />        
      <FIELD name="experimenterId" type="OCTETSTRING" byteLength="4" defaultValue="00000000"
          uiShowName="Experimenter ID" uiNoShow="Interface" />            
      <FIELD name="experimenterValue" type="OCTETSTRING" minByteLength="1" maxByteLength="251" defaultValue="0"
         uiShowName="Experimenter Value" uiNoShow="Interface" />
      <FIELD name="useMask" type="bool_type" defaultValue="0"
        uiShowName="Use Mask" uiNoShow="Interface" doNotBuild="true" />
      <FIELD name="mask" type="OCTETSTRING" minByteLength="1" maxByteLength="125" defaultValue="F"
      uiShowName="Mask" />    
    </TYPE_DEF>
        <TYPE_DEF name="set_field_exp" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="Set Field Experimenter (set_field_exp)" maxOccurs="20" >
      <FIELD name="experimenterField" type="INTEGER" bitLength="7" defaultValue="0"
          uiShowName="Experimenter Field" uiNoShow="Interface" uiNoShowModifier="Tlv"/>        
      <FIELD name="experimenterId" type="OCTETSTRING" byteLength="4" defaultValue="00000000"
          uiShowName="Experimenter ID" uiNoShow="Interface" uiNoShowModifier="Tlv"/>            
      <FIELD name="experimenterValue" type="OCTETSTRING" minByteLength="1" maxByteLength="251" defaultValue="0"
         uiShowName="Experimenter Value" uiNoShow="Interface" uiNoShowModifier="Tlv"/>  
    </TYPE_DEF>
    
    <TYPE_DEF name="set_field_exp_ip" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="Set Field Experimenter IP (set_field_exp_ip)" maxOccurs="20">
      <FIELD name="experimenterField" type="INTEGER" bitLength="7" defaultValue="0"
          uiShowName="Experimenter Field" uiNoShow="Interface" uiNoShowModifier="Tlv"/>        
      <FIELD name="experimenterId" type="OCTETSTRING" byteLength="4" defaultValue="00000000"
          uiShowName="Experimenter ID" uiNoShow="Interface" uiNoShowModifier="Tlv"/>            
      <FIELD name="ipAddr" type="IPV4ADDR" defaultValue="192.0.0.1" uiShowName="IPv4" uiNoShow="Interface" uiNoShowModifier="Tlv"/>
    </TYPE_DEF>
    
    <TYPE_DEF name="set_field_exp_dec" type="SEQUENCE"
        unitType="ELEMENT" uiShowName="Set Field Experimenter Decimal (set_field_exp_dec)" maxOccurs="20">
      <FIELD name="experimenterField" type="INTEGER" bitLength="7" defaultValue="0"
          uiShowName="Experimenter Field" uiNoShow="Interface" uiNoShowModifier="Tlv"/>        
      <FIELD name="experimenterId" type="OCTETSTRING" byteLength="4" defaultValue="00000000"
          uiShowName="Experimenter ID" uiNoShow="Interface" uiNoShowModifier="Tlv"/>            
      <FIELD name="expValueDecimal" type="INTEGER" bitLength="64" defaultValue="0"
      uiShowName="Value(Decimal)" uiNoShow="Interface" uiNoShowModifier="Tlv"/>
    </TYPE_DEF>


