	  <FIELD name="pbb_isid" type="pbb_isid" uiShowName="PBB-ISID " />
	  <FIELD name="nw_ecn" type="nw_ecn" uiShowName="IPv4/IPv6 ToS/ECN" />
      <FIELD name="ipv6_src" type="ipv6_src" uiShowName="IPv6 Source" />
      <FIELD name="ipv6_dst" type="ipv6_dst" uiShowName="IPv6 Destination" />
      <FIELD name="ipv6_label" type="ipv6_label" uiShowName="IPv6 Label" />
      <FIELD name="icmpv6_type" type="icmpv6_type" uiShowName="ICMPv6 Type" />
      <FIELD name="icmpv6_code" type="icmpv6_code" uiShowName="ICMPv6 Code" />
