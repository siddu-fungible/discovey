        <TYPE_DEF name="goto_table" type="SEQUENCE"
                unitType="ELEMENT" uiShowName="Goto Table (goto_table)">
            <FIELD name="table" type="INTEGER"  bitLength="8" defaultValue="1"
                    uiShowName="Table" uiNoShow="Interface" uiNoShowModifier="Tlv"/>
        </TYPE_DEF>
        <TYPE_DEF name="meter" type="SEQUENCE"
                unitType="ELEMENT" uiShowName="Meter (meter)">
            <FIELD name="meter" type="INTEGER"  bitLength="32" defaultValue="1"
                    minValue="1" uiShowName="Meter ID" uiNoShowModifier="Tlv" />
        </TYPE_DEF>
		<TYPE_DEF name="writeMetadata" type="SEQUENCE"
                unitType="ELEMENT" uiShowName="Write-Metadata (write_metadata)">
			<FIELD name="metadata" type="OCTETSTRING" byteLength="8" defaultValue="0000000000000000"
                    uiShowName="Metadata" uiNoShowModifier="Tlv" />
			<FIELD name="metadataMask" type="OCTETSTRING" byteLength="8"
                    defaultValue="FFFFFFFFFFFFFFFF" uiShowName="Metadata Mask" uiNoShowModifier="Tlv" />
		</TYPE_DEF>

		<TYPE_DEF name="clearAction" type="SEQUENCE"
                unitType="ELEMENT" uiShowName="Clear-Action (clear_action)">
			<FIELD name="name" type="ASCII"
                    defaultValue="clear" uiNoShow="Tlv" uiShowCommon="false" />
		</TYPE_DEF>

		<TYPE_DEF name="writeAction" type="SEQUENCE"
                unitType="ELEMENT" uiShowName="Write-Action (write_action)">
			<FIELD name="writeActions" type="OfActionList"
                    uiShowName="Actions" uiNoShow="Interface" />
		</TYPE_DEF>

		<TYPE_DEF name="applyAction" type="SEQUENCE"
                unitType="ELEMENT" uiShowName="Apply-Action (apply_action)">
			<FIELD name="applyActions" type="OfActionList"
                    uiShowName="Actions" uiNoShow="Interface" />
		</TYPE_DEF>
        <TYPE_DEF name="experimenterInstruction" type="SEQUENCE"
                unitType="ELEMENT" uiShowName="Experimenter Instruction" maxOccurs ="20" >
                <FIELD name="exp_id" type="OCTETSTRING" byteLength="4" defaultValue="00000000"
                    uiShowName="Experimenter ID" uiNoShow="Interface" uiNoShowModifier="Tlv"/>
                <FIELD name="exp_data" type="OCTETSTRING"  minByteLength="1" maxByteLength="252" defaultValue="0" 
                    uiShowName="Experimenter Data" uiNoShow="Interface" uiNoShowModifier="Tlv"/>
        </TYPE_DEF>
		<TYPE_DEF name="OfActionList" type="SET_OF" minOccurs="0" maxOccurs ="100" elementType="OfAction" unitType="ELEMENT" />	
        <TYPE_DEF name="RmActionList" type="SET_OF" minOccurs="0" maxOccurs ="40" elementType="RmAction" unitType="ELEMENT" />  
        
        <TYPE_DEF name="InstructionList"  type="CHOICE" unitType="ELEMENT" uiShowName="Instruction">
            <FIELD name="gotoTable" type="goto_table" uiShowName="Goto Table"/>
			<FIELD name="writeMetadata" type="writeMetadata" uiShowName="Write-Metadata"/>
			<FIELD name="writeActions" type="writeAction" uiShowName="Write-Action"/>
			<FIELD name="applyActions" type="applyAction" uiShowName="Apply-Action" uiShowType="OF10"/>
			<FIELD name="clearActions" type="clearAction" uiShowName="Clear-Action"/>
            <FIELD name="meters" type="meter" uiShowName="Meter"/>
            <FIELD name="experimenterInstructions" type="experimenterInstruction" uiShowName="Experimenter"/>
    </TYPE_DEF>

		<TYPE_DEF name="OfInstructionList" type="SET_OF" minOccurs="0" maxOccurs ="100" elementType="InstructionList" unitType="ELEMENT" />

    <TYPE_DEF name="Flow" type="SEQUENCE" unitType="HEADER"
          level="3" uiShowName="Flow" niMapped="true">

        <FIELD name="table" type="INTEGER" bitLength="8" defaultValue="0"
               uiShowName="Table" uiNoShow="Interface"/>
        <FIELD name="idle_timeout" type="INTEGER" bitLength="16" defaultValue="0"
               uiNoShowModifier="Tlv" uiShowName="Idle Timeout" uiNoShow="Interface" />
        <FIELD name="hard_timeout" type="INTEGER" bitLength="16" defaultValue="0"
               uiNoShowModifier="Tlv" uiShowName="Hard Timeout" uiNoShow="Interface" />
        <FIELD name="priority" type="INTEGER" bitLength="16" defaultValue="32768"
               uiShowName="Priority" uiNoShow="Interface" />
        <FIELD name="flags" type="OCTETSTRING" byteLength="2" defaultValue="0001"
               uiNoShowModifier="Tlv" uiShowName="Flags" uiNoShow="Interface" uiOverrideEditor="OpenflowFlowFlagsEditor"/>
        <FIELD name="cookie" type="OCTETSTRING" byteLength="8" defaultValue="0000000000000000"
               uiShowName="Cookie" uiNoShow="Interface" />
        <FIELD name="cookie_mask" type="OCTETSTRING" byteLength="8" defaultValue="0000000000000000"
               uiNoShowModifier="Tlv" uiShowName="Cookie Mask" uiNoShow="Interface" />               

        <FIELD name="Match" type="OfMatchList"
               uiShowName="Match" uiNoShow="Interface" />

		<!-- Do not show Actions. If a config has this then it will be converted to Instructions. -->
        <FIELD name="Actions" type="OfActionList"
               uiShowName="Actions" uiNoShow="Interface,Filter,Tlv" doNotBuild="true" uiShowCommon="false" />

        <FIELD name="Instructions" type="OfInstructionList"
               uiShowName="Instructions" uiNoShow="Interface" />

        <NEXT_HEADERS>
        </NEXT_HEADERS>
    </TYPE_DEF>
    <TYPE_DEF name="ReactiveModeMatch" type="CHOICE" unitType="ELEMENT"
            uiShowName="Reactive Mode Match" niMapped="true">
       <FIELD name="Match" type="RmMatchList"
               uiShowName="Match" uiNoShow="Interface" />
    </TYPE_DEF>
    <TYPE_DEF name="ReactiveModeAction" type="CHOICE" unitType="ELEMENT"
            uiShowName="Reactive Mode Action" niMapped="true">
       <FIELD name="Action" type="RmActionList"
               uiShowName="Action" uiNoShow="Interface" />
    </TYPE_DEF>
  </TYPE_DEFS>
</PDU_LIBRARY>
