    <TYPE_DEF name="RmAction" type="CHOICE" unitType="ELEMENT" uiShowName="Action" >  
        <!--Action Set 1-->
        <FIELD name="copy_ttl_in"  type="copy_ttl_in" uiShowName="Copy TTL In" />
        <!--Action Set 2-->
        <FIELD name="pop_mpls" type="pop_mpls" uiShowName="Pop MPLS" />
        <FIELD name="pop_pbb" type="pop_pbb" uiShowName="Pop PBB" />
        <FIELD name="pop_vlan" type="pop_vlan" uiShowName="Pop VLAN" />
        <!--Action Set 3-->
        <FIELD name="push_mpls" type="push_mpls" uiShowName="Push MPLS" />
        <!--Action Set 4-->
        <FIELD name="push_pbb"  type="push_pbb" uiShowName="Push PBB" />
        <!--Action Set 5-->
        <FIELD name="push_vlan" type="push_vlan" uiShowName="Push VLAN" />
        <!--Action Set 6-->
        <FIELD name="copy_ttl_out" type="copy_ttl_out" uiShowName="Copy TTL Out" />
        <!--Action Set 7-->
        <FIELD name="dec_mpls_ttl" type="dec_mpls_ttl" uiShowName="Decrement MPLS TTL" />
        <FIELD name="dec_ttl" type="dec_ttl" uiShowName="Decrement IPv4/IPv6 TTL" />
        <!--Action Set 8-->
        <FIELD name="mod_dl_src" type="mod_dl_src" uiShowName="Modify Ethernet Source" />
        <FIELD name="mod_dl_dst" type="mod_dl_dst" uiShowName="Modify Ethernet Destination" />
        <FIELD name="mod_vlan_vid" type="mod_vlan_vid" uiShowName="Modify VLAN ID" />
        <FIELD name="mod_vlan_pcp" type="mod_vlan_pcp" uiShowName="Modify VLAN PCP"/>
        <FIELD name="mod_nw_src" type="mod_nw_src" uiShowName="Modify IPv4 Source" />
        <FIELD name="mod_nw_dst" type="mod_nw_dst" uiShowName="Modify IPv4 Destination"/>
        <FIELD name="mod_nw_tos" type="mod_nw_tos" uiShowName="Modify IPv4/IPv6 ToS/DSCP"/>
        <FIELD name="set_mpls_label" type="set_mpls_label" uiShowName="Set MPLS Label" />
        <FIELD name="set_mpls_tc" type="set_mpls_tc" uiShowName="Set MPLS TC" />
        <FIELD name="set_mpls_bos" type="set_mpls_bos" uiShowName="Set MPLS BOS" />
        <FIELD name="set_mpls_ttl" type="set_mpls_ttl" uiShowName="Set MPLS TTL" />
        <FIELD name="mod_ipv6_src" type="mod_ipv6_src" uiShowName="Modify IPv6 Source" />
        <FIELD name="mod_ipv6_dst" type="mod_ipv6_dst" uiShowName="Modify IPv6 Destination" />
        <FIELD name="mod_ipv6_label" type="mod_ipv6_label" uiShowName="Modify IPv6 Label" />
        <FIELD name="mod_icmpv6_type" type="mod_icmpv6_type" uiShowName="Modify ICMPv6 Type" />
        <FIELD name="mod_icmpv6_code" type="mod_icmpv6_code" uiShowName="Modify ICMPv6 Code" />
        <FIELD name="mod_nd_target" type="mod_nd_target" uiShowName="Modify Nbr Discovery Target IPv6" />
        <FIELD name="mod_nd_sll" type="mod_nd_sll" uiShowName="Modify Nbr Solicit Source MAC" />
        <FIELD name="mod_nd_tll" type="mod_nd_tll" uiShowName="Modify Nbr Advertisement Source MAC" />
        <FIELD name="mod_tcp_src" type="mod_tcp_src" uiShowName="Modify TCP Source" />
        <FIELD name="mod_tcp_dst" type="mod_tcp_dst" uiShowName="Modify TCP Destination" />
        <FIELD name="mod_udp_src" type="mod_udp_src" uiShowName="Modify UDP Source" />
        <FIELD name="mod_udp_dst" type="mod_udp_dst" uiShowName="Modify UDP Destination" />
        <FIELD name="mod_sctp_src" type="mod_sctp_src" uiShowName="Modify SCTP Source" />
        <FIELD name="mod_sctp_dst" type="mod_sctp_dst" uiShowName="Modify SCTP Destination" />
        <FIELD name="mod_tun_id" type="mod_tun_id" uiShowName="Modify Tunnel ID" />
        <!--Action Set 9-->
        <FIELD name="set_queue" type="set_queue" uiShowName="Set Queue" />
        <!--Action Set 10-->
        <FIELD name="group" type="group" uiShowName="Group"/>
        
        <FIELD name="output" type="output" uiShowName="Output Port"/>
        <FIELD name="controller" type="controller" uiShowName="Controller" />
        <FIELD name="normal" type="output_normal" uiShowName="Output Normal"/>
        <FIELD name="flood" type="output_flood" uiShowName="Output Flood" />
        <FIELD name="all" type="output_all" uiShowName="Output All" />
