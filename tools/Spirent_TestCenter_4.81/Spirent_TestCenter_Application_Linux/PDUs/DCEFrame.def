<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="pdu_schema.xsd"
  name="dce"
  showName="Cisco DCE Header"
  version="2.01"
  description="This is a sample PDU library for DCE (FCoE) as defined in XML Stream Editor."
  userdefined="false">

  <TYPE_DEFS>
	<TYPE_DEF name="HierarchichalMACAddress" type="SEQUENCE" unitType="ELEMENT" uiShowName="Hierarchichal MAC Address">
	
		<FIELD name="endnodeid" type="INTEGER" bitLength="6" 
			defaultValue="1" uiNoShow="Interface"
			uiShowCommon="true" uiShowName="End Node ID"  />
			
		<FIELD name="u_l" type="BITSTRING" bitLength="1" 
			defaultValue="1" uiNoShow="Interface" uiNoShowModifier="Interface,Frame,Filter"
			uiShowCommon="true" uiShowName="U/L"  />
			
		<FIELD name="i_g" type="BITSTRING" bitLength="1" 
			defaultValue="1" uiNoShow="Interface" uiNoShowModifier="Interface,Frame,Filter"
			uiShowCommon="true" uiShowName="I/G"  />
			
		<FIELD name="endnodeid_2" type="INTEGER" bitLength="2" 
			defaultValue="1" uiNoShow="Interface" uiNoShowModifier="Interface,Frame,Filter"
			uiShowCommon="true" uiShowName="End Node ID"  />
		
		<FIELD name="rsvd" type="BITSTRING" bitLength="1" 
			defaultValue="0" uiNoShow="Interface" uiNoShowModifier="Interface,Frame,Filter"
			uiShowCommon="false" uiShowName="Reserved"  />
			
		<FIELD name="ooo_dl" type="BITSTRING" bitLength="1" 
			defaultValue="1" uiNoShow="Interface" uiNoShowModifier="Interface,Frame,Filter"
			uiShowCommon="true" uiShowName="OOO/DL"  />
		
		<FIELD name="switchid" type="INTEGER" bitLength="12" 
			defaultValue="1" uiNoShow="Interface"
			uiShowCommon="true" uiShowName="Switch ID"  />
			
		<FIELD name="subswitchid" type="INTEGER" bitLength="8" 
			defaultValue="1" uiNoShow="Interface"
			uiShowCommon="true" uiShowName="Sub Switch ID"  />
			
		<FIELD name="localid" type="INTEGER" bitLength="16" 
			defaultValue="1" uiNoShow="Interface"
			uiShowCommon="true" uiShowName="Local ID"  />
	
		
	</TYPE_DEF>
	
	<TYPE_DEF name="InnerEtherEnum" type="OCTETSTRING" byteLength="2" unitType="ELEMENT">
		<VALUE_ENUMERATION>
			<VAL_ENUM uiShowName="S-Tag" value="88a8" />
			<VAL_ENUM uiShowName="dot1Q-Tag" value="8100" />
		</VALUE_ENUMERATION>
	</TYPE_DEF>
	
	<TYPE_DEF name="CiscoSTagDef" type="SEQUENCE" unitType="ELEMENT" uiShowName="Cisco S Tag">
	
		<FIELD name="vl" type="BITSTRING" bitLength="3" defaultValue="0" uiShowName="VL" uiNoShowModifier="Interface"/>
		
		<FIELD name="dei_cfi" type="BITSTRING" bitLength="1" defaultValue="0" uiShowName="DE/CFI" uiNoShowModifier="Interface,Frame,Filter"/>
		
		<FIELD name="vlan" type="INTEGER" bitLength="12" defaultValue="0" uiShowName="VLAN" />
	
	</TYPE_DEF>
	
	<TYPE_DEF name="STag8021adDef" type="SEQUENCE" unitType="ELEMENT" uiShowName="802.1ad S Tag">
	
		<FIELD name="vl" type="BITSTRING" bitLength="3" defaultValue="0" uiShowName="VL" uiNoShowModifier="Interface"/>
		
		<FIELD name="dei_cfi" type="BITSTRING" bitLength="1" defaultValue="0" uiShowName="DE/CFI" uiNoShowModifier="Interface,Frame,Filter"/>
		
		<FIELD name="rsvd" type="INTEGER" bitLength="4" defaultValue="0" uiShowName="Reserved" uiNoShowModifier="Interface,Frame,Filter"/>
		
		<FIELD name="vlan" type="INTEGER" bitLength="8" defaultValue="0" uiShowName="VLAN" />
	
	</TYPE_DEF>
	
	<TYPE_DEF name="DTagDef" type="SEQUENCE" unitType="ELEMENT" uiShowName="D Tag">
	
		<FIELD name="ftag" type="INTEGER" defaultValue="1" bitLength="10" uiShowName="FTag" uiNoShowModifier="Interface,Frame,Filter"/>
		
		<FIELD name="ttl" type="INTEGER" defaultValue="1" bitLength="6" uiShowName="TTL" uiNoShowModifier="Interface,Frame,Filter"/>
		
	</TYPE_DEF>
	
	
	
	<TYPE_DEF name="CiscoDCECiscoStag" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="Cisco DCE Header with Cisco S Tag" >
		
		<FIELD name="outerdamac" type="HierarchichalMACAddress"  uiShowName="OuterDaMac" uiNoShowModifier="Interface,Frame,Filter" />
		<FIELD name="outersamac" type="HierarchichalMACAddress"  uiShowName="OuterSaMac" uiNoShowModifier="Interface,Frame,Filter" />
		
		<!-- this needs to be checked -->
		<FIELD name="ethertype" type="types:EtherTypes"
        defaultValue="88B5"
		uiNoShow="Interface"
		uiNoShowModifier="Interface"
        uiShowName="Ether Type" />
		
		<FIELD name="dtag" type="DTagDef" uiShowName="D-Tag "/>
		
		<FIELD name="originaldamac" type="MACADDR" defaultValue="00:00:01:00:00:01" uiShowName="Original DA MAC "/>
		
		<FIELD name="originalsamac" type="MACADDR"
        defaultValue="00:10:94:00:00:02"
        uiShowName="Original SA MAC" /> 
		
		
		<FIELD name="stagethertype" type="types:EtherTypes" defaultValue="88a8" uiShowName="Ether Type" />
		
		<FIELD name="ciscostag" type="CiscoSTagDef"  uiShowName="Cisco S-Tag" uiNoShowModifier="Interface,Frame,Filter" />
		
		<FIELD name="originalethertype" type="types:EtherTypes"
        defaultValue="88B5"
        uiControlState="ALLOW_OVERRIDE"   
        procfnTarget="NEXTHDR" 
		uiNoShow="Interface"
		uiNoShowModifier="Interface"
        uiShowName="EtherType" />
		
		
		<NEXT_HEADERS>
			<NXT_HDR id="ipv4:IPv4" name="IPv4 Header" value="0800" />
			<NXT_HDR id="ipv6:IPv6" name="IPv6 Header" value="86DD" />
			<!--<NXT_HDR id="fc:FCoE" name="FCoE Header" value="8906"/>-->
        </NEXT_HEADERS>
	</TYPE_DEF>
	
	<TYPE_DEF name="CiscoDCE8021adSTag" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="Cisco DCE Header with Cisco S Tag" >
		
		<FIELD name="outerdamac" type="HierarchichalMACAddress"  uiShowName="OuterDaMac" uiNoShowModifier="Interface,Frame,Filter" />
		<FIELD name="outersamac" type="HierarchichalMACAddress"  uiShowName="OuterSaMac" uiNoShowModifier="Interface,Frame,Filter" />
		
		<!-- this needs to be checked -->
		<FIELD name="ethertype" type="types:EtherTypes"
        defaultValue="88B5"
		uiNoShow="Interface"
		uiNoShowModifier="Interface"
        uiShowName="Ether Type" />
		
		<FIELD name="dtag" type="DTagDef" uiShowName="D-Tag "/>
		
		<FIELD name="originaldamac" type="MACADDR" defaultValue="00:00:01:00:00:01" uiShowName="Original DA MAC "/>
		
		<FIELD name="originalsamac" type="MACADDR"
        defaultValue="00:10:94:00:00:02"
        uiShowName="Original SA MAC" /> 
		
		
		<FIELD name="stagethertype" type="types:EtherTypes" defaultValue="88a8" uiShowName="Ether Type" />
		
		<FIELD name="stag8021ad" type="STag8021adDef"  uiShowName="802.1ad S-Tag" uiNoShowModifier="Interface,Frame,Filter" />
		
		<FIELD name="originalethertype" type="types:EtherTypes"
        defaultValue="88B5"
        uiControlState="ALLOW_OVERRIDE"   
        procfnTarget="NEXTHDR" 
		uiNoShow="Interface"
		uiNoShowModifier="Interface"
        uiShowName="EtherType" />
		
		
		<NEXT_HEADERS>
			<NXT_HDR id="ipv4:IPv4" name="IPv4 Header" value="0800" />
			<NXT_HDR id="ipv6:IPv6" name="IPv6 Header" value="86DD" />
			<NXT_HDR id="fc:FCoE" name="FCoE Header" value="8906"/>
        </NEXT_HEADERS>
	</TYPE_DEF>
	
  </TYPE_DEFS>
 </PDU_LIBRARY>
  
  
  
  
  
