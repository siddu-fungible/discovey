<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="pdu_schema.xsd"
  name="MPLSTPOAMTLV"
  showName="MPLS-TP Y1731 OAM CFM TLV"
  version="2.01"
  description="This is a sample PDU library for MPLS-TP Y1731 OAM as defined in XML Stream Editor.">

<TYPE_DEFS>

  <!-- Data TLV ========================================================== -->
  <TYPE_DEF name="DataTLV" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="Data TLV">

    <FIELD name="Type" type="OCTETSTRING" byteLength="1"
			uiNoShow="Interface" uiShowCommon="false"
			defaultValue="03" uiShowName="Type"/>

    <FIELD name="Length" type="OCTETSTRING" byteLength="2" procfnTarget="MPLSTPOAMTLVLENGTH"
			defaultValue="0000" uiShowName="Length" uiControlState="CALCULATED" />

    <FIELD name="theData" type="serviceoam:DataSelect" uiShowName="Data"/>

    <NEXT_HEADERS>      
      <NXT_HDR id="MPLSTPOAMTLV:EndTLV" name="End TLV" />
      <NXT_HDR id="MPLSTPOAMTLV:DataTLV" name="Data TLV" />
      <NXT_HDR id="MPLSTPOAMTLV:TestTLV" name="Test TLV" />     
    </NEXT_HEADERS>
  </TYPE_DEF>

  <!-- Test TLV ========================================================== -->
  <TYPE_DEF name="TestTLV" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="Test TLV">

    <FIELD name="Type" type="OCTETSTRING" byteLength="1"
			uiNoShow="Interface" uiShowCommon="false"
			defaultValue="20" uiShowName="Type"/>

    <FIELD name="Length" type="OCTETSTRING" byteLength="2" 
			defaultValue="0040" uiShowName="Length" />

    <FIELD name="PatternType" type="serviceoam:PatternTypeValues" byteLength="1"
      uiShowName="Pattern Type"  defaultValue="00" />

    <NEXT_HEADERS>      
      <NXT_HDR id="MPLSTPOAMTLV:EndTLV" name="End TLV" />
      <NXT_HDR id="MPLSTPOAMTLV:DataTLV" name="Data TLV" />
      <NXT_HDR id="MPLSTPOAMTLV:TestTLV" name="Test TLV" />      
    </NEXT_HEADERS>
  </TYPE_DEF>

  <!-- End TLV =========================================================== -->
  <TYPE_DEF name="EndTLV" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="End TLV">

    <FIELD name="Type" type="OCTETSTRING" byteLength="1"
		       	defaultValue="00" uiShowName="Type" uiShowCommon="false" />
  </TYPE_DEF>  

</TYPE_DEFS>
</PDU_LIBRARY>
