          defaultValue="1024"

          uiControlState="ALLOW_OVERRIDE"

          procfnTarget="NEXTHDR" 

          uiShowName="Destination port" />



      <FIELD name="length" type="INTEGER" bitLength="16"

          defaultValue="0"

          uiControlState="CALCULATED"

          procfnTarget="TOTALLENGTH" 

          uiShowName="Length" />



      <FIELD name="checksum" type="INTEGER" bitLength="16"

          defaultValue="0"

          uiControlState="ALLOW_OVERRIDE"

          procfnTarget="PSEUDOCHECKSUM" 

          uiNoShowModifier="Interface,Frame,Filter"

          uiGenerateError="true"

          uiShowName="Checksum" />



