<?xml version="1.0" encoding="UTF-8"?>

<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  
  xsi:noNamespaceSchemaLocation="pdu_schema.xsd" name="udp" showName="Udp" version="1.00"
  description="This is a sample PDU library for Udp as defined in XML Stream Editor.">

    

  <TYPE_DEFS>

    <TYPE_DEF name="Udp" type="SEQUENCE" unitType="HEADER"

        level="4" uiShowName="UDP Header" >

