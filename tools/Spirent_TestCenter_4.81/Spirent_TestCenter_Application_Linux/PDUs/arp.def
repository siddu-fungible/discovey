<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="pdu_schema.xsd" name="arp" showName="ARP" version="1.00"
    description="This is a sample PDU library for ARP as defined in XML Stream Editor.">
    
  <TYPE_DEFS>
        
    <TYPE_DEF name="HardwareByte" type="OCTETSTRING" byteLength="2" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="Ethernet" value="0001" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>
    
    <TYPE_DEF name="ARPOperationByte" type="INTEGER" bitLength="16" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="Unknown" value="0" />
        <VAL_ENUM uiShowName="ARP Request" value="1" />
        <VAL_ENUM uiShowName="ARP Reply" value="2" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>
    
    <TYPE_DEF name="RARPOperationByte" type="INTEGER" bitLength="16" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="RARP Request" value="3" />
        <VAL_ENUM uiShowName="RARP Reply" value="4" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>
    
    <TYPE_DEF name="ProtocolByte" type="OCTETSTRING" byteLength="2" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="Internet IP" value="0800" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>
        
    <TYPE_DEF name="ARP" type="SEQUENCE"
        unitType="HEADER" uiShowName="ARP" >
        
      <FIELD name="hardware" type="HardwareByte"
          defaultValue="0001"
          uiControlState="CALCULATED"
		  uiShowCommon="false"
          uiShowName="Hardware Type" />

      <FIELD name="protocol" type="ProtocolByte"
          defaultValue="0800"
          uiControlState="CALCULATED"
		  uiShowCommon="false"
          uiShowName="Protocol Type" />

      <FIELD name="ihAddr" type="INTEGER" bitLength="8"
          defaultValue="6"
          minValue="6"
          maxValue="6"
		  uiShowCommon="false"
          uiShowName="Hardware Address Length" />

      <FIELD name="ipAddr" type="INTEGER" bitLength="8"
          defaultValue="4"
          minValue="4"
          maxValue="6"
          uiShowName="Protocol Address Length" />
      
      <FIELD name="operation" type="ARPOperationByte"
          defaultValue="1"
		  uiShowCommon="false"
          uiShowName="Operation" />    

      <FIELD name="senderHwAddr" type="MACADDR"
		  defaultValue="00:00:01:00:00:02"
          uiShowName="Sender Hardware Address" />
          
      <FIELD name="senderPAddr" type="IPV4ADDR"
		  defaultValue="192.85.1.2"
          uiShowName="Sender Protocol Address" />
          
      <FIELD name="targetHwAddr" type="MACADDR"
		  defaultValue="00:00:00:00:00:00"
          uiShowName="Target Hardware Address" />
          
      <FIELD name="targetPAddr" type="IPV4ADDR"
		  defaultValue="0.0.0.0"
          uiShowName="Target Protocol Address" />
          
      <NEXT_HEADERS>

        <NXT_HDR id="custom:Custom" name="Custom" value="0" />
		<NXT_HDR id="customrepeat:CustomRepeat" name="CustomRepeat" value="0" />
		
      </NEXT_HEADERS>
			
			
    </TYPE_DEF> 

    <TYPE_DEF name="RARP" type="SEQUENCE"
        unitType="HEADER" uiShowName="RARP" >
        
      <FIELD name="hardware" type="HardwareByte"
          defaultValue="0001"
          uiShowName="Hardware Type" />

      <FIELD name="protocol" type="ProtocolByte"
          defaultValue="0800"
          uiShowName="Protocol Type" />

      <FIELD name="ihAddr" type="INTEGER" bitLength="8"
          defaultValue="6"
          minValue="6"
          maxValue="6"
          uiShowName="Hardware Address Length" />

      <FIELD name="ipAddr" type="INTEGER" bitLength="8"
          defaultValue="4"
          minValue="4"
          maxValue="6"
          uiShowName="Protocol Address Length" />
      
      <FIELD name="operation" type="RARPOperationByte"
          defaultValue="3"
          uiShowName="Operation" />    

      <FIELD name="senderHwAddr" type="MACADDR"
		  defaultValue="00:00:01:00:00:02"
          uiShowName="Sender Hardware Address" />
          
      <FIELD name="senderPAddr" type="IPV4ADDR"
		  defaultValue="192.85.1.2"
          uiShowName="Sender Protocol Address" />
          
      <FIELD name="targetHwAddr" type="MACADDR"
		  defaultValue="00:00:00:00:00:00"
          uiShowName="Target Hardware Address" />
          
      <FIELD name="targetPAddr" type="IPV4ADDR"
		  defaultValue="0.0.0.0"
          uiShowName="Target Protocol Address" />
          
      <NEXT_HEADERS>

        <NXT_HDR id="custom:Custom" name="Custom" value="0" />
		<NXT_HDR id="customrepeat:CustomRepeat" name="CustomRepeat" value="0" />

      </NEXT_HEADERS>
			

			
    </TYPE_DEF> 
    
  </TYPE_DEFS>
</PDU_LIBRARY>
