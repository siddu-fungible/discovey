  <!-- VMWare Provisioning Info ===================================================== -->
  <TYPE_DEF name="VmProfileNameFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="Profile Name TLV">
    <FIELD name="Type" type="INTEGER" bitLength="16"
         defaultValue="0" uiNoShow="Interface"
         uiShowCommon="false" uiShowName="Type" uiControlState="CALCULATED" />

    <FIELD name="Length" type="INTEGER" bitLength="16" procfnTarget="GEN2T2LTLVLENGTH"
         defaultValue="0" uiShowName="Length" uiControlState="CALCULATED" />

    <FIELD name="ProfileName" type="ASCII" minByteLength="1" maxByteLength="65535"
         defaultValue="SPIRENT" uiShowName="Profile Name"/>
  </TYPE_DEF>

  <TYPE_DEF name="VmVirtualMachineMacFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="Virtual Machine MAC TLV">
    <FIELD name="Type" type="INTEGER" bitLength="16"
         defaultValue="1" uiNoShow="Interface"
         uiShowCommon="false" uiShowName="Type" uiControlState="CALCULATED" />

    <FIELD name="Length" type="INTEGER" bitLength="16" procfnTarget="GEN2T2LTLVLENGTH"
         defaultValue="0" uiShowName="Length" uiControlState="CALCULATED" />

    <FIELD name="VirtualMachineMac" type="MACADDR" byteLength="6"
         defaultValue="00:00:00:00:00:01" uiShowName="Virtual Machine MAC"/>
  </TYPE_DEF>

  <TYPE_DEF name="VmVirtualMachineNameFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="Virtual Machine Name TLV">
    <FIELD name="Type" type="INTEGER" bitLength="16"
         defaultValue="2" uiNoShow="Interface"
         uiShowCommon="false" uiShowName="Type" uiControlState="CALCULATED" />

    <FIELD name="Length" type="INTEGER" bitLength="16" procfnTarget="GEN2T2LTLVLENGTH"
         defaultValue="0" uiShowName="Length" uiControlState="CALCULATED" />

