  <!-- ======================================================================= -->
  <!-- Address Array TLV = (enable, register, deregister) ==================== -->
  <TYPE_DEF name="AddrEthMacFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="Ethernet MAC">
    <FIELD name="Type" type="INTEGER" bitLength="8"
         defaultValue="1" uiShowName="Type" uiControlState="CALCULATED"/>

    <FIELD name="Length" type="INTEGER" bitLength="8"
         defaultValue="8" uiShowName="Length" uiControlState="CALCULATED"/>

    <FIELD name="Vlan" type="INTEGER" byteLength="2" minValue="1" maxValue="4095"
         defaultValue="1" uiShowName="VLAN"/>
    
    <FIELD name="MacAddress" type="MACADDR" byteLength="6"
         defaultValue="00:00:00:00:00:01" uiShowName="MAC Address"/>
  </TYPE_DEF>

  <TYPE_DEF name="AddrCustomFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="Custom">
    <FIELD name="Type" type="INTEGER" bitLength="8"
         defaultValue="0" uiShowName="Type"/>

    <FIELD name="Length" type="INTEGER" bitLength="8" procfnTarget="GEN1T1LTLVLENGTH" 
         defaultValue="0" uiShowName="Length" uiControlState="CALCULATED" />

    <FIELD name="Value" type="OCTETSTRING" maxByteLength="255"
         defaultValue="00" uiShowName="Value"/>
  </TYPE_DEF>

  <TYPE_DEF name="AddrChoice" type="CHOICE" unitType="ELEMENT" uiShowName="Address Choice">
    <FIELD name="AddrEthMac" type="AddrEthMacFormat" uiShowName="Ethernet MAC"/>
    <FIELD name="AddrCustom" type="AddrCustomFormat" uiShowName="Custom Address"/>
  </TYPE_DEF>

  <TYPE_DEF name="Address" type="SEQUENCE" unitType="ELEMENT" uiShowName="Address">
    <FIELD name="AddressType" type="AddrChoice" defaultValue="AddrEthMac" uiShowName="Address Type" />
  </TYPE_DEF>
  
  <TYPE_DEF name="AddrList" type="SEQUENCE_OF" minOccurs="0" maxOccurs="4095" elementType="Address" unitType="ELEMENT"/>

  <TYPE_DEF name="AddrArrayTLV" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="Address Array TLV">
    <FIELD name="Type" type="INTEGER" bitLength="16"
         defaultValue="5" uiNoShow="Interface"
         uiShowCommon="false" uiShowName="Type" uiControlState="CALCULATED" />

    <FIELD name="Length" type="INTEGER" bitLength="16" procfnTarget="GEN2T2LTLVLENGTH"
         defaultValue="0" uiShowName="Length" uiControlState="CALCULATED" />

    <FIELD name="Addresses" type="AddrList" uiShowName="Addresses" uiNoShowModifier="Interface,Frame"/>
    
    <NEXT_HEADERS>
