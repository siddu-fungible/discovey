  <!-- VIF State TLV = (enable, set) ===================================== -->
  <TYPE_DEF name="VifStateTLV" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="VIF State TLV">
    <FIELD name="Type" type="INTEGER" bitLength="16"
         defaultValue="6" uiNoShow="Interface"
         uiShowCommon="false" uiShowName="Type" uiControlState="CALCULATED" />

    <FIELD name="Length" type="INTEGER" bitLength="16" procfnTarget="GEN2T2LTLVLENGTH"
         defaultValue="4" uiShowName="Length" uiControlState="CALCULATED" />

    <FIELD name="Reserved" type="BITSTRING" bitLength="4"
         defaultValue="0000" uiNoShow="Interface"
         uiNoShowModifier="Interface,Frame,Filter"
         uiShowCommon="false" uiShowName="Reserved"/>

    <FIELD name="Standby" type="Boolean"
         defaultValue="0" uiShowName="Standby" />

    <FIELD name="Pause" type="Boolean"
         defaultValue="0" uiShowName="Pause" />

    <FIELD name="Down" type="Boolean"
         defaultValue="0" uiShowName="Down" />

    <FIELD name="Enabled" type="Boolean"
         defaultValue="0" uiShowName="Enabled" />

