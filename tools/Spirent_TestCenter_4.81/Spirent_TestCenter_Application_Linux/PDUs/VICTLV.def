<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="pdu_schema.xsd"
  name="VICTLV"
  showName="VIC TLV"
  version="1.00"
  description="This is a sample PDU library for VIC as defined in XML Stream Editor.">

<TYPE_DEFS>
  <!-- ==================================================== -->
  <!-- Common Elements ==================================== -->
  <TYPE_DEF name="Boolean" type="INTEGER" bitLength="1" unitType="ELEMENT">
    <VALUE_ENUMERATION>
      <VAL_ENUM uiShowName="False" value="0"/>
      <VAL_ENUM uiShowName="True"  value="1"/>
    </VALUE_ENUMERATION>
  </TYPE_DEF>

  <!-- =================================================================== -->
  <!-- Control Channel Capability TLV = (open) =========================== -->
  <TYPE_DEF name="BoolRole" type="INTEGER" bitLength="1" unitType="ELEMENT">
    <VALUE_ENUMERATION>
      <VAL_ENUM uiShowName="Adapter" value="0"/>
      <VAL_ENUM uiShowName="Switch"  value="1"/>
    </VALUE_ENUMERATION>
  </TYPE_DEF>
    
  <TYPE_DEF name="CapFlags" type="SEQUENCE" unitType="ELEMENT" uiShowName="Capability Flags">
    <FIELD name="OffsetRewrite" type="Boolean" 
         defaultValue="0" uiShowName="VN-Tag Offset/Rewrite Capability"/>
         
    <FIELD name="Reserved" type="BITSTRING" bitLength="2" 
         defaultValue="00" uiNoShow="Interface"
         uiNoShowModifier="Interface,Frame,Filter"
         uiShowCommon="false" uiShowName="Reserved"/>
         
    <FIELD name="StatefulRestart" type="Boolean"
         defaultValue="0" uiShowName="Stateful Restart Extenstion" />
         
    <FIELD name="DeleteAcceptance" type="Boolean"
         defaultValue="0" uiShowName="Delete Acceptance" />
         
    <FIELD name="VLAN" type="Boolean"
         defaultValue="0" uiShowName="VLAN Tag" />
    
    <FIELD name="Fragmented" type="Boolean"
         defaultValue="0" uiShowName="Fragmented" />
    
    <FIELD name="Role" type="BoolRole"
         defaultValue="0" uiShowName="Role" uiControlState="ALLOW_OVERRIDE" />
  </TYPE_DEF>

  <TYPE_DEF name="CtrlChanCapTLV" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="Control Channel Capability TLV">
    <FIELD name="Type" type="INTEGER" bitLength="16"
         defaultValue="17" uiNoShow="Interface"
         uiShowCommon="false" uiShowName="Type" uiControlState="CALCULATED" />
  
    <FIELD name="Length" type="INTEGER" bitLength="16" procfnTarget="GEN2T2LTLVLENGTH"
         defaultValue="6" uiShowName="Length" uiControlState="CALCULATED" />
   
    <FIELD name="Reserved" type="OCTETSTRING" byteLength="1"
         defaultValue="00" uiShowName="Reserved" uiControlState="ALLOW_OVERRIDE" />

    <FIELD name="CapabilityFlags" type="CapFlags" uiShowName="Capability Flags" />

    <FIELD name="MaxCredit" type="INTEGER" bitLength="16" 
         defaultValue="1" uiShowName="Maximum Credit" />
    
    <FIELD name="MaxMsgSize" type="INTEGER" bitLength="16"
         defaultValue="1518" uiShowName="Maximum Message Size" />

    <NEXT_HEADERS>
      <NXT_HDR id="VICTLV:CtrlChanCapTLV" name="Control Channel Capability TLV" />
      <NXT_HDR id="VICTLV:ResourceLimitCapTLV" name="Resource Limit Capability TLV" />
      <NXT_HDR id="VICTLV:EthernetCapTLV" name="Ethernet Capability TLV" />
      <NXT_HDR id="VICTLV:FcoeCapTLV" name="FCoE Capability TLV" />
      <NXT_HDR id="VICTLV:VifIdTLV" name="VIF ID TLV" />
      <NXT_HDR id="VICTLV:AddrArrayTLV" name="Address Array TLV" />
      <NXT_HDR id="VICTLV:VifStateTLV" name="VIF State TLV" />
      <NXT_HDR id="VICTLV:ProvisioningInfoTLV" name="Provisioning Info TLV" />
      <NXT_HDR id="VICTLV:MsgTypeArrayTLV" name="Message Type Array TLV" />
      <NXT_HDR id="VICTLV:CustomTLV" name="Custom TLV" />
<!--      
      <NXT_HDR id="VICTLV:IndexTLV" name="Index TLV" />
      <NXT_HDR id="VICTLV:IndexArrayTLV" name="Index Array TLV" />
      <NXT_HDR id="VICTLV:VlanTLV" name="VLAN TLV" />
      <NXT_HDR id="VICTLV:PriorityTLV" name="Priority TLV" />
      <NXT_HDR id="VICTLV:RateLimitTLV" name="Rate Limit TLV" />
      <NXT_HDR id="VICTLV:DefaultCosTLV" name="Default CoS TLV" />
      <NXT_HDR id="VICTLV:CosFilterTLV" name="CoS Filter TLV" />
      <NXT_HDR id="VICTLV:StatsTypeTLV" name="Stats Type TLV" />
      <NXT_HDR id="VICTLV:PortProfileNameListTLV" name="Port Profile Name List TLV" />
      <NXT_HDR id="VICTLV:EnumTypeTLV" name="Enum Type TLV" />
      <NXT_HDR id="VICTLV:RestartInfoTLV" name="Restart Info TLV" />
-->
    </NEXT_HEADERS>
  </TYPE_DEF>

  <!-- =================================================================================== -->
  <!-- Resource Limit Capability TLV = (open) ============================================ -->
  <TYPE_DEF name="ResourceLimitCapTLV" type="SEQUENCE" unitType="HEADER" level="2" portType="Ethernet" uiShowName="Resource Limit Capability TLV">
    <FIELD name="Type" type="INTEGER" bitLength="16"
         defaultValue="18" uiNoShow="Interface"
         uiShowCommon="false" uiShowName="Type" uiControlState="CALCULATED" />

    <FIELD name="Length" type="INTEGER" bitLength="16" procfnTarget="GEN2T2LTLVLENGTH"
         defaultValue="6" uiShowName="Length" uiControlState="CALCULATED" />

    <FIELD name="NumVifsSupported" type="INTEGER" bitLength="16"
         defaultValue="1" uiShowName="# VIFs Supported"/>

    <FIELD name="NumVifListsSupported" type="INTEGER" bitLength="16"
         defaultValue="0" uiShowName="# VIF Lists Supported"/>

    <FIELD name="NumUifsSupported" type="INTEGER" bitLength="16"
         defaultValue="0" uiShowName="# UIFs Supported"/>

