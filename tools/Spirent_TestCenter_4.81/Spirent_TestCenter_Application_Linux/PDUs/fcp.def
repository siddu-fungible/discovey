<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="pdu_schema.xsd"
  name="fcp"
  showName="Fibre Channel Protocol for SCSI"
  version="1.00"
  description="This is a sample PDU library for Fibre Channel Protocol for SCSI as defined in XML Stream Editor.">

  <TYPE_DEFS>

    <TYPE_DEF name="FcpCmnd" type="SEQUENCE" unitType="HEADER"
      uiShowName="FCP_CMND">
      <FIELD name="fcpLun" type="OCTETSTRING"
        byteLength="8"
        defaultValue="0000000000000000"
        uiNoShow="Interface"
        uiNoShowModifier="Interface,Filter"
        uiShowName="FCP_LUN"/>
      <FIELD name="cmndRefNum" type="INTEGER"
        bitLength="8"
        defaultValue="0"
        uiNoShow="Interface"
        uiNoShowModifier="Interface,Filter"
        uiShowName="Command Reference Number"/>
      <FIELD name="reserved" type="BITSTRING"
        bitLength="1"
        defaultValue="0"
        uiNoShow="Interface"
        uiNoShowModifier="Interface,Filter"
        uiShowName="Reserved"/>
      <FIELD name="priority" type="INTEGER"
        bitLength="4"
        defaultValue="0"
        uiNoShow="Interface"
        uiNoShowModifier="Interface,Filter"
        uiShowName="Priority"/>
      <FIELD name="taskAttrib" type="BITSTRING"
        bitLength="3"
        defaultValue="000"
        uiNoShow="Interface"
        uiNoShowModifier="Interface,Filter"
        uiShowName="Task Attribute"/>
      <FIELD name="taskMgmtFlags" type="OCTETSTRING"
        byteLength="1"
        defaultValue="00"
        uiNoShow="Interface"
        uiNoShowModifier="Interface,Filter"
        uiShowName="Task Management Flags"/>
      <FIELD name="additionalFcpCdbLen" type="INTEGER"
        bitLength="6"
        defaultValue="0"
        uiNoShow="Interface"
        uiNoShowModifier="Interface,Filter"
        uiShowName="Additional FCP_CDB Length"/>
      <FIELD name="rdData" type="BITSTRING"
        bitLength="1"
        defaultValue="0"
        uiNoShow="Interface"
        uiNoShowModifier="Interface,Filter"
        uiShowName="RDDATA"/>
      <FIELD name="wrData" type="BITSTRING"
        bitLength="1"
        defaultValue="0"
        uiNoShow="Interface"
        uiNoShowModifier="Interface,Filter"
        uiShowName="WRDATA"/>

      <!-- FCP_CDB -->
      <NEXT_HEADERS>
        <NXT_HDR id="scsi:Read6" name="READ (6)" value="08"/>
        <NXT_HDR id="scsi:Write6" name="WRITE (6)" value="0A"/>
        <NXT_HDR id="scsi:ModeSense6" name="MODE SENSE (6)" value="1A"/>
        <NXT_HDR id="scsi:StartStopUnit" name="START STOP UNIT (6)" value="1B"/>
        <NXT_HDR id="scsi:ReadCapacity10" name="READ CAPACITY (10)" value="25"/>
        <NXT_HDR id="scsi:Read10" name="READ (10)" value="28"/>
        <NXT_HDR id="scsi:Write10" name="WRITE (10)" value="2A"/>
        <NXT_HDR id="scsi:WriteAndVerify10" name="WRITE AND VERIFY (10)" value="2E"/>
        <NXT_HDR id="scsi:Verify10" name="VERIFY (10)" value="2F"/>
        <NXT_HDR id="scsi:WriteBuffer" name="WRITE BUFFER (10)" value="3B"/>
        <NXT_HDR id="scsi:ReadBuffer" name="READ BUFFER" value="3C"/>
        <NXT_HDR id="scsi:ReadLong10" name="READ LONG (10)" value="3E"/>
        <NXT_HDR id="scsi:WriteLong10" name="WRITE LONG (10)" value="3F"/>
        <NXT_HDR id="scsi:XDWrite10" name="XDWRITE (10)" value="50"/>
        <NXT_HDR id="scsi:XPWrite10" name="XPWRITE (10)" value="51"/>
        <NXT_HDR id="scsi:XDRead10" name="XDREAD (10)" value="52"/>
        <NXT_HDR id="scsi:ModeSense10" name="MODE SENSE (10)" value="5A"/>
        <NXT_HDR id="scsi:ReportLUNs" name="READ LUNS" value="A0"/>

        <NXT_HDR id="custom:Custom" name="Custom" value="00"/>
		<NXT_HDR id="customrepeat:CustomRepeat" name="CustomRepeat" value="00" />
        <NXT_HDR id="scsi:CDB6" name="Typical 6-byte CDB" value="01"/>
        <NXT_HDR id="scsi:CDB10" name="Typical 10-byte CDB" value="02"/>
      </NEXT_HEADERS>
    </TYPE_DEF>

    <!-- FCP_CMND IU fields that follow the FCP_CDB -->
    <TYPE_DEF name="FcpCmndDl" type="SEQUENCE" unitType="HEADER"
      uiShowName="FCP_CMND DL">
      <FIELD name="pad" type="OCTETSTRING"
        minByteLength="0" maxByteLength="16"
        defaultValue=""
        uiShowCommon="false"
        uiNoShow="Interface"
        uiNoShowModifier="Interface,Filter"
        uiShowName="FCP_CDB Pad"/>
      <FIELD name="fcpDl" type="INTEGER"
        bitLength="32"
        uiNoShow="Interface"
        uiNoShowModifier="Interface,Filter"
        uiShowName="FCP_DL"/>
      <FIELD name="fcpBiDirReadDl" type="INTEGER"
        bitLength="32"
        uiNoShow="Interface"
        uiNoShowModifier="Interface,Filter"
        uiShowName="FCP_BIDIRECTIONAL_READ_DL"/>
    </TYPE_DEF>

  </TYPE_DEFS>
</PDU_LIBRARY>
