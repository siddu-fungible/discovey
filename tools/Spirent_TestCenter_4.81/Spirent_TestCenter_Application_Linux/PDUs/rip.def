<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="pdu_schema.xsd" name="rip" showName="Rip" version="1.00"
    description="This is a sample PDU library for Rip as defined in XML Stream Editor.">

  <TYPE_DEFS>

    <TYPE_DEF name="RipCommand" type="INTEGER" bitLength="8" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="Request" value="1" />
        <VAL_ENUM uiShowName="Response" value="2" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>

    <TYPE_DEF name="Rip1Entry" type="SEQUENCE" unitType="ELEMENT"
        uiShowName="Rip 1 Entry" >

      <FIELD name="afi" type="INTEGER" bitLength="16"
          defaultValue="2"
		  uiShowCommon="false"
          uiShowName="Address Family Identifier" />

      <FIELD name="reserved" type="INTEGER" bitLength="16"
          defaultValue="0"
		  uiShowCommon="false"
          uiShowName="Reserved" />

      <FIELD name="ipaddr" type="IPV4ADDR"
          defaultValue="192.0.0.1"
          uiShowName="IP Address" />

      <FIELD name="reserved1" type="INTEGER" bitLength="32"
          defaultValue="0"
		  uiShowCommon="false"
          uiShowName="Reserved1" />

      <FIELD name="reserved2" type="INTEGER" bitLength="32"
          defaultValue="0"
		  uiShowCommon="false"
          uiShowName="Reserved2" />

      <FIELD name="metric" type="INTEGER" bitLength="32"
          defaultValue="1"
          uiShowName="Metric" />

    </TYPE_DEF>

    <TYPE_DEF name="Rip1EntryList" type="SEQUENCE_OF" elementType="Rip1Entry"
        minOccurs="1" maxOccurs="100" unitType="ELEMENT" />


    <TYPE_DEF name="Ripv1" type="SEQUENCE" unitType="MESSAGE"
        level="7" category="routing" uiShowName="RIPv1" >

      <FIELD name="command" type="RipCommand"
          defaultValue="1"
          uiShowName="Rip Command" />

      <FIELD name="version" type="INTEGER" bitLength="8"
          defaultValue="1"
          uiShowName="Version" />

      <FIELD name="reserved" type="INTEGER" bitLength="16"
          defaultValue="0"
		  uiShowCommon="false"
          uiShowName="Reserved" />

      <FIELD name="rip1Entries" type="Rip1EntryList"
          uiShowName="Rip 1 Entries" />

    </TYPE_DEF>

    <TYPE_DEF name="Rip2Entry" type="SEQUENCE" unitType="ELEMENT"
        uiShowName="Rip 2 Entry" >

      <FIELD name="afi" type="INTEGER" bitLength="16"
          defaultValue="2"
		  uiShowCommon="false"
          uiShowName="Address Family Identifier" />

      <FIELD name="routeTag" type="INTEGER" bitLength="16"
          defaultValue="0"
          uiShowName="Route Tag" />

      <FIELD name="ipaddr" type="IPV4ADDR"
          defaultValue="192.0.0.1"
          uiShowName="IP Address" />

      <FIELD name="subnetMask" type="IPV4ADDR"
          defaultValue="255.255.255.0"
          uiShowName="Subnet Mask" />

      <FIELD name="nextHop" type="IPV4ADDR"
          defaultValue="192.0.0.1"
          uiShowName="Next Hop Address" />

      <FIELD name="metric" type="INTEGER" bitLength="32"
          defaultValue="1"
          uiShowName="Metric" />

    </TYPE_DEF>

    <TYPE_DEF name="Rip2EntryList" type="SEQUENCE_OF" elementType="Rip2Entry"
        minOccurs="1" maxOccurs="100" unitType="ELEMENT" />


    <TYPE_DEF name="Ripv2" type="SEQUENCE" unitType="MESSAGE"
        level="7" category="routing" uiShowName="RIPv2" >

      <FIELD name="command" type="RipCommand"
          defaultValue="1"
          uiShowName="Rip Command" />

      <FIELD name="version" type="INTEGER" bitLength="8"
          defaultValue="2"
          uiShowName="Version" />

      <FIELD name="reserved" type="INTEGER" bitLength="16"
          defaultValue="0"
		  uiShowCommon="false"
          uiShowName="Reserved" />

      <FIELD name="rip2Entries" type="Rip2EntryList"
          uiShowName="Rip 2 Entries" />

    </TYPE_DEF>

    <TYPE_DEF name="RipngEntry" type="SEQUENCE" unitType="ELEMENT"
        uiShowName="Ripng Entry" >

      <FIELD name="ipaddr" type="IPV6ADDR"
          defaultValue="2000::1"
          uiShowName="IP Address" />

      <FIELD name="routeTag" type="INTEGER" bitLength="16"
          defaultValue="0"
          uiShowName="Route Tag" />

      <FIELD name="prefixLen" type="INTEGER" bitLength="8"
          defaultValue="64"
          uiShowName="Prefix Length" />

      <FIELD name="metric" type="INTEGER" bitLength="8"
          defaultValue="1"
          uiShowName="Metric" />

    </TYPE_DEF>

    <TYPE_DEF name="RipngEntryList" type="SEQUENCE_OF" elementType="RipngEntry"
        minOccurs="1" maxOccurs="100" unitType="ELEMENT" />


    <TYPE_DEF name="Ripng" type="SEQUENCE" unitType="MESSAGE"
        level="7" category="routing" uiShowName="RIPng" >

      <FIELD name="command" type="RipCommand"
          defaultValue="1"
          uiShowName="Rip Command" />

      <FIELD name="version" type="INTEGER" bitLength="8"
          defaultValue="1"
          uiShowName="Version" />

      <FIELD name="reserved" type="INTEGER" bitLength="16"
          defaultValue="0"
		  uiShowCommon="false"
          uiShowName="Reserved" />

      <FIELD name="ripngEntries" type="RipngEntryList"
          uiShowName="Ripng Entries" />

    </TYPE_DEF>

  </TYPE_DEFS>
</PDU_LIBRARY>
