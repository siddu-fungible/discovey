<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="pdu_schema.xsd"
  name="EOAM"
  showName="Ethernet OAM CFM"
  version="2.01"
  description="This is a sample PDU library for Ethernet OAM as defined in XML Stream Editor.">

<TYPE_DEFS>
	<TYPE_DEF name="MDLvalues" type="INTEGER" bitLength="3" unitType="ELEMENT">
		<VALUE_ENUMERATION>
			<VAL_ENUM uiShowName="zero" value="0" />
				<VAL_ENUM uiShowName="one" value="1" />
				<VAL_ENUM uiShowName="two" value="2" />
				<VAL_ENUM uiShowName="three" value="3" />
				<VAL_ENUM uiShowName="four" value="4" />
				<VAL_ENUM uiShowName="five" value="5" />
			<VAL_ENUM uiShowName="six" value="6" />
			<VAL_ENUM uiShowName="seven" value="7" />
			</VALUE_ENUMERATION>
	</TYPE_DEF>


	<TYPE_DEF name="RDIfield" type="INTEGER" bitLength="1" unitType="ELEMENT">
		<VALUE_ENUMERATION>
			<VAL_ENUM uiShowName="zero" value="0" />
			<VAL_ENUM uiShowName="one" value="1" />
		</VALUE_ENUMERATION>
	</TYPE_DEF>


	<TYPE_DEF name="CCMIntField" type="INTEGER" bitLength="3" unitType="ELEMENT">
		<VALUE_ENUMERATION>
			<VAL_ENUM uiShowName="invalid" value="0" />
			<VAL_ENUM uiShowName="3 1/3 ms" value="1" />
		        <VAL_ENUM uiShowName="10 ms" value="2" />
        		<VAL_ENUM uiShowName="100 ms" value="3" />
        		<VAL_ENUM uiShowName="1 s" value="4" />
        		<VAL_ENUM uiShowName="10 s" value="5" />
			<VAL_ENUM uiShowName="1 min" value="6" />
			<VAL_ENUM uiShowName="10 min" value="7" />
      		</VALUE_ENUMERATION>
	</TYPE_DEF>

	<TYPE_DEF name="psTLVvalues" type="OCTETSTRING" byteLength="1" unitType="ELEMENT">
		<VALUE_ENUMERATION>
			<VAL_ENUM uiShowName="No (psBlocked)" value="01" />
			<VAL_ENUM uiShowName="Yes (psUp)" value="02" />
		</VALUE_ENUMERATION>
	</TYPE_DEF>

	<TYPE_DEF name="intTLVvalues" type="OCTETSTRING" byteLength="1" unitType="ELEMENT">
		<VALUE_ENUMERATION>
			<VAL_ENUM uiShowName="Up" value="01" />
			<VAL_ENUM uiShowName="Down" value="02" />
			<VAL_ENUM uiShowName="Testing" value="03" />
			<VAL_ENUM uiShowName="Unknown" value="04" />
			<VAL_ENUM uiShowName="Dormant" value="05" />
			<VAL_ENUM uiShowName="notPresent" value="06" />
			<VAL_ENUM uiShowName="lowerLayerDown" value="07" />
		</VALUE_ENUMERATION>
	</TYPE_DEF>


	<TYPE_DEF name="IngressActionFieldValues" type="OCTETSTRING" byteLength="1" unitType="ELEMENT">
		<VALUE_ENUMERATION>
			<VAL_ENUM uiShowName="IngOk" value="01" />
			<VAL_ENUM uiShowName="IngDown" value="02" />
			<VAL_ENUM uiShowName="IngBlock" value="03" />
			<VAL_ENUM uiShowName="IngVID" value="04" />
		</VALUE_ENUMERATION>
	</TYPE_DEF>

	<TYPE_DEF name="EgressActionFieldValues" type="OCTETSTRING" byteLength="1" unitType="ELEMENT">
		<VALUE_ENUMERATION>
			<VAL_ENUM uiShowName="EgrOk" value="01" />
			<VAL_ENUM uiShowName="EgrDown" value="02" />
			<VAL_ENUM uiShowName="EgrBlocked" value="03" />
			<VAL_ENUM uiShowName="EgrVID" value="04" />
		</VALUE_ENUMERATION>
	</TYPE_DEF>

	<TYPE_DEF name="MDNFF" type="OCTETSTRING" byteLength="1" unitType="ELEMENT">
		<VALUE_ENUMERATION>
			<VAL_ENUM uiShowName="Reserved for IEEE 802.1 (00)" value="00"/>
			<VAL_ENUM uiShowName="No Maintenance Domain Name Present" value="01" />
			<VAL_ENUM uiShowName="Domain Name Based String" value="02" />
			<VAL_ENUM uiShowName="MAC address + 2-octet Integer" value="03" />
			<VAL_ENUM uiShowName="Character String" value="04"/>
			<VAL_ENUM uiShowName="Reserved for IEEE 802.1 (05)" value="05"/>
			<VAL_ENUM uiShowName="Defined by ITU-T Y.1731" value="20"/>
			<VAL_ENUM uiShowName="Reserved for IEEE 802.1 (64)" value="40"/>
		</VALUE_ENUMERATION>
	</TYPE_DEF>

	<TYPE_DEF name="SMANF" type="OCTETSTRING" byteLength="1" unitType="ELEMENT">
		<VALUE_ENUMERATION>
			<VAL_ENUM uiShowName="Reserved for IEEE 802.1 (00)" value="00" />
			<VAL_ENUM uiShowName="Primary VID" value="01" />
			<VAL_ENUM uiShowName="Characer String" value="02" />
			<VAL_ENUM uiShowName="2-octet Integer" value="03" />
			<VAL_ENUM uiShowName="RFC 2685 VPN ID" value="04"/>
			<VAL_ENUM uiShowName="Reserved for IEEE 802.1 (05)" value="05"/>
			<VAL_ENUM uiShowName="ICC-Based" value="20" />
			<VAL_ENUM uiShowName="CC- and ICC-Based" value="21" />
			<VAL_ENUM uiShowName="Reserved for IEEE 802.1 (64)" value="40"/>
		</VALUE_ENUMERATION>
	</TYPE_DEF>

	<TYPE_DEF name="LTMFF" type="OCTETSTRING" byteLength="1" unitType="ELEMENT">
		<VALUE_ENUMERATION>
			<VAL_ENUM uiShowName="UseFDBonly (on)" value="80" />
			<VAL_ENUM uiShowName="UseFDBonly (off)" value="00" />
		</VALUE_ENUMERATION>
	</TYPE_DEF>

	<TYPE_DEF name="LTRfwd" type="INTEGER" bitLength="1" unitType="ELEMENT">
		<VALUE_ENUMERATION>
			<VAL_ENUM uiShowName="LTM was NOT Forwarded" value="0" />
			<VAL_ENUM uiShowName="LTM was Forwarded" value="1" />
		</VALUE_ENUMERATION>
	</TYPE_DEF>

	<TYPE_DEF name="LTRTermMep" type="INTEGER" bitLength="1" unitType="ELEMENT">
		<VALUE_ENUMERATION>
			<VAL_ENUM uiShowName="MP reported in TLV is NOT a MEP" value="0" />
			<VAL_ENUM uiShowName="MP reported in TLV is a MEP" value="1" />
		</VALUE_ENUMERATION>
	</TYPE_DEF>

	<TYPE_DEF name="LTRRelayActionField" type="OCTETSTRING" byteLength="1" unitType="ELEMENT">
		<VALUE_ENUMERATION>
			<VAL_ENUM uiShowName="RlyHit" value="01" />
			<VAL_ENUM uiShowName="RlyFDB" value="02" />
			<VAL_ENUM uiShowName="RlyMPDB" value="03" />
		</VALUE_ENUMERATION>
	</TYPE_DEF>

	<TYPE_DEF name="CFMlv" type="SEQUENCE" unitType="ELEMENT" uiShowName="Connectivity Fault Management">

		<FIELD name="MDlevel" type="MDLvalues" defaultValue="0" uiShowName="MD Level" />

		<FIELD name="Version" type="BITSTRING" bitLength="5"
		defaultValue="00000"
		uiNoShow="Interface"
		uiShowCommon="false"
		uiShowName="Version" />

	</TYPE_DEF>

	<TYPE_DEF name="SenderIDTLVFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="Sender ID TLV">

		<FIELD name="Type" type="OCTETSTRING" byteLength="1" 
			defaultValue="01" uiNoShow="Interface"
			uiShowCommon="false" uiShowName="Type" />

    <FIELD name="Length" type="OCTETSTRING" byteLength="2"
			defaultValue="0000" uiShowName="Length" />

		<FIELD name="ChassisIDLen" type="OCTETSTRING" byteLength="1"
			defaultValue="01" uiShowName="Chassis ID Length" />

		<FIELD name="ChassisIDSubtype" type="OCTETSTRING" byteLength="1"
			defaultValue="01" uiShowName="Chassis ID Subtype" />

		<FIELD name="ChassisID" type="OCTETSTRING" minByteLength="1"
			maxByteLength="255" defaultValue="00" uiShowName="Chassis ID" />

		<FIELD name="MADL" type="OCTETSTRING" byteLength="01"
			defaultValue="00" uiShowName="Management Address Domain Length"/>

		<FIELD name="MAD" type="OCTETSTRING" minByteLength="0" maxByteLength="255" 
			uiShowName="Management Address Domain" />

		<FIELD name="MAL" type="OCTETSTRING" minByteLength="0" maxByteLength="1"  
			uiShowName="Management Address Length" />

		<FIELD name="ManAdd" type="OCTETSTRING" minByteLength="0" maxByteLength="255" 
			uiShowName="Management Address" />
	</TYPE_DEF>

	<TYPE_DEF name="Value" type="SEQUENCE" unitType="ELEMENT" uiShowName="Value">
		<FIELD name="Value" type="OCTETSTRING" 
			uiNoShowModifier="Interface,Frame" minByteLength="0"
		       	maxByteLength="65535" uiShowName="Value" />
	</TYPE_DEF>

	<TYPE_DEF name="ValueSelect" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1" elementType="Value" unitType="ELEMENT"/>

	<TYPE_DEF name="OrgSpecTLVFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="Organization-Specific TLV">

		<FIELD name="Type" type="OCTETSTRING" byteLength="1"
			uiNoShow="Interface" uiShowCommon="false"
			defaultValue="1F" uiShowName="Type"/>

		<FIELD name="Length" type="OCTETSTRING" byteLength="2"
			defaultValue="0000" uiShowName="Length" />

		<FIELD name="OUI" type="OCTETSTRING" byteLength="3"
			defaultValue="000000" uiShowName="OUI" />

		<FIELD name="SubType" type="OCTETSTRING" byteLength="1"
			defaultValue="00" uiShowName="Sub-Type" />

		<FIELD name="theValue" type="ValueSelect" uiShowName="Value"/>
	</TYPE_DEF>

	<TYPE_DEF name="PortStatTLVFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="Port Status TLV">

		<FIELD name="Type" type="OCTETSTRING" byteLength="1"
			defaultValue="02" uiNoShow="Interface"
			uiShowCommon="false" uiShowName="Type"/>

		<FIELD name="Length" type="OCTETSTRING" byteLength="2"
			defaultValue="0000" uiShowName="Length" />

		<FIELD name="PortStatusValues" type="psTLVvalues"
		       	defaultValue="01" uiShowName="Port Status" />
	</TYPE_DEF>

	<TYPE_DEF name="IntfaceStatTLVFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="Interface Status TLV">

		<FIELD name="Type" type="OCTETSTRING" byteLength="1" uiNoShow="Interface"
			uiShowCommon="false"
			defaultValue="04" uiShowName="Type"/>

		<FIELD name="Length" type="OCTETSTRING" byteLength="2"
			defaultValue="0000" uiShowName="Length" />

		<FIELD name="IntStatusValues" type="intTLVvalues"
		       	defaultValue="01" uiShowName="Interface Status" />
	</TYPE_DEF>

	<TYPE_DEF name="Data" type="SEQUENCE" unitType="ELEMENT" uiShowName="Data">
		<FIELD name="Data" type="OCTETSTRING" minByteLength="0" 
			uiNoShowModifier="Interface,Frame" 
			maxByteLength="65535" uiShowName="Data" />
	</TYPE_DEF>

	<TYPE_DEF name="DataSelect" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1" elementType="Data" unitType="ELEMENT"/>

	<TYPE_DEF name="DataTLVFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="Data TLV">

		<FIELD name="Type" type="OCTETSTRING" byteLength="1" 
			uiNoShow="Interface" uiShowCommon="false"
			defaultValue="03" uiShowName="Type"/>

		<FIELD name="Length" type="OCTETSTRING" byteLength="2"
			defaultValue="0000" uiShowName="Length" />

		<FIELD name="theData" type="DataSelect" uiShowName="Data"/>
	</TYPE_DEF>

	<TYPE_DEF name="EndTLVFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="End TLV">

		<FIELD name="Type" type="OCTETSTRING" byteLength="1"
		       	defaultValue="00" uiShowName="Type"/>
	</TYPE_DEF>

	<TYPE_DEF name="LTMEgressIdentTLVFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="LTM Egress Identifier TLV">

		<FIELD name="Type" type="OCTETSTRING" byteLength="1"
			defaultValue="07" uiNoShow="Interface"
			uiShowCommon="false" uiShowName="Type"/>

		<FIELD name="Length" type="OCTETSTRING" byteLength="2" 
			defaultValue="0000" uiShowName="Length" />

		<FIELD name="EgressIdent" type="OCTETSTRING" byteLength="8" 
			defaultValue="0102030405060708" uiShowName="Egress Identifier"/>
	</TYPE_DEF>	

	<TYPE_DEF name="LTREgressIdentTLVFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="LTR Egress Identifier TLV">

		<FIELD name="Type" type="OCTETSTRING" byteLength="1" 
			defaultValue="08" uiNoShow="Interface"
			uiShowCommon="false" uiShowName="Type"/>

		<FIELD name="Length" type="OCTETSTRING" byteLength="2" 
			defaultValue="0000" uiShowName="Length" />

		<FIELD name="LastEgressIdent" type="OCTETSTRING" byteLength="8"
			defaultValue="0102030405060708" uiShowName="Last Egress Identifier"/>

		<FIELD name="NextEgressIdent" type="OCTETSTRING" byteLength="8"
		       	defaultValue="0102030405060708" uiShowName="Next Egress Identifier"/>
	</TYPE_DEF>	

	<TYPE_DEF name="Ingress_Port_ID_Length" type="SEQUENCE" unitType="ELEMENT" uiShowName="Ingress Port ID Length">
			<FIELD name="IngressPortIDLength" type="OCTETSTRING"  minByteLength="0" 
			uiNoShowModifier="Interface,Frame" maxByteLength="1" defaultValue="00"
			uiShowName="Ingress Port ID Length" />
	</TYPE_DEF>

	<TYPE_DEF name="Ingress_Port_ID_Subtype" type="SEQUENCE" unitType="ELEMENT" uiShowName="Ingress Port ID Subtype">
		<FIELD name="IngressPortIDLengthSub" type="OCTETSTRING" minByteLength="0"
			uiNoShowModifier="Interface,Frame" maxByteLength="1" defaultValue="00"
			uiShowName="Ingress Port ID Length Subtype"/>
	</TYPE_DEF>

	<TYPE_DEF name="Ingress_Port_ID" type="SEQUENCE" unitType="ELEMENT" uiShowName="Ingress Port ID">

		<FIELD name="IngressPortID" type="OCTETSTRING" minByteLength="0"
			uiNoShowModifier="Interface,Frame" maxByteLength="65535"
		       	uiShowName="Ingress Port ID"/>	
	</TYPE_DEF>

	<TYPE_DEF name="IPIDSelect" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1" elementType="Ingress_Port_ID" unitType="ELEMENT"/>
	<TYPE_DEF name="IPIDLSelect" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1" elementType="Ingress_Port_ID_Length" unitType="ELEMENT"/>
	<TYPE_DEF name="IPIDLSSelect" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1" elementType="Ingress_Port_ID_Subtype" unitType="ELEMENT"/>

	<TYPE_DEF name="ReplyIngressTLVFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="Reply Ingress TLV">

		<FIELD name="Type" type="OCTETSTRING" byteLength="1"
			defaultValue="05" uiNoShow="Interface"
			uiShowCommon="false" uiShowName="Type"/>

		<FIELD name="Length" type="OCTETSTRING" byteLength="2"
			defaultValue="0000" uiShowName="Length" />

		<FIELD name="IngressAction" type="IngressActionFieldValues"
			defaultValue="01" uiShowName="Ingress Action" />

		<FIELD name="IngressMac" type="MACADDR" defaultValue="00:00:01:00:00:01"
			byteLength="6" uiShowName="Ingress MAC Address" />

		<FIELD name="theIPIDL" type="IPIDLSelect" uiShowName="Ingress Port ID Length"/>	
		<FIELD name="theIPIDLS" type="IPIDLSSelect" uiShowName="Ingress Port ID Subtype"/>	
		<FIELD name="theIPID" type="IPIDSelect" uiShowName="Ingress Port ID"/>	

	</TYPE_DEF>

	<TYPE_DEF name="Egress_Port_ID_Length" type="SEQUENCE" unitType="ELEMENT" uiShowName="Egress Port ID Length">
			<FIELD name="EgressPortIDLength" type="OCTETSTRING"  minByteLength="0" 
			uiNoShowModifier="Interface,Frame" maxByteLength="1" defaultValue="00"
			uiShowName="Egress Port ID Length" />
	</TYPE_DEF>

	<TYPE_DEF name="Egress_Port_ID_Subtype" type="SEQUENCE" unitType="ELEMENT" uiShowName="Egress Port ID Subtype">
		<FIELD name="EgressPortIDLengthSub" type="OCTETSTRING" minByteLength="0"
			uiNoShowModifier="Interface,Frame" maxByteLength="1" defaultValue="00"
			uiShowName="Egress Port ID Subtype"/>
	</TYPE_DEF>

	<TYPE_DEF name="Egress_Port_ID" type="SEQUENCE" unitType="ELEMENT" uiShowName="Egress Port ID">

		<FIELD name="EgressPortID" type="OCTETSTRING" minByteLength="0"
			uiNoShowModifier="Interface,Frame" maxByteLength="65535"
		       	uiShowName="Egress Port ID"/>	
	</TYPE_DEF>

	<TYPE_DEF name="EPIDSelect" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1" elementType="Egress_Port_ID" unitType="ELEMENT"/>
	<TYPE_DEF name="EPIDLSelect" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1" elementType="Egress_Port_ID_Length" unitType="ELEMENT"/>
	<TYPE_DEF name="EPIDLSSelect" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1" elementType="Egress_Port_ID_Subtype" unitType="ELEMENT"/>

	<TYPE_DEF name="ReplyEgressTLVFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="Reply Egress TLV">

		<FIELD name="Type" type="OCTETSTRING" byteLength="1" 
			defaultValue="06" uiNoShow="Interface"
			uiShowCommon="false" uiShowName="Type"/>

		<FIELD name="Length" type="OCTETSTRING" byteLength="2" 
			defaultValue="0000" uiShowName="Length" />

		<FIELD name="EgressAction" type="EgressActionFieldValues"
			defaultValue="01" uiShowName="Egress Action" />

		<FIELD name="EgressMac" type="MACADDR" defaultValue="00:00:01:00:00:01"
			byteLength="6" uiShowName="Egress MAC Address" />

		<FIELD name="theEPIDL" type="EPIDLSelect" uiShowName="Egress Port ID Length"/>	
		<FIELD name="theEPIDLS" type="EPIDLSSelect" uiShowName="Egress Port ID Subtype"/>	
		<FIELD name="theEPID" type="EPIDSelect" uiShowName="Egress Port ID"/>	
	</TYPE_DEF>

	<TYPE_DEF name="CCMTLVList" type="CHOICE" unitType="ELEMENT" uiShowName="TLVs">

		<FIELD name="SenderIDTLV" type="SenderIDTLVFormat" 
			uiShowName="SenderID TLV" />

		<FIELD name="PortStatusTLV" type="PortStatTLVFormat"  
			uiShowName="Port Status TLV" />

		<FIELD name="InterfaceStatusTLV" type="IntfaceStatTLVFormat"
			uiShowName="Interface Status TLV" />

		<FIELD name="OrgSpecTLV" type="OrgSpecTLVFormat"
			uiShowName="Organaization-Specific TLV" />

		<FIELD name="EndTLV" type="EndTLVFormat" uiShowName="End TLV" />
	</TYPE_DEF>

	<TYPE_DEF name="LBMTLVList" type="CHOICE" unitType="ELEMENT" uiShowName="TLVs">

		<FIELD name="SenderIDTLV" type="SenderIDTLVFormat" 
			uiShowName="SenderID TLV" />

		<FIELD name="DataTLV" type="DataTLVFormat" 
			uiShowName="Data TLV" />	

		<FIELD name="OrgSpecTLV" type="OrgSpecTLVFormat"
			uiShowName="Organaization-Specific TLV" />

		<FIELD name="EndTLV" type="EndTLVFormat" uiShowName="End TLV" />
	</TYPE_DEF>

	<TYPE_DEF name="LTMTLVList" type="CHOICE"  unitType="ELEMENT" uiShowName="TLVs">

		<FIELD name="SenderIDTLV" type="SenderIDTLVFormat" 
			uiShowName="SenderID TLV" />

		<FIELD name="OrgSpecTLV" type="OrgSpecTLVFormat"
			uiShowName="Organaization-Specific TLV" />

		<FIELD name="LTMEgressIdentTLV" type="LTMEgressIdentTLVFormat" uiShowName="LTM Egress Identifier TLV" />

		<FIELD name="EndTLV" type="EndTLVFormat" uiShowName="End TLV" />

	</TYPE_DEF>

	<TYPE_DEF name="LTRTLVList" type="CHOICE"  unitType="ELEMENT" uiShowName="TLVs">

		<FIELD name="ReplyIngressTLVFormatTLV"  type="ReplyIngressTLVFormat" 
			uiShowName="Reply Ingress TLV" />

		<FIELD name="ReplyEgressTLVFormatTLV" type="ReplyEgressTLVFormat" 
			uiShowName="Reply Egress TLV" />

		<FIELD name="SenderIDTLV" type="SenderIDTLVFormat" 
			uiShowName="SenderID TLV" />

		<FIELD name="OrgSpecTLV" type="OrgSpecTLVFormat"
			uiShowName="Organaization-Specific TLV" />

		<FIELD name="LTREgressIdentTLV" type="LTREgressIdentTLVFormat" uiShowName="LTR Egress Identifier TLV" />

		<FIELD name="EndTLV" type="EndTLVFormat" uiShowName="End TLV" />

	</TYPE_DEF>

	<TYPE_DEF name="CCMTLVs" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1" elementType="CCMTLVList" unitType="ELEMENT"/>
	<TYPE_DEF name="LBMTLVs" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1" elementType="LBMTLVList" unitType="ELEMENT"/>
	<TYPE_DEF name="LTMTLVs" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1"  elementType="LTMTLVList" unitType="ELEMENT"/>
	<TYPE_DEF name="LTRTLVs" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1" elementType="LTRTLVList" unitType="ELEMENT"/>

	<TYPE_DEF name="MDN_Length" type="SEQUENCE" unitType="ELEMENT" uiShowName="Maintenance Domain Name Length">
		<FIELD name="MDNL" type="OCTETSTRING" minByteLength="0" defaultValue="00" maxByteLength="1" uiShowName="Maintenance Domain Name Length" />
	</TYPE_DEF>

	<TYPE_DEF name="MaintenanceDomainName" type="SEQUENCE" unitType="ELEMENT" uiShowName="Maintenance Domain Name">
		<FIELD name="MDN" type="OCTETSTRING" minByteLength="0" defaultValue="00" maxByteLength="255" uiShowName="Maintenance Domain Name" />
	</TYPE_DEF>

	<TYPE_DEF name="Padding" type="SEQUENCE" unitType="ELEMENT" uiShowName="Padding">
		<FIELD name="pad" type="OCTETSTRING" minByteLength="0" defaultValue="00" maxByteLength="48" uiShowName="padding" />

	</TYPE_DEF>

	<TYPE_DEF name="MDNLSelection" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1" elementType="MDN_Length" unitType="ELEMENT"/>
	<TYPE_DEF name="MDNSelection" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1" elementType="MaintenanceDomainName" unitType="ELEMENT"/>
	<TYPE_DEF name="PadSelection" type="SEQUENCE_OF" minOccurs="0" maxOccurs="1" elementType="Padding" unitType="ELEMENT"/>

	<TYPE_DEF name="MAIDFieldFormat" type="SEQUENCE" unitType="ELEMENT" uiShowName="MAID">

		<FIELD name="MDNF" type="MDNFF" defaultValue="01" byteLength="1" uiShowName="Maintenance Domain Name Format" />
		<FIELD name="theMDNL" type="MDNLSelection" uiShowName="Maintenance Domain Name Length"/>
		<FIELD name="theMDN" type="MDNSelection" uiShowName="Maintenance Domain Name"/>
		<FIELD name="SMAF" type="SMANF" defaultValue="01" uiShowName="Short MA Name" />
		<FIELD name="SMAL" type="OCTETSTRING" byteLength="1" defaultValue="01" uiShowName="Short MA Name Length" />
		<FIELD name="SMAN" type="OCTETSTRING" minByteLength="1" maxByteLength="255" uiShowName="Short MA Name" />
		<FIELD name="thePad" type="PadSelection" uiShowName="Padding" />
	</TYPE_DEF>

	<TYPE_DEF name="CTag" type="SEQUENCE" unitType="ELEMENT" uiShowName="CTag" niMapped="true">

		<FIELD name="Type" type="OCTETSTRING" 
			byteLength="2" defaultValue="8100" 
			uiShowName="Customer VLAN Tag"/>

		<FIELD name="PCP" type="BITSTRING" 
			bitLength="3" defaultValue="000"  
			uiNoShowModifier="Interface" 
			isNiMapped="true" uiShowName="PCP"/>

		<FIELD name="CFI" type="BITSTRING"
			bitLength="1" defaultValue="0"
			uiNoShowModifier="Interface"
			isNiMapped="true" uiShowName="CFI"/>

		<FIELD name="VID" type="INTEGER" 
			bitLength="12" defaultValue="0" 
			minValue="0" maxValue="4094"
			isNiMapped="true" isNiPrimaryKey="true" 
			uiShowName="VID"/>
	</TYPE_DEF>

	<TYPE_DEF name="STag" type="SEQUENCE" unitType="ELEMENT" uiShowName="STag" niMapped="true">

		<FIELD name="Type" type="OCTETSTRING"
			byteLength="2" defaultValue="88a8" 
			uiShowName="Service VLAN Tag"/>

		<FIELD name="PCP" type="BITSTRING"
			bitLength="3" defaultValue="000"
			uiNoShowModifier="Interface" 
			isNiMapped="true" uiShowName="PCP"/>

		<FIELD name="DEI" type="BITSTRING" 
			bitLength="1" defaultValue="0"
			uiNoShowModifier="Interface"
			isNiMapped="true" uiShowName="DEI"/>

		<FIELD name="VID" type="INTEGER"
			bitLength="12" defaultValue="0"
