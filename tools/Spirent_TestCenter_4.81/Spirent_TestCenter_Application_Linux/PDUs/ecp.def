<?xml version="1.0" encoding="utf-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
             xsi:noNamespaceSchemaLocation="pdu_schema.xsd" 
             name="ecp" 
             showName="ECP" 
             version="1.00" 
             description="This is a sample PDU library for ECP as defined in XML Stream Editor.">
  <TYPE_DEFS>
    <TYPE_DEF name="OperationType" type="INTEGER" bitLength="2" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="ECP Request" value="0" />
        <VAL_ENUM uiShowName="ECP Acknowledgement" value="1" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>
    <TYPE_DEF name="SubType" type="INTEGER" bitLength="10" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="VDP" value="1" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>
    
    <!-- ECPPDU ==================-->
    <TYPE_DEF name="ECP" type="SEQUENCE" unitType="HEADER" level="3" portType="Ethernet" uiShowName="ECP">
      <FIELD name="version" type="INTEGER" bitLength="4" defaultValue="1" uiShowName="Version" uiControlState="ALLOW_OVERRIDE" />
      <FIELD name="opType" type="OperationType" defaultValue="0" uiShowName="Operation Type" />
      <FIELD name="subtype" type="SubType" defaultValue="1" uiShowName="Sub-type" uiControlState="ALLOW_OVERRIDE" />
      <FIELD name="seqNumber" type="OCTETSTRING" byteLength="2" defaultValue="0001" uiShowName="Sequence Number" />
      <NEXT_HEADERS>
        <NXT_HDR id="vdp:VdpManagerIdTlv" name="VDP Manager ID TLV" />
        <NXT_HDR id="vdp:VdpAssociationTlv" name="VDP Association TLV" />
      </NEXT_HEADERS>
    </TYPE_DEF>
  </TYPE_DEFS>
</PDU_LIBRARY>