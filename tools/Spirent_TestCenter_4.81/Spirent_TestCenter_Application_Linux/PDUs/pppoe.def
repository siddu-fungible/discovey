<?xml version="1.0" encoding="UTF-8"?>
<PDU_LIBRARY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="pdu_schema.xsd" name="pppoe" showName="PPPoE" version="1.00"
    description="This is a sample PDU library for PPPoE as defined in XML Stream Editor.">

  <TYPE_DEFS>

    <TYPE_DEF name="PPPoECode" type="INTEGER" bitLength="8" unitType="ELEMENT">
      <VALUE_ENUMERATION>
        <VAL_ENUM uiShowName="PADI" value="9" />
        <VAL_ENUM uiShowName="PADO" value="7" />
        <VAL_ENUM uiShowName="PADR" value="25" />
        <VAL_ENUM uiShowName="PADS" value="101" />
        <VAL_ENUM uiShowName="PADT" value="167" />
      </VALUE_ENUMERATION>
    </TYPE_DEF>

    <TYPE_DEF name="EndOfListTag" type="SEQUENCE" unitType="ELEMENT"
        uiShowName="End Of List" >
      <FIELD name="type" type="OCTETSTRING" byteLength="2"
          defaultValue="0000" uiShowName="Tag Type" />
      <FIELD name="length" type="OCTETSTRING" byteLength="2"
          defaultValue="0000" uiShowName="Tag Length" />
    </TYPE_DEF>

    <TYPE_DEF name="ServiceNameTag" type="SEQUENCE" unitType="ELEMENT"
        uiShowName="Service Name" maxOccurs="8">
      <FIELD name="type" type="OCTETSTRING" byteLength="2"
          defaultValue="0101" uiShowName="Tag Type" />
      <FIELD name="length" type="OCTETSTRING" byteLength="2"
          defaultValue="0000" uiShowName="Tag Length" />
      <FIELD name="value" type="OCTETSTRING"
          minByteLength="0" maxByteLength="256"
          uiShowName="Tag Value" />
    </TYPE_DEF>

    <TYPE_DEF name="ACNameTag" type="SEQUENCE" unitType="ELEMENT"
        uiShowName="AC Name" >
      <FIELD name="type" type="OCTETSTRING" byteLength="2"
          defaultValue="0102" uiShowName="Tag Type" />
      <FIELD name="length" type="OCTETSTRING" byteLength="2"
          defaultValue="0000" uiShowName="Tag Length" />
      <FIELD name="value" type="OCTETSTRING"
          minByteLength="0" maxByteLength="256"
          uiShowName="Tag Value" />
    </TYPE_DEF>

    <TYPE_DEF name="HostUniqTag" type="SEQUENCE" unitType="ELEMENT"
        uiShowName="Host Uniq" >
      <FIELD name="type" type="OCTETSTRING" byteLength="2"
          defaultValue="0103" uiShowName="Tag Type" />
      <FIELD name="length" type="OCTETSTRING" byteLength="2"
          defaultValue="0000" uiShowName="Tag Length" />
      <FIELD name="value" type="OCTETSTRING"
          minByteLength="0" maxByteLength="256"
          uiShowName="Tag Value" />
    </TYPE_DEF>

    <TYPE_DEF name="ACCookieTag" type="SEQUENCE" unitType="ELEMENT"
        uiShowName="AC Cookie" >
      <FIELD name="type" type="OCTETSTRING" byteLength="2"
          defaultValue="0104" uiShowName="Tag Type" />
      <FIELD name="length" type="OCTETSTRING" byteLength="2"
          defaultValue="0000" uiShowName="Tag Length" />
      <FIELD name="value" type="OCTETSTRING"
          minByteLength="0" maxByteLength="256"
          uiShowName="Tag Value" />
    </TYPE_DEF>

    <TYPE_DEF name="VendorSpecificTag" type="SEQUENCE" unitType="ELEMENT"
        uiShowName="Vendor Specific" >
      <FIELD name="type" type="OCTETSTRING" byteLength="2"
          defaultValue="0105" uiShowName="Tag Type" />
      <FIELD name="length" type="OCTETSTRING" byteLength="2"
          defaultValue="0000" uiShowName="Tag Length" />
      <FIELD name="value" type="OCTETSTRING"
          minByteLength="0" maxByteLength="256"
          uiShowName="Tag Value" />
    </TYPE_DEF>

    <TYPE_DEF name="RelaySessionIdTag" type="SEQUENCE" unitType="ELEMENT"
        uiShowName="Relay Session Id" >
      <FIELD name="type" type="OCTETSTRING" byteLength="2"
          defaultValue="0110" uiShowName="Tag Type" />
      <FIELD name="length" type="OCTETSTRING" byteLength="2"
          defaultValue="0000" uiShowName="Tag Length" />
      <FIELD name="value" type="OCTETSTRING"
          minByteLength="0" maxByteLength="256"
          uiShowName="Tag Value" />
    </TYPE_DEF>

    <TYPE_DEF name="ServiceNameErrorTag" type="SEQUENCE" unitType="ELEMENT"
        uiShowName="Service Name Error" >
      <FIELD name="type" type="OCTETSTRING" byteLength="2"
          defaultValue="0201" uiShowName="Tag Type" />
      <FIELD name="length" type="OCTETSTRING" byteLength="2"
          defaultValue="0000" uiShowName="Tag Length" />
      <FIELD name="value" type="OCTETSTRING"
          minByteLength="0" maxByteLength="256"
          uiShowName="Tag Value" />
    </TYPE_DEF>

    <TYPE_DEF name="ACSystemErrorTag" type="SEQUENCE" unitType="ELEMENT"
        uiShowName="AC System Error" >
      <FIELD name="type" type="OCTETSTRING" byteLength="2"
          defaultValue="0202" uiShowName="Tag Type" />
      <FIELD name="length" type="OCTETSTRING" byteLength="2"
          defaultValue="0000" uiShowName="Tag Length" />
      <FIELD name="value" type="OCTETSTRING"
          minByteLength="0" maxByteLength="256"
          uiShowName="Tag Value" />
    </TYPE_DEF>

    <TYPE_DEF name="GenericErrorTag" type="SEQUENCE" unitType="ELEMENT"
        uiShowName="Generic Error" >
      <FIELD name="type" type="OCTETSTRING" byteLength="2"
          defaultValue="0203" uiShowName="Tag Type" />
      <FIELD name="length" type="OCTETSTRING" byteLength="2"
          defaultValue="0000" uiShowName="Tag Length" />
      <FIELD name="value" type="OCTETSTRING"
          minByteLength="0" maxByteLength="256"
          uiShowName="Tag Value" />
    </TYPE_DEF>

    <TYPE_DEF name="PPPoETag" type="CHOICE" unitType="ELEMENT"
        uiShowName="PPPoE Tag" >
      <FIELD name="endOfList" type="EndOfListTag"
          uiShowName="End Of List" />
      <FIELD name="serviceName" type="ServiceNameTag"
          uiShowName="Service Name" />
      <FIELD name="acName" type="ACNameTag"
          uiShowName="AC Name" />
      <FIELD name="hostUniq" type="HostUniqTag"
          uiShowName="Host Uniq" />
      <FIELD name="acCookie" type="ACCookieTag"
          uiShowName="AC Cookie" />
      <FIELD name="vendorSpecific" type="VendorSpecificTag"
          uiShowName="Vendor Specific" />
      <FIELD name="relaySessionId" type="RelaySessionIdTag"
          uiShowName="Relay Session Id" />
      <FIELD name="serviceNameError" type="ServiceNameErrorTag"
          uiShowName="Service Name Error" />
      <FIELD name="acSystemError" type="ACSystemErrorTag"
          uiShowName="AC System Error" />
      <FIELD name="genericError" type="GenericErrorTag"
          uiShowName="Generic Error" />
    </TYPE_DEF>

    <TYPE_DEF name="PPPoETagsList" type="SET_OF" unitType="ELEMENT"
        elementType="PPPoETag" minOccurs="0" maxOccurs="16"/>

    <TYPE_DEF name="PPPoEDiscovery" type="SEQUENCE" unitType="HEADER"
        level="2" uiShowName="PPPoE Discovery Header" >

      <FIELD name="version" type="INTEGER" bitLength="4"
          defaultValue="1" uiShowName="Version" />

      <FIELD name="type" type="INTEGER" bitLength="4"
          defaultValue="1" uiShowName="Type" />

      <FIELD name="code" type="PPPoECode" defaultValue="9"
          uiShowName="Code" />

      <FIELD name="sessionId" type="INTEGER" bitLength="16"
          defaultValue="0" uiShowName="Session ID" />

      <FIELD name="length" type="INTEGER" bitLength="16"
          defaultValue="0" uiControlState="CALCULATED"  uiNoShowModifier="Interface,Frame" uiShowName="Length" />

      <FIELD name="tags" type="PPPoETagsList"
          uiShowName="Tags" />
       <NEXT_HEADERS>
        <NXT_HDR id="ppp:PPP" name="PPP Header" value="880B" />
        <NXT_HDR id="custom:Custom" name="Custom" value="0" />
		<NXT_HDR id="customrepeat:CustomRepeat" name="CustomRepeat" value="00" />
      </NEXT_HEADERS>

    </TYPE_DEF>

    <TYPE_DEF name="PPPoESession" type="SEQUENCE" unitType="HEADER"
        level="2" uiShowName="PPPoE Header" niMapped="true">

      <FIELD name="version" type="INTEGER" bitLength="4"
        defaultValue="1"
        uiShowName="Version"
        uiNoShowModifier="Interface"/>

      <FIELD name="type" type="INTEGER" bitLength="4"
          defaultValue="1" uiShowName="Type" uiNoShowModifier="Interface" />

      <FIELD name="code" type="INTEGER" bitLength="8"
          defaultValue="0"
          uiShowName="Code" uiNoShowModifier="Interface"  />

      <FIELD name="sessionId" type="INTEGER" bitLength="16"
          defaultValue="0" uiShowName="Session ID" isNiMapped="true" isNiPrimaryKey="true" uiNoShowModifier="Interface"  />

      <FIELD name="length" type="INTEGER" bitLength="16"
            procfnTarget="PAYLOADLENGTH"
          defaultValue="0" uiControlState="CALCULATED"  uiNoShowModifier="Interface,Frame"  uiShowName="Length" />

      <NEXT_HEADERS>
        <NXT_HDR id="ppp:PPP" name="PPP Header" value="880B" />
        <NXT_HDR id="custom:Custom" name="Custom" value="0" />
		<NXT_HDR id="customrepeat:CustomRepeat" name="CustomRepeat" value="00" />
      </NEXT_HEADERS>
    </TYPE_DEF>

  </TYPE_DEFS>
</PDU_LIBRARY>
